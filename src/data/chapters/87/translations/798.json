{
  "translations": [
    {
      "resource_id": 798,
      "text": "Liswejesyani lina lya Ambuje ŵenu (Allah) Ŵapenani kusyene."
    },
    {
      "resource_id": 798,
      "text": "Aŵala ŵaŵagumbile (chilichose) ni kuchikolosya."
    },
    {
      "resource_id": 798,
      "text": "Soni Aŵala ŵaŵapimiile (chilichose mwachiichiŵeela) ni kuchijongolela (ku yaŵachipimileyo)."
    },
    { "resource_id": 798, "text": "Soni Aŵala ŵaakumesya mopo." },
    { "resource_id": 798, "text": "Ni kuwusyusya kuŵa wejumu ŵakupiililila." },
    { "resource_id": 798, "text": "Chitunsoomesye ni ngan'diŵalila." },
    {
      "resource_id": 798,
      "text": "Ikaŵeje yasachile Allah, chisimu Jwalakwe jukumanyililaga yakuonechela ni yayikasasisika."
    },
    {
      "resource_id": 798,
      "text": "Ni chitun'jepepachisye litala lyakwalukangana (lyakutendela yambone)."
    },
    {
      "resource_id": 798,
      "text": "Basi kumbusyani naga kumbusyako tikuŵe kwakamuchisya."
    },
    {
      "resource_id": 798,
      "text": "Talikumbusye mundu jwaakun'jogopa (Allah)."
    },
    { "resource_id": 798, "text": "Ni takuŵambale (mundu) jwakusakala nnope." },
    { "resource_id": 798, "text": "Ajula jwatakajinjile ku Moto Wekulungwa." },
    {
      "resource_id": 798,
      "text": "Soni ngachija kumpata chiwa mwalakwemo, nambo soni ngaaja kuŵa ni umi (wambone)."
    },
    {
      "resource_id": 798,
      "text": "Pamasile paapundile mundu jwaliswejesye (ntima wakwe)."
    },
    {
      "resource_id": 798,
      "text": "Ni akuŵa ali nkulikumbuchila Lina lya Ambujegwe (pakulikolanga) aku achiswaliji."
    },
    {
      "resource_id": 798,
      "text": "Nambo ŵanganyammwe nkulongosya umi wa duniya."
    },
    {
      "resource_id": 798,
      "text": "Kutendaga Akhera ni jambone nnope soni jandamo."
    },
    {
      "resource_id": 798,
      "text": "Chisimu yalakweyi yili mu ikalakala yandanda."
    },
    { "resource_id": 798, "text": "Ikalakala ya Ibrahima ni Musa." }
  ],
  "meta": {
    "translation_name": "Abdul Hamid Silika",
    "author_name": "Abdul Hamid Silika",
