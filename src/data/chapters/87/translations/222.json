{
  "translations": [
    {
      "resource_id": 222,
      "text": "(Мұхаммед Ғ.С) ұлы Раббыңның атын дәріпте."
    },
    { "resource_id": 222, "text": "Ол сондай Алла; жаратып,толықтаған," },
    { "resource_id": 222, "text": "Ол сондай Алла,шамалап оңғарған." },
    { "resource_id": 222, "text": "Ол сондай Алла,жайылысты жаратқан." },
    { "resource_id": 222, "text": "Сосын оны қапқара қоқымға айналдырған." },
    {
      "resource_id": 222,
      "text": "(Мұхаммед Ғ.С) саған Құран оқытамыз ; сонда оны ұмытпайсың."
    },
    {
      "resource_id": 222,
      "text": "Бірақ,Алланың қалағаны босқа. Негізінде Ол,көрнеу,көместі біледі.(,6А)"
    },
    { "resource_id": 222, "text": "Саған оңай болғанды қолайластырамыз." },
    { "resource_id": 222, "text": "Пайда береті болса,насихат ет." },
    { "resource_id": 222, "text": "Әлде кім Алладан қорықса,үгіт алады." },
    { "resource_id": 222, "text": "Сорлы болғандар одан бой тежейді." },
    { "resource_id": 222, "text": "Ол,сондай зор тозаққа салынады." },
    { "resource_id": 222, "text": "Сосын ол арада өлмейді де тірілмейді." },
    { "resource_id": 222, "text": "Әлде кім тазарса,ол кұтылады." },
    {
      "resource_id": 222,
      "text": "Ол,Раббының атын еске алып,намаз оқыды.(Бұл,айт намазыүБ.Ж.М)"
    },
    {
      "resource_id": 222,
      "text": "Бірақ сендер дүние тіршілігін артық көресіңдер."
    },
    { "resource_id": 222, "text": "Ақирет хайырлы да тұрақты." },
    {
      "resource_id": 222,
      "text": "Негізінн бұл үкімдер бұрынғы нұсқаларда бар;"
    },
    { "resource_id": 222, "text": "Ыбырайым,Мұса (Ғ.С) лардың нұсқаларында." }
  ],
