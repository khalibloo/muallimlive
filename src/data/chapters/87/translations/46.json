{
  "translations": [
    { "resource_id": 46, "text": "Waynee Magaca Eebe ee Sarreeya." },
    { "resource_id": 46, "text": "Eebaha abuuray khalqiga ekeeyayna." },
    { "resource_id": 46, "text": "Eebaha wax walba qaddaray Jidkana tusiyey." },
    { "resource_id": 46, "text": "Eebaha soo bixiya Daaqa." },
    {
      "resource_id": 46,
      "text": "Kana dhiga markaas ka dib caws Engagagan oo Madow."
    },
    {
      "resource_id": 46,
      "text": "Waxaan kuu akhriyaynaa Quraanka halmaaminamaysid."
    },
    {
      "resource_id": 46,
      "text": "Wuxuu Eebe doono mooyee, Eebana waa ogyahay waxa muuqda iyo waxa qarsoonba."
    },
    { "resource_id": 46, "text": "Waxaana kuu fududaynaynaa Waddo fiican." },
    {
      "resource_id": 46,
      "text": "Ee wax waani (Nabiyow) markay wax tari waanadu."
    },
    { "resource_id": 46, "text": "Waxaa waantoobi Ruuxa Eebe ka yaabee." },
    { "resource_id": 46, "text": "Waxaana ka fogaan waanada Ruuxa xune." },
    { "resource_id": 46, "text": "Ee ah mid gali Naar wayn." },
    {
      "resource_id": 46,
      "text": "Oon kuna dhimanayn dhexdeeda kuna noolanayn."
    },
    { "resource_id": 46, "text": "Waxaana liibaanay Ruuxii Daahir noqda." },
    { "resource_id": 46, "text": "magaca Eebana Xusa, Salaaddana tukada." },
    {
      "resource_id": 46,
      "text": "Saas ma'aha ee Dadku wuxuu doortay nolosha Adduunyo ee dhaw."
    },
    { "resource_id": 46, "text": "Aakhiraase khayr badan baaqina ah (hadhi)." },
    {
      "resource_id": 46,
      "text": "Arrintaasuna waxay ku sugantahay Kutubtii hore."
    },
    { "resource_id": 46, "text": "Kutubtii (Nabi) Ibraahim iyo (Nabi) Muuse." }
  ],
