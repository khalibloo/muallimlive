{
  "translations": [
    {
      "resource_id": 49,
      "text": "Litakase jina la Mola wako Mlezi aliye juu kabisa,"
    },
    { "resource_id": 49, "text": "Aliye umba, na akaweka sawa," },
    { "resource_id": 49, "text": "Na ambaye amekadiria na akaongoa," },
    { "resource_id": 49, "text": "Na aliye otesha malisho," },
    { "resource_id": 49, "text": "Kisha akayafanya makavu, meusi." },
    { "resource_id": 49, "text": "Tutakusomesha wala hutasahau," },
    {
      "resource_id": 49,
      "text": "Ila akipenda Mwenyezi Mungu. Hakika Yeye anayajua yaliyo dhaahiri na yaliyo fichikana."
    },
    { "resource_id": 49, "text": "Na tutakusahilishia yawe mepesi." },
    { "resource_id": 49, "text": "Basi kumbusha, kama kukumbusha kunafaa." },
    { "resource_id": 49, "text": "Atakumbuka mwenye kuogopa." },
    { "resource_id": 49, "text": "Na atajitenga mbali nayo mpotovu," },
    { "resource_id": 49, "text": "Ambaye atauingia Moto mkubwa." },
    { "resource_id": 49, "text": "Tena humo hatakufa wala hawi hai." },
    { "resource_id": 49, "text": "Hakika amekwisha fanikiwa aliye jitakasa." },
    {
      "resource_id": 49,
      "text": "Na akakumbuka jina la Mola wake Mlezi, na akasali."
    },
    { "resource_id": 49, "text": "Lakini nyinyi mnakhiari maisha ya dunia!" },
    { "resource_id": 49, "text": "Na Akhera ni bora na yenye kudumu zaidi." },
    { "resource_id": 49, "text": "Hakika haya yamo katika Vitabu vya mwanzo," },
    { "resource_id": 49, "text": "Vitabu vya Ibrahimu na Musa." }
  ],
  "meta": {
