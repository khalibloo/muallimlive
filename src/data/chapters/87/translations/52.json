{
  "translations": [
    { "resource_id": 52, "text": "Rabbinin yüce adını tesbih et." },
    { "resource_id": 52, "text": "Yaratıp düzene koyan O'dur." },
    { "resource_id": 52, "text": "Takdir edip hidayeti gösteren O'dur." },
    { "resource_id": 52, "text": "Otlağı çıkaran," },
    {
      "resource_id": 52,
      "text": "Sonra da onu karamsı bir sel köpüğü haline getiren O'dur."
    },
    {
      "resource_id": 52,
      "text": "Bundan böyle sana Kur'ân'ı okutacağız da unutmayacaksın."
    },
    {
      "resource_id": 52,
      "text": "Yalnız Allah'ın dilediği başkadır. Çünkü o açığı da bilir, gizliyi de."
    },
    { "resource_id": 52, "text": "Seni en kolay yola muvaffak kılacağız." },
    {
      "resource_id": 52,
      "text": "Onun için öğüt ver, eğer öğüt fayda verirse."
    },
    { "resource_id": 52, "text": "Saygısı olan öğüt alacaktır." },
    { "resource_id": 52, "text": "Pek bedbaht olan da ondan kaçınacaktır." },
    { "resource_id": 52, "text": "O ki, en büyük ateşe girecektir." },
    {
      "resource_id": 52,
      "text": "Sonra ne ölecek onda, ne de hayat bulacaktır."
    },
    {
      "resource_id": 52,
      "text": "Doğrusu felah buldu (günahtan) temizlenen.  "
    },
    { "resource_id": 52, "text": "Rabbinin adını anıp namaz kılan." },
    {
      "resource_id": 52,
      "text": "Fakat siz dünya hayatını tercih ediyorsunuz."
    },
    {
      "resource_id": 52,
      "text": "Oysa ahiret daha hayırlı ve daha kalıcıdır."
    },
    { "resource_id": 52, "text": "Kuşkusuz bu ilk sahifelerde vardır," },
    { "resource_id": 52, "text": "İbrahim ve Musa'nın sahifelerinde." }
  ],
  "meta": {
    "translation_name": "Elmalili Hamdi Yazir",
    "author_name": "Elmalili Hamdi Yazir\t",
