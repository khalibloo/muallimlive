{
  "translations": [
    { "resource_id": 30, "text": "Ylistä Herrasi, Korkeimman, nimeä," },
    { "resource_id": 30, "text": "Hänen, joka luo ja tekee täydelliseksi," },
    { "resource_id": 30, "text": "joka määrää ja ohjaa," },
    { "resource_id": 30, "text": "joka kasvattaa viheriän laidunruohon" },
    { "resource_id": 30, "text": "ja kuivattaa sen jälleen tomun harmaaksi." },
    {
      "resource_id": 30,
      "text": "Me annamme sinun julkilukea (Koraania), etkä ole mitään unohtava,"
    },
    {
      "resource_id": 30,
      "text": "paitsi mitä Jumala tahtoo: totisesti Hän tietää, mitä ilmaistaan ja mikä pidetään salassa."
    },
    { "resource_id": 30, "text": "Tahdomme tasoittaa sinun lähetystiesi." },
    {
      "resource_id": 30,
      "text": "Käy siis eteenpäin varoittaen, totisesti koituu varoituksesta hyvää:"
    },
    {
      "resource_id": 30,
      "text": "se, joka (Jumalaa) pelkää, ottaa siitä vaarin,"
    },
    { "resource_id": 30, "text": "mutta viheliäinen karttaa sitä," },
    { "resource_id": 30, "text": "hän joutuu ankarimpaan tuleen," },
    { "resource_id": 30, "text": "jossa hän ei elä eikä kuole." },
    {
      "resource_id": 30,
      "text": "Mutta onnellinen se, joka tekee parannuksen,"
    },
    { "resource_id": 30, "text": "ylistää Herransa nimeä ja rukoilee." },
    {
      "resource_id": 30,
      "text": "Mutta te pidätte parempana tämän maailman elämää,"
    },
    {
      "resource_id": 30,
      "text": "vaikka tuleva elämä on parempi ja kestävämpi."
    },
    {
      "resource_id": 30,
      "text": "Näin on totisesti vanhimmissa kirjoituksissa,"
    },
    { "resource_id": 30, "text": "Aabrahamin ja Mooseksen kirjoissa." }
  ],
  "meta": {
    "translation_name": "Finnish",
