    { "resource_id": 57, "text": "Kullu nafsin bima kasabat raheena" },
    { "resource_id": 57, "text": "Illa as-haba alyameen" },
    { "resource_id": 57, "text": "Fee jannatin yatasaaloon" },
    { "resource_id": 57, "text": "AAani almujrimeen" },
    { "resource_id": 57, "text": "Ma salakakum fee saqar" },
    { "resource_id": 57, "text": "Qaloo lam naku mina almusalleen" },
