{
  "verses": [
    { "id": 6158, "verse_key": "101:1", "text_imlaei": "الْقَارِعَةُ" },
    { "id": 6159, "verse_key": "101:2", "text_imlaei": "مَا الْقَارِعَةُ" },
    {
      "id": 6160,
      "verse_key": "101:3",
      "text_imlaei": "وَمَا أَدْرَاكَ مَا الْقَارِعَةُ"
    },
    {
      "id": 6161,
      "verse_key": "101:4",
      "text_imlaei": "يَوْمَ يَكُونُ النَّاسُ كَالْفَرَاشِ الْمَبْثُوثِ"
    },
    {
      "id": 6162,
      "verse_key": "101:5",
      "text_imlaei": "وَتَكُونُ الْجِبَالُ كَالْعِهْنِ الْمَنفُوشِ"
    },
    {
      "id": 6163,
      "verse_key": "101:6",
      "text_imlaei": "فَأَمَّا مَن ثَقُلَتْ مَوَازِينُهُ"
    },
    {
      "id": 6164,
      "verse_key": "101:7",
      "text_imlaei": "فَهُوَ فِي عِيشَةٍ رَّاضِيَةٍ"
    },
    {
      "id": 6165,
      "verse_key": "101:8",
      "text_imlaei": "وَأَمَّا مَنْ خَفَّتْ مَوَازِينُهُ"
    },
    { "id": 6166, "verse_key": "101:9", "text_imlaei": "فَأُمُّهُ هَاوِيَةٌ" },
    {
      "id": 6167,
      "verse_key": "101:10",
      "text_imlaei": "وَمَا أَدْرَاكَ مَا هِيَهْ"
    },
    { "id": 6168, "verse_key": "101:11", "text_imlaei": "نَارٌ حَامِيَةٌ" }
  ],
  "meta": { "filters": { "chapter_number": "101" } }
}
