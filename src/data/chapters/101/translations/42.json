{
  "translations": [
    { "resource_id": 42, "text": "Wydarzenie przerażające!" },
    { "resource_id": 42, "text": "Co to jest wydarzenie przerażające?" },
    {
      "resource_id": 42,
      "text": "I co ciebie pouczy, co to jest wydarzenie przerażające?"
    },
    {
      "resource_id": 42,
      "text": "Tego Dnia ludzie będą podobni do rozproszonych motyli,"
    },
    {
      "resource_id": 42,
      "text": "A góry będą podobne do różnobarwnej gręplowanej wełny."
    },
    { "resource_id": 42, "text": "Wtedy ten, którego szalki będą ciężkie," },
    { "resource_id": 42, "text": "Zazna życia przyjemnego;" },
    { "resource_id": 42, "text": "A ten; którego szalki będą lekkie," },
    { "resource_id": 42, "text": "Pogrąży się w głębi Czeluści." },
    { "resource_id": 42, "text": "A co ciebie pouczy, co to jest Czeluść?" },
    { "resource_id": 42, "text": "To jest Ogień Palący!" }
  ],
