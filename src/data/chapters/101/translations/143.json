{
  "translations": [
    { "resource_id": 143, "text": "ڕۆژی داچڵەکێنەر (ناوە بۆ ڕۆژی قیامەت)" },
    { "resource_id": 143, "text": "ئاخۆ چۆن بێ ئەو ڕۆژە؟" },
    { "resource_id": 143, "text": "تۆ چوزانییت ئەو ڕۆژە چیە؟" },
    {
      "resource_id": 143,
      "text": "ڕۆژێکە خەڵکی وەك پەپولەی پەراگەندە بڵاودەبنەوە"
    },
    { "resource_id": 143, "text": "وە شاخەکان وەك خوری شیکراویان لێ دێت" },
    { "resource_id": 143, "text": "ئەمجا ئەوەی کێشی(چاکەکانی) قورس وگران بێت" },
    { "resource_id": 143, "text": "ئەوە ئەو کەسە لە گوزەرانێکی پەسەندا ئەبێت" },
    { "resource_id": 143, "text": "وە ئەوەی کێشی(چاکەکانی) سووک بێت" },
    { "resource_id": 143, "text": "ئەوە جێی ئەو (ھاویە) یە" },
    { "resource_id": 143, "text": "تۆ چوزانییت (ھاویە) چیە؟" },
    { "resource_id": 143, "text": "ئاگرێکی ھەڵگیرساوە" }
  ],
