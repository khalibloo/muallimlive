{
  "translations": [
    { "resource_id": 74, "text": "Қориъаҳ." },
    { "resource_id": 74, "text": "Чист қориъаҳ?" },
    { "resource_id": 74, "text": "ва ту чӣ донӣ, ки қориъаҳ чист?" },
    {
      "resource_id": 74,
      "text": "Рӯзест, ки мардум чун парвонагон пароканда бошанд"
    },
    { "resource_id": 74, "text": "ва кӯҳҳо чун пашми задашуда." },
    {
      "resource_id": 74,
      "text": "Аммо ҳар кӣ паллаи некиҳои тарозуяш вазнин бошад,"
    },
    { "resource_id": 74, "text": "Пас ӯ дар айши писандида бошад." },
    {
      "resource_id": 74,
      "text": "ва аммо ҳар кӣ сабук шуд паллаи некиҳои тарозуяш"
    },
    { "resource_id": 74, "text": "ҷойгоҳаш дар ҳовияҳ аст." },
    { "resource_id": 74, "text": "Ва ту чӣ донӣ, ки ҳовияҳ чист?" },
    { "resource_id": 74, "text": "Оташест дар ниҳояти гармӣ." }
  ],
