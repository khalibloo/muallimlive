{
  "translations": [
    { "resource_id": 28, "text": "¡La Calamidad!" },
    { "resource_id": 28, "text": "¿Qué es la Calamidad?" },
    { "resource_id": 28, "text": "Y ¿cómo sabrás qué es la calamidad?" },
    {
      "resource_id": 28,
      "text": "El día que los hombres parezcan mariposas dispersas"
    },
    { "resource_id": 28, "text": "y las montañas copos de lana cardada," },
    { "resource_id": 28, "text": "entonces, el autor de obras de peso" },
    { "resource_id": 28, "text": "gozará de una vida agradable," },
    { "resource_id": 28, "text": "mientras que el autor de obras ligeras" },
    { "resource_id": 28, "text": "tendrá un abismo por morada." },
    { "resource_id": 28, "text": "Y ¿cómo sabrás qué es?" },
    { "resource_id": 28, "text": "¡Un fuego ardiente!" }
  ],
