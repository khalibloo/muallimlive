{
  "translations": [
    { "resource_id": 222, "text": "(Мұхаммед Ғ.С) көңіліңді ашпадық па?" },
    { "resource_id": 222, "text": "Жүгіңді түсірмедік пе?" },
    { "resource_id": 222, "text": "Сондай беліңді қайыстырған," },
    { "resource_id": 222, "text": "Сенің абыройыңды көтермедік пе?" },
    {
      "resource_id": 222,
      "text": "Негізінде ауырлықпен бірге бір жеңілдік бар."
    },
    {
      "resource_id": 222,
      "text": "Шын мәнінде қиыншылықпен бірге бір оңайлық бар."
    },
    { "resource_id": 222, "text": "Раббыңа қарай ұмтыл." },
    { "resource_id": 222, "text": "Раббыңа ғана бел байла." }
  ],
