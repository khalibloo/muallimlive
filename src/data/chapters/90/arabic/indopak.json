{
  "verses": [
    {
      "id": 6024,
      "verse_key": "90:1",
      "text_indopak": "لَاۤ اُقۡسِمُ بِهٰذَا الۡبَلَدِۙ‏"
    },
    {
      "id": 6025,
      "verse_key": "90:2",
      "text_indopak": "وَاَنۡتَ حِلٌّ ۢ بِهٰذَا الۡبَلَدِۙ‏"
    },
    {
      "id": 6026,
      "verse_key": "90:3",
      "text_indopak": "وَوَالِدٍ وَّمَا وَلَدَ ۙ‏"
    },
    {
      "id": 6027,
      "verse_key": "90:4",
      "text_indopak": "لَقَدۡ خَلَقۡنَا الۡاِنۡسَانَ فِىۡ كَبَدٍؕ‏"
    },
    {
      "id": 6028,
      "verse_key": "90:5",
      "text_indopak": "اَيَحۡسَبُ اَنۡ لَّنۡ يَّقۡدِرَ عَلَيۡهِ اَحَدٌ​ ۘ‏"
    },
    {
      "id": 6029,
      "verse_key": "90:6",
      "text_indopak": "يَقُوۡلُ اَهۡلَكۡتُ مَالًا لُّبَدًا ؕ‏"
    },
    {
      "id": 6030,
      "verse_key": "90:7",
      "text_indopak": "اَيَحۡسَبُ اَنۡ لَّمۡ يَرَهٗۤ اَحَدٌ ؕ‏"
    },
    {
      "id": 6031,
      "verse_key": "90:8",
      "text_indopak": "اَلَمۡ نَجۡعَلۡ لَّهٗ عَيۡنَيۡنِۙ‏"
    },
    {
      "id": 6032,
      "verse_key": "90:9",
      "text_indopak": "وَلِسَانًا وَّشَفَتَيۡنِۙ‏"
    },
    {
      "id": 6033,
      "verse_key": "90:10",
      "text_indopak": "وَهَدَيۡنٰهُ النَّجۡدَيۡنِ​ۚ‏"
    },
    {
      "id": 6034,
      "verse_key": "90:11",
      "text_indopak": "فَلَا اقۡتَحَمَ الۡعَقَبَةَ ۖ‏"
    },
    {
      "id": 6035,
      "verse_key": "90:12",
      "text_indopak": "وَمَاۤ اَدۡرٰٮكَ مَا الۡعَقَبَةُ ؕ‏"
    },
    { "id": 6036, "verse_key": "90:13", "text_indopak": "فَكُّ رَقَبَةٍ ۙ‏" },
    {
      "id": 6037,
      "verse_key": "90:14",
      "text_indopak": "اَوۡ اِطۡعٰمٌ فِىۡ يَوۡمٍ ذِىۡ مَسۡغَبَةٍ ۙ‏"
    },
    {
      "id": 6038,
      "verse_key": "90:15",
      "text_indopak": "يَّتِيۡمًا ذَا مَقۡرَبَةٍ ۙ‏"
    },
    {
      "id": 6039,
      "verse_key": "90:16",
      "text_indopak": "اَوۡ مِسۡكِيۡنًا ذَا مَتۡرَبَةٍ ؕ‏"
    },
    {
      "id": 6040,
      "verse_key": "90:17",
      "text_indopak": "ثُمَّ كَانَ مِنَ الَّذِيۡنَ اٰمَنُوۡا وَتَوَاصَوۡا بِالصَّبۡرِ وَتَوَاصَوۡا بِالۡمَرۡحَمَةِ ؕ‏"
    },
    {
      "id": 6041,
      "verse_key": "90:18",
      "text_indopak": "اُولٰٓٮِٕكَ اَصۡحٰبُ الۡمَيۡمَنَةِ ؕ‏"
    },
    {
      "id": 6042,
      "verse_key": "90:19",
      "text_indopak": "وَالَّذِيۡنَ كَفَرُوۡا بِاٰيٰتِنَا هُمۡ اَصۡحٰبُ الۡمَشۡـَٔـمَةِ ؕ‏"
    },
    {
      "id": 6043,
      "verse_key": "90:20",
      "text_indopak": "عَلَيۡهِمۡ نَارٌ مُّؤۡصَدَةٌ‏"
    }
  ],
  "meta": { "filters": { "chapter_number": "90" } }
}
