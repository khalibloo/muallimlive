{
  "verses": [
    { "id": 5994, "verse_key": "89:1", "text_imlaei": "وَالْفَجْرِ" },
    { "id": 5995, "verse_key": "89:2", "text_imlaei": "وَلَيَالٍ عَشْرٍ" },
    {
      "id": 5996,
      "verse_key": "89:3",
      "text_imlaei": "وَالشَّفْعِ وَالْوَتْرِ"
    },
    {
      "id": 5997,
      "verse_key": "89:4",
      "text_imlaei": "وَاللَّيْلِ إِذَا يَسْرِ"
    },
    {
      "id": 5998,
      "verse_key": "89:5",
      "text_imlaei": "هَلْ فِي ذَٰلِكَ قَسَمٌ لِّذِي حِجْرٍ"
    },
    {
      "id": 5999,
      "verse_key": "89:6",
      "text_imlaei": "أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِعَادٍ"
    },
    {
      "id": 6000,
      "verse_key": "89:7",
      "text_imlaei": "إِرَمَ ذَاتِ الْعِمَادِ"
    },
    {
      "id": 6001,
      "verse_key": "89:8",
      "text_imlaei": "الَّتِي لَمْ يُخْلَقْ مِثْلُهَا فِي الْبِلَادِ"
    },
    {
      "id": 6002,
      "verse_key": "89:9",
      "text_imlaei": "وَثَمُودَ الَّذِينَ جَابُوا الصَّخْرَ بِالْوَادِ"
    },
    {
      "id": 6003,
      "verse_key": "89:10",
      "text_imlaei": "وَفِرْعَوْنَ ذِي الْأَوْتَادِ"
    },
    {
      "id": 6004,
      "verse_key": "89:11",
      "text_imlaei": "الَّذِينَ طَغَوْا فِي الْبِلَادِ"
    },
    {
      "id": 6005,
      "verse_key": "89:12",
      "text_imlaei": "فَأَكْثَرُوا فِيهَا الْفَسَادَ"
    },
    {
      "id": 6006,
      "verse_key": "89:13",
      "text_imlaei": "فَصَبَّ عَلَيْهِمْ رَبُّكَ سَوْطَ عَذَابٍ"
    },
    {
      "id": 6007,
      "verse_key": "89:14",
      "text_imlaei": "إِنَّ رَبَّكَ لَبِالْمِرْصَادِ"
    },
    {
      "id": 6008,
      "verse_key": "89:15",
      "text_imlaei": "فَأَمَّا الْإِنسَانُ إِذَا مَا ابْتَلَاهُ رَبُّهُ فَأَكْرَمَهُ وَنَعَّمَهُ فَيَقُولُ رَبِّي أَكْرَمَنِ"
    },
    {
      "id": 6009,
      "verse_key": "89:16",
      "text_imlaei": "وَأَمَّا إِذَا مَا ابْتَلَاهُ فَقَدَرَ عَلَيْهِ رِزْقَهُ فَيَقُولُ رَبِّي أَهَانَنِ"
    },
    {
      "id": 6010,
      "verse_key": "89:17",
      "text_imlaei": "كَلَّا ۖ بَل لَّا تُكْرِمُونَ الْيَتِيمَ"
    },
    {
      "id": 6011,
      "verse_key": "89:18",
      "text_imlaei": "وَلَا تَحَاضُّونَ عَلَىٰ طَعَامِ الْمِسْكِينِ"
    },
    {
      "id": 6012,
      "verse_key": "89:19",
      "text_imlaei": "وَتَأْكُلُونَ التُّرَاثَ أَكْلًا لَّمًّا"
    },
    {
      "id": 6013,
      "verse_key": "89:20",
      "text_imlaei": "وَتُحِبُّونَ الْمَالَ حُبًّا جَمًّا"
    },
    {
      "id": 6014,
      "verse_key": "89:21",
      "text_imlaei": "كَلَّا إِذَا دُكَّتِ الْأَرْضُ دَكًّا دَكًّا"
    },
    {
      "id": 6015,
      "verse_key": "89:22",
      "text_imlaei": "وَجَاءَ رَبُّكَ وَالْمَلَكُ صَفًّا صَفًّا"
    },
    {
      "id": 6016,
      "verse_key": "89:23",
      "text_imlaei": "وَجِيءَ يَوْمَئِذٍ بِجَهَنَّمَ ۚ يَوْمَئِذٍ يَتَذَكَّرُ الْإِنسَانُ وَأَنَّىٰ لَهُ الذِّكْرَىٰ"
    },
    {
      "id": 6017,
      "verse_key": "89:24",
      "text_imlaei": "يَقُولُ يَا لَيْتَنِي قَدَّمْتُ لِحَيَاتِي"
    },
    {
      "id": 6018,
      "verse_key": "89:25",
      "text_imlaei": "فَيَوْمَئِذٍ لَّا يُعَذِّبُ عَذَابَهُ أَحَدٌ"
    },
    {
      "id": 6019,
      "verse_key": "89:26",
      "text_imlaei": "وَلَا يُوثِقُ وَثَاقَهُ أَحَدٌ"
    },
    {
      "id": 6020,
      "verse_key": "89:27",
      "text_imlaei": "يَا أَيَّتُهَا النَّفْسُ الْمُطْمَئِنَّةُ"
    },
    {
      "id": 6021,
      "verse_key": "89:28",
      "text_imlaei": "ارْجِعِي إِلَىٰ رَبِّكِ رَاضِيَةً مَّرْضِيَّةً"
    },
    {
      "id": 6022,
      "verse_key": "89:29",
      "text_imlaei": "فَادْخُلِي فِي عِبَادِي"
    },
    { "id": 6023, "verse_key": "89:30", "text_imlaei": "وَادْخُلِي جَنَّتِي" }
  ],
  "meta": { "filters": { "chapter_number": "89" } }
}
