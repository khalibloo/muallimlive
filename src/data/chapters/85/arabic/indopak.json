{
  "verses": [
    {
      "id": 5910,
      "verse_key": "85:1",
      "text_indopak": "وَالسَّمَآءِ ذَاتِ الۡبُرُوۡجِۙ‏"
    },
    {
      "id": 5911,
      "verse_key": "85:2",
      "text_indopak": "وَالۡيَوۡمِ الۡمَوۡعُوۡدِۙ‏"
    },
    {
      "id": 5912,
      "verse_key": "85:3",
      "text_indopak": "وَشَاهِدٍ وَّمَشۡهُوۡدٍؕ‏"
    },
    {
      "id": 5913,
      "verse_key": "85:4",
      "text_indopak": "قُتِلَ اَصۡحٰبُ الۡاُخۡدُوۡدِۙ‏"
    },
    {
      "id": 5914,
      "verse_key": "85:5",
      "text_indopak": "النَّارِ ذَاتِ الۡوَقُوۡدِۙ‏"
    },
    {
      "id": 5915,
      "verse_key": "85:6",
      "text_indopak": "اِذۡ هُمۡ عَلَيۡهَا قُعُوۡدٌ ۙ‏"
    },
    {
      "id": 5916,
      "verse_key": "85:7",
      "text_indopak": "وَّهُمۡ عَلٰى مَا يَفۡعَلُوۡنَ بِالۡمُؤۡمِنِيۡنَ شُهُوۡدٌ ؕ‏"
    },
    {
      "id": 5917,
      "verse_key": "85:8",
      "text_indopak": "وَمَا نَقَمُوۡا مِنۡهُمۡ اِلَّاۤ اَنۡ يُّؤۡمِنُوۡا بِاللّٰهِ الۡعَزِيۡزِ الۡحَمِيۡدِۙ‏ ﻿﻿"
    },
    {
      "id": 5918,
      "verse_key": "85:9",
      "text_indopak": "الَّذِىۡ لَهٗ مُلۡكُ السَّمٰوٰتِ وَالۡاَرۡضِ​ؕ وَ اللّٰهُ عَلٰى كُلِّ شَىۡءٍ شَهِيۡدٌ ؕ‏"
    },
    {
      "id": 5919,
      "verse_key": "85:10",
      "text_indopak": "اِنَّ الَّذِيۡنَ فَتَـنُوا الۡمُؤۡمِنِيۡنَ وَ الۡمُؤۡمِنٰتِ ثُمَّ لَمۡ يَتُوۡبُوۡا فَلَهُمۡ عَذَابُ جَهَنَّمَ وَلَهُمۡ عَذَابُ الۡحَرِيۡقِؕ‏"
    },
    {
      "id": 5920,
      "verse_key": "85:11",
      "text_indopak": "اِنَّ الَّذِيۡنَ اٰمَنُوۡا وَعَمِلُوا الصّٰلِحٰتِ لَهُمۡ جَنّٰتٌ تَجۡرِىۡ مِنۡ تَحۡتِهَا الۡاَنۡهٰرُ ؕ ذٰلِكَ الۡفَوۡزُ الۡكَبِيۡرُؕ‏"
    },
    {
      "id": 5921,
      "verse_key": "85:12",
      "text_indopak": "اِنَّ بَطۡشَ رَبِّكَ لَشَدِيۡدٌ ؕ‏"
    },
    {
      "id": 5922,
      "verse_key": "85:13",
      "text_indopak": "اِنَّهٗ هُوَ يُبۡدِئُ وَيُعِيۡدُ​ ۚ‏"
    },
    {
      "id": 5923,
      "verse_key": "85:14",
      "text_indopak": "وَهُوَ الۡغَفُوۡرُ الۡوَدُوۡدُۙ‏"
    },
    {
      "id": 5924,
      "verse_key": "85:15",
      "text_indopak": "ذُو الۡعَرۡشِ الۡمَجِيۡدُ ۙ‏"
    },
    {
      "id": 5925,
      "verse_key": "85:16",
      "text_indopak": "فَعَّالٌ لِّمَا يُرِيۡدُ ؕ‏"
    },
    {
      "id": 5926,
      "verse_key": "85:17",
      "text_indopak": "هَلۡ اَتٰٮكَ حَدِيۡثُ الۡجُـنُوۡدِۙ‏"
    },
    {
      "id": 5927,
      "verse_key": "85:18",
      "text_indopak": "فِرۡعَوۡنَ وَثَمُوۡدَؕ‏"
    },
    {
      "id": 5928,
      "verse_key": "85:19",
      "text_indopak": "بَلِ الَّذِيۡنَ كَفَرُوۡا فِىۡ تَكۡذِيۡبٍۙ‏"
    },
    {
      "id": 5929,
      "verse_key": "85:20",
      "text_indopak": "وَّاللّٰهُ مِنۡ وَّرَآٮِٕهِمۡ مُّحِيۡطٌۚ‏"
    },
    {
      "id": 5930,
      "verse_key": "85:21",
      "text_indopak": "بَلۡ هُوَ قُرۡاٰنٌ مَّجِيۡدٌ ۙ‏"
    },
    {
      "id": 5931,
      "verse_key": "85:22",
      "text_indopak": "فِىۡ لَوۡحٍ مَّحۡفُوۡظٍ‏"
    }
  ],
  "meta": { "filters": { "chapter_number": "85" } }
}
