    {
      "id": 6231,
      "verse_key": "114:1",
      "text_indopak": "قُلۡ اَعُوۡذُ بِرَبِّ النَّاسِۙ‏"
    },
    { "id": 6232, "verse_key": "114:2", "text_indopak": "مَلِكِ النَّاسِۙ‏" },
    { "id": 6233, "verse_key": "114:3", "text_indopak": "اِلٰهِ النَّاسِۙ‏" },
    {
      "id": 6234,
      "verse_key": "114:4",
      "text_indopak": "مِنۡ شَرِّ الۡوَسۡوَاسِ  ۙ الۡخَـنَّاسِ ۙ‏"
    },
    {
      "id": 6235,
      "verse_key": "114:5",
      "text_indopak": "الَّذِىۡ يُوَسۡوِسُ فِىۡ صُدُوۡرِ النَّاسِۙ‏"
    },
    {
      "id": 6236,
      "verse_key": "114:6",
      "text_indopak": "مِنَ الۡجِنَّةِ وَالنَّاسِ‏"
    }
