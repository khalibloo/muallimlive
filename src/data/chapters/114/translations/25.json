    { "resource_id": 25, "text": "Reci: \"Utječem se Gospodaru ljudi," },
    { "resource_id": 25, "text": "Vladaru ljudi," },
    { "resource_id": 25, "text": "Bogu ljudi," },
    {
      "resource_id": 25,
      "text": "od zla šejtana - koji nanosi zle misli pa se skrije,"
