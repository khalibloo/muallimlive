    {
      "resource_id": 798,
      "text": "Jilani: “Ngulijuya mwa M'mbuje jwa ŵandu.”"
    },
    {
      "resource_id": 798,
      "text": "“Nchimwene jwa ŵandu.”<sup foot_note=195486>1</sup>"
    },
    { "resource_id": 798, "text": "“Nnungu jwa ŵandu.”" },
    {
      "resource_id": 798,
      "text": "“Ku yakusakala ya nkusongona (pakuunyenjelela ntima yakusakala), jwakuujiilila ni kuliisisa (naga nsyene ntimao juli junkolasile Allah).”"
    },
    {
      "resource_id": 798,
      "text": "“Ajula jwaakusongonaga m’mitima mwa ŵandu.”"
    },
    {
      "resource_id": 798,
      "text": "“Jwakuumila m’majini ni mu ŵandu.”<sup foot_note=195487>1</sup>"
    }
  ],
