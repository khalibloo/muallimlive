      "resource_id": 211,
      "text": "Sabihin mo: \"Nagpapakupkop ako sa Panginoon ng mga tao,"
    },
    { "resource_id": 211, "text": "na Hari ng mga tao," },
    { "resource_id": 211, "text": "na Diyos ng mga tao," },
    {
      "resource_id": 211,
      "text": "laban sa kasamaan ng tagapasaring na palaurong,"
    },
    {
      "resource_id": 211,
      "text": "na nagpapasaring sa mga dibdib ng mga tao,"
    },
    { "resource_id": 211, "text": "kabilang sa mga jinn at mga tao.\"" }
