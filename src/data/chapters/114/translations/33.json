    {
      "resource_id": 33,
      "text": "Katakanlah, \"Aku berlindung kepada Tuhannya manusia,"
    },
    { "resource_id": 33, "text": "Raja manusia," },
    { "resource_id": 33, "text": "sembahan manusia," },
    {
      "resource_id": 33,
      "text": "dari kejahatan (bisikan) setan yang bersembunyi,"
    },
    {
      "resource_id": 33,
      "text": "yang membisikkan (kejahatan) ke dalam dada manusia,"
    },
    { "resource_id": 33, "text": "dari (golongan) jin dan manusia.\"" }
