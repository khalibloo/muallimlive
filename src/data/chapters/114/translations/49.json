      "resource_id": 49,
      "text": "Sema: Ninajikinga kwa Mola Mlezi wa wanaadamu,"
    },
    { "resource_id": 49, "text": "Mfalme wa wanaadamu," },
    { "resource_id": 49, "text": "Mungu wa wanaadamu," },
    { "resource_id": 49, "text": "Na shari ya wasiwasi wa Shetani, Khannas," },
    { "resource_id": 49, "text": "Anaye tia wasiwasi katika vifua vya watu," },
    { "resource_id": 49, "text": "Kutokana na majini na wanaadamu." }
