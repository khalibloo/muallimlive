    { "resource_id": 124, "text": "De ki:  İnsanların ilâhına sığınırım!" },
    {
      "resource_id": 124,
      "text": "İnsanların Melikine (mutlak sahip ve hakimine),"
    },
    { "resource_id": 124, "text": "insanların Rabbine sığınırım," },
    {
      "resource_id": 124,
      "text": "(insan Allah'ı andığında) pusuya çekilen şeytanının şerrinden,"
    },
    { "resource_id": 124, "text": "insanların kalplerine vesvese sokan," },
    { "resource_id": 124, "text": "cin ve insanlardan." }
