{
  "translations": [
    { "resource_id": 111, "text": "Jedhi: \"Gooftaa namaattin maganfadha." },
    { "resource_id": 111, "text": "Mootii namaatti," },
    { "resource_id": 111, "text": "Gabbaramaa namaatti," },
    {
      "resource_id": 111,
      "text": "Hamtuu waswaasii dhokatuufi mul’atuu irraa,"
    },
    { "resource_id": 111, "text": "Isa qoma namaa keessa waswaasu irraa," },
    {
      "resource_id": 111,
      "text": "Jinniifi nama irraa kan ta’e (irraan maganfadha).”"
    }
  ],
