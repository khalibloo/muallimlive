{
  "verses": [
    {
      "id": 5552,
      "verse_key": "75:1",
      "text_indopak": "لَاۤ اُقۡسِمُ بِيَوۡمِ الۡقِيٰمَةِۙ‏"
    },
    {
      "id": 5553,
      "verse_key": "75:2",
      "text_indopak": "وَلَاۤ اُقۡسِمُ بِالنَّفۡسِ اللَّوَّامَةِؕ‏"
    },
    {
      "id": 5554,
      "verse_key": "75:3",
      "text_indopak": "اَيَحۡسَبُ الۡاِنۡسَانُ اَلَّنۡ نَّجۡمَعَ عِظَامَهٗؕ‏"
    },
    {
      "id": 5555,
      "verse_key": "75:4",
      "text_indopak": "بَلٰى قٰدِرِيۡنَ عَلٰٓى اَنۡ نُّسَوِّىَ بَنَانَهٗ‏"
    },
    {
      "id": 5556,
      "verse_key": "75:5",
      "text_indopak": "بَلۡ يُرِيۡدُ الۡاِنۡسَانُ لِيَفۡجُرَ اَمَامَهٗ​ۚ‏"
    },
    {
      "id": 5557,
      "verse_key": "75:6",
      "text_indopak": "يَسۡـَٔـلُ اَيَّانَ يَوۡمُ الۡقِيٰمَةِؕ‏"
    },
    {
      "id": 5558,
      "verse_key": "75:7",
      "text_indopak": "فَاِذَا بَرِقَ الۡبَصَرُۙ‏"
    },
    { "id": 5559, "verse_key": "75:8", "text_indopak": "وَخَسَفَ الۡقَمَرُۙ‏" },
    {
      "id": 5560,
      "verse_key": "75:9",
      "text_indopak": "وَجُمِعَ الشَّمۡسُ وَالۡقَمَرُۙ‏"
    },
    {
      "id": 5561,
      "verse_key": "75:10",
      "text_indopak": "يَقُوۡلُ الۡاِنۡسَانُ يَوۡمَٮِٕذٍ اَيۡنَ الۡمَفَرُّ​ ۚ‏"
    },
    { "id": 5562, "verse_key": "75:11", "text_indopak": "كَلَّا لَا وَزَرَؕ‏" },
    {
      "id": 5563,
      "verse_key": "75:12",
      "text_indopak": "اِلٰى رَبِّكَ يَوۡمَٮِٕذِ اۨلۡمُسۡتَقَرُّ ؕ‏"
    },
    {
      "id": 5564,
      "verse_key": "75:13",
      "text_indopak": "يُنَبَّؤُا الۡاِنۡسَانُ يَوۡمَٮِٕذٍۢ بِمَا قَدَّمَ وَاَخَّرَؕ‏"
    },
    {
      "id": 5565,
      "verse_key": "75:14",
      "text_indopak": "بَلِ الۡاِنۡسَانُ عَلٰى نَفۡسِهٖ بَصِيۡرَةٌ ۙ‏"
    },
    {
      "id": 5566,
      "verse_key": "75:15",
      "text_indopak": "وَّلَوۡ اَلۡقٰى مَعَاذِيۡرَهٗؕ‏"
    },
    {
      "id": 5567,
      "verse_key": "75:16",
      "text_indopak": "لَا تُحَرِّكۡ بِهٖ لِسَانَكَ لِتَعۡجَلَ بِهٖؕ‏"
    },
    {
      "id": 5568,
      "verse_key": "75:17",
      "text_indopak": "اِنَّ عَلَيۡنَا جَمۡعَهٗ وَقُرۡاٰنَهٗۚ  ۖ‏"
    },
    {
      "id": 5569,
      "verse_key": "75:18",
      "text_indopak": "فَاِذَا قَرَاۡنٰهُ فَاتَّبِعۡ قُرۡاٰنَهٗ​ۚ‏"
    },
    {
      "id": 5570,
      "verse_key": "75:19",
      "text_indopak": "ثُمَّ اِنَّ عَلَيۡنَا بَيَانَهٗؕ‏"
    },
    {
      "id": 5571,
      "verse_key": "75:20",
      "text_indopak": "كَلَّا بَلۡ تُحِبُّوۡنَ الۡعَاجِلَةَ ۙ‏"
    },
    {
      "id": 5572,
      "verse_key": "75:21",
      "text_indopak": "وَتَذَرُوۡنَ الۡاٰخِرَةَ ؕ‏"
    },
    {
      "id": 5573,
      "verse_key": "75:22",
      "text_indopak": "وُجُوۡهٌ يَّوۡمَٮِٕذٍ نَّاضِرَةٌ ۙ‏"
    },
    {
      "id": 5574,
      "verse_key": "75:23",
      "text_indopak": "اِلٰى رَبِّهَا نَاظِرَةٌ​ ۚ‏"
    },
    {
      "id": 5575,
      "verse_key": "75:24",
      "text_indopak": "وَوُجُوۡهٌ يَّوۡمَٮِٕذٍۢ بَاسِرَةٌ ۙ‏"
    },
    {
      "id": 5576,
      "verse_key": "75:25",
      "text_indopak": "تَظُنُّ اَنۡ يُّفۡعَلَ بِهَا فَاقِرَةٌ ؕ‏"
    },
    {
      "id": 5577,
      "verse_key": "75:26",
      "text_indopak": "كَلَّاۤ اِذَا بَلَغَتِ التَّرَاقِىَۙ‏"
    },
    {
      "id": 5578,
      "verse_key": "75:27",
      "text_indopak": "وَقِيۡلَ مَنۡ رَاقٍۙ‏"
    },
    {
      "id": 5579,
      "verse_key": "75:28",
      "text_indopak": "وَّظَنَّ اَنَّهُ الۡفِرَاقُۙ‏"
    },
    {
      "id": 5580,
      "verse_key": "75:29",
      "text_indopak": "وَالۡتَفَّتِ السَّاقُ بِالسَّاقِۙ‏"
    },
    {
      "id": 5581,
      "verse_key": "75:30",
      "text_indopak": "اِلٰى رَبِّكَ يَوۡمَٮِٕذِ اۨلۡمَسَاقُؕ‏"
    },
    {
      "id": 5582,
      "verse_key": "75:31",
      "text_indopak": "فَلَا صَدَّقَ وَلَا صَلّٰىۙ‏"
    },
    {
      "id": 5583,
      "verse_key": "75:32",
      "text_indopak": "وَلٰڪِنۡ كَذَّبَ وَتَوَلّٰىۙ‏"
    },
    {
      "id": 5584,
      "verse_key": "75:33",
      "text_indopak": "ثُمَّ ذَهَبَ اِلٰٓى اَهۡلِهٖ يَتَمَطّٰىؕ‏"
    },
    {
      "id": 5585,
      "verse_key": "75:34",
      "text_indopak": "اَوۡلٰى لَكَ فَاَوۡلٰىۙ‏"
    },
    {
      "id": 5586,
      "verse_key": "75:35",
      "text_indopak": "ثُمَّ اَوۡلٰى لَكَ فَاَوۡلٰىؕ‏"
    },
    {
      "id": 5587,
      "verse_key": "75:36",
      "text_indopak": "اَيَحۡسَبُ الۡاِنۡسَانُ اَنۡ يُّتۡرَكَ سُدًىؕ‏"
    },
    {
      "id": 5588,
      "verse_key": "75:37",
      "text_indopak": "اَلَمۡ يَكُ نُطۡفَةً مِّنۡ مَّنِىٍّ يُّمۡنٰىۙ‏"
    },
    {
      "id": 5589,
      "verse_key": "75:38",
      "text_indopak": "ثُمَّ كَانَ عَلَقَةً فَخَلَقَ فَسَوّٰىۙ‏"
    },
    {
      "id": 5590,
      "verse_key": "75:39",
      "text_indopak": "فَجَعَلَ مِنۡهُ الزَّوۡجَيۡنِ الذَّكَرَ وَالۡاُنۡثٰىؕ‏"
    },
    {
      "id": 5591,
      "verse_key": "75:40",
      "text_indopak": "اَلَيۡسَ ذٰلِكَ بِقٰدِرٍ عَلٰٓى اَنۡ يُّحۡـىِۦَ الۡمَوۡتٰى‏"
    }
  ],
  "meta": { "filters": { "chapter_number": "75" } }
}
