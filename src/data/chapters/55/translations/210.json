{
  "translations": [
    { "resource_id": 210, "text": "Rahmân." },
    { "resource_id": 210, "text": "Kur’an'ı öğretti." },
    { "resource_id": 210, "text": "İnsanı yarattı." },
    { "resource_id": 210, "text": "Ona beyanı öğretti." },
    { "resource_id": 210, "text": "Güneş ve ay belli bir hesapladır." },
    {
      "resource_id": 210,
      "text": "Bitkiler ve ağaçlar (Allah'a) secde etmektedirler."
    },
    { "resource_id": 210, "text": "Göğü yükseltti ve mizanı (adaleti) koydu." },
    { "resource_id": 210, "text": "O halde ölçüde haddi aşmayın." },
    {
      "resource_id": 210,
      "text": "Tartıyı adaletle tutup doğrultun ve tartıyı noksan tutmayın."
    },
    { "resource_id": 210, "text": "Allah, yeri canlılar için döşedi." },
    {
      "resource_id": 210,
      "text": "Orada meyveler ve tomurcuklu hurmalar vardır."
    },
    {
      "resource_id": 210,
      "text": "Yapraklı taneler, hoş kokulu bitkiler vardır."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "O, insanı pişmiş çamur gibi kuru balçıktan yaratmıştır."
    },
    { "resource_id": 210, "text": "Cinleri de yalın bir ateşten yarattı." },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "O, iki doğunun Rabbidir, iki batının Rabbidir."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "İki denizi birbiriyle kavuşsun diye salıvermiştir."
    },
    {
      "resource_id": 210,
      "text": "Aralarında bir engel vardır, birbirine geçip karışmazlar."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    { "resource_id": 210, "text": "O iki denizden inci ve mercan çıkar." },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "Denizde yüce dağlar gibi yükselen gemiler de O'nundur."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "Onun üzerinde bulunan her şey yok olacaktır."
    },
    {
      "resource_id": 210,
      "text": "Azamet ve ikram sahibi olan Rabbinizin vechi/yüzü bakidir."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "Göklerde ve yerde kim varsa hepsi O’ndan ister. O, her gün bir iştedir."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "Ey insanlar ve cinler! Sizin de hesabınızı göreceğiz."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "Ey cin ve insan toplulukları! Göklerin ve yerin çerçevesinden çıkıp gitmeye gücünüz yetiyorsa geçin. Ancak büyük bir güçle çıkıp gidebilirsiniz."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "Ey insanlar ve cinler! Üzerinize dumansız bir alev ve ateşsiz bir duman gönderilir de kurtulamazsınız."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "Gök yarılıp da kızarmış yağ renginde gül gibi olduğu zaman."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "O gün hiçbir insana ve cine günahı sorulmaz."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "Günahkârlar simalarından tanınır, sonra da perçemlerinden ve ayaklarından yakalanır."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "İşte bu o günahkârların yalan saydığı cehennemdir."
    },
    {
      "resource_id": 210,
      "text": "Onlar, Cehennem ateşi ile kaynar su arasında gider gelirler."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "Rabbinin makamından korkanlara iki Cennet vardır."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "Her ikisi de çeşit çeşit dallı ağaçlara sahiptir."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "Onların ikisinde de akan iki pınar vardır."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "İkisinde de her türlü meyveden çift çift vardır."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "(Orada) astarları kalın atlastan yataklara yaslanırlar. Her iki cennetten devşirilen meyveler de (oradakilere) yakındır."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "Oralarda bakışlarını sadece eşlerine çevirmiş dilberler vardır. Onlara eşlerinden önce ne bir insan, ne de bir cin dokunmuştur."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    { "resource_id": 210, "text": "Sanki onlar yakut ve mercan gibidir." },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "İyiliğin karşılığı iyilikten başkası olabilir mi?"
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "Bu ikisinden başka iki Cennet daha vardır."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    { "resource_id": 210, "text": "İkisi de yemyeşildirler." },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "İkisinde de suları durmaksızın coşan iki pınar vardır."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "Her ikisinde de meyve, hurma ve nar vardır."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "Orada iyi huylu güzel yüzlü kadınlar vardır."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "Otağlar içerisinde sahiplerine tahsis edilmiş huriler vardır."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "Onlara eşlerinden önce ne bir insan, ne de bir cin dokunmuştur."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "Yeşil yastıklara ve harikulâde güzel döşemelere yaslanırlar."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse Rabbinizin hangi nimetlerini yalanlayabilirsiniz?"
    },
    {
      "resource_id": 210,
      "text": "Azamet ve ikram sahibi Rabbinin adı ne yücedir."
    }
  ],
