{
  "translations": [
    { "resource_id": 57, "text": "Arrahman" },
    { "resource_id": 57, "text": "AAallama alqur-an" },
    { "resource_id": 57, "text": "Khalaqa al-insan" },
    { "resource_id": 57, "text": "AAallamahu albayan" },
    { "resource_id": 57, "text": "Ashshamsu walqamaru bihusban" },
    { "resource_id": 57, "text": "Wannajmu washshajaru yasjudan" },
    { "resource_id": 57, "text": "Wassamaa rafaAAahawawadaAAa almeezan" },
    { "resource_id": 57, "text": "Alla tatghaw fee almeezan" },
    {
      "resource_id": 57,
      "text": "Waaqeemoo alwazna bilqisti walatukhsiroo almeezan"
    },
    { "resource_id": 57, "text": "Wal-arda wadaAAahalil-anam" },
    { "resource_id": 57, "text": "Feeha fakihatun wannakhluthatu al-akmam" },
    { "resource_id": 57, "text": "Walhabbu thoo alAAasfiwarrayhan" },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    { "resource_id": 57, "text": "Khalaqa al-insana min salsalinkalfakhkhar" },
    { "resource_id": 57, "text": "Wakhalaqa aljanna min marijinmin nar" },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    { "resource_id": 57, "text": "Rabbu almashriqayni warabbu almaghribayn" },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    { "resource_id": 57, "text": "Maraja albahrayni yaltaqiyan" },
    { "resource_id": 57, "text": "Baynahuma barzakhun la yabghiyan" },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    { "resource_id": 57, "text": "Yakhruju minhuma allu/luo walmarjan" },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththibani" },
    {
      "resource_id": 57,
      "text": "Walahu aljawari almunshaatufee albahri kal-aAAlam"
    },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    { "resource_id": 57, "text": "Kullu man AAalayha fan" },
    {
      "resource_id": 57,
      "text": "Wayabqa wajhu rabbika thooaljalali wal-ikram"
    },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    {
      "resource_id": 57,
      "text": "Yas-aluhu man fee assamawatiwal-ardi kulla yawmin huwa fee sha/n"
    },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    { "resource_id": 57, "text": "Sanafrughu lakum ayyuha aththaqalan" },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    {
      "resource_id": 57,
      "text": "Ya maAAshara aljinni wal-insiini istataAAtum an tanfuthoo min aqtari assamawatiwal-ardi fanfuthoo la tanfuthoonailla bisultan"
    },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    {
      "resource_id": 57,
      "text": "Yursalu AAalaykuma shuwathunmin narin wanuhasun fala tantasiran"
    },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    {
      "resource_id": 57,
      "text": "Fa-itha inshaqqati assamaofakanat wardatan kaddihan"
    },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    {
      "resource_id": 57,
      "text": "Fayawma-ithin la yus-alu AAan thanbihiinsun wala jan"
    },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    {
      "resource_id": 57,
      "text": "YuAArafu almujrimoona biseemahumfayu/khathu binnawasee wal-aqdam"
    },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    {
      "resource_id": 57,
      "text": "Hathihi jahannamu allatee yukaththibubiha almujrimoon"
    },
    { "resource_id": 57, "text": "Yatoofoona baynaha wabayna hameeminan" },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    { "resource_id": 57, "text": "Waliman khafa maqama rabbihijannatan" },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    { "resource_id": 57, "text": "Thawata afnan" },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    { "resource_id": 57, "text": "Feehima AAaynani tajriyan" },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    { "resource_id": 57, "text": "Feehima min kulli fakihatinzawjan" },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    {
      "resource_id": 57,
      "text": "Muttaki-eena AAala furushin bata-inuhamin istabraqin wajana aljannatayni dan"
    },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    {
      "resource_id": 57,
      "text": "Feehinna qasiratu attarfilam yatmithhunna insun qablahum wala jan"
    },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    { "resource_id": 57, "text": "Kaannahunna alyaqootu walmarjan" },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    { "resource_id": 57, "text": "Hal jazao al-ihsani illaal-ihsan" },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    { "resource_id": 57, "text": "Wamin doonihima jannatan" },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    { "resource_id": 57, "text": "Mudhammatan" },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    { "resource_id": 57, "text": "Feehima AAaynani naddakhatan" },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    { "resource_id": 57, "text": "Feehima fakihatun wanakhlunwarumman" },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    { "resource_id": 57, "text": "Feehinna khayratun hisan" },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    { "resource_id": 57, "text": "Hoorun maqsooratun feealkhiyam" },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    { "resource_id": 57, "text": "Lam yatmithhunna insun qablahum walajan" },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    {
      "resource_id": 57,
      "text": "Muttaki-eena AAala rafrafin khudrinwaAAabqariyyin hisan"
    },
    { "resource_id": 57, "text": "Fabi-ayyi ala-i rabbikumatukaththiban" },
    {
      "resource_id": 57,
      "text": "Tabaraka ismu rabbika theealjalali wal-ikram"
    }
