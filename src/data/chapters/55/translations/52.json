{
  "translations": [
    { "resource_id": 52, "text": "Rahmân (çok merhametli olan Allah)" },
    { "resource_id": 52, "text": "Kurân'ı öğretti." },
    { "resource_id": 52, "text": "İnsanı yarattı." },
    { "resource_id": 52, "text": "Ona beyanı öğretti." },
    { "resource_id": 52, "text": "Güneş de ay da bir hesab iledir." },
    { "resource_id": 52, "text": "Bitkiler ve ağaçlar secde etmektedirler." },
    { "resource_id": 52, "text": "Göğü yükseltti ve mizanı koydu." },
    { "resource_id": 52, "text": "Sakın tartıda taşkınlık etmeyin." },
    {
      "resource_id": 52,
      "text": "Tartıyı adaletle yapın, terazide eksiklik yapmayın."
    },
    {
      "resource_id": 52,
      "text": "(Allah) yeri mahlukat için (aşağıya) koydu."
    },
    {
      "resource_id": 52,
      "text": "Orada meyvalar ve salkımlı hurma ağaçları vardır."
    },
    {
      "resource_id": 52,
      "text": "Yapraklı taneler ve hoş kokulu bitkiler vardır."
    },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    {
      "resource_id": 52,
      "text": "Allah insanı, pişmiş bir çamura benzeyen bir balçıktan yarattı."
    },
    { "resource_id": 52, "text": "Cinleri de hâlis ateşten yarattı." },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    { "resource_id": 52, "text": "(O) iki doğunun ve iki batının Rabbidir." },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    {
      "resource_id": 52,
      "text": "(Acı ve tatlı) iki denizi salıverdi birbirine kavuşuyorlar."
    },
    {
      "resource_id": 52,
      "text": "Fakat aralarında bir engel vardır, birbirlerine geçip karışmıyorlar."
    },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    { "resource_id": 52, "text": "İkisinden de inci ve mercan çıkar." },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    {
      "resource_id": 52,
      "text": "Denizde koca dağlar gibi yükselen gemiler de onundur."
    },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    { "resource_id": 52, "text": "Yer üzerinde bulunan her şey fânidir." },
    {
      "resource_id": 52,
      "text": "Yalnız celâl ve ikram sahibi Rabbinin yüzü (zâtı) baki kalacaktır."
    },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    {
      "resource_id": 52,
      "text": "Göklerde ve yerde bulunanlar, O'ndan isterler. O, her gün yeni bir iştedir."
    },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    {
      "resource_id": 52,
      "text": "Ey insan ve cin! sizin de hesabınızı ele alacağız."
    },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    {
      "resource_id": 52,
      "text": "Ey cin ve insan toplulukları! Göklerin ve yerin çevresinden geçmeye gücünüz yeterse geçin gidin. Allah'ın verdiği bir güç olmadan geçemezsiniz."
    },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    {
      "resource_id": 52,
      "text": "Üzerinize ateşten alev ve duman gönderilir, kendinizi savunamazsınız."
    },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz"
    },
    {
      "resource_id": 52,
      "text": "Gök yarılıp da, erimiş yağ gibi kıpkırmızı bir gül olduğu zaman..."
    },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    {
      "resource_id": 52,
      "text": "İşte o gün, ne insana ne de cinne günahından sorulmaz."
    },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    {
      "resource_id": 52,
      "text": "Suçlular simalarından tanınır, alınlarından ve ayaklarından tutulur."
    },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    {
      "resource_id": 52,
      "text": "İşte bu, suçluların yalanladığı cehennemdir."
    },
    { "resource_id": 52, "text": "Onunla kaynar su arasında dolaşırlar." },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    {
      "resource_id": 52,
      "text": "Rabbinin makamından korkan kimselere iki cennet vardır."
    },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    {
      "resource_id": 52,
      "text": "İkisinin de çeşitli ağaçları, meyvaları vardır."
    },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    { "resource_id": 52, "text": "İkisinde de akıp giden iki kaynak vardır." },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    {
      "resource_id": 52,
      "text": "İkisinde de her türlü meyvadan çift çift vardır."
    },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    {
      "resource_id": 52,
      "text": "Astarları atlastan yataklara yaslanırlar. İki cennetin de devşirmesi yakındır."
    },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    {
      "resource_id": 52,
      "text": "Oralarda gözlerini yalnız eşlerine çevirmiş dilberler var ki, bunlardan önce onlara ne insan ne de cin dokunmuştur."
    },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    { "resource_id": 52, "text": "Sanki onlar yâkut ve mercandırlar." },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    {
      "resource_id": 52,
      "text": "İyiliğin karşılığı, yalnız iyilik değil midir?"
    },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    { "resource_id": 52, "text": "Bu ikisinden başka iki cennet daha vardır." },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    { "resource_id": 52, "text": "(Bu cennetler) yemyeşildirler." },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    { "resource_id": 52, "text": "İkisinde de fışkıran iki kaynak vardır." },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    {
      "resource_id": 52,
      "text": "İkisinde de her türlü meyva, hurma ve nar vardır."
    },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    {
      "resource_id": 52,
      "text": "İçlerinde güzel huylu, güzel yüzlü kadınlar vardır."
    },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    {
      "resource_id": 52,
      "text": "Çadırlar içerisinde gözlerini yalnız kocalarına çevirmiş hûriler vardır."
    },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    {
      "resource_id": 52,
      "text": "Bunlardan önce onlara ne insan ne de cin dokunmuştur."
    },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    {
      "resource_id": 52,
      "text": "Yeşil yastıklara ve hârikulâde güzel işlemeli döşeklere yaslanırlar."
    },
    {
      "resource_id": 52,
      "text": "Şimdi Rabbinizin hangi nimetlerini yalanlıyorsunuz?"
    },
    {
      "resource_id": 52,
      "text": "Büyüklük ve ikram sahibi Rabbinin adı ne yücedir!"
    }
  ],
  "meta": {
