{
  "translations": [
    { "resource_id": 210, "text": "Bismillâhirrahmânirrahîm" },
    {
      "resource_id": 210,
      "text": "Hamd, alemlerin Rabbi olan Allah'a mahsustur."
    },
    { "resource_id": 210, "text": "O Rahmân'dır, Rahîm'dir." },
    { "resource_id": 210, "text": "Din/Hesap gününün sahibidir." },
    {
      "resource_id": 210,
      "text": "Yalnız sana ibadet eder ve yalnız senden yardım dileriz."
    },
    { "resource_id": 210, "text": "Bizi doğru yola ilet." },
    {
      "resource_id": 210,
      "text": "Nimet verdiğin kimselerin yoluna. Gazaba uğrayanların ve sapanların değil."
    }
  ],
