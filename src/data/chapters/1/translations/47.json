      "resource_id": 47,
      "text": "﻿Me emrin e All-llahut, Mëshiruesit, Mëshirëbërësit!"
    },
    {
      "resource_id": 47,
      "text": "Falënderimi i takon All-llahut , Zotit të botërave !"
    },
    { "resource_id": 47, "text": "Mëshiruesit, Mëshirëbërësit!" },
    { "resource_id": 47, "text": "Sunduesit të ditës së Gjykimit!" },
    {
      "resource_id": 47,
      "text": "Ty të adhurojmë dhe prej Teje ndihmë kërkojmë!"
    },
    { "resource_id": 47, "text": "Udhëzona në rrugën e drejtë!" },
    {
      "resource_id": 47,
      "text": "Në rrugën e atyre ndaj të cilëve ke bekimin, e jo në të atyre që je i hidhëruar , dhe që kanë humbur !"
    }
