{
  "translations": [
    {
      "resource_id": 111,
      "text": "  (Maleykota) wal hordoftuu ta'anii ergamaniinin kakadha."
    },
    {
      "resource_id": 111,
      "text": "  (Bubbeewwan) humnaan bubbisuu bubbisaniinis;"
    },
    {
      "resource_id": 111,
      "text": "  (Bubbeewwan rooba) faffacaasuu facaasaniinis;"
    },
    {
      "resource_id": 111,
      "text": "  (Keeyyatoota haqaafi dhara) adda baasuu adda baasanittis;"
    },
    { "resource_id": 111, "text": "  (Malaykota) ergaa buuftotattis;" },
    {
      "resource_id": 111,
      "text": "  Sababaa dhabamsiisuuf yookiin akeekkachiisuuf jecha (kan buusanittin kakadha)."
    },
    {
      "resource_id": 111,
      "text": "  Wanti (Qiyaamaan) isin sodaachifamtan argamaadha."
    },
    { "resource_id": 111, "text": "Yeroo urjiiwwan ifti ishee haqamte," },
    { "resource_id": 111, "text": "  Yeroo samiinis babbanamte;" },
    {
      "resource_id": 111,
      "text": "  Yeroo gaarreenis (bakka irraa) bubuqqifamte,"
    },
    {
      "resource_id": 111,
      "text": "  Yeroo Ergamoonnis beellamaan walitii qabamte;"
    },
    { "resource_id": 111, "text": "  Guyyaa kamiif beellamaman?" },
    {
      "resource_id": 111,
      "text": "  Guyyaa addaan bahiinsaatiif (beellamaman)"
    },
    {
      "resource_id": 111,
      "text": "  Guyyaan Addaan bahiinsaa maal akka ta'e maaltu si beeksise?"
    },
    {
      "resource_id": 111,
      "text": "  Kijibsiiftotaaf guyyaa san badiin mirkanaa'e"
    },
    {
      "resource_id": 111,
      "text": "  Sila Nuti warra duraanii hin balleessinee?"
    },
    {
      "resource_id": 111,
      "text": "  Ergasiis warra boodaa isaan hordofsiifna."
    },
    { "resource_id": 111, "text": "  Yakkamtootas akkanuma goona." },
    {
      "resource_id": 111,
      "text": "  Kijibsiiftotaaf guyyaa san badiin mirkanaa'e."
    },
    {
      "resource_id": 111,
      "text": "  Sila Nuti bishaan dadhabaa (maniyyii) irraa isin hin uumnee?"
    },
    { "resource_id": 111, "text": "  Bakka mijaa'aa keessas isa goone." },
    { "resource_id": 111, "text": "  Hanga yeroo beekamaatti" },
    {
      "resource_id": 111,
      "text": "  Ni shallagnes; nuti shallagdoota akkaan tole"
    },
    {
      "resource_id": 111,
      "text": "  Kijibsiiftotaaf guyyaa san badiin mirkanaa'e."
    },
    {
      "resource_id": 111,
      "text": "  Sila Nuti dachii kan hunda walitti qabdu hin taasifnee?"
    },
    { "resource_id": 111, "text": "  Jiraafi du'aas (walitti qabdi)?" },
    {
      "resource_id": 111,
      "text": "  Gaarreen ragga'oo dhedheeraa ishee keessatti gooneBishaan mi'aawaas isin obaafne."
    },
    {
      "resource_id": 111,
      "text": "  Kijibsiiftotaaf guyyaa san badiin mirkanaa'e."
    },
    {
      "resource_id": 111,
      "text": "  (Isaaniin ni jedhama): \"gara waan sobsiisaa turtanii deemaa."
    },
    {
      "resource_id": 111,
      "text": "  Gara gaaddisa abbaa damee sadihii deemaa.\""
    },
    {
      "resource_id": 111,
      "text": "  Gaaddiseessaa miti; boba'aa ibiddaa irraas hin duroomsu"
    },
    { "resource_id": 111, "text": "  Isheen qaanqeewwan akka gamoo darbatti." },
    {
      "resource_id": 111,
      "text": "  Inni (qaanqechi) gaalota bobboora fakkaata."
    },
    {
      "resource_id": 111,
      "text": "  Kijibsiiftotaaf guyyaa san badiin mirkanaa'e."
    },
    { "resource_id": 111, "text": "  Kun Guyyaa isaan hin dubbanneedha." },
    {
      "resource_id": 111,
      "text": "  Rakkoo haa himataniif isaaniif hin eeyyamamu."
    },
    {
      "resource_id": 111,
      "text": "  Kijibsiiftotaaf guyyaa san badiin mirkanaa'e."
    },
    {
      "resource_id": 111,
      "text": "  Kun guyyaa adda foo'amiinsaati; Isiniifi warra duraas walitti qabne."
    },
    {
      "resource_id": 111,
      "text": "  Yoo tooftaa wahii qabaattan tooftaa Natti baafadhaa."
    },
    {
      "resource_id": 111,
      "text": "  Kijibsiiftotaaf guyyaa san badiin mirkanaa'e."
    },
    {
      "resource_id": 111,
      "text": "  Dhugumatti, warri Rabbiin sodaatan gaaddisootaafi burqituuwwan keessa jiraatu."
    },
    {
      "resource_id": 111,
      "text": "  Asheeta irraa waan fedhan (keessa taa'u)."
    },
    {
      "resource_id": 111,
      "text": "\"Sababaa waan dalagaa turtaniitiif gammachuun nyaadhaa; dhugaa\" (jedhamu)."
    },
    {
      "resource_id": 111,
      "text": "  Nuti akka kanatti warra toltuu hojjatan mindeessina."
    },
    {
      "resource_id": 111,
      "text": "  Kijibsiiftotaaf guyyaa san badiin mirkanaa'e."
    },
    {
      "resource_id": 111,
      "text": "  (Addunyaa irratti) \"nyaadhaa; xiqqoo qanani'aa; dhugumatti isin badii hojjattoota” (isaaniin jedhama)."
    },
    {
      "resource_id": 111,
      "text": "  Kijibsiiftotaaf guyyaa san badiin mirkanaa'e."
    },
    {
      "resource_id": 111,
      "text": "  Yeroo ‘salaataa’ isaaniin jedhames hin salaatanu."
    },
    {
      "resource_id": 111,
      "text": "  Kijibsiiftotaaf guyyaa san badiin mirkanaa'e."
    },
    { "resource_id": 111, "text": "  Isaan ala haasawa kamitti amanu ree?" }
  ],
