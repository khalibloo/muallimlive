{
  "translations": [
    { "resource_id": 127, "text": "Кет-кет юборилганлар билан қасам." },
    { "resource_id": 127, "text": "Қаттиқ эсган шамоллар билан қасам." },
    {
      "resource_id": 127,
      "text": "Ва (шариатни) нашр қилувчилар билан қасам."
    },
    { "resource_id": 127, "text": "Фарқловчилар билан қасам." },
    { "resource_id": 127, "text": "Ваҳийни ташувчилар билан қасам." },
    { "resource_id": 127, "text": "Узрларни ёки огоҳлантиришларни ҳам." },
    {
      "resource_id": 127,
      "text": "Албатта, сизга ваъда қилинаётган нарса воқеъ бўлгувчидир."
    },
    { "resource_id": 127, "text": "Бас, вақтики юлдузлар ўчирилса." },
    { "resource_id": 127, "text": "Ва вақтики, осмон ёрилса." },
    { "resource_id": 127, "text": "Ва вақтики, тоғлар кўчирилса." },
    {
      "resource_id": 127,
      "text": "Ва вақтики, Пайғамбарларга вақт белгиланса."
    },
    { "resource_id": 127, "text": "Қандоқ кунга сурилган-а?" },
    { "resource_id": 127, "text": "Ажратиш кунига." },
    { "resource_id": 127, "text": "Ажратиш кунини сенга нима билдирди?" },
    {
      "resource_id": 127,
      "text": "Ўшал кунда ёлғонга чиқарувчиларга вайл бўлсин!"
    },
    { "resource_id": 127, "text": "Аввалгиларни ҳалок қилмадикми?" },
    {
      "resource_id": 127,
      "text": "Сўнгра кейингиларини уларга эргаштирмадикми?"
    },
    { "resource_id": 127, "text": "Гуноҳкорларни шундоқ қиламиз." },
    {
      "resource_id": 127,
      "text": "Ўшал кунда ёлғонга чиқарувчиларга вайл бўлсин!"
    },
    { "resource_id": 127, "text": "Сизларни ҳақир сувдан яратмадикми?" },
    {
      "resource_id": 127,
      "text": "Бас, уни мустаҳкам қароргоҳга жойламадикми?!"
    },
    { "resource_id": 127, "text": "Маълум вақтгача." },
    {
      "resource_id": 127,
      "text": "Бас, Биз қодир бўлдик ва қандоқ ҳам яхши қодир бўлувчимиз.<sup foot_note=48867>1</sup>"
    },
    {
      "resource_id": 127,
      "text": "Ўшал кунда ёлғонга чиқарувчиларга вайл бўлсин!"
    },
    { "resource_id": 127, "text": "Ерни ўзига тортувчи қилиб қўймадикми?!" },
    { "resource_id": 127, "text": "Тирикларни ва ўликларни." },
    {
      "resource_id": 127,
      "text": "Ва Биз унда юксак тоғлар қилдик ва сизларни зилол сув билан суғордик."
    },
    {
      "resource_id": 127,
      "text": "Ўшал кунда ёлғонга чиқарувчиларга вайл бўлсин!"
    },
    {
      "resource_id": 127,
      "text": "Юрингиз! Ўзингиз ёлғонга чиқарган нарсага қараб."
    },
    { "resource_id": 127, "text": "Юрингиз! Уч шўъбаси бор «соя»га қараб." },
    {
      "resource_id": 127,
      "text": "У, салқин қилувчимас ва оловни ҳам қайтармас."
    },
    {
      "resource_id": 127,
      "text": "Албатта, у (жаҳаннам) қасрдек учқунларни отадир."
    },
    {
      "resource_id": 127,
      "text": "У (учқун) худди қорамтир-сарғиш туяларга ўхшайдир."
    },
    {
      "resource_id": 127,
      "text": "Ўшал кунда ёлғонга чиқарувчиларга вайл бўлсин!"
    },
    { "resource_id": 127, "text": "Бу, улар гапира олмайдиган кундир." },
    { "resource_id": 127, "text": "Ва уларга узр айтишга изн берилмайдир." },
    {
      "resource_id": 127,
      "text": "Ўшал кунда ёлғонга чиқарувчиларга вайл бўлсин!"
    },
    {
      "resource_id": 127,
      "text": "Бу ажратиш куни сизларни ва аввалгиларни жамладик."
    },
    {
      "resource_id": 127,
      "text": "Бас, сизларнинг ҳийлангиз бўлса, Менга ишлатиб кўринг-чи?"
    },
    {
      "resource_id": 127,
      "text": "Ўшал кунда ёлғонга чиқарувчиларга вайл бўлсин!"
    },
    {
      "resource_id": 127,
      "text": "Албатта, тақводорлар соялар ва булоқлардадир."
    },
    { "resource_id": 127, "text": "Ва ўзлари хоҳлаган мевалардадир." },
    {
      "resource_id": 127,
      "text": "Енглар ва ичинглар, ош бўлсин. Қилган амалларингиз сабабидандир."
    },
    {
      "resource_id": 127,
      "text": "Албатта, Биз муҳсинларни шундай мукофотлармиз."
    },
    {
      "resource_id": 127,
      "text": "Ўшал кунда ёлғонга чиқарувчиларга вайл бўлсин!"
    },
    {
      "resource_id": 127,
      "text": "Енглар ва мазза қилинглар, озгинагина. Албатта, сизлар гуноҳкорсизлар."
    },
    {
      "resource_id": 127,
      "text": "Ўшал кунда ёлғонга чиқарувчиларга вайл бўлсин!"
    },
    {
      "resource_id": 127,
      "text": "Ва агар уларга рукуъ қилинглар дейилса, рукуъ қилмаслар."
    },
    {
      "resource_id": 127,
      "text": "Ўшал кунда ёлғонга чиқарувчиларга вайл бўлсин!"
    },
    { "resource_id": 127, "text": "Бас, ундан кейин қайси сўзга ишонадилар?!" }
  ],
