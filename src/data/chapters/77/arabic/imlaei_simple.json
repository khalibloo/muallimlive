{
  "verses": [
    { "id": 5623, "verse_key": "77:1", "text_imlaei_simple": "والمرسلات عرفا" },
    { "id": 5624, "verse_key": "77:2", "text_imlaei_simple": "فالعاصفات عصفا" },
    { "id": 5625, "verse_key": "77:3", "text_imlaei_simple": "والناشرات نشرا" },
    { "id": 5626, "verse_key": "77:4", "text_imlaei_simple": "فالفارقات فرقا" },
    { "id": 5627, "verse_key": "77:5", "text_imlaei_simple": "فالملقيات ذكرا" },
    { "id": 5628, "verse_key": "77:6", "text_imlaei_simple": "عذرا او نذرا" },
    {
      "id": 5629,
      "verse_key": "77:7",
      "text_imlaei_simple": "انما توعدون لواقع"
    },
    {
      "id": 5630,
      "verse_key": "77:8",
      "text_imlaei_simple": "فاذا النجوم طمست"
    },
    {
      "id": 5631,
      "verse_key": "77:9",
      "text_imlaei_simple": "واذا السماء فرجت"
    },
    {
      "id": 5632,
      "verse_key": "77:10",
      "text_imlaei_simple": "واذا الجبال نسفت"
    },
    {
      "id": 5633,
      "verse_key": "77:11",
      "text_imlaei_simple": "واذا الرسل اقتت"
    },
    { "id": 5634, "verse_key": "77:12", "text_imlaei_simple": "لاي يوم اجلت" },
    { "id": 5635, "verse_key": "77:13", "text_imlaei_simple": "ليوم الفصل" },
    {
      "id": 5636,
      "verse_key": "77:14",
      "text_imlaei_simple": "وما ادراك ما يوم الفصل"
    },
    {
      "id": 5637,
      "verse_key": "77:15",
      "text_imlaei_simple": "ويل يومئذ للمكذبين"
    },
    {
      "id": 5638,
      "verse_key": "77:16",
      "text_imlaei_simple": "الم نهلك الاولين"
    },
    {
      "id": 5639,
      "verse_key": "77:17",
      "text_imlaei_simple": "ثم نتبعهم الاخرين"
    },
    {
      "id": 5640,
      "verse_key": "77:18",
      "text_imlaei_simple": "كذلك نفعل بالمجرمين"
    },
    {
      "id": 5641,
      "verse_key": "77:19",
      "text_imlaei_simple": "ويل يومئذ للمكذبين"
    },
    {
      "id": 5642,
      "verse_key": "77:20",
      "text_imlaei_simple": "الم نخلقكم من ماء مهين"
    },
    {
      "id": 5643,
      "verse_key": "77:21",
      "text_imlaei_simple": "فجعلناه في قرار مكين"
    },
    { "id": 5644, "verse_key": "77:22", "text_imlaei_simple": "الى قدر معلوم" },
    {
      "id": 5645,
      "verse_key": "77:23",
      "text_imlaei_simple": "فقدرنا فنعم القادرون"
    },
    {
      "id": 5646,
      "verse_key": "77:24",
      "text_imlaei_simple": "ويل يومئذ للمكذبين"
    },
    {
      "id": 5647,
      "verse_key": "77:25",
      "text_imlaei_simple": "الم نجعل الارض كفاتا"
    },
    { "id": 5648, "verse_key": "77:26", "text_imlaei_simple": "احياء وامواتا" },
    {
      "id": 5649,
      "verse_key": "77:27",
      "text_imlaei_simple": "وجعلنا فيها رواسي شامخات واسقيناكم ماء فراتا"
    },
    {
      "id": 5650,
      "verse_key": "77:28",
      "text_imlaei_simple": "ويل يومئذ للمكذبين"
    },
    {
      "id": 5651,
      "verse_key": "77:29",
      "text_imlaei_simple": "انطلقوا الى ما كنتم به تكذبون"
    },
    {
      "id": 5652,
      "verse_key": "77:30",
      "text_imlaei_simple": "انطلقوا الى ظل ذي ثلاث شعب"
    },
    {
      "id": 5653,
      "verse_key": "77:31",
      "text_imlaei_simple": "لا ظليل ولا يغني من اللهب"
    },
    {
      "id": 5654,
      "verse_key": "77:32",
      "text_imlaei_simple": "انها ترمي بشرر كالقصر"
    },
    {
      "id": 5655,
      "verse_key": "77:33",
      "text_imlaei_simple": "كانه جمالت صفر"
    },
    {
      "id": 5656,
      "verse_key": "77:34",
      "text_imlaei_simple": "ويل يومئذ للمكذبين"
    },
    {
      "id": 5657,
      "verse_key": "77:35",
      "text_imlaei_simple": "هذا يوم لا ينطقون"
    },
    {
      "id": 5658,
      "verse_key": "77:36",
      "text_imlaei_simple": "ولا يؤذن لهم فيعتذرون"
    },
    {
      "id": 5659,
      "verse_key": "77:37",
      "text_imlaei_simple": "ويل يومئذ للمكذبين"
    },
    {
      "id": 5660,
      "verse_key": "77:38",
      "text_imlaei_simple": "هذا يوم الفصل جمعناكم والاولين"
    },
    {
      "id": 5661,
      "verse_key": "77:39",
      "text_imlaei_simple": "فان كان لكم كيد فكيدون"
    },
    {
      "id": 5662,
      "verse_key": "77:40",
      "text_imlaei_simple": "ويل يومئذ للمكذبين"
    },
    {
      "id": 5663,
      "verse_key": "77:41",
      "text_imlaei_simple": "ان المتقين في ظلال وعيون"
    },
    {
      "id": 5664,
      "verse_key": "77:42",
      "text_imlaei_simple": "وفواكه مما يشتهون"
    },
    {
      "id": 5665,
      "verse_key": "77:43",
      "text_imlaei_simple": "كلوا واشربوا هنيئا بما كنتم تعملون"
    },
    {
      "id": 5666,
      "verse_key": "77:44",
      "text_imlaei_simple": "انا كذلك نجزي المحسنين"
    },
    {
      "id": 5667,
      "verse_key": "77:45",
      "text_imlaei_simple": "ويل يومئذ للمكذبين"
    },
    {
      "id": 5668,
      "verse_key": "77:46",
      "text_imlaei_simple": "كلوا وتمتعوا قليلا انكم مجرمون"
    },
    {
      "id": 5669,
      "verse_key": "77:47",
      "text_imlaei_simple": "ويل يومئذ للمكذبين"
    },
    {
      "id": 5670,
      "verse_key": "77:48",
      "text_imlaei_simple": "واذا قيل لهم اركعوا لا يركعون"
    },
    {
      "id": 5671,
      "verse_key": "77:49",
      "text_imlaei_simple": "ويل يومئذ للمكذبين"
    },
    {
      "id": 5672,
      "verse_key": "77:50",
      "text_imlaei_simple": "فباي حديث بعده يؤمنون"
    }
  ],
  "meta": { "filters": { "chapter_number": "77" } }
}
