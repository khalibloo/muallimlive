  "verses": [
    {
      "id": 1803,
      "verse_key": "15:1",
      "text_uthmani_tajweed": "ا<tajweed class=madda_necessary>لٓ</tajweed>ر‌ۚ تِلْكَ ءَايَ<tajweed class=madda_normal>ـٰ</tajweed>تُ <tajweed class=ham_wasl>ٱ</tajweed>لْكِتَ<tajweed class=madda_normal>ـٰ</tajweed>بِ وَقُرْءَا<tajweed class=idgham_ghunnah>نٍ م</tajweed>ُّب<tajweed class=madda_permissible>ِي</tajweed>نٍ <span class=end>١</span>"
    },
    {
      "id": 1804,
      "verse_key": "15:2",
      "text_uthmani_tajweed": "رُّبَمَا يَوَدُّ <tajweed class=ham_wasl>ٱ</tajweed>لَّذِينَ كَفَرُو<tajweed class=slnt>اْ</tajweed> لَوْ كَانُو<tajweed class=slnt>اْ</tajweed> مُسْلِم<tajweed class=madda_permissible>ِي</tajweed>نَ <span class=end>٢</span>"
    },
    {
      "id": 1805,
      "verse_key": "15:3",
      "text_uthmani_tajweed": "ذَرْهُمْ يَأْكُلُو<tajweed class=slnt>اْ</tajweed> وَيَتَمَتَّعُو<tajweed class=slnt>اْ</tajweed> وَيُلْهِهِمُ <tajweed class=ham_wasl>ٱ</tajweed>لْأَمَلُ‌ۖ فَسَوْفَ يَعْلَم<tajweed class=madda_permissible>ُو</tajweed>نَ <span class=end>٣</span>"
    },
    {
      "id": 1806,
      "verse_key": "15:4",
      "text_uthmani_tajweed": "وَم<tajweed class=madda_obligatory>َآ</tajweed> أَهْلَكْنَا مِ<tajweed class=ikhafa>ن ق</tajweed>َرْيَةٍ إِلَّا وَلَهَا كِتَا<tajweed class=idgham_ghunnah>بٌ م</tajweed>َّعْل<tajweed class=madda_permissible>ُو</tajweed>مٌ <span class=end>٤</span>"
    },
    {
      "id": 1807,
      "verse_key": "15:5",
      "text_uthmani_tajweed": "مَّا تَسْبِقُ مِنْ أُ<tajweed class=ghunnah>مّ</tajweed>َةٍ أَجَلَهَا وَمَا يَسْتَــْٔخِر<tajweed class=madda_permissible>ُو</tajweed>نَ <span class=end>٥</span>"
    },
    {
      "id": 1808,
      "verse_key": "15:6",
      "text_uthmani_tajweed": "وَقَالُو<tajweed class=slnt>اْ</tajweed> يَ<tajweed class=madda_obligatory>ـٰٓ</tajweed>أَيُّهَا <tajweed class=ham_wasl>ٱ</tajweed>لَّذِى نُزِّلَ عَلَيْهِ <tajweed class=ham_wasl>ٱ</tajweed><tajweed class=laam_shamsiyah>ل</tajweed>ذِّكْرُ إِ<tajweed class=ghunnah>نّ</tajweed>َكَ لَمَ<tajweed class=qalaqah>جْ</tajweed>ن<tajweed class=madda_permissible>ُو</tajweed>نٌ <span class=end>٦</span>"
    },
    {
      "id": 1809,
      "verse_key": "15:7",
      "text_uthmani_tajweed": "لَّوْ مَا تَأْتِينَا بِ<tajweed class=ham_wasl>ٱ</tajweed>لْمَلَ<tajweed class=madda_obligatory>ـٰٓ</tajweed>ئِكَةِ إِ<tajweed class=ikhafa>ن ك</tajweed>ُ<tajweed class=ikhafa>نت</tajweed>َ مِنَ <tajweed class=ham_wasl>ٱ</tajweed><tajweed class=laam_shamsiyah>ل</tajweed>صَّ<tajweed class=madda_normal>ـٰ</tajweed>دِق<tajweed class=madda_permissible>ِي</tajweed>نَ <span class=end>٧</span>"
    },
    {
      "id": 1810,
      "verse_key": "15:8",
      "text_uthmani_tajweed": "مَا نُنَزِّلُ <tajweed class=ham_wasl>ٱ</tajweed>لْمَلَ<tajweed class=madda_obligatory>ـٰٓ</tajweed>ئِكَةَ إِلَّا بِ<tajweed class=ham_wasl>ٱ</tajweed>لْحَقِّ وَمَا كَان<tajweed class=madda_obligatory>ُوٓ</tajweed><tajweed class=slnt>اْ</tajweed> إِ<tajweed class=idgham_ghunnah>ذًا م</tajweed>ُّ<tajweed class=ikhafa>نظ</tajweed>َر<tajweed class=madda_permissible>ِي</tajweed>نَ <span class=end>٨</span>"
    },
    {
      "id": 1811,
      "verse_key": "15:9",
      "text_uthmani_tajweed": "إِ<tajweed class=ghunnah>نّ</tajweed>َا نَحْنُ نَزَّلْنَا <tajweed class=ham_wasl>ٱ</tajweed><tajweed class=laam_shamsiyah>ل</tajweed>ذِّكْرَ وَإِ<tajweed class=ghunnah>نّ</tajweed>َا لَهُ<tajweed class=madda_normal>ۥ</tajweed> لَحَ<tajweed class=madda_normal>ـٰ</tajweed>فِظ<tajweed class=madda_permissible>ُو</tajweed>نَ <span class=end>٩</span>"
    },
    {
      "id": 1812,
      "verse_key": "15:10",
      "text_uthmani_tajweed": "وَلَقَ<tajweed class=qalaqah>دْ</tajweed> أَرْسَلْنَا مِ<tajweed class=ikhafa>ن ق</tajweed>َ<tajweed class=qalaqah>بْ</tajweed>لِكَ فِى شِيَعِ <tajweed class=ham_wasl>ٱ</tajweed>لْأَوَّل<tajweed class=madda_permissible>ِي</tajweed>نَ <span class=end>١٠</span>"
    },
    {
      "id": 1813,
      "verse_key": "15:11",
      "text_uthmani_tajweed": "وَمَا يَأْتِيه<tajweed class=idgham_shafawi>ِم م</tajweed>ّ<tajweed class=idgham_wo_ghunnah>ِن ر</tajweed>َّسُولٍ إِلَّا كَانُو<tajweed class=slnt>اْ</tajweed> بِه<tajweed class=madda_normal>ِۦ</tajweed> يَسْتَهْزِء<tajweed class=madda_permissible>ُو</tajweed>نَ <span class=end>١١</span>"
    },
    {
      "id": 1814,
      "verse_key": "15:12",
      "text_uthmani_tajweed": "كَذ<tajweed class=madda_normal>َٲ</tajweed>لِكَ نَسْلُكُهُ<tajweed class=madda_normal>ۥ</tajweed> فِى قُلُوبِ <tajweed class=ham_wasl>ٱ</tajweed>لْمُ<tajweed class=qalaqah>جْ</tajweed>رِم<tajweed class=madda_permissible>ِي</tajweed>نَ <span class=end>١٢</span>"
    },
    {
      "id": 1815,
      "verse_key": "15:13",
      "text_uthmani_tajweed": "لَا يُؤْمِنُونَ بِه<tajweed class=madda_normal>ِۦ</tajweed>‌ۖ وَقَ<tajweed class=qalaqah>دْ</tajweed> خَلَتْ سُ<tajweed class=ghunnah>نّ</tajweed>َةُ <tajweed class=ham_wasl>ٱ</tajweed>لْأَوَّل<tajweed class=madda_permissible>ِي</tajweed>نَ <span class=end>١٣</span>"
    },
    {
      "id": 1816,
      "verse_key": "15:14",
      "text_uthmani_tajweed": "وَلَوْ فَتَحْنَا عَلَيْه<tajweed class=ikhafa_shafawi>ِم ب</tajweed>َا<tajweed class=idgham_ghunnah>بًا م</tajweed>ِّنَ <tajweed class=ham_wasl>ٱ</tajweed><tajweed class=laam_shamsiyah>ل</tajweed>سَّم<tajweed class=madda_obligatory>َا</tajweed>ٓءِ فَظَلُّو<tajweed class=slnt>اْ</tajweed> فِيهِ يَعْرُج<tajweed class=madda_permissible>ُو</tajweed>نَ <span class=end>١٤</span>"
    },
    {
      "id": 1817,
      "verse_key": "15:15",
      "text_uthmani_tajweed": "لَقَال<tajweed class=madda_obligatory>ُوٓ</tajweed><tajweed class=slnt>اْ</tajweed> إِ<tajweed class=ghunnah>نّ</tajweed>َمَا سُكِّرَتْ أَ<tajweed class=qalaqah>بْ</tajweed>صَ<tajweed class=madda_normal>ـٰ</tajweed>رُنَا بَلْ نَحْنُ قَوْ<tajweed class=idgham_ghunnah>مٌ م</tajweed>َّسْحُور<tajweed class=madda_permissible>ُو</tajweed>نَ <span class=end>١٥</span>"
    },
    {
      "id": 1818,
      "verse_key": "15:16",
      "text_uthmani_tajweed": "وَلَقَ<tajweed class=qalaqah>دْ</tajweed> جَعَلْنَا فِى <tajweed class=ham_wasl>ٱ</tajweed><tajweed class=laam_shamsiyah>ل</tajweed>سَّم<tajweed class=madda_obligatory>َا</tajweed>ٓءِ بُرُو<tajweed class=idgham_ghunnah>جًا و</tajweed>َزَيَّ<tajweed class=ghunnah>نّ</tajweed>َ<tajweed class=madda_normal>ـٰ</tajweed>هَا لِل<tajweed class=ghunnah>نّ</tajweed>َ<tajweed class=madda_normal>ـٰ</tajweed>ظِر<tajweed class=madda_permissible>ِي</tajweed>نَ <span class=end>١٦</span>"
    },
    {
      "id": 1819,
      "verse_key": "15:17",
      "text_uthmani_tajweed": "وَحَفِظْنَ<tajweed class=madda_normal>ـٰ</tajweed>هَا مِ<tajweed class=ikhafa>ن ك</tajweed>ُلِّ شَيْطَ<tajweed class=madda_normal>ـٰ</tajweed><tajweed class=idgham_wo_ghunnah>نٍ ر</tajweed>َّج<tajweed class=madda_permissible>ِي</tajweed>مٍ <span class=end>١٧</span>"
    },
    {
      "id": 1820,
      "verse_key": "15:18",
      "text_uthmani_tajweed": "إِلَّا مَنِ <tajweed class=ham_wasl>ٱ</tajweed>سْتَرَقَ <tajweed class=ham_wasl>ٱ</tajweed><tajweed class=laam_shamsiyah>ل</tajweed>سَّمْعَ فَأَتْبَعَهُ<tajweed class=madda_normal>ۥ</tajweed> شِهَا<tajweed class=idgham_ghunnah>بٌ م</tajweed>ُّب<tajweed class=madda_permissible>ِي</tajweed>نٌ <span class=end>١٨</span>"
    },
    {
      "id": 1821,
      "verse_key": "15:19",
      "text_uthmani_tajweed": "وَ<tajweed class=ham_wasl>ٱ</tajweed>لْأَرْضَ مَدَ<tajweed class=qalaqah>دْ</tajweed>نَ<tajweed class=madda_normal>ـٰ</tajweed>هَا وَأَلْقَيْنَا فِيهَا رَو<tajweed class=madda_normal>َٲ</tajweed>سِىَ وَأَ<tajweed class=iqlab>نۢب</tajweed>َتْنَا فِيهَا مِ<tajweed class=ikhafa>ن ك</tajweed>ُلِّ ش<tajweed class=idgham_ghunnah>َىْءٍ م</tajweed>َّوْز<tajweed class=madda_permissible>ُو</tajweed>نٍ <span class=end>١٩</span>"
    },
    {
      "id": 1822,
      "verse_key": "15:20",
      "text_uthmani_tajweed": "وَجَعَلْنَا لَكُمْ فِيهَا مَعَ<tajweed class=madda_normal>ـٰ</tajweed>يِشَ وَم<tajweed class=idgham_wo_ghunnah>َن ل</tajweed>َّسْتُمْ لَهُ<tajweed class=madda_normal>ۥ</tajweed> بِر<tajweed class=madda_normal>َٲ</tajweed>زِق<tajweed class=madda_permissible>ِي</tajweed>نَ <span class=end>٢٠</span>"
    },
    {
      "id": 1823,
      "verse_key": "15:21",
      "text_uthmani_tajweed": "وَإ<tajweed class=idgham_ghunnah>ِن م</tajweed>ِّ<tajweed class=ikhafa>ن ش</tajweed>َىْءٍ إِلَّا عِ<tajweed class=ikhafa>ند</tajweed>َنَا خَز<tajweed class=madda_obligatory>َا</tajweed>ٓئِنُهُ<tajweed class=madda_normal>ۥ</tajweed> وَمَا نُنَزِّلُهُ<tajweed class=madda_obligatory>ۥٓ</tajweed> إِلَّا بِقَدَ<tajweed class=idgham_ghunnah>رٍ م</tajweed>َّعْل<tajweed class=madda_permissible>ُو</tajweed>مٍ <span class=end>٢١</span>"
