{
  "verses": [
    {
      "id": 4980,
      "verse_key": "56:1",
      "text_indopak": "اِذَا وَقَعَتِ الۡوَاقِعَةُ ۙ‏"
    },
    {
      "id": 4981,
      "verse_key": "56:2",
      "text_indopak": "لَيۡسَ لِـوَقۡعَتِهَا كَاذِبَةٌ​ ۘ‏"
    },
    {
      "id": 4982,
      "verse_key": "56:3",
      "text_indopak": "خَافِضَةٌ رَّافِعَةٌ ۙ‏"
    },
    {
      "id": 4983,
      "verse_key": "56:4",
      "text_indopak": "اِذَا رُجَّتِ الۡاَرۡضُ رَجًّا ۙ‏"
    },
    {
      "id": 4984,
      "verse_key": "56:5",
      "text_indopak": "وَّبُسَّتِ الۡجِبَالُ بَسًّا ۙ‏"
    },
    {
      "id": 4985,
      "verse_key": "56:6",
      "text_indopak": "فَكَانَتۡ هَبَآءً مُّنۡۢبَـثًّا ۙ‏"
    },
    {
      "id": 4986,
      "verse_key": "56:7",
      "text_indopak": "وَّكُنۡـتُمۡ اَزۡوَاجًا ثَلٰـثَـةً ؕ‏"
    },
    {
      "id": 4987,
      "verse_key": "56:8",
      "text_indopak": "فَاَصۡحٰبُ الۡمَيۡمَنَةِ ۙ مَاۤ اَصۡحٰبُ الۡمَيۡمَنَةِ ؕ‏"
    },
    {
      "id": 4988,
      "verse_key": "56:9",
      "text_indopak": "وَاَصۡحٰبُ الۡمَشۡـَٔـمَةِ ۙ مَاۤ اَصۡحٰبُ الۡمَشۡـَٔـمَةِؕ‏"
    },
    {
      "id": 4989,
      "verse_key": "56:10",
      "text_indopak": "وَالسّٰبِقُوۡنَ السّٰبِقُوۡنَۚ  ۙ‏"
    },
    {
      "id": 4990,
      "verse_key": "56:11",
      "text_indopak": "اُولٰٓٮِٕكَ الۡمُقَرَّبُوۡنَ​ۚ‏"
    },
    {
      "id": 4991,
      "verse_key": "56:12",
      "text_indopak": "فِىۡ جَنّٰتِ النَّعِيۡمِ‏"
    },
    {
      "id": 4992,
      "verse_key": "56:13",
      "text_indopak": "ثُلَّةٌ مِّنَ الۡاَوَّلِيۡنَۙ‏"
    },
    {
      "id": 4993,
      "verse_key": "56:14",
      "text_indopak": "وَقَلِيۡلٌ مِّنَ الۡاٰخِرِيۡنَؕ‏"
    },
    {
      "id": 4994,
      "verse_key": "56:15",
      "text_indopak": "عَلٰى سُرُرٍ مَّوۡضُوۡنَةٍۙ‏"
    },
    {
      "id": 4995,
      "verse_key": "56:16",
      "text_indopak": "مُّتَّكِـــِٕيۡنَ عَلَيۡهَا مُتَقٰبِلِيۡنَ‏"
    },
    {
      "id": 4996,
      "verse_key": "56:17",
      "text_indopak": "يَطُوۡفُ عَلَيۡهِمۡ وِلۡدَانٌ مُّخَلَّدُوۡنَۙ‏"
    },
    {
      "id": 4997,
      "verse_key": "56:18",
      "text_indopak": "بِاَكۡوَابٍ وَّاَبَارِيۡقَ ۙ وَكَاۡسٍ مِّنۡ مَّعِيۡنٍۙ‏"
    },
    {
      "id": 4998,
      "verse_key": "56:19",
      "text_indopak": "لَّا يُصَدَّعُوۡنَ عَنۡهَا وَلَا يُنۡزِفُوۡنَۙ‏"
    },
    {
      "id": 4999,
      "verse_key": "56:20",
      "text_indopak": "وَفَاكِهَةٍ مِّمَّا يَتَخَيَّرُوۡنَۙ‏"
    },
    {
      "id": 5000,
      "verse_key": "56:21",
      "text_indopak": "وَلَحۡمِ طَيۡرٍ مِّمَّا يَشۡتَهُوۡنَؕ‏"
    },
    { "id": 5001, "verse_key": "56:22", "text_indopak": "وَحُوۡرٌ عِيۡنٌۙ‏" },
    {
      "id": 5002,
      "verse_key": "56:23",
      "text_indopak": "كَاَمۡثَالِ اللُّـؤۡلُـوٴِالۡمَكۡنُوۡنِ​ۚ‏"
    },
    {
      "id": 5003,
      "verse_key": "56:24",
      "text_indopak": "جَزَآءًۢ بِمَا كَانُوۡا يَعۡمَلُوۡنَ‏"
    },
    {
      "id": 5004,
      "verse_key": "56:25",
      "text_indopak": "لَا يَسۡمَعُوۡنَ فِيۡهَا لَغۡوًا وَّلَا تَاۡثِيۡمًا ۙ‏"
    },
    {
      "id": 5005,
      "verse_key": "56:26",
      "text_indopak": "اِلَّا قِيۡلًا سَلٰمًا سَلٰمًا‏"
    },
    {
      "id": 5006,
      "verse_key": "56:27",
      "text_indopak": "وَاَصۡحٰبُ الۡيَمِيۡنِ ۙ مَاۤ اَصۡحٰبُ الۡيَمِيۡنِؕ‏"
    },
    {
      "id": 5007,
      "verse_key": "56:28",
      "text_indopak": "فِىۡ سِدۡرٍ مَّخۡضُوۡدٍۙ‏"
    },
    {
      "id": 5008,
      "verse_key": "56:29",
      "text_indopak": "وَّطَلۡحٍ مَّنۡضُوۡدٍۙ‏"
    },
    {
      "id": 5009,
      "verse_key": "56:30",
      "text_indopak": "وَّظِلٍّ مَّمۡدُوۡدٍۙ‏"
    },
    {
      "id": 5010,
      "verse_key": "56:31",
      "text_indopak": "وَّ مَآءٍ مَّسۡكُوۡبٍۙ‏"
    },
    {
      "id": 5011,
      "verse_key": "56:32",
      "text_indopak": "وَّفَاكِهَةٍ كَثِيۡرَةٍۙ‏"
    },
    {
      "id": 5012,
      "verse_key": "56:33",
      "text_indopak": "لَّا مَقۡطُوۡعَةٍ وَّلَا مَمۡنُوۡعَةٍۙ‏"
    },
    {
      "id": 5013,
      "verse_key": "56:34",
      "text_indopak": "وَّ فُرُشٍ مَّرۡفُوۡعَةٍؕ‏"
    },
    {
      "id": 5014,
      "verse_key": "56:35",
      "text_indopak": "اِنَّاۤ اَنۡشَاۡنٰهُنَّ اِنۡشَآءًۙ‏"
    },
    {
      "id": 5015,
      "verse_key": "56:36",
      "text_indopak": "فَجَعَلۡنٰهُنَّ اَبۡكَارًاۙ‏"
    },
    {
      "id": 5016,
      "verse_key": "56:37",
      "text_indopak": "عُرُبًا اَتۡرَابًاۙ‏"
    },
    {
      "id": 5017,
      "verse_key": "56:38",
      "text_indopak": "لِّاَصۡحٰبِ الۡيَمِيۡنِؕ‏"
    },
    {
      "id": 5018,
      "verse_key": "56:39",
      "text_indopak": "ثُلَّةٌ مِّنَ الۡاَوَّلِيۡنَۙ‏"
    },
    {
      "id": 5019,
      "verse_key": "56:40",
      "text_indopak": "وَثُلَّةٌ مِّنَ الۡاٰخِرِيۡنَؕ‏"
    },
    {
      "id": 5020,
      "verse_key": "56:41",
      "text_indopak": "وَاَصۡحٰبُ الشِّمَالِ ۙ مَاۤ اَصۡحٰبُ الشِّمَالِؕ‏"
    },
    {
      "id": 5021,
      "verse_key": "56:42",
      "text_indopak": "فِىۡ سَمُوۡمٍ وَّحَمِيۡمٍۙ‏"
    },
    {
      "id": 5022,
      "verse_key": "56:43",
      "text_indopak": "وَّظِلٍّ مِّنۡ يَّحۡمُوۡمٍۙ‏"
    },
    {
      "id": 5023,
      "verse_key": "56:44",
      "text_indopak": "لَّا بَارِدٍ وَّلَا كَرِيۡمٍ‏"
    },
    {
      "id": 5024,
      "verse_key": "56:45",
      "text_indopak": "اِنَّهُمۡ كَانُوۡا قَبۡلَ ذٰ لِكَ مُتۡرَفِيۡنَۚ  ۖ‏"
    },
    {
      "id": 5025,
      "verse_key": "56:46",
      "text_indopak": "وَكَانُوۡا يُصِرُّوۡنَ عَلَى الۡحِنۡثِ الۡعَظِيۡمِ​ۚ‏"
    },
    {
      "id": 5026,
      "verse_key": "56:47",
      "text_indopak": "وَكَانُوۡا يَقُوۡلُوۡنَ ۙ اَٮِٕذَا مِتۡنَا وَكُنَّا تُرَابًا وَّعِظَامًا ءَاِنَّا لَمَبۡعُوۡثُوۡنَۙ‏"
    },
    {
      "id": 5027,
      "verse_key": "56:48",
      "text_indopak": "اَوَاٰبَآؤُنَا الۡاَوَّلُوۡنَ‏"
    },
    {
      "id": 5028,
      "verse_key": "56:49",
      "text_indopak": "قُلۡ اِنَّ الۡاَوَّلِيۡنَ وَالۡاٰخِرِيۡنَۙ‏"
    },
    {
      "id": 5029,
      "verse_key": "56:50",
      "text_indopak": "لَمَجۡمُوۡعُوۡنَ ۙ اِلٰى مِيۡقَاتِ يَوۡمٍ مَّعۡلُوۡمٍ‏"
    },
    {
      "id": 5030,
      "verse_key": "56:51",
      "text_indopak": "ثُمَّ اِنَّكُمۡ اَيُّهَا الضَّآلُّوۡنَ الۡمُكَذِّبُوۡنَۙ‏"
    },
    {
      "id": 5031,
      "verse_key": "56:52",
      "text_indopak": "لَاٰكِلُوۡنَ مِنۡ شَجَرٍ مِّنۡ زَقُّوۡمٍۙ‏"
    },
    {
      "id": 5032,
      "verse_key": "56:53",
      "text_indopak": "فَمٰلِــُٔوۡنَ مِنۡهَا الۡبُطُوۡنَ​ۚ‏"
    },
    {
      "id": 5033,
      "verse_key": "56:54",
      "text_indopak": "فَشٰرِبُوۡنَ عَلَيۡهِ مِنَ الۡحَمِيۡمِ​ۚ‏"
    },
    {
      "id": 5034,
      "verse_key": "56:55",
      "text_indopak": "فَشٰرِبُوۡنَ شُرۡبَ الۡهِيۡمِؕ‏"
    },
    {
      "id": 5035,
      "verse_key": "56:56",
      "text_indopak": "هٰذَا نُزُلُهُمۡ يَوۡمَ الدِّيۡنِؕ‏"
    },
    {
      "id": 5036,
      "verse_key": "56:57",
      "text_indopak": "نَحۡنُ خَلَقۡنٰكُمۡ فَلَوۡلَا تُصَدِّقُوۡنَ‏"
    },
    {
      "id": 5037,
      "verse_key": "56:58",
      "text_indopak": "اَفَرَءَيۡتُمۡ مَّا تُمۡنُوۡنَؕ‏"
    },
    {
      "id": 5038,
      "verse_key": "56:59",
      "text_indopak": "ءَاَنۡتُمۡ تَخۡلُقُوۡنَهٗۤ اَمۡ نَحۡنُ الۡخٰلِقُوۡنَ‏"
    },
    {
      "id": 5039,
      "verse_key": "56:60",
      "text_indopak": "نَحۡنُ قَدَّرۡنَا بَيۡنَكُمُ الۡمَوۡتَ وَمَا نَحۡنُ بِمَسۡبُوۡقِيۡنَۙ‏"
    },
    {
      "id": 5040,
      "verse_key": "56:61",
      "text_indopak": "عَلٰٓى اَنۡ نُّبَدِّلَ اَمۡثَالَـكُمۡ وَنُـنۡشِئَكُمۡ فِىۡ مَا لَا تَعۡلَمُوۡنَ‏ ﻿﻿"
    },
    {
      "id": 5041,
      "verse_key": "56:62",
      "text_indopak": "وَلَـقَدۡ عَلِمۡتُمُ النَّشۡاَةَ الۡاُوۡلٰى فَلَوۡلَا تَذَكَّرُوۡنَ‏"
    },
    {
      "id": 5042,
      "verse_key": "56:63",
      "text_indopak": "اَفَرَءَيۡتُمۡ مَّا تَحۡرُثُوۡنَؕ‏"
    },
    {
      "id": 5043,
      "verse_key": "56:64",
      "text_indopak": "ءَاَنۡتُمۡ تَزۡرَعُوۡنَهٗۤ اَمۡ نَحۡنُ الزّٰرِعُوۡنَ‏"
    },
    {
      "id": 5044,
      "verse_key": "56:65",
      "text_indopak": "لَوۡ نَشَآءُ لَجَـعَلۡنٰهُ حُطَامًا فَظَلۡتُمۡ تَفَكَّهُوۡنَ‏"
    },
    {
      "id": 5045,
      "verse_key": "56:66",
      "text_indopak": "اِنَّا لَمُغۡرَمُوۡنَۙ‏"
    },
    {
      "id": 5046,
      "verse_key": "56:67",
      "text_indopak": "بَلۡ نَحۡنُ مَحۡرُوۡمُوۡنَ‏"
    },
    {
      "id": 5047,
      "verse_key": "56:68",
      "text_indopak": "اَفَرَءَيۡتُمُ الۡمَآءَ الَّذِىۡ تَشۡرَبُوۡنَؕ‏"
    },
    {
      "id": 5048,
      "verse_key": "56:69",
      "text_indopak": "ءَاَنۡـتُمۡ اَنۡزَلۡـتُمُوۡهُ مِنَ الۡمُزۡنِ اَمۡ نَحۡنُ الۡمُنۡزِلُوۡنَ‏ ﻿﻿"
    },
    {
      "id": 5049,
      "verse_key": "56:70",
      "text_indopak": "لَوۡ نَشَآءُ جَعَلۡنٰهُ اُجَاجًا فَلَوۡلَا تَشۡكُرُوۡنَ‏"
    },
    {
      "id": 5050,
      "verse_key": "56:71",
      "text_indopak": "اَفَرَءَيۡتُمُ النَّارَ الَّتِىۡ تُوۡرُوۡنَؕ‏"
    },
    {
      "id": 5051,
      "verse_key": "56:72",
      "text_indopak": "ءَاَنۡتُمۡ اَنۡشَاۡتُمۡ شَجَرَتَهَاۤ اَمۡ نَحۡنُ الۡمُنۡشِـُٔـوۡنَ‏"
    },
    {
      "id": 5052,
      "verse_key": "56:73",
      "text_indopak": "نَحۡنُ جَعَلۡنٰهَا تَذۡكِرَةً وَّمَتَاعًا لِّلۡمُقۡوِيۡنَ​ۚ‏"
    },
    {
      "id": 5053,
      "verse_key": "56:74",
      "text_indopak": "فَسَبِّحۡ بِاسۡمِ رَبِّكَ الۡعَظِيۡمِ‏"
    },
    {
      "id": 5054,
      "verse_key": "56:75",
      "text_indopak": "فَلَاۤ اُقۡسِمُ بِمَوٰقِعِ النُّجُوۡمِۙ‏‏"
    },
    {
      "id": 5055,
      "verse_key": "56:76",
      "text_indopak": "وَاِنَّهٗ لَقَسَمٌ لَّوۡ تَعۡلَمُوۡنَ عَظِيۡمٌۙ‏"
    },
    {
      "id": 5056,
      "verse_key": "56:77",
      "text_indopak": "اِنَّهٗ لَـقُرۡاٰنٌ كَرِيۡمٌۙ‏"
    },
    {
      "id": 5057,
      "verse_key": "56:78",
      "text_indopak": "فِىۡ كِتٰبٍ مَّكۡنُوۡنٍۙ‏"
    },
    {
      "id": 5058,
      "verse_key": "56:79",
      "text_indopak": "لَّا يَمَسُّهٗۤ اِلَّا الۡمُطَهَّرُوۡنَؕ‏"
    },
    {
      "id": 5059,
      "verse_key": "56:80",
      "text_indopak": "تَنۡزِيۡلٌ مِّنۡ رَّبِّ الۡعٰلَمِيۡنَ‏"
    },
    {
      "id": 5060,
      "verse_key": "56:81",
      "text_indopak": "اَفَبِهٰذَا الۡحَـدِيۡثِ اَنۡتُمۡ مُّدۡهِنُوۡنَۙ‏"
    },
    {
      "id": 5061,
      "verse_key": "56:82",
      "text_indopak": "وَتَجۡعَلُوۡنَ رِزۡقَكُمۡ اَنَّكُمۡ تُكَذِّبُوۡنَ‏"
    },
    {
      "id": 5062,
      "verse_key": "56:83",
      "text_indopak": "فَلَوۡلَاۤ اِذَا بَلَغَتِ الۡحُـلۡقُوۡمَۙ‏"
    },
    {
      "id": 5063,
      "verse_key": "56:84",
      "text_indopak": "وَاَنۡتُمۡ حِيۡنَٮِٕذٍ تَـنۡظُرُوۡنَۙ‏"
    },
    {
      "id": 5064,
      "verse_key": "56:85",
      "text_indopak": "وَنَحۡنُ اَقۡرَبُ اِلَيۡهِ مِنۡكُمۡ وَلٰـكِنۡ لَّا تُبۡصِرُوۡنَ‏"
    },
    {
      "id": 5065,
      "verse_key": "56:86",
      "text_indopak": "فَلَوۡلَاۤ اِنۡ كُنۡتُمۡ غَيۡرَ مَدِيۡنِيۡنَۙ‏"
    },
    {
      "id": 5066,
      "verse_key": "56:87",
      "text_indopak": "تَرۡجِعُوۡنَهَاۤ اِنۡ كُنۡتُمۡ صٰدِقِيۡنَ‏"
    },
    {
      "id": 5067,
      "verse_key": "56:88",
