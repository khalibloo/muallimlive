{
  "verses": [
    {
      "id": 5932,
      "verse_key": "86:1",
      "text_indopak": "وَالسَّمَآءِ وَالطَّارِقِۙ‏"
    },
    {
      "id": 5933,
      "verse_key": "86:2",
      "text_indopak": "وَمَاۤ اَدۡرٰٮكَ مَا الطَّارِقُۙ‏"
    },
    {
      "id": 5934,
      "verse_key": "86:3",
      "text_indopak": "النَّجۡمُ الثَّاقِبُۙ‏"
    },
    {
      "id": 5935,
      "verse_key": "86:4",
      "text_indopak": "اِنۡ كُلُّ نَفۡسٍ لَّمَّا عَلَيۡهَا حَافِظٌؕ‏"
    },
    {
      "id": 5936,
      "verse_key": "86:5",
      "text_indopak": "فَلۡيَنۡظُرِ الۡاِنۡسَانُ مِمَّ خُلِقَؕ‏"
    },
    {
      "id": 5937,
      "verse_key": "86:6",
      "text_indopak": "خُلِقَ مِنۡ مَّآءٍ دَافِقٍۙ‏"
    },
    {
      "id": 5938,
      "verse_key": "86:7",
      "text_indopak": "يَّخۡرُجُ مِنۡۢ بَيۡنِ الصُّلۡبِ وَالتَّرَآٮِٕبِؕ‏"
    },
    {
      "id": 5939,
      "verse_key": "86:8",
      "text_indopak": "اِنَّهٗ عَلٰى رَجۡعِهٖ لَقَادِرٌؕ‏"
    },
    {
      "id": 5940,
      "verse_key": "86:9",
      "text_indopak": "يَوۡمَ تُبۡلَى السَّرَآٮِٕرُۙ‏"
    },
    {
      "id": 5941,
      "verse_key": "86:10",
      "text_indopak": "فَمَا لَهٗ مِنۡ قُوَّةٍ وَّلَا نَاصِرٍؕ‏"
    },
    {
      "id": 5942,
      "verse_key": "86:11",
      "text_indopak": "وَالسَّمَآءِ ذَاتِ الرَّجۡعِۙ‏"
    },
    {
      "id": 5943,
      "verse_key": "86:12",
      "text_indopak": "وَالۡاَرۡضِ ذَاتِ الصَّدۡعِۙ‏"
    },
    {
      "id": 5944,
      "verse_key": "86:13",
      "text_indopak": "اِنَّهٗ لَقَوۡلٌ فَصۡلٌۙ‏"
    },
    {
      "id": 5945,
      "verse_key": "86:14",
      "text_indopak": "وَّمَا هُوَ بِالۡهَزۡلِؕ‏"
    },
    {
      "id": 5946,
      "verse_key": "86:15",
      "text_indopak": "اِنَّهُمۡ يَكِيۡدُوۡنَ كَيۡدًا ۙ‏"
    },
    {
      "id": 5947,
      "verse_key": "86:16",
      "text_indopak": "وَّاَكِيۡدُ كَيۡدًا ۚۖ‏"
    },
    {
      "id": 5948,
      "verse_key": "86:17",
      "text_indopak": "فَمَهِّلِ الۡكٰفِرِيۡنَ اَمۡهِلۡهُمۡ رُوَيۡدًا‏"
    }
  ],
  "meta": { "filters": { "chapter_number": "86" } }
}
