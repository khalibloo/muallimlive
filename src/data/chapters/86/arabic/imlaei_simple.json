{
  "verses": [
    {
      "id": 5932,
      "verse_key": "86:1",
      "text_imlaei_simple": "والسماء والطارق"
    },
    {
      "id": 5933,
      "verse_key": "86:2",
      "text_imlaei_simple": "وما ادراك ما الطارق"
    },
    { "id": 5934, "verse_key": "86:3", "text_imlaei_simple": "النجم الثاقب" },
    {
      "id": 5935,
      "verse_key": "86:4",
      "text_imlaei_simple": "ان كل نفس لما عليها حافظ"
    },
    {
      "id": 5936,
      "verse_key": "86:5",
      "text_imlaei_simple": "فلينظر الانسان مم خلق"
    },
    {
      "id": 5937,
      "verse_key": "86:6",
      "text_imlaei_simple": "خلق من ماء دافق"
    },
    {
      "id": 5938,
      "verse_key": "86:7",
      "text_imlaei_simple": "يخرج من بين الصلب والترائب"
    },
    {
      "id": 5939,
      "verse_key": "86:8",
      "text_imlaei_simple": "انه على رجعه لقادر"
    },
    {
      "id": 5940,
      "verse_key": "86:9",
      "text_imlaei_simple": "يوم تبلى السرائر"
    },
    {
      "id": 5941,
      "verse_key": "86:10",
      "text_imlaei_simple": "فما له من قوة ولا ناصر"
    },
    {
      "id": 5942,
      "verse_key": "86:11",
      "text_imlaei_simple": "والسماء ذات الرجع"
    },
    {
      "id": 5943,
      "verse_key": "86:12",
      "text_imlaei_simple": "والارض ذات الصدع"
    },
    { "id": 5944, "verse_key": "86:13", "text_imlaei_simple": "انه لقول فصل" },
    { "id": 5945, "verse_key": "86:14", "text_imlaei_simple": "وما هو بالهزل" },
    {
      "id": 5946,
      "verse_key": "86:15",
      "text_imlaei_simple": "انهم يكيدون كيدا"
    },
    { "id": 5947, "verse_key": "86:16", "text_imlaei_simple": "واكيد كيدا" },
    {
      "id": 5948,
      "verse_key": "86:17",
      "text_imlaei_simple": "فمهل الكافرين امهلهم رويدا"
    }
  ],
  "meta": { "filters": { "chapter_number": "86" } }
}
