{
  "translations": [
    { "resource_id": 127, "text": "Осмон билан қасам ва ториқ билан қасам." },
    { "resource_id": 127, "text": "Ториқ нималигини сенга не билдирди?" },
    { "resource_id": 127, "text": "У, нур сочиб турувчи юлдуздир." },
    {
      "resource_id": 127,
      "text": "Ҳеч бир жон йўқки, унинг ҳифз қилиб турувчиси бўлмаса."
    },
    { "resource_id": 127, "text": "Инсон нимадан яралганига назар солсин." },
    { "resource_id": 127, "text": "У отилиб чиқувчи сувдан яралгандир." },
    {
      "resource_id": 127,
      "text": "У (сув) сулб (орқа умуртқа) ва тароиб (кўкрак суяги) орасидан чиқадир."
    },
    {
      "resource_id": 127,
      "text": "Албатта, У зот уни қайта тирилтиришга қодирдир."
    },
    { "resource_id": 127, "text": "Сирлар фош бўладиган кундадир." },
    {
      "resource_id": 127,
      "text": "Бас (ўшанда) у(инсон)да қувват ҳам, нусрат берувчи ҳам бўлмайдир."
    },
    { "resource_id": 127, "text": "Қайтувчи соҳиби бўлган осмон билан қасам." },
    { "resource_id": 127, "text": "Ёриб чиқувчи эгаси бўлган ер билан қасам." },
    { "resource_id": 127, "text": "Албатта, у (Қуръон) ажратувчи сўздир." },
    { "resource_id": 127, "text": "Ва у ҳазил эмасдир." },
    { "resource_id": 127, "text": "Албатта, улар ҳийла қиладир." },
    {
      "resource_id": 127,
      "text": "Ва Мен ҳам «ҳийла» қилурман.<sup foot_note=48877>1</sup>"
    },
    {
      "resource_id": 127,
      "text": "Бас, кофирларга муҳлат бер. Ҳа, уларга озгина муҳлат бер.<sup foot_note=48878>1</sup>"
    }
  ],
  "meta": {
