{
  "translations": [
    { "resource_id": 210, "text": "Göğe ve târıka andolsun." },
    { "resource_id": 210, "text": "Târıkın ne olduğunu sen ne bileceksin?" },
    { "resource_id": 210, "text": "O, (karanlığı) delip, geçen yıldızdır." },
    {
      "resource_id": 210,
      "text": "Üzerinde gözetleyici/koruyucu bulunmayan hiç bir nefis (kimse) yoktur."
    },
    {
      "resource_id": 210,
      "text": "Öyleyse, insan neyden yaratıldığına bir baksın."
    },
    { "resource_id": 210, "text": "Fışkırıp çıkan bir sudan yaratıldı." },
    {
      "resource_id": 210,
      "text": "Bu su, bel ile kaburga kemikleri arasından çıkar."
    },
    {
      "resource_id": 210,
      "text": "Şüphesiz Allah’ın onu, öldükten sonra tekrar diriltmeye de gücü yeter."
    },
    { "resource_id": 210, "text": "Sırların açığa çıktığı gün." },
    {
      "resource_id": 210,
      "text": "(O gün) artık insan için ne bir kuvvet vardır, ne de bir yardımcı."
    },
    { "resource_id": 210, "text": "Yağmurlu göğe andolsun." },
    { "resource_id": 210, "text": "Yarık yarık çatlamış yere andolsun." },
    {
      "resource_id": 210,
      "text": "Şüphesiz o Kur’an, hak ile batılı ayırt eden bir sözdür."
    },
    { "resource_id": 210, "text": "O, boş bir söz değildir." },
    { "resource_id": 210, "text": "Şüphesiz onlar bir tuzak kurarlar." },
    { "resource_id": 210, "text": "Ben de bir tuzak kurarım." },
    {
      "resource_id": 210,
      "text": "Öyleyse kâfirlere mühlet ver, onlara biraz zaman tanı!"
    }
  ],
