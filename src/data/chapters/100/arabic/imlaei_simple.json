{
  "verses": [
    {
      "id": 6147,
      "verse_key": "100:1",
      "text_imlaei_simple": "والعاديات ضبحا"
    },
    {
      "id": 6148,
      "verse_key": "100:2",
      "text_imlaei_simple": "فالموريات قدحا"
    },
    {
      "id": 6149,
      "verse_key": "100:3",
      "text_imlaei_simple": "فالمغيرات صبحا"
    },
    { "id": 6150, "verse_key": "100:4", "text_imlaei_simple": "فاثرن به نقعا" },
    { "id": 6151, "verse_key": "100:5", "text_imlaei_simple": "فوسطن به جمعا" },
    {
      "id": 6152,
      "verse_key": "100:6",
      "text_imlaei_simple": "ان الانسان لربه لكنود"
    },
    {
      "id": 6153,
      "verse_key": "100:7",
      "text_imlaei_simple": "وانه على ذلك لشهيد"
    },
    {
      "id": 6154,
      "verse_key": "100:8",
      "text_imlaei_simple": "وانه لحب الخير لشديد"
    },
    {
      "id": 6155,
      "verse_key": "100:9",
      "text_imlaei_simple": "افلا يعلم اذا بعثر ما في القبور"
    },
    {
      "id": 6156,
      "verse_key": "100:10",
      "text_imlaei_simple": "وحصل ما في الصدور"
    },
    {
      "id": 6157,
      "verse_key": "100:11",
      "text_imlaei_simple": "ان ربهم بهم يومئذ لخبير"
    }
  ],
  "meta": { "filters": { "chapter_number": "100" } }
}
