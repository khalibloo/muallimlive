{
  "translations": [
    {
      "resource_id": 232,
      "text": "Ndayira embalaasi ezizinda(ezidduka) nga ziweevuuma."
    },
    {
      "resource_id": 232,
      "text": "Nezimansula e nsasi z’omuliroolw’okukoona enjazi."
    },
    { "resource_id": 232, "text": "Ezizinda ku maliiri." },
    {
      "resource_id": 232,
      "text": "Nezisiikuula enfuufu, mu kutyekuula emisinde okwo."
    },
    { "resource_id": 232, "text": "Neziyingira mu balabe wakati." },
    { "resource_id": 232, "text": "Mazima ddala omuntu mutomi eri omuleziwe." },
    {
      "resource_id": 232,
      "text": "Era mazima ye (omuntu) ku ky'obutomi mujulizi."
    },
    {
      "resource_id": 232,
      "text": "Era mazima yye muyitirivu nnyo mu   kwagala ebyenfuna."
    },
    {
      "resource_id": 232,
      "text": "Abaffe (omuntu) tamanyi nti ebiri mu kabbuli bwe biriba bizuukiziddwa."
    },
    { "resource_id": 232, "text": "N’ebiri mu bifuba ne byolesebwa." },
    {
      "resource_id": 232,
      "text": "Mazima Katonda omulezi waabwe (nga bulijjo ne) ku lunaku olwo agenda kuba ng'amanyi byonna ebibakwatako."
    }
  ],
