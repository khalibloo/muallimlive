{
  "translations": [
    { "resource_id": 210, "text": "Soluk soluğa koşanlara yemin olsun!" },
    { "resource_id": 210, "text": "(Koşarken) kıvılcım saçanlara." },
    { "resource_id": 210, "text": "Sabah vakti baskın yapanlara." },
    { "resource_id": 210, "text": "Tozu dumana katanlara." },
    { "resource_id": 210, "text": "Topluluğun tam ortasına dalanlara." },
    {
      "resource_id": 210,
      "text": "İnsan, Rabbine karşı gerçekten çok nankördür."
    },
    { "resource_id": 210, "text": "Ve şüphesiz kendisi de buna şahittir." },
    {
      "resource_id": 210,
      "text": "Şüphesiz onun mal sevgisi de çok şiddetlidir."
    },
    {
      "resource_id": 210,
      "text": "Bilmez mi ki kabirlerde olanlar alt üst edildiğinde."
    },
    { "resource_id": 210, "text": "Kalplerde bulunan ortaya konduğunda." },
    {
      "resource_id": 210,
      "text": "Şüphesiz o gün Rableri kendilerinden elbette haberdardır."
    }
