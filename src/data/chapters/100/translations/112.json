{
  "translations": [
    { "resource_id": 112, "text": "Andolsun, soluya soluya koşanlara," },
    { "resource_id": 112, "text": "(Koşarken) kıvılcım saçanlara ," },
    { "resource_id": 112, "text": "Sabah vakti baskın yapanlara," },
    { "resource_id": 112, "text": "Tozu dumana katanlara," },
    { "resource_id": 112, "text": "Topluluğun ortasına dalanlara," },
    { "resource_id": 112, "text": "İnsan, Rabbine karşı gerçekten nankördür." },
    { "resource_id": 112, "text": "Kendisi de buna şahittir." },
    {
      "resource_id": 112,
      "text": "Şüphesiz onun mal sevgisi de çok şiddetlidir."
    },
    {
      "resource_id": 112,
      "text": "Bilmez mi (insan), kabirlerde bulunanların çıkarılacağı."
    },
    { "resource_id": 112, "text": "Kalplerdeki ortaya konduğunda," },
    {
      "resource_id": 112,
      "text": "Şüphesiz o gün, Rableri kendilerinden haberdardır."
    }
  ],
