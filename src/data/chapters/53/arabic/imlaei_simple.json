{
  "verses": [
    { "id": 4785, "verse_key": "53:1", "text_imlaei_simple": "والنجم اذا هوى" },
    {
      "id": 4786,
      "verse_key": "53:2",
      "text_imlaei_simple": "ما ضل صاحبكم وما غوى"
    },
    {
      "id": 4787,
      "verse_key": "53:3",
      "text_imlaei_simple": "وما ينطق عن الهوى"
    },
    {
      "id": 4788,
      "verse_key": "53:4",
      "text_imlaei_simple": "ان هو الا وحي يوحى"
    },
    {
      "id": 4789,
      "verse_key": "53:5",
      "text_imlaei_simple": "علمه شديد القوى"
    },
    { "id": 4790, "verse_key": "53:6", "text_imlaei_simple": "ذو مرة فاستوى" },
    {
      "id": 4791,
      "verse_key": "53:7",
      "text_imlaei_simple": "وهو بالافق الاعلى"
    },
    { "id": 4792, "verse_key": "53:8", "text_imlaei_simple": "ثم دنا فتدلى" },
    {
      "id": 4793,
      "verse_key": "53:9",
      "text_imlaei_simple": "فكان قاب قوسين او ادنى"
    },
    {
      "id": 4794,
      "verse_key": "53:10",
      "text_imlaei_simple": "فاوحى الى عبده ما اوحى"
    },
    {
      "id": 4795,
      "verse_key": "53:11",
      "text_imlaei_simple": "ما كذب الفؤاد ما راى"
    },
    {
      "id": 4796,
      "verse_key": "53:12",
      "text_imlaei_simple": "افتمارونه على ما يرى"
    },
    {
      "id": 4797,
      "verse_key": "53:13",
      "text_imlaei_simple": "ولقد راه نزلة اخرى"
    },
    {
      "id": 4798,
      "verse_key": "53:14",
      "text_imlaei_simple": "عند سدرة المنتهى"
    },
    {
      "id": 4799,
      "verse_key": "53:15",
      "text_imlaei_simple": "عندها جنة الماوى"
    },
    {
      "id": 4800,
      "verse_key": "53:16",
      "text_imlaei_simple": "اذ يغشى السدرة ما يغشى"
    },
    {
      "id": 4801,
      "verse_key": "53:17",
      "text_imlaei_simple": "ما زاغ البصر وما طغى"
    },
    {
      "id": 4802,
      "verse_key": "53:18",
      "text_imlaei_simple": "لقد راى من ايات ربه الكبرى"
    },
    {
      "id": 4803,
      "verse_key": "53:19",
      "text_imlaei_simple": "افرايتم اللات والعزى"
    },
    {
      "id": 4804,
      "verse_key": "53:20",
      "text_imlaei_simple": "ومناة الثالثة الاخرى"
    },
    {
      "id": 4805,
      "verse_key": "53:21",
      "text_imlaei_simple": "الكم الذكر وله الانثى"
    },
    {
      "id": 4806,
      "verse_key": "53:22",
      "text_imlaei_simple": "تلك اذا قسمة ضيزى"
    },
    {
      "id": 4807,
      "verse_key": "53:23",
      "text_imlaei_simple": "ان هي الا اسماء سميتموها انتم واباؤكم ما انزل الله بها من سلطان ان يتبعون الا الظن وما تهوى الانفس ولقد جاءهم من ربهم الهدى"
    },
    {
      "id": 4808,
      "verse_key": "53:24",
      "text_imlaei_simple": "ام للانسان ما تمنى"
    },
    {
      "id": 4809,
      "verse_key": "53:25",
      "text_imlaei_simple": "فلله الاخرة والاولى"
    },
    {
      "id": 4810,
      "verse_key": "53:26",
      "text_imlaei_simple": "وكم من ملك في السماوات لا تغني شفاعتهم شيئا الا من بعد ان ياذن الله لمن يشاء ويرضى"
    },
    {
      "id": 4811,
      "verse_key": "53:27",
      "text_imlaei_simple": "ان الذين لا يؤمنون بالاخرة ليسمون الملائكة تسمية الانثى"
    },
    {
      "id": 4812,
      "verse_key": "53:28",
      "text_imlaei_simple": "وما لهم به من علم ان يتبعون الا الظن وان الظن لا يغني من الحق شيئا"
    },
    {
      "id": 4813,
      "verse_key": "53:29",
      "text_imlaei_simple": "فاعرض عن من تولى عن ذكرنا ولم يرد الا الحياة الدنيا"
    },
    {
      "id": 4814,
      "verse_key": "53:30",
      "text_imlaei_simple": "ذلك مبلغهم من العلم ان ربك هو اعلم بمن ضل عن سبيله وهو اعلم بمن اهتدى"
    },
    {
      "id": 4815,
      "verse_key": "53:31",
      "text_imlaei_simple": "ولله ما في السماوات وما في الارض ليجزي الذين اساءوا بما عملوا ويجزي الذين احسنوا بالحسنى"
    },
    {
      "id": 4816,
      "verse_key": "53:32",
      "text_imlaei_simple": "الذين يجتنبون كبائر الاثم والفواحش الا اللمم ان ربك واسع المغفرة هو اعلم بكم اذ انشاكم من الارض واذ انتم اجنة في بطون امهاتكم فلا تزكوا انفسكم هو اعلم بمن اتقى"
    },
    {
      "id": 4817,
      "verse_key": "53:33",
      "text_imlaei_simple": "افرايت الذي تولى"
    },
    {
      "id": 4818,
      "verse_key": "53:34",
      "text_imlaei_simple": "واعطى قليلا واكدى"
    },
    {
      "id": 4819,
      "verse_key": "53:35",
      "text_imlaei_simple": "اعنده علم الغيب فهو يرى"
    },
    {
      "id": 4820,
      "verse_key": "53:36",
      "text_imlaei_simple": "ام لم ينبا بما في صحف موسى"
    },
    {
      "id": 4821,
      "verse_key": "53:37",
      "text_imlaei_simple": "وابراهيم الذي وفى"
    },
    {
      "id": 4822,
      "verse_key": "53:38",
      "text_imlaei_simple": "الا تزر وازرة وزر اخرى"
    },
    {
      "id": 4823,
      "verse_key": "53:39",
      "text_imlaei_simple": "وان ليس للانسان الا ما سعى"
    },
    {
      "id": 4824,
      "verse_key": "53:40",
      "text_imlaei_simple": "وان سعيه سوف يرى"
    },
    {
      "id": 4825,
      "verse_key": "53:41",
      "text_imlaei_simple": "ثم يجزاه الجزاء الاوفى"
    },
    {
      "id": 4826,
      "verse_key": "53:42",
      "text_imlaei_simple": "وان الى ربك المنتهى"
    },
    {
      "id": 4827,
      "verse_key": "53:43",
      "text_imlaei_simple": "وانه هو اضحك وابكى"
    },
    {
      "id": 4828,
      "verse_key": "53:44",
      "text_imlaei_simple": "وانه هو امات واحيا"
    },
    {
      "id": 4829,
      "verse_key": "53:45",
      "text_imlaei_simple": "وانه خلق الزوجين الذكر والانثى"
    },
    {
      "id": 4830,
      "verse_key": "53:46",
      "text_imlaei_simple": "من نطفة اذا تمنى"
    },
    {
      "id": 4831,
      "verse_key": "53:47",
      "text_imlaei_simple": "وان عليه النشاة الاخرى"
    },
    {
      "id": 4832,
      "verse_key": "53:48",
      "text_imlaei_simple": "وانه هو اغنى واقنى"
    },
    {
      "id": 4833,
      "verse_key": "53:49",
      "text_imlaei_simple": "وانه هو رب الشعرى"
    },
    {
      "id": 4834,
      "verse_key": "53:50",
      "text_imlaei_simple": "وانه اهلك عادا الاولى"
    },
    {
      "id": 4835,
      "verse_key": "53:51",
      "text_imlaei_simple": "وثمود فما ابقى"
    },
    {
      "id": 4836,
      "verse_key": "53:52",
      "text_imlaei_simple": "وقوم نوح من قبل انهم كانوا هم اظلم واطغى"
    },
    {
      "id": 4837,
      "verse_key": "53:53",
      "text_imlaei_simple": "والمؤتفكة اهوى"
    },
    { "id": 4838, "verse_key": "53:54", "text_imlaei_simple": "فغشاها ما غشى" },
    {
      "id": 4839,
      "verse_key": "53:55",
      "text_imlaei_simple": "فباي الاء ربك تتمارى"
    },
    {
      "id": 4840,
      "verse_key": "53:56",
      "text_imlaei_simple": "هذا نذير من النذر الاولى"
    },
    { "id": 4841, "verse_key": "53:57", "text_imlaei_simple": "ازفت الازفة" },
    {
      "id": 4842,
      "verse_key": "53:58",
      "text_imlaei_simple": "ليس لها من دون الله كاشفة"
    },
    {
      "id": 4843,
      "verse_key": "53:59",
      "text_imlaei_simple": "افمن هذا الحديث تعجبون"
    },
    {
      "id": 4844,
      "verse_key": "53:60",
      "text_imlaei_simple": "وتضحكون ولا تبكون"
    },
    { "id": 4845, "verse_key": "53:61", "text_imlaei_simple": "وانتم سامدون" },
    {
      "id": 4846,
      "verse_key": "53:62",
      "text_imlaei_simple": "فاسجدوا لله واعبدوا"
    }
  ],
  "meta": { "filters": { "chapter_number": "53" } }
}
