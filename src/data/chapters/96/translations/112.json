{
  "translations": [
    { "resource_id": 112, "text": "Oku! Yaratan Rabbinin adıyla." },
    { "resource_id": 112, "text": "İnsanı alaktan yaratan," },
    { "resource_id": 112, "text": "Oku! Rabbin, en büyük kerem sahibidir." },
    { "resource_id": 112, "text": "Kalemle öğreten…" },
    { "resource_id": 112, "text": "İnsana bilmediğini öğretendir." },
    { "resource_id": 112, "text": "Hayır! Şüphesiz insan azgınlık eder." },
    { "resource_id": 112, "text": "Kendini müstağni gördüğünde." },
    { "resource_id": 112, "text": "Oysa, dönüş Rabbinedir." },
    { "resource_id": 112, "text": "Engelleyeni gördün mü?" },
    { "resource_id": 112, "text": "Kulu, namaz kılarken." },
    { "resource_id": 112, "text": "Gördün mü? Eğer o hidayet üzerinde ise," },
    {
      "resource_id": 112,
      "text": "Veya (onu) takvayı emrediyorken (mi engelliyor)?"
    },
    {
      "resource_id": 112,
      "text": "Gördün mü? Eğer, yalanlamış ve yüz çevirmişse,"
    },
    { "resource_id": 112, "text": "Allah’ın gördüğünü bilmiyor mu?" },
    {
      "resource_id": 112,
      "text": "Hayır! Eğer son vermezse, elbette perçeminden yakalayıp, sürükleriz."
    },
    { "resource_id": 112, "text": "Yalancı, günahkâr perçeminden." },
    { "resource_id": 112, "text": "Haydi çağırsın meclisini." },
    { "resource_id": 112, "text": "Çağıracağız zebanileri." },
    {
      "resource_id": 112,
      "text": "Hayır! ona itaat etme! (Allah'a) Secde et ve (Ona) yaklaş!"
    }
  ],
  "meta": {
