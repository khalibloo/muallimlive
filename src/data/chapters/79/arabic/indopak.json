{
  "verses": [
    {
      "id": 5713,
      "verse_key": "79:1",
      "text_indopak": "وَالنّٰزِعٰتِ غَرۡقًا ۙ‏"
    },
    {
      "id": 5714,
      "verse_key": "79:2",
      "text_indopak": "وَّالنّٰشِطٰتِ نَشۡطًا ۙ‏"
    },
    {
      "id": 5715,
      "verse_key": "79:3",
      "text_indopak": "وَّالسّٰبِحٰتِ سَبۡحًا ۙ‏"
    },
    {
      "id": 5716,
      "verse_key": "79:4",
      "text_indopak": "فَالسّٰبِقٰتِ سَبۡقًا ۙ‏"
    },
    {
      "id": 5717,
      "verse_key": "79:5",
      "text_indopak": "فَالۡمُدَبِّرٰتِ اَمۡرًا​ ۘ‏"
    },
    {
      "id": 5718,
      "verse_key": "79:6",
      "text_indopak": "يَوۡمَ تَرۡجُفُ الرَّاجِفَةُ ۙ‏"
    },
    {
      "id": 5719,
      "verse_key": "79:7",
      "text_indopak": "تَتۡبَعُهَا الرَّادِفَةُ ؕ‏"
    },
    {
      "id": 5720,
      "verse_key": "79:8",
      "text_indopak": "قُلُوۡبٌ يَّوۡمَٮِٕذٍ وَّاجِفَةٌ ۙ‏"
    },
    {
      "id": 5721,
      "verse_key": "79:9",
      "text_indopak": "اَبۡصَارُهَا خَاشِعَةٌ​ ۘ‏"
    },
    {
      "id": 5722,
      "verse_key": "79:10",
      "text_indopak": "يَقُوۡلُوۡنَ ءَاِنَّا لَمَرۡدُوۡدُوۡنَ فِى الۡحَـافِرَةِ ؕ‏"
    },
    {
      "id": 5723,
      "verse_key": "79:11",
      "text_indopak": "ءَاِذَا كُنَّا عِظَامًا نَّخِرَةً ؕ‏"
    },
    {
      "id": 5724,
      "verse_key": "79:12",
      "text_indopak": "قَالُوۡا تِلۡكَ اِذًا كَرَّةٌ خَاسِرَةٌ​ ۘ‏"
    },
    {
      "id": 5725,
      "verse_key": "79:13",
      "text_indopak": "فَاِنَّمَا هِىَ زَجۡرَةٌ وَّاحِدَةٌ ۙ‏"
    },
    {
      "id": 5726,
      "verse_key": "79:14",
      "text_indopak": "فَاِذَا هُمۡ بِالسَّاهِرَةِ ؕ‏"
    },
    {
      "id": 5727,
      "verse_key": "79:15",
      "text_indopak": "هَلۡ اَتٰٮكَ حَدِيۡثُ مُوۡسٰى​ۘ‏"
    },
    {
      "id": 5728,
      "verse_key": "79:16",
      "text_indopak": "اِذۡ نَادٰٮهُ رَبُّهٗ بِالۡوَادِ الۡمُقَدَّسِ طُوًى​ۚ‏"
    },
    {
      "id": 5729,
      "verse_key": "79:17",
      "text_indopak": "اِذۡهَبۡ اِلٰى فِرۡعَوۡنَ اِنَّهٗ طَغٰى ۖ‏"
    },
    {
      "id": 5730,
      "verse_key": "79:18",
      "text_indopak": "فَقُلۡ هَلۡ لَّكَ اِلٰٓى اَنۡ تَزَكّٰى ۙ‏"
    },
    {
      "id": 5731,
      "verse_key": "79:19",
      "text_indopak": "وَاَهۡدِيَكَ اِلٰى رَبِّكَ فَتَخۡشٰى​ۚ‏"
    },
    {
      "id": 5732,
      "verse_key": "79:20",
      "text_indopak": "فَاَرٰٮهُ الۡاٰيَةَ الۡكُبۡرٰى ۖ‏"
    },
    {
      "id": 5733,
      "verse_key": "79:21",
      "text_indopak": "فَكَذَّبَ وَعَصٰى ۖ‏"
    },
    {
      "id": 5734,
      "verse_key": "79:22",
      "text_indopak": "ثُمَّ اَدۡبَرَ يَسۡعٰىۖ‏"
    },
    {
      "id": 5735,
      "verse_key": "79:23",
      "text_indopak": "فَحَشَرَ فَنَادٰىۖ‏"
    },
    {
      "id": 5736,
      "verse_key": "79:24",
      "text_indopak": "فَقَالَ اَنَا رَبُّكُمُ الۡاَعۡلٰى ۖ‏"
    },
    {
      "id": 5737,
      "verse_key": "79:25",
      "text_indopak": "فَاَخَذَهُ اللّٰهُ نَڪَالَ الۡاٰخِرَةِ وَالۡاُوۡلٰى ؕ‏"
    },
    {
      "id": 5738,
      "verse_key": "79:26",
      "text_indopak": "اِنَّ فِىۡ ذٰلِكَ لَعِبۡرَةً لِّمَنۡ يَّخۡشٰىؕ‏"
    },
    {
      "id": 5739,
      "verse_key": "79:27",
      "text_indopak": "ءَاَنۡتُمۡ اَشَدُّ خَلۡقًا اَمِ السَّمَآءُ​ ؕ بَنٰٮهَا‏"
    },
    {
      "id": 5740,
      "verse_key": "79:28",
      "text_indopak": "رَفَعَ سَمۡكَهَا فَسَوَّٮهَا ۙ‏"
    },
    {
      "id": 5741,
      "verse_key": "79:29",
      "text_indopak": "وَ اَغۡطَشَ لَيۡلَهَا وَاَخۡرَجَ ضُحٰٮهَا‏"
    },
    {
      "id": 5742,
      "verse_key": "79:30",
      "text_indopak": "وَالۡاَرۡضَ بَعۡدَ ذٰلِكَ دَحٰٮهَا ؕ‏"
    },
    {
      "id": 5743,
      "verse_key": "79:31",
      "text_indopak": "اَخۡرَجَ مِنۡهَا مَآءَهَا وَمَرۡعٰٮهَا‏"
    },
    {
      "id": 5744,
      "verse_key": "79:32",
      "text_indopak": "وَالۡجِبَالَ اَرۡسٰٮهَا ۙ‏"
    },
    {
      "id": 5745,
      "verse_key": "79:33",
      "text_indopak": "مَتَاعًا لَّـكُمۡ وَلِاَنۡعَامِكُمۡؕ‏"
    },
    {
      "id": 5746,
      "verse_key": "79:34",
      "text_indopak": "فَاِذَا جَآءَتِ الطَّآمَّةُ الۡكُبۡرٰى ۖ‏"
    },
    {
      "id": 5747,
      "verse_key": "79:35",
      "text_indopak": "يَوۡمَ يَتَذَكَّرُ الۡاِنۡسَانُ مَا سَعٰىۙ‏"
    },
    {
      "id": 5748,
      "verse_key": "79:36",
      "text_indopak": "وَبُرِّزَتِ الۡجَحِيۡمُ لِمَنۡ يَّرٰى‏"
    },
    {
      "id": 5749,
      "verse_key": "79:37",
      "text_indopak": "فَاَمَّا مَنۡ طَغٰىۙ‏"
    },
    {
      "id": 5750,
      "verse_key": "79:38",
      "text_indopak": "وَاٰثَرَ الۡحَيٰوةَ الدُّنۡيَا ۙ‏"
    },
    {
      "id": 5751,
      "verse_key": "79:39",
      "text_indopak": "فَاِنَّ الۡجَحِيۡمَ هِىَ الۡمَاۡوٰىؕ‏"
    },
    {
      "id": 5752,
      "verse_key": "79:40",
      "text_indopak": "وَاَمَّا مَنۡ خَافَ مَقَامَ رَبِّهٖ وَ نَهَى النَّفۡسَ عَنِ الۡهَوٰىۙ‏"
    },
    {
      "id": 5753,
      "verse_key": "79:41",
      "text_indopak": "فَاِنَّ الۡجَـنَّةَ هِىَ الۡمَاۡوٰىؕ‏"
    },
    {
      "id": 5754,
      "verse_key": "79:42",
      "text_indopak": "يَسۡـــَٔلُوۡنَكَ عَنِ السَّاعَةِ اَيَّانَ مُرۡسٰٮهَا ؕ‏"
    },
    {
      "id": 5755,
      "verse_key": "79:43",
      "text_indopak": "فِيۡمَ اَنۡتَ مِنۡ ذِكۡرٰٮهَاؕ‏"
    },
    {
      "id": 5756,
      "verse_key": "79:44",
      "text_indopak": "اِلٰى رَبِّكَ مُنۡتَهٰٮهَاؕ‏"
    },
    {
      "id": 5757,
      "verse_key": "79:45",
      "text_indopak": "اِنَّمَاۤ اَنۡتَ مُنۡذِرُ مَنۡ يَّخۡشٰٮهَاؕ‏"
    },
    {
      "id": 5758,
      "verse_key": "79:46",
      "text_indopak": "كَاَنَّهُمۡ يَوۡمَ يَرَوۡنَهَا لَمۡ يَلۡبَثُوۡۤا اِلَّا عَشِيَّةً اَوۡ ضُحٰٮهَا‏ ﻿﻿"
    }
  ],
  "meta": { "filters": { "chapter_number": "79" } }
}
