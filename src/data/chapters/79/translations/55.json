{
  "translations": [
    {
      "resource_id": 55,
      "text": "Shiddat bilan (jonni) sug`urib oluvchi(farishta)lar bilan qasam."
    },
    {
      "resource_id": 55,
      "text": "Yumshoqlik bilan jon oluvchi(farishta)lar bilan qasam."
    },
    { "resource_id": 55, "text": "Suzib yuruvchi(farishta)lar bilan qasam." },
    {
      "resource_id": 55,
      "text": "O`zishda musobaqa qiluvchi(farishta)lar bilan qasam."
    },
    {
      "resource_id": 55,
      "text": "Amrning tadbirini qiluvchi(farishta)lar bilan qasam."
    },
    {
      "resource_id": 55,
      "text": "Iztirobga tushuvchi iztirobga tushgan kunda."
    },
    { "resource_id": 55, "text": "Unga ergashuvchi ergashganda." },
    { "resource_id": 55, "text": "O`sha kunda qalblar xavfga to`ladir." },
    { "resource_id": 55, "text": "Ularning ko`zlari qo`rqinchga to`ladir." },
    {
      "resource_id": 55,
      "text": "Ular: «Biz, albatta, ortga qaytarilamizmi?» – derlar."
    },
    {
      "resource_id": 55,
      "text": "«Chirigan suyak bo`lganimizdan keyin ham-a?»"
    },
    {
      "resource_id": 55,
      "text": "Agar shundoq bo`lsa, bu hasratiy qaytish-ku!» – derlar."
    },
    {
      "resource_id": 55,
      "text": "Albatta, u(qiyomat) bir qichqiriqdan boshqa narsa emas."
    },
    {
      "resource_id": 55,
      "text": "Ko`ribsizki, ular (qiyomat) maydonida turibdir."
    },
    { "resource_id": 55, "text": "Senga Musoning xabari keldimi?" },
    {
      "resource_id": 55,
      "text": "Qachonki Robbisi unga Muqaddas Tuva vodiysida nido qildi."
    },
    {
      "resource_id": 55,
      "text": "«Fir'avn huzuriga bor, albatta, u tug`yonga ketdi."
    },
    {
      "resource_id": 55,
      "text": "Bas unga ayt: «Senda poklanishga (rag`bat) bormi?"
    },
    {
      "resource_id": 55,
      "text": "Va seni Robbingga hidoyat qilsam, shoyadki (undan) qo`rqsang», degin»."
    },
    { "resource_id": 55, "text": "Va unga ulkan alomatni ko`rsatdi." },
    { "resource_id": 55, "text": "Ul uni yolg`onga chiqardi va osiy bo`ldi." },
    { "resource_id": 55, "text": "So`ng ortiga o`girilib, tezlab ketdi." },
    { "resource_id": 55, "text": "Keyin (odamlarni) to`pladi va nido qildi." },
    {
      "resource_id": 55,
      "text": "Va, men sizlarning eng oliy Robbingizman, dedi."
    },
    {
      "resource_id": 55,
      "text": "Bas, Alloh uni avvalgi va oxirgi gunohlari uchun azobga oldi."
    },
    {
      "resource_id": 55,
      "text": "Albatta, bunda U zotdan qo`rqqanlar uchun ibrat bordir."
    },
    {
      "resource_id": 55,
      "text": "Sizlarni yaratish qiyinmi yoki U zot bino qilgan osmonnimi?"
    },
    {
      "resource_id": 55,
      "text": "Uning shiftini yuqori ko`tardi va bekami-ko`st, mustahkam qilib qo`ydi."
    },
    {
      "resource_id": 55,
      "text": "Kechasini qorong`u va kunduzini yorug` qildi."
    },
    { "resource_id": 55, "text": "Va undan so`ng yerni tuxum shaklida qildi." },
    { "resource_id": 55, "text": "Undan suvni, o`t-o`lanlarni chiqardi." },
    { "resource_id": 55, "text": "Va tog`larni sobit qildi." },
    {
      "resource_id": 55,
      "text": "Sizlarga va chorvalaringizga manfaat bo`lsin uchun."
    },
    { "resource_id": 55, "text": "Vaqtiki katta g`olib (qiyomat) kelsa." },
    { "resource_id": 55, "text": "U kuni inson qilgan ishlarini eslaydir." },
    {
      "resource_id": 55,
      "text": "Va jahannam ko`radiganlarga zohir qilinadir."
    },
    { "resource_id": 55, "text": "Ammo kimiki tug`yonga ketgan bo`lsa." },
    { "resource_id": 55, "text": "Va bu dunyo hayotini ustun qo`ygan bo`lsa." },
    {
      "resource_id": 55,
      "text": "Bas, albatta, Jahiym (unga) o`rin bo`ladir."
    },
    {
      "resource_id": 55,
      "text": "Va ammo, kim Robbisining maqomidan qo`rqqan va o`z nafsini havolanib ketishidan qaytargan bo`lsa..."
    },
    {
      "resource_id": 55,
      "text": "Bas, albatta, jannat (unga) o`rin bo`ladir."
    },
    {
      "resource_id": 55,
      "text": "Sendan, qiyomat qachon bo`lur, deb so`rashadir."
    },
    {
      "resource_id": 55,
      "text": "U(qiyomat)ning zikrini sen qandoq bilarding."
    },
    { "resource_id": 55, "text": "Uning intihosi Robbinggagina xos." },
    {
      "resource_id": 55,
      "text": "Albatta, sen u(qiyomat)dan qo`rquvchilarga ogohlantiruvchisan, xolos."
    },
    {
      "resource_id": 55,
      "text": "U(qiyomat)ni ko`rgan kunlari, xuddi, bu dunyoda bir kecha yoki kunduzdan boshqa turmaganga o`xsharlar."
    }
  ],
