{
  "translations": [
    { "resource_id": 30, "text": "Kun aurinko menee umpeen," },
    { "resource_id": 30, "text": "kun tähdet sammuvat," },
    { "resource_id": 30, "text": "kun vuoret järkkyvät," },
    { "resource_id": 30, "text": "kun kantavat kameelit hylätään," },
    { "resource_id": 30, "text": "kun pedot peloitetaan muille tienoille," },
    { "resource_id": 30, "text": "kun meret kuohuvat yli äyräittensä," },
    { "resource_id": 30, "text": "kun sielut ja ruumiit jälleen yhtyvät" },
    {
      "resource_id": 30,
      "text": "ja kun elävänä haudatulta naiselta kysytään,"
    },
    { "resource_id": 30, "text": "minkä synnin vuoksi hänet surmattiin;" },
    { "resource_id": 30, "text": "kun kirjoitukset avataan," },
    { "resource_id": 30, "text": "kun taivaan kansi pyyhkäistään pois," },
    { "resource_id": 30, "text": "kun helvetin tuli viritetään" },
    { "resource_id": 30, "text": "ja kun paratiisi lähestyy," },
    {
      "resource_id": 30,
      "text": "silloin saa jokainen sielu tietää, mitä se on itselleen valmistanut."
    },
    { "resource_id": 30, "text": "Vannon kautta tähtien," },
    { "resource_id": 30, "text": "jotka kulkevat ratojaan ja katoavat;" },
    { "resource_id": 30, "text": "kautta saapuvan yön" },
    { "resource_id": 30, "text": "ja valkenevan aamun:" },
    {
      "resource_id": 30,
      "text": "»Tämä on totisesti jalon sananjulistajan sanoma,"
    },
    {
      "resource_id": 30,
      "text": "sellaisen, jonka valta tulee voiman Herralta,"
    },
    {
      "resource_id": 30,
      "text": "jota on toteltava ja joka ei luottamusta petä."
    },
    {
      "resource_id": 30,
      "text": "Ei teidän heimolaisenne (profeetta) houraile."
    },
    {
      "resource_id": 30,
      "text": "Totisesti hän on nähnyt enkeli Gabrielin kirkkaalla taivaanrannalla,"
    },
    {
      "resource_id": 30,
      "text": "eikä hän ole kitsas näkymättömien asiain salaaja;"
    },
    {
      "resource_id": 30,
      "text": "ei tämä myöskään ole karkoitetun paholaisen ennustelua."
    },
    { "resource_id": 30, "text": "Mitä kohti siis kuljette?" },
    {
      "resource_id": 30,
      "text": "Tämä ei ole muuta kuin varoitus maailman kansoille,"
    },
    {
      "resource_id": 30,
      "text": "jokaiselle teistä, joka tahtoo pysytellä oikealla tiellä."
    },
    {
      "resource_id": 30,
      "text": "Mutta ette te itse tahdo, ellei Jumala, maailmojen Herra,sitä tahdo.»"
    }
  ],
