{
  "translations": [
    { "resource_id": 55, "text": "Vaqtiki, quyoshni buklab, o`ralsa." },
    {
      "resource_id": 55,
      "text": "Va vaqtiki yulduzlar uzilib tushib, sochilib ketsa."
    },
    { "resource_id": 55, "text": "Va vaqtiki, tog`lar joyidan ko`chirilsa." },
    {
      "resource_id": 55,
      "text": "Va vaqtiki, homilasiga o`n oy to`lgan tuyalar qarovsiz qolsa."
    },
    {
      "resource_id": 55,
      "text": "Va vaqtiki, vahshiy hayvonlar bir joyga to`plansa."
    },
    { "resource_id": 55, "text": "Va vaqtiki, dengizlar alanga olib yonsa." },
    {
      "resource_id": 55,
      "text": "Va vaqtiki, nafslar juftlashtirilsa.<sup foot_note=\"169887\">1</sup>"
    },
    {
      "resource_id": 55,
      "text": "Va vaqtiki, tiriklay ko`milgan qizdan so`ralsa."
    },
    { "resource_id": 55, "text": "U nima gunoh uchun o`ldirilgan?" },
    { "resource_id": 55, "text": "Va vaqtiki, sahifalar nashr qilinsa." },
    { "resource_id": 55, "text": "Va vaqtiki, osmon qo`porilsa." },
    { "resource_id": 55, "text": "Va vaqtiki, jahannam qizdirilsa." },
    { "resource_id": 55, "text": "Va vaqtiki, jannat yaqinlashtirilsa." },
    { "resource_id": 55, "text": "Har bir jon nima hozirlaganini biladir." },
    {
      "resource_id": 55,
      "text": "(Kunduzi) maxfiy bo`luvchi(yulduz)lar bilan qasam."
    },
    {
      "resource_id": 55,
      "text": "(Kechalari) yuruvchi, berkinuvchi(yulduz)lar bilan qasam."
    },
    {
      "resource_id": 55,
      "text": "Qorong`uligi ila kirib kelgan kecha bilan qasam."
    },
    { "resource_id": 55, "text": "Nafas olib kirib kelgan tong bilan qasam." },
    {
      "resource_id": 55,
      "text": "Albatta u(Qur'on) karamli elchining so`zidir."
    },
    {
      "resource_id": 55,
      "text": "U quvvatli, arsh Egasi huzurida martabasi oliydir."
    },
    { "resource_id": 55, "text": "Itoat qilingan, omonatlidir." },
    { "resource_id": 55, "text": "Sizning sohibingiz majnun emasdir." },
    {
      "resource_id": 55,
      "text": "Batahqiq, ul ani ochiq-oydin ufqda ko`rdi.<sup foot_note=\"170546\">1</sup>"
    },
    { "resource_id": 55, "text": "Va u g`aybga baxil emasdir." },
    {
      "resource_id": 55,
      "text": "Va u(Qur'on) toshbo`ron qilingan shaytonning so`zi emasdir."
    },
    { "resource_id": 55, "text": "Bas, qayoqqa ketmoqdasiz?!" },
    {
      "resource_id": 55,
      "text": "U (Qur'on) olamlarga eslatmadan boshqa narsa emasdir."
    },
    {
      "resource_id": 55,
      "text": "Sizlardan istiqomat(to`g`ri yo`l)da bo`lishni xohlovchilar uchundir."
    },
    {
      "resource_id": 55,
      "text": "Olamlarning Robbi Alloh xohlamasa, sizlar xohlay olmassizlar.<sup foot_note=\"169888\">1</sup>"
    }
  ],
  "meta": {
    "translation_name": "Muhammad Sodiq Muhammad Yusuf (Latin) ",
    "author_name": "Muhammad Sodiq Muhammad Yusuf",
