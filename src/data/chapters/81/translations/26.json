{
  "translations": [
    { "resource_id": 26, "text": "Až slunce bude svinuto," },
    { "resource_id": 26, "text": "až hvězdy pohasnou," },
    { "resource_id": 26, "text": "až hory se z místa pohnou," },
    { "resource_id": 26, "text": "až březí velbloudice bude zanedbána" },
    { "resource_id": 26, "text": "a divoká zvířata shromážděna," },
    { "resource_id": 26, "text": "až moře se povaří" },
    { "resource_id": 26, "text": "a duše až se spáří," },
    { "resource_id": 26, "text": "až zaživa pohřbená bude tázána," },
    { "resource_id": 26, "text": "pro jakou vinu byla utracena," },
    { "resource_id": 26, "text": "až svitky budou rozvinuty" },
    { "resource_id": 26, "text": "a nebe bude staženo" },
    { "resource_id": 26, "text": "a peklo rozpáleno" },
    { "resource_id": 26, "text": "a zahrada rajská přiblížena," },
    { "resource_id": 26, "text": "tehdy duše každá, co vykonala, pozná!" },
    {
      "resource_id": 26,
      "text": "Hle, přísahám při planetách zpět se vracejících,"
    },
    { "resource_id": 26, "text": "spěchajících a mizejících," },
    { "resource_id": 26, "text": "při noci, když v temno se halí," },
    { "resource_id": 26, "text": "při jitru, když dechu nabírá," },
    { "resource_id": 26, "text": "že toto věru slova jsou posla vznešeného," },
    { "resource_id": 26, "text": "silou vládnoucího u Pána trůnu mohutného," },
    { "resource_id": 26, "text": "poslušnost nalézajícího a spolehlivého!" },
    { "resource_id": 26, "text": "Váš druh není džiny posedlý blázen" },
    { "resource_id": 26, "text": "a vskutku jej viděl na obzoru jasném -" },
    { "resource_id": 26, "text": "a není skoupý o nepoznatelném!" },
    { "resource_id": 26, "text": "A nejsou to slova satana prokletého!" },
    { "resource_id": 26, "text": "Kam tedy odcházíte?" },
    {
      "resource_id": 26,
      "text": "Vždyť toto slouží jen lidstvu k připomenutí,"
    },
    {
      "resource_id": 26,
      "text": "těm z vás, kdo přejí si po stezce přímé jíti."
    },
    {
      "resource_id": 26,
      "text": "Však přát si toho nebudete, pokud Bůh, Pán lidstva veškerého, nebude chtíti!"
    }
  ],
  "meta": {
    "translation_name": "Czech",
