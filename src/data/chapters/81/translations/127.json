{
  "translations": [
    { "resource_id": 127, "text": "Вақтики, қуёшни буклаб, ўралса." },
    {
      "resource_id": 127,
      "text": "Ва вақтики юлдузлар узилиб тушиб, сочилиб кетса."
    },
    { "resource_id": 127, "text": "Ва вақтики, тоғлар жойидан кўчирилса." },
    {
      "resource_id": 127,
      "text": "Ва вақтики, ҳомиласига ўн ой тўлган туялар қаровсиз қолса."
    },
    {
      "resource_id": 127,
      "text": "Ва вақтики, ваҳший ҳайвонлар бир жойга тўпланса."
    },
    { "resource_id": 127, "text": "Ва вақтики, денгизлар аланга олиб ёнса." },
    {
      "resource_id": 127,
      "text": "Ва вақтики, нафслар жуфтлаштирилса.<sup foot_note=48869>1</sup>"
    },
    {
      "resource_id": 127,
      "text": "Ва вақтики, тириклай кўмилган қиздан сўралса."
    },
    { "resource_id": 127, "text": "У нима гуноҳ учун ўлдирилган?" },
    { "resource_id": 127, "text": "Ва вақтики, саҳифалар нашр қилинса." },
    { "resource_id": 127, "text": "Ва вақтики, осмон қўпорилса." },
    { "resource_id": 127, "text": "Ва вақтики, жаҳаннам қиздирилса." },
    { "resource_id": 127, "text": "Ва вақтики, жаннат яқинлаштирилса." },
    { "resource_id": 127, "text": "Ҳар бир жон нима ҳозирлаганини биладир." },
    {
      "resource_id": 127,
      "text": "(Кундузи) махфий бўлувчи(юлдуз)лар билан қасам."
    },
    {
      "resource_id": 127,
      "text": "(Кечалари) юрувчи, беркинувчи(юлдуз)лар билан қасам."
    },
    {
      "resource_id": 127,
      "text": "Қоронғулиги ила кириб келган кеча билан қасам."
    },
    { "resource_id": 127, "text": "Нафас олиб кириб келган тонг билан қасам." },
    {
      "resource_id": 127,
      "text": "Албатта у(Қуръон) карамли элчининг сўзидир."
    },
    {
      "resource_id": 127,
      "text": "У қувватли, арш Эгаси ҳузурида мартабаси олийдир."
    },
    { "resource_id": 127, "text": "Итоат қилинган, омонатлидир." },
    { "resource_id": 127, "text": "Сизнинг соҳибингиз мажнун эмасдир." },
    {
      "resource_id": 127,
      "text": "Батаҳқиқ, ул ани очиқ-ойдин уфқда кўрди.<sup foot_note=48870>1</sup>"
    },
    { "resource_id": 127, "text": "Ва у ғайбга бахил эмасдир." },
    {
      "resource_id": 127,
      "text": "Ва у(Қуръон) тошбўрон қилинган шайтоннинг сўзи эмасдир."
    },
    { "resource_id": 127, "text": "Бас, қаёққа кетмоқдасиз?!" },
    {
      "resource_id": 127,
      "text": "У (Қуръон) оламларга эслатмадан бошқа нарса эмасдир."
    },
    {
      "resource_id": 127,
      "text": "Сизлардан истиқомат(тўғри йўл)да бўлишни хоҳловчилар учундир."
    },
    {
      "resource_id": 127,
      "text": "Оламларнинг Робби Аллоҳ хоҳламаса, сизлар хоҳлай олмассизлар.<sup foot_note=48871>1</sup>"
    }
  ],
  "meta": {
