{
  "verses": [
    { "id": 5759, "verse_key": "80:1", "text_indopak": "عَبَسَ وَتَوَلّٰٓىۙ‏" },
    {
      "id": 5760,
      "verse_key": "80:2",
      "text_indopak": "اَنۡ جَآءَهُ الۡاَعۡمٰىؕ‏"
    },
    {
      "id": 5761,
      "verse_key": "80:3",
      "text_indopak": "وَمَا يُدۡرِيۡكَ لَعَلَّهٗ يَزَّكّٰٓىۙ‏"
    },
    {
      "id": 5762,
      "verse_key": "80:4",
      "text_indopak": "اَوۡ يَذَّكَّرُ فَتَنۡفَعَهُ الذِّكۡرٰىؕ‏"
    },
    {
      "id": 5763,
      "verse_key": "80:5",
      "text_indopak": "اَمَّا مَنِ اسۡتَغۡنٰىۙ‏"
    },
    {
      "id": 5764,
      "verse_key": "80:6",
      "text_indopak": "فَاَنۡتَ لَهٗ تَصَدّٰىؕ‏"
    },
    {
      "id": 5765,
      "verse_key": "80:7",
      "text_indopak": "وَمَا عَلَيۡكَ اَلَّا يَزَّكّٰٓىؕ‏"
    },
    {
      "id": 5766,
      "verse_key": "80:8",
      "text_indopak": "وَاَمَّا مَنۡ جَآءَكَ يَسۡعٰىۙ‏"
    },
    { "id": 5767, "verse_key": "80:9", "text_indopak": "وَهُوَ يَخۡشٰىۙ‏" },
    {
      "id": 5768,
      "verse_key": "80:10",
      "text_indopak": "فَاَنۡتَ عَنۡهُ تَلَهّٰى​ۚ‏"
    },
    {
      "id": 5769,
      "verse_key": "80:11",
      "text_indopak": "كَلَّاۤ اِنَّهَا تَذۡكِرَةٌ ۚ‏"
    },
    {
      "id": 5770,
      "verse_key": "80:12",
      "text_indopak": "فَمَنۡ شَآءَ ذَكَرَهٗ​ۘ‏"
    },
    {
      "id": 5771,
      "verse_key": "80:13",
      "text_indopak": "فِىۡ صُحُفٍ مُّكَرَّمَةٍۙ‏"
    },
    {
      "id": 5772,
      "verse_key": "80:14",
      "text_indopak": "مَّرۡفُوۡعَةٍ مُّطَهَّرَةٍ ۭۙ‏"
    },
    {
      "id": 5773,
      "verse_key": "80:15",
      "text_indopak": "بِاَيۡدِىۡ سَفَرَةٍۙ‏"
    },
    { "id": 5774, "verse_key": "80:16", "text_indopak": "كِرَامٍۢ بَرَرَةٍؕ‏" },
    {
      "id": 5775,
      "verse_key": "80:17",
      "text_indopak": "قُتِلَ الۡاِنۡسَانُ مَاۤ اَكۡفَرَهٗؕ‏"
    },
    {
      "id": 5776,
      "verse_key": "80:18",
      "text_indopak": "مِنۡ اَىِّ شَىۡءٍ خَلَقَهٗؕ‏"
    },
    {
      "id": 5777,
      "verse_key": "80:19",
      "text_indopak": "مِنۡ نُّطۡفَةٍؕ خَلَقَهٗ فَقَدَّرَهٗ ۙ‏"
    },
    {
      "id": 5778,
      "verse_key": "80:20",
      "text_indopak": "ثُمَّ السَّبِيۡلَ يَسَّرَهٗۙ‏"
    },
    {
      "id": 5779,
      "verse_key": "80:21",
      "text_indopak": "ثُمَّ اَمَاتَهٗ فَاَقۡبَرَهٗۙ‏"
    },
    {
      "id": 5780,
      "verse_key": "80:22",
      "text_indopak": "ثُمَّ اِذَا شَآءَ اَنۡشَرَهٗؕ‏"
    },
    {
      "id": 5781,
      "verse_key": "80:23",
      "text_indopak": "كَلَّا لَـمَّا يَقۡضِ مَاۤ اَمَرَهٗؕ‏"
    },
    {
      "id": 5782,
      "verse_key": "80:24",
      "text_indopak": "فَلۡيَنۡظُرِ الۡاِنۡسَانُ اِلٰى طَعَامِهٖۤۙ‏"
    },
    {
      "id": 5783,
      "verse_key": "80:25",
      "text_indopak": "اَنَّا صَبَبۡنَا الۡمَآءَ صَبًّا ۙ‏"
    },
    {
      "id": 5784,
      "verse_key": "80:26",
      "text_indopak": "ثُمَّ شَقَقۡنَا الۡاَرۡضَ شَقًّا ۙ‏"
    },
    {
      "id": 5785,
      "verse_key": "80:27",
      "text_indopak": "فَاَنۡۢبَتۡنَا فِيۡهَا حَبًّا ۙ‏"
    },
    {
      "id": 5786,
      "verse_key": "80:28",
      "text_indopak": "وَّ عِنَبًا وَّقَضۡبًا ۙ‏"
    },
    {
      "id": 5787,
      "verse_key": "80:29",
      "text_indopak": "وَّزَيۡتُوۡنًا وَّنَخۡلًا ؕ‏"
    },
    {
      "id": 5788,
      "verse_key": "80:30",
      "text_indopak": "وَحَدَآٮِٕقَ غُلۡبًا ۙ‏"
    },
    {
      "id": 5789,
      "verse_key": "80:31",
      "text_indopak": "وَّفَاكِهَةً وَّاَبًّا ۙ‏"
    },
    {
      "id": 5790,
      "verse_key": "80:32",
      "text_indopak": "مَّتَاعًا لَّـكُمۡ وَلِاَنۡعَامِكُمۡؕ‏"
    },
    {
      "id": 5791,
      "verse_key": "80:33",
      "text_indopak": "فَاِذَا جَآءَتِ الصَّآخَّةُ‏"
    },
    {
      "id": 5792,
      "verse_key": "80:34",
      "text_indopak": "يَوۡمَ يَفِرُّ الۡمَرۡءُ مِنۡ اَخِيۡهِۙ‏"
    },
    {
      "id": 5793,
      "verse_key": "80:35",
      "text_indopak": "وَاُمِّهٖ وَاَبِيۡهِۙ‏"
    },
    {
      "id": 5794,
      "verse_key": "80:36",
      "text_indopak": "وَصَاحِبَتِهٖ وَبَنِيۡهِؕ‏"
    },
    {
      "id": 5795,
      "verse_key": "80:37",
      "text_indopak": "لِكُلِّ امۡرِیءٍ مِّنۡهُمۡ يَوۡمَٮِٕذٍ شَاۡنٌ يُّغۡنِيۡهِؕ‏"
    },
    {
      "id": 5796,
      "verse_key": "80:38",
      "text_indopak": "وُجُوۡهٌ يَّوۡمَٮِٕذٍ مُّسۡفِرَةٌ ۙ‏"
    },
    {
      "id": 5797,
      "verse_key": "80:39",
      "text_indopak": "ضَاحِكَةٌ مُّسۡتَبۡشِرَةٌ ۚ‏"
    },
    {
      "id": 5798,
      "verse_key": "80:40",
      "text_indopak": "وَوُجُوۡهٌ يَّوۡمَٮِٕذٍ عَلَيۡهَا غَبَرَةٌ ۙ‏"
    },
    {
      "id": 5799,
      "verse_key": "80:41",
      "text_indopak": "تَرۡهَقُهَا قَتَرَةٌ ؕ‏"
    },
    {
      "id": 5800,
      "verse_key": "80:42",
      "text_indopak": "اُولٰٓٮِٕكَ هُمُ الۡكَفَرَةُ الۡفَجَرَةُ‏"
    }
  ],
  "meta": { "filters": { "chapter_number": "80" } }
}
