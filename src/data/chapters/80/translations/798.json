{
  "translations": [
    {
      "resource_id": 798,
      "text": "Jwautile sinya (Muhammadi ﷺ) ni kulola kumbali."
    },
    { "resource_id": 798, "text": "Ligongo lyakum’bichiilila jwangalola." },
    {
      "resource_id": 798,
      "text": "Ana chichi chachimmanyisye kuti mwine jwalakwejo kuŵa nkuliswejesya (ku ilemwa)?"
    },
    {
      "resource_id": 798,
      "text": "Mwine chakumbuchile, basi nikunkamuchisya kumbuchilako?"
    },
    { "resource_id": 798, "text": "Nambo mundu jwaliweni kusichila," },
    { "resource_id": 798, "text": "Basi jwalakwejo mmwe ni nkunkosoŵalila;" },
    {
      "resource_id": 798,
      "text": "Pangalitu ulemwa kukwenu naga ngaakuliswejesya (ku ukafili)."
    },
    {
      "resource_id": 798,
      "text": "Nambo mundu jwam’bichilile aku juchikangamalaga,"
    },
    { "resource_id": 798, "text": "Soni jwalakwejo ali nkun'jogopa (Allah)," },
    { "resource_id": 798, "text": "Basi ya jwalakwejo mmwe ni nkuikwanjila;" },
    {
      "resource_id": 798,
      "text": "Ngwamba (ngasintenda iyyo)! Chisimu galakwe (ma Ȃyaga) gaŵele chikumbusyo."
    },
    {
      "resource_id": 798,
      "text": "Basi jwaakusaka talikumbusye najo (Qur’anijo)."
    },
    { "resource_id": 798, "text": "(Jili) mu ikalakala yakuchimbichikwa." },
    { "resource_id": 798, "text": "Yapenani, yakuswejesyedwa." },
    { "resource_id": 798, "text": "Ili m’makono mwa akulemba (Achimalaika)." },
    { "resource_id": 798, "text": "Ŵakuchimbichika, ŵaakunda (malamusi)." },
    {
      "resource_id": 798,
      "text": "Pamasile palaniwe mundu (kafili), kanila kwakwe kwalakweku ni kwantiuli!"
    },
    { "resource_id": 798, "text": "Ana an'gumbile kuumila mu chinduchi?" },
    {
      "resource_id": 798,
      "text": "Kuumila m’mesi (gaŵalume ni ŵakongwe) an'gumbile, ni kumpanjila mpango (wakagumbidwe kakwe)."
    },
    {
      "resource_id": 798,
      "text": "Kaneka naakun'jepepachisyaga litala (lyakopochela ku duniya)."
    },
    {
      "resource_id": 798,
      "text": "Kaneko nakasampaga chiwa ni kunsiika m’malembe."
    },
    { "resource_id": 798, "text": "Soni patachisakapo tachinsyusya." },
    {
      "resource_id": 798,
      "text": "Ngwamba! Chisimu (kafili) nganakwanilisyepe yannamwile (Allah)."
    },
    { "resource_id": 798, "text": "Basi achetelele mundu ya yakulya yakwe." },
    {
      "resource_id": 798,
      "text": "Chisimu Uwwe tukasakusula mesi (gawula) mwamachili."
    },
    {
      "resource_id": 798,
      "text": "Kaneka ni tukasaligangaga litaka kuliganga kusyene."
    },
    { "resource_id": 798, "text": "Ni tukasakoposya mwalakwemo songolo." },
    {
      "resource_id": 798,
      "text": "Kwisa soni isogosi ya zabibu ni maupulu (gakulya ilango)."
    },
    {
      "resource_id": 798,
      "text": "Ni kwisa soni isogosi ya zaituni (yakwenga mauta) ni itela yatende."
    },
    { "resource_id": 798, "text": "Ni migunda jaitela yejinji." },
    {
      "resource_id": 798,
      "text": "Kwisa soni isogosi (yamitundu mitundu) ni mopo."
    },
    {
      "resource_id": 798,
      "text": "(Kuŵa) yakunsengwasya ŵanganyammwe ni ilango yenu."
    },
    {
      "resource_id": 798,
      "text": "Basi ndema jaliichiika lipenga lyakusiŵa m’mawiwi (ligongo lya machili gakwe)."
    },
    {
      "resource_id": 798,
      "text": "Lyele lisikulyo mundu tachatila achimwenegwe."
    },
    { "resource_id": 798, "text": "Kwisa soni mamagwe ni babagwe." },
    { "resource_id": 798, "text": "Ni kwisa soni ŵankwakwe ni ŵanache ŵakwe." },
    {
      "resource_id": 798,
      "text": "Mundu jwalijose mwa ŵanganyao lyele lisikulyo tachikola yakunkwanila (kuntendekasya kuti akakosoŵalila ya jwine.)"
    },
    {
      "resource_id": 798,
      "text": "Ngope sine lyele lisikulyo sichiŵa syakumelemenda."
    },
    {
      "resource_id": 798,
      "text": "Syakusechelela ni kusengwa (pakupikana yakuja ku Mbepo)."
    },
    {
      "resource_id": 798,
      "text": "Sano ngope sine lyele lisikulyo sichipakala luwundu."
    },
    { "resource_id": 798, "text": "Sichiŵa siunichilidwe ni chipi." },
    {
      "resource_id": 798,
      "text": "Ŵanganyao ni ŵali achimakafili ŵakusakasya nnope."
    }
  ],
