{
  "translations": [
    { "resource_id": 222, "text": "Қабақ шытып,бет бұрды;" },
    { "resource_id": 222, "text": "Оған соқырдың келуімен." },
    { "resource_id": 222, "text": "Қайдан білесің,мүмкін ол тазарар?" },
    { "resource_id": 222, "text": "Не үгіт алар,оған үгіт пайда берер." },
    {
      "resource_id": 222,
      "text": "Ал енді байсынған;(Құрайш бастықтары) келсе,"
    },
    { "resource_id": 222, "text": "Оған көңіл бөлесің." },
    { "resource_id": 222, "text": "Ол тазармаса саған не?" },
    { "resource_id": 222, "text": "Ал енді саған жұгіріп келген біреу;" },
    { "resource_id": 222, "text": "Ол,(Алладан) қорқады." },
    { "resource_id": 222, "text": "Сонда да оған селқос қарайсың." },
    { "resource_id": 222, "text": "Жоқ,олай емес.Әлбетте Құран бір насихат." },
    { "resource_id": 222, "text": "Сондықтан кім қаласа,үгіт алады." },
    { "resource_id": 222, "text": "Құрметті нұсқаларда жазулы," },
    { "resource_id": 222, "text": "Жоғары бағаланған тап-таза;" },
    { "resource_id": 222, "text": "Елшілердің қолында," },
    {
      "resource_id": 222,
      "text": "Ардақта,игі.(Періштелер;Лаухы Махфуздан көшірген.Б.Ж.Х)"
    },
    {
      "resource_id": 222,
      "text": "Жаны шықсын) Оған,адам баласы қалай қарсы?"
    },
    { "resource_id": 222, "text": "Алла адамды не нәрседен жаратты?" },
    { "resource_id": 222, "text": "Бір тамшыдан жаратып,оны бейнеледі." },
    { "resource_id": 222, "text": "Сосын оның жолын оңайластырды." },
    {
      "resource_id": 222,
      "text": "Кейін оған өлім біріп,оны қабырға қойдырды."
    },
    { "resource_id": 222, "text": "Қалаған сәтте,оны тірілтеді." },
    { "resource_id": 222, "text": "Жоқ,ол Алланың әмірін орындамады." },
    {
      "resource_id": 222,
      "text": "Ал енді адамзат,жеген тамағына бір көз салсын."
    },
    { "resource_id": 222, "text": "Әрине мол су бердік." },
    { "resource_id": 222, "text": "Сосын қара жерді қақ айырдық." },
    { "resource_id": 222, "text": "Сонда жерде дәнді дақылдар өсірдік." },
    { "resource_id": 222, "text": "Жүзімдер,жоңышқалар," },
    { "resource_id": 222, "text": "Зәйтіндер,құрмалар," },
    { "resource_id": 222, "text": "Ну бақшалар," },
    { "resource_id": 222, "text": "Жемістер,өлеңдер;" },
    { "resource_id": 222, "text": "Сендер әрі малдарың пайдалану үшін." },
    { "resource_id": 222, "text": "Ал енді қатты дауыс келген сәтте;" },
    { "resource_id": 222, "text": "Сол күні,кісі туысынан қашады;" },
    { "resource_id": 222, "text": "Шешесінен,әкесінен" },
    { "resource_id": 222, "text": "Әйелінен және балаларынан." },
    {
      "resource_id": 222,
      "text": "Ол күні әркімнің ісі ауыр.(Әркім өз халымен әлек.)"
    },
    { "resource_id": 222, "text": "Ол күні кейбір жүздер,жарқырайды," },
    { "resource_id": 222, "text": "Күлген қуанышты." },
    {
      "resource_id": 222,
      "text": "Ол күні кейбір жүздің үстін шаң басқан болады;"
    },
    { "resource_id": 222, "text": "Оны қара түнек қаптаған болады." },
    { "resource_id": 222, "text": "Міне солар,қарсы келген бұзақылар." }
  ],
