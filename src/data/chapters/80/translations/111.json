{
  "translations": [
    { "resource_id": 111, "text": "Fuula guuree gara gale" },
    { "resource_id": 111, "text": "Sababa jaamaan isatti dhufeef." },
    {
      "resource_id": 111,
      "text": "Maaltu si beeksise? Qulqullaa’uun isaa ni kajeelama"
    },
    { "resource_id": 111, "text": "Yookiin gorfameeti gorsi isa fayyaddi." },
    { "resource_id": 111, "text": "Nama duroome immoo," },
    {
      "resource_id": 111,
      "text": "Ati [akka amananuuf jecha] gara isaa gara galta."
    },
    {
      "resource_id": 111,
      "text": "Haala qulqullaa'uu dhiisuun isaa homaa sirra hin jirreen."
    },
    { "resource_id": 111, "text": "Nama fiigaa sitti dhufe immoo," },
    { "resource_id": 111, "text": "Kan Rabbiin sodaatu ta’ee," },
    { "resource_id": 111, "text": "Ati isa irraa dagatta." },
    { "resource_id": 111, "text": "Dhorgami! Isheen gorsa." },
    { "resource_id": 111, "text": "Nama fedhetu isa (Qur'aana) yaadata." },
    { "resource_id": 111, "text": "Galmeewwan kabajamtuu keessa jira." },
    {
      "resource_id": 111,
      "text": "Ol fuudhamtuu, qulqulleeffamtuu (keessa jira)."
    },
    { "resource_id": 111, "text": "Harka barressitootaa (maleykotaa) jira." },
    { "resource_id": 111, "text": "Kabajamoo, ajajamtoota (harka jira)." },
    {
      "resource_id": 111,
      "text": "Namni abaarame! Sila maaltu isa kafarsiise?"
    },
    {
      "resource_id": 111,
      "text": "(Rabbiin) waan akkamii irraa isa uume? (hin xiinxaluu?)"
    },
    {
      "resource_id": 111,
      "text": "Bishaan saalaa irraa isa uumeeti, ergasii isa sirreesse."
    },
    {
      "resource_id": 111,
      "text": "Ergasii karaa (inni ittiin dhalatu) isaaf laaffise."
    },
    { "resource_id": 111, "text": "Ergasii isa ajjeeseti isa awwaalchise." },
    { "resource_id": 111, "text": "Ergasii yeroo fedhe isa kaasa." },
    {
      "resource_id": 111,
      "text": "Dhugumatti, (namni) waan Rabbiin isa ajaje ammallee hin raawwanne."
    },
    { "resource_id": 111, "text": "Namni gara nyaata isaa haa ilaalu." },
    { "resource_id": 111, "text": "Nuti bishaan buusiinsa baay’inaan buufne." },
    { "resource_id": 111, "text": "Ergasii dachii dhodhoosiinsa dhodhoofne." },
    {
      "resource_id": 111,
      "text": " Achumaan Ishee keessatti firii magarsine."
    },
    { "resource_id": 111, "text": "Inabaafi margas," },
    { "resource_id": 111, "text": "Zeytunaafi muka timiraas," },
    { "resource_id": 111, "text": "Ashaakiltiilee muka baay’atanis," },
    {
      "resource_id": 111,
      "text": "Fuduraafi kaloo [akka nuti uumne haa ilaalu]."
    },
    { "resource_id": 111, "text": "Isiniifi beylada keessanis qanansiisuuf." },
    { "resource_id": 111, "text": "Yeroo gurra duuchituun (Qiyaamaan)dhufte," },
    {
      "resource_id": 111,
      "text": "Guyyaa namni obboleessa isaa irraa dheessu,"
    },
    { "resource_id": 111, "text": "Haadha isaatiifi abbaa isaa irraas," },
    {
      "resource_id": 111,
      "text": "Niitii isaatiifi ijoollee isaa irraas (baqatu san dhufti)."
    },
    {
      "resource_id": 111,
      "text": "Nama Isaan irraa ta’e hundaaf guyyaa sana dhimma isa duroomsutu jira."
    },
    { "resource_id": 111, "text": "Fuulli gariin guyyaa san ni ifti." },
    { "resource_id": 111, "text": "Kolfituu, gammadduudha." },
    {
      "resource_id": 111,
      "text": "Fuulli gariin immoo guyyaa san dhukkeetu (quttootu) irra jira."
    },
    { "resource_id": 111, "text": "Gurraachinni ishee haguuga." },
    {
      "resource_id": 111,
      "text": "Isaan sun warra Rabbitti kafaranii, balleessa hojjataniidha."
    }
  ],
  "meta": {
    "translation_name": "Ghali Apapur Apaghuna",
    "author_name": "Ghali Apapur Apaghuna",
