{
  "translations": [
    { "resource_id": 74, "text": "Рӯйро турш кард ва сар баргардонид," },
    { "resource_id": 74, "text": "чун он нобино ба наздаш омад," },
    { "resource_id": 74, "text": "Ва ту чӣ донӣ, шояд, ки ӯ покиза шавад," },
    { "resource_id": 74, "text": "ё панд гирад ва панди ту фоидааш кунад." },
    { "resource_id": 74, "text": "Аммо он ки ӯ тавонгар аст," },
    { "resource_id": 74, "text": "ту рӯи худ ба ӯ мекунӣ" },
    {
      "resource_id": 74,
      "text": "ва агар ҳам пок нагардад, чизе бар зиёни ту нест."
    },
    { "resource_id": 74, "text": "Ва аммо он, ки шитобон ба назди ту меояд" },
    { "resource_id": 74, "text": "ва метарсад, (аз худо)." },
    { "resource_id": 74, "text": "ту аз ӯ ба дигаре рӯй, меорӣ." },
    { "resource_id": 74, "text": "Оре, ин Қуръон пандест," },
    { "resource_id": 74, "text": "пас ҳар кӣ хоҳад, аз он панд гирад." },
    { "resource_id": 74, "text": "Дар саҳифаҳое гиромӣ." },
    { "resource_id": 74, "text": "баландқадру покиза," },
    { "resource_id": 74, "text": "ба дасти котибоне," },
    { "resource_id": 74, "text": "бузургвору некӯкор." },
    {
      "resource_id": 74,
      "text": "Марг бар одамӣ бод, ки чӣ носипос (ношукр) аст!"
    },
    { "resource_id": 74, "text": "Ӯро аз чӣ офаридааст?" },
    { "resource_id": 74, "text": "Аз нутфае офарид ва ба андоза муайян кард," },
    { "resource_id": 74, "text": "сипас роҳашро осон сохт." },
    { "resource_id": 74, "text": "Он гоҳ бимирондаш ва дар гӯр кард." },
    { "resource_id": 74, "text": "Ва он гоҳ, ки хоҳад, зиндааш созад." },
    {
      "resource_id": 74,
      "text": "На ки ҳанӯз он чиро ба ӯ фармон дода буд, ба ҷой наёвардааст."
    },
    { "resource_id": 74, "text": "Пас одамӣ ба таъоми худ бингарад" },
    { "resource_id": 74, "text": "Мо боронро фурӯ боридем, бориданӣ" },
    { "resource_id": 74, "text": "ва заминро шикофтем, шикофтанӣ" },
    { "resource_id": 74, "text": "ва дар он донаҳо рӯёнидем" },
    { "resource_id": 74, "text": "ва току сабзавот" },
    { "resource_id": 74, "text": "ва зайтуну нахл (дарахти хурмо)" },
    { "resource_id": 74, "text": "ва боғҳои пурдарахт" },
    { "resource_id": 74, "text": "ва меваву алаф," },
    { "resource_id": 74, "text": "то шумову чорпоёнатон баҳра баред," },
    { "resource_id": 74, "text": "Чун бонги қиёмат барояд," },
    { "resource_id": 74, "text": "рӯзе, ки одамӣ аз бародараш мегурезад" },
    { "resource_id": 74, "text": "ва аз модарашу падараш" },
    { "resource_id": 74, "text": "ва аз занашу фарзандонаш." },
    {
      "resource_id": 74,
      "text": "Ҳар касро дар рӯз корест, ки ба худ машғулаш дорад."
    },
    { "resource_id": 74, "text": "Чеҳраҳое дар он рӯз дурахшонанд," },
    { "resource_id": 74, "text": "хандонанду шодонанд." },
    { "resource_id": 74, "text": "Ва чеҳраҳое дар он рӯз ғуборолуданд," },
    { "resource_id": 74, "text": "дар сиёҳӣ фурӯ рафтаанд." },
    { "resource_id": 74, "text": "Инҳо кофирону фоҷиронанд (бадкорон)." }
  ],
