    {
      "resource_id": 113,
      "text": "Әбу Ләһабтың екі қолы құрысын әрі өзі де құрыды."
    },
    {
      "resource_id": 113,
      "text": "Оған дүниесі мен тапқандары пайда бермеді."
    },
    { "resource_id": 113, "text": "Ол жақында жалындаған отқа кіреді" },
    { "resource_id": 113, "text": "және отын көтеріп оның әйелі де." },
    {
      "resource_id": 113,
      "text": "Оның мойнында - есілген арқан  / болады / ."
    }
