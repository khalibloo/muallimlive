{
  "translations": [
    {
      "resource_id": 774,
      "text": "Vuga (yewe Muhamadi) uti “Niragije (Allah), Nyagasani w’umuseke”,"
    },
    { "resource_id": 774, "text": "“Ngo andinde inabi y’ibyo yaremye,”" },
    {
      "resource_id": 774,
      "text": "“Anandinde ikibi cyo mu ijoro ryijimye igihe riguye,”"
    },
    {
      "resource_id": 774,
      "text": "“Anandinde ikibi cy’abarozikazi bahuha ku mapfundo,”"
    },
    {
      "resource_id": 774,
      "text": "“Ndetse anandinde inabi y’umunyeshyari igihe arigize.”"
    }
  ],
