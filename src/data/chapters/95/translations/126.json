{
  "translations": [
    { "resource_id": 126, "text": "Tako mi smokve i masline," },
    { "resource_id": 126, "text": "i Sinajske gore," },
    { "resource_id": 126, "text": "i grada ovog, sigurnog –" },
    { "resource_id": 126, "text": "Mi čovjeka stvaramo u skladu najljepšem," },
    { "resource_id": 126, "text": "zatim ćemo ga u najnakazniji lik vratiti," },
    {
      "resource_id": 126,
      "text": "samo ne one koji budu vjerovali i dobra djela činili, njih čeka nagrada neprekidna."
    },
    {
      "resource_id": 126,
      "text": "Pa šta te onda navodi da poričeš onaj svijet –"
    },
    { "resource_id": 126, "text": "zar Allah nije sudija najpravedniji?!" }
  ],
