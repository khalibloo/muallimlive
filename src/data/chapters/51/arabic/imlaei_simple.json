{
  "verses": [
    { "id": 4676, "verse_key": "51:1", "text_imlaei_simple": "والذاريات ذروا" },
    { "id": 4677, "verse_key": "51:2", "text_imlaei_simple": "فالحاملات وقرا" },
    { "id": 4678, "verse_key": "51:3", "text_imlaei_simple": "فالجاريات يسرا" },
    { "id": 4679, "verse_key": "51:4", "text_imlaei_simple": "فالمقسمات امرا" },
    {
      "id": 4680,
      "verse_key": "51:5",
      "text_imlaei_simple": "انما توعدون لصادق"
    },
    {
      "id": 4681,
      "verse_key": "51:6",
      "text_imlaei_simple": "وان الدين لواقع"
    },
    {
      "id": 4682,
      "verse_key": "51:7",
      "text_imlaei_simple": "والسماء ذات الحبك"
    },
    {
      "id": 4683,
      "verse_key": "51:8",
      "text_imlaei_simple": "انكم لفي قول مختلف"
    },
    {
      "id": 4684,
      "verse_key": "51:9",
      "text_imlaei_simple": "يؤفك عنه من افك"
    },
    { "id": 4685, "verse_key": "51:10", "text_imlaei_simple": "قتل الخراصون" },
    {
      "id": 4686,
      "verse_key": "51:11",
      "text_imlaei_simple": "الذين هم في غمرة ساهون"
    },
    {
      "id": 4687,
      "verse_key": "51:12",
      "text_imlaei_simple": "يسالون ايان يوم الدين"
    },
    {
      "id": 4688,
      "verse_key": "51:13",
      "text_imlaei_simple": "يوم هم على النار يفتنون"
    },
    {
      "id": 4689,
      "verse_key": "51:14",
      "text_imlaei_simple": "ذوقوا فتنتكم هذا الذي كنتم به تستعجلون"
    },
    {
      "id": 4690,
      "verse_key": "51:15",
      "text_imlaei_simple": "ان المتقين في جنات وعيون"
    },
    {
      "id": 4691,
      "verse_key": "51:16",
      "text_imlaei_simple": "اخذين ما اتاهم ربهم انهم كانوا قبل ذلك محسنين"
    },
    {
      "id": 4692,
      "verse_key": "51:17",
      "text_imlaei_simple": "كانوا قليلا من الليل ما يهجعون"
    },
    {
      "id": 4693,
      "verse_key": "51:18",
      "text_imlaei_simple": "وبالاسحار هم يستغفرون"
    },
    {
      "id": 4694,
      "verse_key": "51:19",
      "text_imlaei_simple": "وفي اموالهم حق للسائل والمحروم"
    },
    {
      "id": 4695,
      "verse_key": "51:20",
      "text_imlaei_simple": "وفي الارض ايات للموقنين"
    },
    {
      "id": 4696,
      "verse_key": "51:21",
      "text_imlaei_simple": "وفي انفسكم افلا تبصرون"
    },
    {
      "id": 4697,
      "verse_key": "51:22",
      "text_imlaei_simple": "وفي السماء رزقكم وما توعدون"
    },
    {
      "id": 4698,
      "verse_key": "51:23",
      "text_imlaei_simple": "فورب السماء والارض انه لحق مثل ما انكم تنطقون"
    },
    {
      "id": 4699,
      "verse_key": "51:24",
      "text_imlaei_simple": "هل اتاك حديث ضيف ابراهيم المكرمين"
    },
    {
      "id": 4700,
      "verse_key": "51:25",
      "text_imlaei_simple": "اذ دخلوا عليه فقالوا سلاما قال سلام قوم منكرون"
    },
    {
      "id": 4701,
      "verse_key": "51:26",
      "text_imlaei_simple": "فراغ الى اهله فجاء بعجل سمين"
    },
    {
      "id": 4702,
      "verse_key": "51:27",
      "text_imlaei_simple": "فقربه اليهم قال الا تاكلون"
    },
    {
      "id": 4703,
      "verse_key": "51:28",
      "text_imlaei_simple": "فاوجس منهم خيفة قالوا لا تخف وبشروه بغلام عليم"
    },
    {
      "id": 4704,
      "verse_key": "51:29",
      "text_imlaei_simple": "فاقبلت امراته في صرة فصكت وجهها وقالت عجوز عقيم"
    },
    {
      "id": 4705,
      "verse_key": "51:30",
      "text_imlaei_simple": "قالوا كذلك قال ربك انه هو الحكيم العليم"
    },
    {
      "id": 4706,
      "verse_key": "51:31",
      "text_imlaei_simple": "قال فما خطبكم ايها المرسلون"
    },
    {
      "id": 4707,
      "verse_key": "51:32",
      "text_imlaei_simple": "قالوا انا ارسلنا الى قوم مجرمين"
    },
    {
      "id": 4708,
      "verse_key": "51:33",
      "text_imlaei_simple": "لنرسل عليهم حجارة من طين"
    },
    {
      "id": 4709,
      "verse_key": "51:34",
      "text_imlaei_simple": "مسومة عند ربك للمسرفين"
    },
    {
      "id": 4710,
      "verse_key": "51:35",
      "text_imlaei_simple": "فاخرجنا من كان فيها من المؤمنين"
    },
    {
      "id": 4711,
      "verse_key": "51:36",
      "text_imlaei_simple": "فما وجدنا فيها غير بيت من المسلمين"
    },
    {
      "id": 4712,
      "verse_key": "51:37",
      "text_imlaei_simple": "وتركنا فيها اية للذين يخافون العذاب الاليم"
    },
    {
      "id": 4713,
      "verse_key": "51:38",
      "text_imlaei_simple": "وفي موسى اذ ارسلناه الى فرعون بسلطان مبين"
    },
    {
      "id": 4714,
      "verse_key": "51:39",
      "text_imlaei_simple": "فتولى بركنه وقال ساحر او مجنون"
    },
    {
      "id": 4715,
      "verse_key": "51:40",
      "text_imlaei_simple": "فاخذناه وجنوده فنبذناهم في اليم وهو مليم"
    },
    {
      "id": 4716,
      "verse_key": "51:41",
      "text_imlaei_simple": "وفي عاد اذ ارسلنا عليهم الريح العقيم"
    },
    {
      "id": 4717,
      "verse_key": "51:42",
      "text_imlaei_simple": "ما تذر من شيء اتت عليه الا جعلته كالرميم"
    },
    {
      "id": 4718,
      "verse_key": "51:43",
      "text_imlaei_simple": "وفي ثمود اذ قيل لهم تمتعوا حتى حين"
    },
    {
      "id": 4719,
      "verse_key": "51:44",
      "text_imlaei_simple": "فعتوا عن امر ربهم فاخذتهم الصاعقة وهم ينظرون"
    },
    {
      "id": 4720,
      "verse_key": "51:45",
      "text_imlaei_simple": "فما استطاعوا من قيام وما كانوا منتصرين"
    },
    {
      "id": 4721,
      "verse_key": "51:46",
      "text_imlaei_simple": "وقوم نوح من قبل انهم كانوا قوما فاسقين"
    },
    {
      "id": 4722,
      "verse_key": "51:47",
      "text_imlaei_simple": "والسماء بنيناها بايد وانا لموسعون"
    },
    {
      "id": 4723,
      "verse_key": "51:48",
      "text_imlaei_simple": "والارض فرشناها فنعم الماهدون"
    },
    {
      "id": 4724,
      "verse_key": "51:49",
      "text_imlaei_simple": "ومن كل شيء خلقنا زوجين لعلكم تذكرون"
    },
    {
      "id": 4725,
      "verse_key": "51:50",
      "text_imlaei_simple": "ففروا الى الله اني لكم منه نذير مبين"
    },
    {
      "id": 4726,
      "verse_key": "51:51",
      "text_imlaei_simple": "ولا تجعلوا مع الله الها اخر اني لكم منه نذير مبين"
    },
    {
      "id": 4727,
      "verse_key": "51:52",
      "text_imlaei_simple": "كذلك ما اتى الذين من قبلهم من رسول الا قالوا ساحر او مجنون"
    },
    {
      "id": 4728,
      "verse_key": "51:53",
      "text_imlaei_simple": "اتواصوا به بل هم قوم طاغون"
    },
    {
      "id": 4729,
      "verse_key": "51:54",
      "text_imlaei_simple": "فتول عنهم فما انت بملوم"
    },
    {
      "id": 4730,
      "verse_key": "51:55",
      "text_imlaei_simple": "وذكر فان الذكرى تنفع المؤمنين"
    },
    {
      "id": 4731,
      "verse_key": "51:56",
      "text_imlaei_simple": "وما خلقت الجن والانس الا ليعبدون"
    },
    {
      "id": 4732,
      "verse_key": "51:57",
      "text_imlaei_simple": "ما اريد منهم من رزق وما اريد ان يطعمون"
    },
    {
      "id": 4733,
      "verse_key": "51:58",
      "text_imlaei_simple": "ان الله هو الرزاق ذو القوة المتين"
    },
    {
      "id": 4734,
      "verse_key": "51:59",
      "text_imlaei_simple": "فان للذين ظلموا ذنوبا مثل ذنوب اصحابهم فلا يستعجلون"
    },
    {
      "id": 4735,
      "verse_key": "51:60",
      "text_imlaei_simple": "فويل للذين كفروا من يومهم الذي يوعدون"
    }
  ],
  "meta": { "filters": { "chapter_number": "51" } }
}
