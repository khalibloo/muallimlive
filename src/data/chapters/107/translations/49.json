{
  "translations": [
    { "resource_id": 49, "text": "Je! Umemwona anaye kadhibisha Malipo?" },
    { "resource_id": 49, "text": "Huyo ndiye anaye msukuma yatima," },
    { "resource_id": 49, "text": "Wala hahimizi kumlisha masikini." },
    { "resource_id": 49, "text": "Basi, ole wao wanao sali," },
    { "resource_id": 49, "text": "Ambao wanapuuza Sala zao;" },
    { "resource_id": 49, "text": "Ambao wanajionyesha," },
    { "resource_id": 49, "text": "Nao huku wanazuia msaada." }
  ],
