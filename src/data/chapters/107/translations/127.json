{
  "translations": [
    {
      "resource_id": 127,
      "text": "Охират(жазоси)ни ёлғонга чиқарувчини кўрдингми?"
    },
    { "resource_id": 127, "text": "У, етимни қўполлик билан ҳайдайдир." },
    {
      "resource_id": 127,
      "text": "Ва мискинларга таом беришга қизиқтирмайдир."
    },
    { "resource_id": 127, "text": "Бир «намозхонлар»га вайл бўлсинки." },
    { "resource_id": 127, "text": "Улар намозларини унутувчилардир." },
    { "resource_id": 127, "text": "Ундоқ кишилар риё қиладиганлардир." },
    { "resource_id": 127, "text": "Ва мааъуунни ман қиладиганлардир." }
  ],
