    {
      "resource_id": 55,
      "text": "Oxirat(jazosi)ni yolg`onga chiqaruvchini ko`rdingmi?"
    },
    { "resource_id": 55, "text": "U, yetimni qo`pollik bilan haydaydir." },
    {
      "resource_id": 55,
      "text": "Va miskinlarga taom berishga qiziqtirmaydir."
    },
    { "resource_id": 55, "text": "Bir «namozxonlar»ga vayl bo`lsinki." },
    { "resource_id": 55, "text": "Ular namozlarini unutuvchilardir." },
    { "resource_id": 55, "text": "Undoq kishilar riyo qiladiganlardir." },
    { "resource_id": 55, "text": "Va maa'uunni man qiladiganlardir." }
  ],
