{
  "verses": [
    {
      "id": 6198,
      "verse_key": "107:1",
      "text_imlaei": "أَرَأَيْتَ الَّذِي يُكَذِّبُ بِالدِّينِ"
    },
    {
      "id": 6199,
      "verse_key": "107:2",
      "text_imlaei": "فَذَٰلِكَ الَّذِي يَدُعُّ الْيَتِيمَ"
    },
    {
      "id": 6200,
      "verse_key": "107:3",
      "text_imlaei": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ الْمِسْكِينِ"
    },
    {
      "id": 6201,
      "verse_key": "107:4",
      "text_imlaei": "فَوَيْلٌ لِّلْمُصَلِّينَ"
    },
    {
      "id": 6202,
      "verse_key": "107:5",
      "text_imlaei": "الَّذِينَ هُمْ عَن صَلَاتِهِمْ سَاهُونَ"
    },
    {
      "id": 6203,
      "verse_key": "107:6",
      "text_imlaei": "الَّذِينَ هُمْ يُرَاءُونَ"
    },
    {
      "id": 6204,
      "verse_key": "107:7",
      "text_imlaei": "وَيَمْنَعُونَ الْمَاعُونَ"
    }
  ],
