{
  "verses": [
    {
      "id": 6198,
      "verse_key": "107:1",
      "text_indopak": "اَرَءَيۡتَ الَّذِىۡ يُكَذِّبُ بِالدِّيۡنِؕ‏"
    },
    {
      "id": 6199,
      "verse_key": "107:2",
      "text_indopak": "فَذٰلِكَ الَّذِىۡ يَدُعُّ الۡيَتِيۡمَۙ‏"
    },
    {
      "id": 6200,
      "verse_key": "107:3",
      "text_indopak": "وَ لَا يَحُضُّ عَلٰى طَعَامِ الۡمِسۡكِيۡنِؕ‏"
    },
    {
      "id": 6201,
      "verse_key": "107:4",
      "text_indopak": "فَوَيۡلٌ لِّلۡمُصَلِّيۡنَۙ‏"
    },
    {
      "id": 6202,
      "verse_key": "107:5",
      "text_indopak": "الَّذِيۡنَ هُمۡ عَنۡ صَلَاتِهِمۡ سَاهُوۡنَۙ‏"
    },
    {
      "id": 6203,
      "verse_key": "107:6",
      "text_indopak": "الَّذِيۡنَ هُمۡ يُرَآءُوۡنَۙ‏"
    },
    {
      "id": 6204,
      "verse_key": "107:7",
      "text_indopak": "وَيَمۡنَعُوۡنَ الۡمَاعُوۡنَ‏"
    }
  ],
  "meta": { "filters": { "chapter_number": "107" } }
}
