{
  "translations": [
    {
      "resource_id": 46,
      "text": "Halaag Wuxuu u Sugnaaday xanle wax ceebeya oo Dhan,"
    },
    { "resource_id": 46, "text": "Oo Xoolaha kulmiya (xaqana ku bixinin)." },
    { "resource_id": 46, "text": "Ma wuxuu malayn in xoolihiisu waarin." },
    {
      "resource_id": 46,
      "text": "Saas ma aha ee waxaa lagu tuuri (Naarta) Xudhama."
    },
    {
      "resource_id": 46,
      "text": "Ma taqaannaa Xudhama( Tii Wax burburinaysay)."
    },
    { "resource_id": 46, "text": "Waa Naarta Eebe ee la huriyay." },
    { "resource_id": 46, "text": "Gubaysana Quluubta (Kulayl dartiis)." },
    { "resource_id": 46, "text": "Waana lagu dabooli Naartaas Gaalada." },
    { "resource_id": 46, "text": "Tiraar la fidiyay yayna ahaan dhexdeeda." }
  ],
