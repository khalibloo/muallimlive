{
  "translations": [
    { "resource_id": 49, "text": "Ole wake kila safihi, msengenyaji!" },
    { "resource_id": 49, "text": "Aliye kusanya mali na kuyahisabu." },
    {
      "resource_id": 49,
      "text": "Anadhani ya kuwa mali yake yatambakisha milele!"
    },
    { "resource_id": 49, "text": "Hasha! Atavurumishwa katika H'ut'ama." },
    { "resource_id": 49, "text": "Na nani atakujuvya ni nini H'ut'ama?" },
    { "resource_id": 49, "text": "Moto wa Mwenyezi Mungu ulio washwa." },
    { "resource_id": 49, "text": "Ambao unapanda nyoyoni." },
    { "resource_id": 49, "text": "Hakika huo utafungiwa nao" },
    { "resource_id": 49, "text": "Kwenye nguzo zilio nyooshwa." }
  ],
