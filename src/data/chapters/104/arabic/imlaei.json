{
  "verses": [
    {
      "id": 6180,
      "verse_key": "104:1",
      "text_imlaei": "وَيْلٌ لِّكُلِّ هُمَزَةٍ لُّمَزَةٍ"
    },
    {
      "id": 6181,
      "verse_key": "104:2",
      "text_imlaei": "الَّذِي جَمَعَ مَالًا وَعَدَّدَهُ"
    },
    {
      "id": 6182,
      "verse_key": "104:3",
      "text_imlaei": "يَحْسَبُ أَنَّ مَالَهُ أَخْلَدَهُ"
    },
    {
      "id": 6183,
      "verse_key": "104:4",
      "text_imlaei": "كَلَّا ۖ لَيُنبَذَنَّ فِي الْحُطَمَةِ"
    },
    {
      "id": 6184,
      "verse_key": "104:5",
      "text_imlaei": "وَمَا أَدْرَاكَ مَا الْحُطَمَةُ"
    },
    {
      "id": 6185,
      "verse_key": "104:6",
      "text_imlaei": "نَارُ اللَّهِ الْمُوقَدَةُ"
    },
    {
      "id": 6186,
      "verse_key": "104:7",
      "text_imlaei": "الَّتِي تَطَّلِعُ عَلَى الْأَفْئِدَةِ"
    },
    {
      "id": 6187,
      "verse_key": "104:8",
      "text_imlaei": "إِنَّهَا عَلَيْهِم مُّؤْصَدَةٌ"
    },
    {
      "id": 6188,
      "verse_key": "104:9",
      "text_imlaei": "فِي عَمَدٍ مُّمَدَّدَةٍ"
    }
  ],
