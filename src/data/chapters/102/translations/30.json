    {
      "resource_id": 30,
      "text": "Rikkauksien kartuttaminen vieroittaa mielenne (uskosta)"
    },
    {
      "resource_id": 30,
      "text": "aina siihen saakka, kunnes eteenne avautuu hauta."
    },
    { "resource_id": 30, "text": "Totisesti saatte sen nähdä!" },
    { "resource_id": 30, "text": "Ja vieläkin kerran te saatte sen nähdä." },
    { "resource_id": 30, "text": "Jospa olisitte sen varmuudella tietäneet," },
    { "resource_id": 30, "text": "olisitte ilmielävästi nähneet helvetin." },
    {
      "resource_id": 30,
      "text": "Vielä kerran, totisesti saatte nähdä sen omin silmin."
    },
    {
      "resource_id": 30,
      "text": "Silloin, tuona päivänä, teiltä toden totta vaaditaan (aikanaan) annettuja almuja."
    }
  ],
