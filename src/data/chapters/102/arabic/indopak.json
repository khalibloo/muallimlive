      "text_indopak": "حَتّٰى زُرۡتُمُ الۡمَقَابِرَؕ‏"
    },
    {
      "id": 6171,
      "verse_key": "102:3",
      "text_indopak": "كَلَّا سَوۡفَ تَعۡلَمُوۡنَۙ‏"
    },
    {
      "id": 6172,
      "verse_key": "102:4",
      "text_indopak": "ثُمَّ كَلَّا سَوۡفَ تَعۡلَمُوۡنَؕ‏"
    },
    {
      "id": 6173,
      "verse_key": "102:5",
      "text_indopak": "كَلَّا لَوۡ تَعۡلَمُوۡنَ عِلۡمَ الۡيَقِيۡنِؕ‏"
    },
    {
      "id": 6174,
      "verse_key": "102:6",
      "text_indopak": "لَتَرَوُنَّ الۡجَحِيۡمَۙ‏"
    },
    {
      "id": 6175,
      "verse_key": "102:7",
      "text_indopak": "ثُمَّ لَتَرَوُنَّهَا عَيۡنَ الۡيَقِيۡنِۙ‏"
    },
    {
      "id": 6176,
      "verse_key": "102:8",
      "text_indopak": "ثُمَّ لَـتُسۡـَٔـلُنَّ يَوۡمَٮِٕذٍ عَنِ النَّعِيۡمِ‏"
    }
