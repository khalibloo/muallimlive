{
  "verses": [
    {
      "id": 6059,
      "verse_key": "92:1",
      "text_imlaei": "وَاللَّيْلِ إِذَا يَغْشَىٰ"
    },
    {
      "id": 6060,
      "verse_key": "92:2",
      "text_imlaei": "وَالنَّهَارِ إِذَا تَجَلَّىٰ"
    },
    {
      "id": 6061,
      "verse_key": "92:3",
      "text_imlaei": "وَمَا خَلَقَ الذَّكَرَ وَالْأُنثَىٰ"
    },
    {
      "id": 6062,
      "verse_key": "92:4",
      "text_imlaei": "إِنَّ سَعْيَكُمْ لَشَتَّىٰ"
    },
    {
      "id": 6063,
      "verse_key": "92:5",
      "text_imlaei": "فَأَمَّا مَنْ أَعْطَىٰ وَاتَّقَىٰ"
    },
    {
      "id": 6064,
      "verse_key": "92:6",
      "text_imlaei": "وَصَدَّقَ بِالْحُسْنَىٰ"
    },
    {
      "id": 6065,
      "verse_key": "92:7",
      "text_imlaei": "فَسَنُيَسِّرُهُ لِلْيُسْرَىٰ"
    },
    {
      "id": 6066,
      "verse_key": "92:8",
      "text_imlaei": "وَأَمَّا مَن بَخِلَ وَاسْتَغْنَىٰ"
    },
    {
      "id": 6067,
      "verse_key": "92:9",
      "text_imlaei": "وَكَذَّبَ بِالْحُسْنَىٰ"
    },
    {
      "id": 6068,
      "verse_key": "92:10",
      "text_imlaei": "فَسَنُيَسِّرُهُ لِلْعُسْرَىٰ"
    },
    {
      "id": 6069,
      "verse_key": "92:11",
      "text_imlaei": "وَمَا يُغْنِي عَنْهُ مَالُهُ إِذَا تَرَدَّىٰ"
    },
    {
      "id": 6070,
      "verse_key": "92:12",
      "text_imlaei": "إِنَّ عَلَيْنَا لَلْهُدَىٰ"
    },
    {
      "id": 6071,
      "verse_key": "92:13",
      "text_imlaei": "وَإِنَّ لَنَا لَلْآخِرَةَ وَالْأُولَىٰ"
    },
    {
      "id": 6072,
      "verse_key": "92:14",
      "text_imlaei": "فَأَنذَرْتُكُمْ نَارًا تَلَظَّىٰ"
    },
    {
      "id": 6073,
      "verse_key": "92:15",
      "text_imlaei": "لَا يَصْلَاهَا إِلَّا الْأَشْقَى"
    },
    {
      "id": 6074,
      "verse_key": "92:16",
      "text_imlaei": "الَّذِي كَذَّبَ وَتَوَلَّىٰ"
    },
    {
      "id": 6075,
      "verse_key": "92:17",
      "text_imlaei": "وَسَيُجَنَّبُهَا الْأَتْقَى"
    },
    {
      "id": 6076,
      "verse_key": "92:18",
      "text_imlaei": "الَّذِي يُؤْتِي مَالَهُ يَتَزَكَّىٰ"
    },
    {
      "id": 6077,
      "verse_key": "92:19",
      "text_imlaei": "وَمَا لِأَحَدٍ عِندَهُ مِن نِّعْمَةٍ تُجْزَىٰ"
    },
    {
      "id": 6078,
      "verse_key": "92:20",
      "text_imlaei": "إِلَّا ابْتِغَاءَ وَجْهِ رَبِّهِ الْأَعْلَىٰ"
    },
    { "id": 6079, "verse_key": "92:21", "text_imlaei": "وَلَسَوْفَ يَرْضَىٰ" }
  ],
  "meta": { "filters": { "chapter_number": "92" } }
}
