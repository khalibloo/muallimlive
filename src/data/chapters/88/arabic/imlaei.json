{
  "verses": [
    {
      "id": 5968,
      "verse_key": "88:1",
      "text_imlaei": "هَلْ أَتَاكَ حَدِيثُ الْغَاشِيَةِ"
    },
    {
      "id": 5969,
      "verse_key": "88:2",
      "text_imlaei": "وُجُوهٌ يَوْمَئِذٍ خَاشِعَةٌ"
    },
    { "id": 5970, "verse_key": "88:3", "text_imlaei": "عَامِلَةٌ نَّاصِبَةٌ" },
    {
      "id": 5971,
      "verse_key": "88:4",
      "text_imlaei": "تَصْلَىٰ نَارًا حَامِيَةً"
    },
    {
      "id": 5972,
      "verse_key": "88:5",
      "text_imlaei": "تُسْقَىٰ مِنْ عَيْنٍ آنِيَةٍ"
    },
    {
      "id": 5973,
      "verse_key": "88:6",
      "text_imlaei": "لَّيْسَ لَهُمْ طَعَامٌ إِلَّا مِن ضَرِيعٍ"
    },
    {
      "id": 5974,
      "verse_key": "88:7",
      "text_imlaei": "لَّا يُسْمِنُ وَلَا يُغْنِي مِن جُوعٍ"
    },
    {
      "id": 5975,
      "verse_key": "88:8",
      "text_imlaei": "وُجُوهٌ يَوْمَئِذٍ نَّاعِمَةٌ"
    },
    {
      "id": 5976,
      "verse_key": "88:9",
      "text_imlaei": "لِّسَعْيِهَا رَاضِيَةٌ"
    },
    {
      "id": 5977,
      "verse_key": "88:10",
      "text_imlaei": "فِي جَنَّةٍ عَالِيَةٍ"
    },
    {
      "id": 5978,
      "verse_key": "88:11",
      "text_imlaei": "لَّا تَسْمَعُ فِيهَا لَاغِيَةً"
    },
    {
      "id": 5979,
      "verse_key": "88:12",
      "text_imlaei": "فِيهَا عَيْنٌ جَارِيَةٌ"
    },
    {
      "id": 5980,
      "verse_key": "88:13",
      "text_imlaei": "فِيهَا سُرُرٌ مَّرْفُوعَةٌ"
    },
    {
      "id": 5981,
      "verse_key": "88:14",
      "text_imlaei": "وَأَكْوَابٌ مَّوْضُوعَةٌ"
    },
    {
      "id": 5982,
      "verse_key": "88:15",
      "text_imlaei": "وَنَمَارِقُ مَصْفُوفَةٌ"
    },
    {
      "id": 5983,
      "verse_key": "88:16",
      "text_imlaei": "وَزَرَابِيُّ مَبْثُوثَةٌ"
    },
    {
      "id": 5984,
      "verse_key": "88:17",
      "text_imlaei": "أَفَلَا يَنظُرُونَ إِلَى الْإِبِلِ كَيْفَ خُلِقَتْ"
    },
    {
      "id": 5985,
      "verse_key": "88:18",
      "text_imlaei": "وَإِلَى السَّمَاءِ كَيْفَ رُفِعَتْ"
    },
    {
      "id": 5986,
      "verse_key": "88:19",
      "text_imlaei": "وَإِلَى الْجِبَالِ كَيْفَ نُصِبَتْ"
    },
    {
      "id": 5987,
      "verse_key": "88:20",
      "text_imlaei": "وَإِلَى الْأَرْضِ كَيْفَ سُطِحَتْ"
    },
    {
      "id": 5988,
      "verse_key": "88:21",
      "text_imlaei": "فَذَكِّرْ إِنَّمَا أَنتَ مُذَكِّرٌ"
    },
    {
      "id": 5989,
      "verse_key": "88:22",
      "text_imlaei": "لَّسْتَ عَلَيْهِم بِمُصَيْطِرٍ"
    },
    {
      "id": 5990,
      "verse_key": "88:23",
      "text_imlaei": "إِلَّا مَن تَوَلَّىٰ وَكَفَرَ"
    },
    {
      "id": 5991,
      "verse_key": "88:24",
      "text_imlaei": "فَيُعَذِّبُهُ اللَّهُ الْعَذَابَ الْأَكْبَرَ"
    },
    {
      "id": 5992,
      "verse_key": "88:25",
      "text_imlaei": "إِنَّ إِلَيْنَا إِيَابَهُمْ"
    },
    {
      "id": 5993,
      "verse_key": "88:26",
      "text_imlaei": "ثُمَّ إِنَّ عَلَيْنَا حِسَابَهُم"
    }
  ],
  "meta": { "filters": { "chapter_number": "88" } }
}
