{
  "translations": [
    {
      "resource_id": 222,
      "text": "(Мұхаммед Ғ.С әлемді) қаптайтын қияметтің хабары саған келді ме?"
    },
    { "resource_id": 222, "text": "Ол күні, кейбір жүздерді қорқу басады." },
    { "resource_id": 222, "text": "Қатты қиналған шаршаған болады." },
    { "resource_id": 222, "text": "Жанып тұрған отқа тасталады." },
    { "resource_id": 222, "text": "Қайнап тұрған бастаудан ішкізіледі." },
    {
      "resource_id": 222,
      "text": "Олардың ашы,сасық тікенектен басқа ешбір тамағы болмайды."
    },
    { "resource_id": 222, "text": "Ол семіртпейді де аштықты баспайды." },
    { "resource_id": 222, "text": "Ол күні,кейбір жүздер шаттықта," },
    { "resource_id": 222, "text": "Ғамалдарына разы," },
    { "resource_id": 222, "text": "Көтеріңкі баұшада;" },
    { "resource_id": 222, "text": "Ол арада бір бос сөз естімейді." },
    { "resource_id": 222, "text": "Онда аққан бір бұлақ бар." },
    { "resource_id": 222, "text": "Онда биік дивандар бар." },
    { "resource_id": 222, "text": "Қойылған кіселер бар." },
    { "resource_id": 222, "text": "Тізілген жатықтар бар." },
    { "resource_id": 222, "text": ") Төселген төсеніштер бар." },
    {
      "resource_id": 222,
      "text": "Олар,түйеге қарамай ма, қайтып жаратылған?"
    },
    {
      "resource_id": 222,
      "text": "Олар,аспанға қарамай ма,қайтіп көтерілген?"
    },
    {
      "resource_id": 222,
      "text": "Олар,тауларға қарамай ма? Қайтіп тігілген?"
    },
    { "resource_id": 222, "text": "Олар,жерге қамамай ма,қайтып төселген?" },
    {
      "resource_id": 222,
      "text": "(Мұхаммед Ғ.С) ал енді насихат ет; негізінен сен бір насихатшысың."
    },
    { "resource_id": 222, "text": "Сен оларды зорлаушы емессің." },
    { "resource_id": 222, "text": "Бірақ әлде кім бет бұрса,қарсы келсе;" },
    { "resource_id": 222, "text": "Сонда Алла,оны зор азаппен қинайды." },
    { "resource_id": 222, "text": "Расында олардың қайтар жері Біз жақ." },
    { "resource_id": 222, "text": "Сосын сөз жоқ. Олардың есебі Бізге тән." }
  ],
  "meta": {
