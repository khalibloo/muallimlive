{
  "translations": [
    { "resource_id": 127, "text": "Батаҳқиқ, сенга ғошия хабари келди." },
    {
      "resource_id": 127,
      "text": "Ўшал кунда бир чеҳралар қўрқинч билан тўладир."
    },
    {
      "resource_id": 127,
      "text": "Улар иш бажарувчидир ва ниҳоятда чарчагандир."
    },
    { "resource_id": 127, "text": "Ўта қизиб турган оловга кирадир." },
    { "resource_id": 127, "text": "Улар қайнаб турган булоқдан суғориладир." },
    { "resource_id": 127, "text": "Уларга тикандан бошқа таом йўқдир." },
    {
      "resource_id": 127,
      "text": "У на семиртирмас ва на очликни кетказмасдир."
    },
    {
      "resource_id": 127,
      "text": "Ўшал кунда бир чеҳралар неъмат хурсандлиги билан тўладир."
    },
    { "resource_id": 127, "text": "Улар ўз ишларидан розидир." },
    { "resource_id": 127, "text": "Улар олий даражали жаннатдадир." },
    { "resource_id": 127, "text": "Унда бекорчи гапларни эшитмаслар." },
    { "resource_id": 127, "text": "Унда оқиб турган булоқлар бордир." },
    { "resource_id": 127, "text": "Унда баланд-баланд сўрилар бордир." },
    { "resource_id": 127, "text": "Ва тайёрлаб қўйилган қадаҳлар бордир." },
    {
      "resource_id": 127,
      "text": "Ва саф-саф қилиб тизилган ёстиқлар бордир."
    },
    { "resource_id": 127, "text": "Ҳамма ёққа тўшалган гиламлар бордир." },
    {
      "resource_id": 127,
      "text": "Улар назар солмасми: Туянинг қандай яратилганига?"
    },
    { "resource_id": 127, "text": "Ва осмоннинг қандоқ кўтарилганига?" },
    { "resource_id": 127, "text": "Ва тоғларнинг қандоқ ўрнаштирилганига?" },
    { "resource_id": 127, "text": "Ва ернинг қандоқ текислаб қўйилганига?" },
    {
      "resource_id": 127,
      "text": "Бас, эслатгин, сен, албатта, эслатувчидирсан."
    },
    {
      "resource_id": 127,
      "text": "Сен уларнинг устидан ҳукмингни ўтказувчи эмассан."
    },
    { "resource_id": 127, "text": "Илло, ким юз ўгирса ва кофир бўлса..." },
    {
      "resource_id": 127,
      "text": "Бас, уни Аллоҳ катта азоб билан азоблайдир."
    },
    { "resource_id": 127, "text": "Албатта, уларнинг қайтиши Бизгадир." },
    {
      "resource_id": 127,
      "text": "Сўнгра, албатта, уларнинг ҳисоб-китоби ҳам Бизда бўладир."
    }
  ],
  "meta": {
    "translation_name": "Muhammad Sodik Muhammad Yusuf",
