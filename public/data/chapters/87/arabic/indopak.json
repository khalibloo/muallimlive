{
  "verses": [
    {
      "id": 5949,
      "verse_key": "87:1",
      "text_indopak": "سَبِّحِ اسۡمَ رَبِّكَ الۡاَعۡلَىۙ‏"
    },
    {
      "id": 5950,
      "verse_key": "87:2",
      "text_indopak": "الَّذِىۡ خَلَقَ فَسَوّٰى ۙ‏"
    },
    {
      "id": 5951,
      "verse_key": "87:3",
      "text_indopak": "وَالَّذِىۡ قَدَّرَ فَهَدٰى ۙ‏"
    },
    {
      "id": 5952,
      "verse_key": "87:4",
      "text_indopak": "وَالَّذِىۡۤ اَخۡرَجَ الۡمَرۡعٰى ۙ‏"
    },
    {
      "id": 5953,
      "verse_key": "87:5",
      "text_indopak": "فَجَعَلَهٗ غُثَآءً اَحۡوٰىؕ‏"
    },
    {
      "id": 5954,
      "verse_key": "87:6",
      "text_indopak": "سَنُقۡرِئُكَ فَلَا تَنۡسٰٓىۙ‏"
    },
    {
      "id": 5955,
      "verse_key": "87:7",
      "text_indopak": "اِلَّا مَا شَآءَ اللّٰهُ​ؕ اِنَّهٗ يَعۡلَمُ الۡجَهۡرَ وَمَا يَخۡفٰىؕ‏"
    },
    {
      "id": 5956,
      "verse_key": "87:8",
      "text_indopak": "وَنُيَسِّرُكَ لِلۡيُسۡرٰى ​ۖ​ۚ‏"
    },
    {
      "id": 5957,
      "verse_key": "87:9",
      "text_indopak": "فَذَكِّرۡ اِنۡ نَّفَعَتِ الذِّكۡرٰىؕ‏"
    },
    {
      "id": 5958,
      "verse_key": "87:10",
      "text_indopak": "سَيَذَّكَّرُ مَنۡ يَّخۡشٰىۙ‏"
    },
    {
      "id": 5959,
      "verse_key": "87:11",
      "text_indopak": "وَيَتَجَنَّبُهَا الۡاَشۡقَىۙ‏"
    },
    {
      "id": 5960,
      "verse_key": "87:12",
      "text_indopak": "الَّذِىۡ يَصۡلَى النَّارَ الۡكُبۡرٰى​ۚ‏"
    },
    {
      "id": 5961,
      "verse_key": "87:13",
      "text_indopak": "ثُمَّ لَا يَمُوۡتُ فِيۡهَا وَلَا يَحۡيٰىؕ‏"
    },
    {
      "id": 5962,
      "verse_key": "87:14",
      "text_indopak": "قَدۡ اَفۡلَحَ مَنۡ تَزَكّٰىۙ‏"
    },
    {
      "id": 5963,
      "verse_key": "87:15",
      "text_indopak": "وَذَكَرَ اسۡمَ رَبِّهٖ فَصَلّٰى​ ؕ‏"
    },
    {
      "id": 5964,
      "verse_key": "87:16",
      "text_indopak": "بَلۡ تُؤۡثِرُوۡنَ الۡحَيٰوةَ الدُّنۡيَا ۖ‏"
    },
    {
      "id": 5965,
      "verse_key": "87:17",
      "text_indopak": "وَالۡاٰخِرَةُ خَيۡرٌ وَّ اَبۡقٰىؕ‏"
    },
    {
      "id": 5966,
      "verse_key": "87:18",
      "text_indopak": "اِنَّ هٰذَا لَفِى الصُّحُفِ الۡاُوۡلٰىۙ‏"
    },
    {
      "id": 5967,
      "verse_key": "87:19",
      "text_indopak": "صُحُفِ اِبۡرٰهِيۡمَ وَمُوۡسٰى‏"
    }
  ],
  "meta": { "filters": { "chapter_number": "87" } }
}
