{
  "verses": [
    { "id": 4415, "verse_key": "44:1", "text_imlaei_simple": "حم" },
    { "id": 4416, "verse_key": "44:2", "text_imlaei_simple": "والكتاب المبين" },
    {
      "id": 4417,
      "verse_key": "44:3",
      "text_imlaei_simple": "انا انزلناه في ليلة مباركة انا كنا منذرين"
    },
    {
      "id": 4418,
      "verse_key": "44:4",
      "text_imlaei_simple": "فيها يفرق كل امر حكيم"
    },
    {
      "id": 4419,
      "verse_key": "44:5",
      "text_imlaei_simple": "امرا من عندنا انا كنا مرسلين"
    },
    {
      "id": 4420,
      "verse_key": "44:6",
      "text_imlaei_simple": "رحمة من ربك انه هو السميع العليم"
    },
    {
      "id": 4421,
      "verse_key": "44:7",
      "text_imlaei_simple": "رب السماوات والارض وما بينهما ان كنتم موقنين"
    },
    {
      "id": 4422,
      "verse_key": "44:8",
      "text_imlaei_simple": "لا اله الا هو يحيي ويميت ربكم ورب ابائكم الاولين"
    },
    {
      "id": 4423,
      "verse_key": "44:9",
      "text_imlaei_simple": "بل هم في شك يلعبون"
    },
    {
      "id": 4424,
      "verse_key": "44:10",
      "text_imlaei_simple": "فارتقب يوم تاتي السماء بدخان مبين"
    },
    {
      "id": 4425,
      "verse_key": "44:11",
      "text_imlaei_simple": "يغشى الناس هذا عذاب اليم"
    },
    {
      "id": 4426,
      "verse_key": "44:12",
      "text_imlaei_simple": "ربنا اكشف عنا العذاب انا مؤمنون"
    },
    {
      "id": 4427,
      "verse_key": "44:13",
      "text_imlaei_simple": "انى لهم الذكرى وقد جاءهم رسول مبين"
    },
    {
      "id": 4428,
      "verse_key": "44:14",
      "text_imlaei_simple": "ثم تولوا عنه وقالوا معلم مجنون"
    },
    {
      "id": 4429,
      "verse_key": "44:15",
      "text_imlaei_simple": "انا كاشفو العذاب قليلا انكم عائدون"
    },
    {
      "id": 4430,
      "verse_key": "44:16",
      "text_imlaei_simple": "يوم نبطش البطشة الكبرى انا منتقمون"
    },
    {
      "id": 4431,
      "verse_key": "44:17",
      "text_imlaei_simple": "ولقد فتنا قبلهم قوم فرعون وجاءهم رسول كريم"
    },
    {
      "id": 4432,
      "verse_key": "44:18",
      "text_imlaei_simple": "ان ادوا الي عباد الله اني لكم رسول امين"
    },
    {
      "id": 4433,
      "verse_key": "44:19",
      "text_imlaei_simple": "وان لا تعلوا على الله اني اتيكم بسلطان مبين"
    },
    {
      "id": 4434,
      "verse_key": "44:20",
      "text_imlaei_simple": "واني عذت بربي وربكم ان ترجمون"
    },
    {
      "id": 4435,
      "verse_key": "44:21",
      "text_imlaei_simple": "وان لم تؤمنوا لي فاعتزلون"
    },
    {
      "id": 4436,
      "verse_key": "44:22",
      "text_imlaei_simple": "فدعا ربه ان هؤلاء قوم مجرمون"
    },
    {
      "id": 4437,
      "verse_key": "44:23",
      "text_imlaei_simple": "فاسر بعبادي ليلا انكم متبعون"
    },
    {
      "id": 4438,
      "verse_key": "44:24",
      "text_imlaei_simple": "واترك البحر رهوا انهم جند مغرقون"
    },
    {
      "id": 4439,
      "verse_key": "44:25",
      "text_imlaei_simple": "كم تركوا من جنات وعيون"
    },
    {
      "id": 4440,
      "verse_key": "44:26",
      "text_imlaei_simple": "وزروع ومقام كريم"
    },
    {
      "id": 4441,
      "verse_key": "44:27",
      "text_imlaei_simple": "ونعمة كانوا فيها فاكهين"
    },
    {
      "id": 4442,
      "verse_key": "44:28",
      "text_imlaei_simple": "كذلك واورثناها قوما اخرين"
    },
    {
      "id": 4443,
      "verse_key": "44:29",
      "text_imlaei_simple": "فما بكت عليهم السماء والارض وما كانوا منظرين"
    },
    {
      "id": 4444,
      "verse_key": "44:30",
      "text_imlaei_simple": "ولقد نجينا بني اسرائيل من العذاب المهين"
    },
    {
      "id": 4445,
      "verse_key": "44:31",
      "text_imlaei_simple": "من فرعون انه كان عاليا من المسرفين"
    },
    {
      "id": 4446,
      "verse_key": "44:32",
      "text_imlaei_simple": "ولقد اخترناهم على علم على العالمين"
    },
    {
      "id": 4447,
      "verse_key": "44:33",
      "text_imlaei_simple": "واتيناهم من الايات ما فيه بلاء مبين"
    },
    {
      "id": 4448,
      "verse_key": "44:34",
      "text_imlaei_simple": "ان هؤلاء ليقولون"
    },
    {
      "id": 4449,
      "verse_key": "44:35",
      "text_imlaei_simple": "ان هي الا موتتنا الاولى وما نحن بمنشرين"
    },
    {
      "id": 4450,
      "verse_key": "44:36",
      "text_imlaei_simple": "فاتوا بابائنا ان كنتم صادقين"
    },
    {
      "id": 4451,
      "verse_key": "44:37",
      "text_imlaei_simple": "اهم خير ام قوم تبع والذين من قبلهم اهلكناهم انهم كانوا مجرمين"
    },
    {
      "id": 4452,
      "verse_key": "44:38",
      "text_imlaei_simple": "وما خلقنا السماوات والارض وما بينهما لاعبين"
    },
    {
      "id": 4453,
      "verse_key": "44:39",
      "text_imlaei_simple": "ما خلقناهما الا بالحق ولكن اكثرهم لا يعلمون"
    },
    {
      "id": 4454,
      "verse_key": "44:40",
      "text_imlaei_simple": "ان يوم الفصل ميقاتهم اجمعين"
    },
    {
      "id": 4455,
      "verse_key": "44:41",
      "text_imlaei_simple": "يوم لا يغني مولى عن مولى شيئا ولا هم ينصرون"
    },
    {
      "id": 4456,
      "verse_key": "44:42",
      "text_imlaei_simple": "الا من رحم الله انه هو العزيز الرحيم"
    },
    {
      "id": 4457,
      "verse_key": "44:43",
      "text_imlaei_simple": "ان شجرت الزقوم"
    },
    { "id": 4458, "verse_key": "44:44", "text_imlaei_simple": "طعام الاثيم" },
    {
      "id": 4459,
      "verse_key": "44:45",
      "text_imlaei_simple": "كالمهل يغلي في البطون"
    },
    { "id": 4460, "verse_key": "44:46", "text_imlaei_simple": "كغلي الحميم" },
    {
      "id": 4461,
      "verse_key": "44:47",
      "text_imlaei_simple": "خذوه فاعتلوه الى سواء الجحيم"
    },
    {
      "id": 4462,
      "verse_key": "44:48",
      "text_imlaei_simple": "ثم صبوا فوق راسه من عذاب الحميم"
    },
    {
      "id": 4463,
      "verse_key": "44:49",
      "text_imlaei_simple": "ذق انك انت العزيز الكريم"
    },
    {
      "id": 4464,
      "verse_key": "44:50",
      "text_imlaei_simple": "ان هذا ما كنتم به تمترون"
    },
    {
      "id": 4465,
      "verse_key": "44:51",
      "text_imlaei_simple": "ان المتقين في مقام امين"
    },
    { "id": 4466, "verse_key": "44:52", "text_imlaei_simple": "في جنات وعيون" },
    {
      "id": 4467,
      "verse_key": "44:53",
      "text_imlaei_simple": "يلبسون من سندس واستبرق متقابلين"
    },
    {
      "id": 4468,
      "verse_key": "44:54",
      "text_imlaei_simple": "كذلك وزوجناهم بحور عين"
    },
    {
      "id": 4469,
      "verse_key": "44:55",
      "text_imlaei_simple": "يدعون فيها بكل فاكهة امنين"
    },
    {
      "id": 4470,
      "verse_key": "44:56",
      "text_imlaei_simple": "لا يذوقون فيها الموت الا الموتة الاولى ووقاهم عذاب الجحيم"
    },
    {
      "id": 4471,
      "verse_key": "44:57",
      "text_imlaei_simple": "فضلا من ربك ذلك هو الفوز العظيم"
    },
    {
      "id": 4472,
      "verse_key": "44:58",
      "text_imlaei_simple": "فانما يسرناه بلسانك لعلهم يتذكرون"
    },
    {
      "id": 4473,
      "verse_key": "44:59",
      "text_imlaei_simple": "فارتقب انهم مرتقبون"
    }
  ],
  "meta": { "filters": { "chapter_number": "44" } }
}
