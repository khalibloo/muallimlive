{
  "translations": [
    { "resource_id": 52, "text": "Burçlar sahibi gökyüzüne," },
    { "resource_id": 52, "text": "Vaad olunan o güne," },
    { "resource_id": 52, "text": "Şahitlik edene ve edilene andolsun ki," },
    { "resource_id": 52, "text": "Kahroldu o hendeğin sahipleri," },
    { "resource_id": 52, "text": "O çıralı ateşin," },
    { "resource_id": 52, "text": "Hani o ateşin başına oturmuşlar," },
    { "resource_id": 52, "text": "Müminlere yaptıklarını seyrediyorlardı." },
    {
      "resource_id": 52,
      "text": "Müminlere kızmalarının sebebi de, onların yalnız çok güçlü ve övgüye lâyık olan Allah'a iman etmeleri idi."
    },
    {
      "resource_id": 52,
      "text": "O Allah ki, göklerin ve yerin hükümranlığı O'nundur ve Allah her şeye şahittir."
    },
    {
      "resource_id": 52,
      "text": "İnanan erkek ve kadınlara işkence yapıp sonra da tevbe etmeyenlere cehennem azabı ve yangın azabı vardır."
    },
    {
      "resource_id": 52,
      "text": "İnanan ve iyi amel yapanlar için de altından ırmaklar akan cennetler vardır. İşte büyük kurtuluş odur."
    },
    { "resource_id": 52, "text": "Kuşkusuz Rabbinin yakalaması serttir." },
    { "resource_id": 52, "text": "Yoktan o yaratır ve tekrar o diriltir." },
    {
      "resource_id": 52,
      "text": "Bununla beraber çok bağışlayandır, çok sevendir."
    },
    { "resource_id": 52, "text": "Arş'ın sahibidir, yücedir." },
    { "resource_id": 52, "text": "Dilediğini yapandır." },
    { "resource_id": 52, "text": "O orduların kıssası sana geldi mi?" },
    { "resource_id": 52, "text": "Yani Firavun ve Semud'un?" },
    {
      "resource_id": 52,
      "text": "Fakat o inkarcılar hâlâ bir yalanlama içinde."
    },
    {
      "resource_id": 52,
      "text": "Oysa Allah onları arkalarından kuşatmıştır."
    },
    { "resource_id": 52, "text": "Hayır o şerefli bir Kur'ân'dır." },
    { "resource_id": 52, "text": "Levhi Mahfuz'dadır." }
  ],
  "meta": {
    "translation_name": "Elmalili Hamdi Yazir",
