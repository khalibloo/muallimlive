{
  "translations": [
    { "resource_id": 57, "text": "Wassama-i thatialburooj" },
    { "resource_id": 57, "text": "Walyawmi almawAAood" },
    { "resource_id": 57, "text": "Washahidin wamashhood" },
    { "resource_id": 57, "text": "Qutila as-habu alukhdood" },
    { "resource_id": 57, "text": "Annari thati alwaqood" },
    { "resource_id": 57, "text": "Ith hum AAalayha quAAood" },
    {
      "resource_id": 57,
      "text": "Wahum AAala ma yafAAaloona bilmu/mineenashuhood"
    },
    {
      "resource_id": 57,
      "text": "Wama naqamoo minhum illa anyu/minoo billahi alAAazeezi alhameed"
    },
    {
      "resource_id": 57,
      "text": "Allathee lahu mulku assamawatiwal-ardi wallahu AAala kullishay-in shaheed"
    },
    {
      "resource_id": 57,
      "text": "Inna allatheena fatanoo almu/mineenawalmu/minati thumma lam yatooboo falahum AAathabujahannama walahum AAathabu alhareeq"
    },
    {
      "resource_id": 57,
      "text": "Inna allatheena amanoowaAAamiloo assalihati lahum jannatuntajree min tahtiha al-anharu thalikaalfawzu alkabeer"
    },
    { "resource_id": 57, "text": "Inna batsha rabbika lashadeed" },
    { "resource_id": 57, "text": "Innahu huwa yubdi-o wayuAAeed" },
    { "resource_id": 57, "text": "Wahuwa alghafooru alwadood" },
    { "resource_id": 57, "text": "Thoo alAAarshi almajeed" },
    { "resource_id": 57, "text": "FaAAAAalun lima yureed" },
    { "resource_id": 57, "text": "Hal ataka hadeethu aljunood" },
    { "resource_id": 57, "text": "FirAAawna wathamood" },
    { "resource_id": 57, "text": "Bali allatheena kafaroo fee taktheeb" },
    { "resource_id": 57, "text": "Wallahu min wara-ihimmuheet" },
    { "resource_id": 57, "text": "Bal huwa qur-anun majeed" },
    { "resource_id": 57, "text": "Fee lawhin mahfooth" }
  ],
