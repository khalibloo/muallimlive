{
  "verses": [
    {
      "id": 5910,
      "verse_key": "85:1",
      "text_imlaei_simple": "والسماء ذات البروج"
    },
    { "id": 5911, "verse_key": "85:2", "text_imlaei_simple": "واليوم الموعود" },
    { "id": 5912, "verse_key": "85:3", "text_imlaei_simple": "وشاهد ومشهود" },
    {
      "id": 5913,
      "verse_key": "85:4",
      "text_imlaei_simple": "قتل اصحاب الاخدود"
    },
    {
      "id": 5914,
      "verse_key": "85:5",
      "text_imlaei_simple": "النار ذات الوقود"
    },
    {
      "id": 5915,
      "verse_key": "85:6",
      "text_imlaei_simple": "اذ هم عليها قعود"
    },
    {
      "id": 5916,
      "verse_key": "85:7",
      "text_imlaei_simple": "وهم على ما يفعلون بالمؤمنين شهود"
    },
    {
      "id": 5917,
      "verse_key": "85:8",
      "text_imlaei_simple": "وما نقموا منهم الا ان يؤمنوا بالله العزيز الحميد"
    },
    {
      "id": 5918,
      "verse_key": "85:9",
      "text_imlaei_simple": "الذي له ملك السماوات والارض والله على كل شيء شهيد"
    },
    {
      "id": 5919,
      "verse_key": "85:10",
      "text_imlaei_simple": "ان الذين فتنوا المؤمنين والمؤمنات ثم لم يتوبوا فلهم عذاب جهنم ولهم عذاب الحريق"
    },
    {
      "id": 5920,
      "verse_key": "85:11",
      "text_imlaei_simple": "ان الذين امنوا وعملوا الصالحات لهم جنات تجري من تحتها الانهار ذلك الفوز الكبير"
    },
    {
      "id": 5921,
      "verse_key": "85:12",
      "text_imlaei_simple": "ان بطش ربك لشديد"
    },
    {
      "id": 5922,
      "verse_key": "85:13",
      "text_imlaei_simple": "انه هو يبدئ ويعيد"
    },
    {
      "id": 5923,
      "verse_key": "85:14",
      "text_imlaei_simple": "وهو الغفور الودود"
    },
    {
      "id": 5924,
      "verse_key": "85:15",
      "text_imlaei_simple": "ذو العرش المجيد"
    },
    { "id": 5925, "verse_key": "85:16", "text_imlaei_simple": "فعال لما يريد" },
    {
      "id": 5926,
      "verse_key": "85:17",
      "text_imlaei_simple": "هل اتاك حديث الجنود"
    },
    { "id": 5927, "verse_key": "85:18", "text_imlaei_simple": "فرعون وثمود" },
    {
      "id": 5928,
      "verse_key": "85:19",
      "text_imlaei_simple": "بل الذين كفروا في تكذيب"
    },
    {
      "id": 5929,
      "verse_key": "85:20",
      "text_imlaei_simple": "والله من ورائهم محيط"
    },
    {
      "id": 5930,
      "verse_key": "85:21",
      "text_imlaei_simple": "بل هو قران مجيد"
    },
    { "id": 5931, "verse_key": "85:22", "text_imlaei_simple": "في لوح محفوظ" }
  ],
  "meta": { "filters": { "chapter_number": "85" } }
}
