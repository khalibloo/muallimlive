{
  "translations": [
    { "resource_id": 30, "text": "Kautta taivaan ja aamutähden!" },
    { "resource_id": 30, "text": "Tiedätkö, mikä on aamutähti?" },
    { "resource_id": 30, "text": "Se on häikäisevän kirkas tähti." },
    {
      "resource_id": 30,
      "text": "Ei ole sielua, jolla ei olisi suojelijaansa."
    },
    { "resource_id": 30, "text": "Miettiköön ihminen, mistä hänet on luotu!" },
    {
      "resource_id": 30,
      "text": "Hänet on luotu pienestä, pursuavasta pisarasta,"
    },
    {
      "resource_id": 30,
      "text": "joka on lähtöisin kupeiden ja rintakehän välistä."
    },
    {
      "resource_id": 30,
      "text": "Totisesti, Jumala voi herättää hänet jälleen eloon"
    },
    {
      "resource_id": 30,
      "text": "päivänä, jona kaikki, mikä salassa on, tulee ilmi."
    },
    {
      "resource_id": 30,
      "text": "Eikä hänellä ole silloin voimaa eikä auttajaa."
    },
    { "resource_id": 30, "text": "Nimessä taivaan, josta sateet tulevat," },
    { "resource_id": 30, "text": "ja maan, joka avaa uumenensa!" },
    { "resource_id": 30, "text": "Totisesti tämä on ratkaiseva sana," },
    { "resource_id": 30, "text": "eikä mitään joutavaa puhetta." },
    { "resource_id": 30, "text": "Totisesti heillä on juonensa," },
    { "resource_id": 30, "text": "mutta Minulla myös on suunnitelmani." },
    {
      "resource_id": 30,
      "text": "Anna siis aikaa uskottomille; myönnä heille aikaa hetkiseksi!"
    }
  ],
