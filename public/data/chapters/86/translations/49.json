{
  "translations": [
    { "resource_id": 49, "text": "Naapa kwa mbingu na Kinacho kuja usiku!" },
    {
      "resource_id": 49,
      "text": "Na nini kitakacho kujuulisha ni nini hicho Kinacho kuja usiku?"
    },
    { "resource_id": 49, "text": "Ni Nyota yenye mwanga mkali." },
    { "resource_id": 49, "text": "Hapana nafsi ila inayo mwangalizi." },
    {
      "resource_id": 49,
      "text": "Hebu naajitazame mwanaadamu ameumbwa kwa kitu gani?"
    },
    { "resource_id": 49, "text": "Ameumbwa kwa maji yatokayo kwa kuchupa," },
    {
      "resource_id": 49,
      "text": "Yatokayo baina ya mifupa ya mgongo na mbavu."
    },
    { "resource_id": 49, "text": "Hakika Yeye ana uweza wa kumrudisha." },
    { "resource_id": 49, "text": "Siku zitakapo dhihirishwa siri." },
    { "resource_id": 49, "text": "Basi hatakuwa na nguvu wala msaidizi." },
    { "resource_id": 49, "text": "Naapa kwa mbingu yenye marejeo!" },
    { "resource_id": 49, "text": "Na kwa ardhi inayo pasuka!" },
    { "resource_id": 49, "text": "Hakika hii ni kauli ya kupambanua." },
    { "resource_id": 49, "text": "Wala si mzaha." },
    { "resource_id": 49, "text": "Hakika wao wanapanga mpango." },
    { "resource_id": 49, "text": "Na Mimi napanga mpango." },
    {
      "resource_id": 49,
      "text": "Basi wape muhula makafiri - wape muhula pole pole."
    }
  ],
