{
  "translations": [
    { "resource_id": 55, "text": "Osmon bilan qasam va toriq bilan qasam." },
    { "resource_id": 55, "text": "Toriq nimaligini senga ne bildirdi?" },
    { "resource_id": 55, "text": "U, nur sochib turuvchi yulduzdir." },
    {
      "resource_id": 55,
      "text": "Hech bir jon yo`qki, uning hifz qilib turuvchisi bo`lmasa."
    },
    { "resource_id": 55, "text": "Inson nimadan yaralganiga nazar solsin." },
    { "resource_id": 55, "text": "U otilib chiquvchi suvdan yaralgandir." },
    {
      "resource_id": 55,
      "text": "U (suv) sulb (orqa umurtqa) va taroib (ko`krak suyagi) orasidan chiqadir."
    },
    {
      "resource_id": 55,
      "text": "Albatta, U zot uni qayta tiriltirishga qodirdir."
    },
    { "resource_id": 55, "text": "Sirlar fosh bo`ladigan kundadir." },
    {
      "resource_id": 55,
      "text": "Bas (o`shanda) u(inson)da quvvat ham, nusrat beruvchi ham bo`lmaydir."
    },
    {
      "resource_id": 55,
      "text": "Qaytuvchi sohibi bo`lgan osmon bilan qasam."
    },
    {
      "resource_id": 55,
      "text": "Yorib chiquvchi egasi bo`lgan yer bilan qasam."
    },
    { "resource_id": 55, "text": "Albatta, u (Qur'on) ajratuvchi so`zdir." },
    { "resource_id": 55, "text": "Va u hazil emasdir." },
    { "resource_id": 55, "text": "Albatta, ular hiyla qiladir." },
    {
      "resource_id": 55,
      "text": "Va Men ham «hiyla» qilurman.<sup foot_note=\"169911\">1</sup>"
    },
    {
      "resource_id": 55,
      "text": "Bas, kofirlarga muhlat ber. Ha, ularga ozgina muhlat ber.<sup foot_note=\"170141\">1</sup>"
    }
  ],
