{
  "translations": [
    { "resource_id": 113, "text": "Ол қабағы түсіп, бұрылды," },
    { "resource_id": 113, "text": "оған соқыр кісінің келгендігінен." },
    {
      "resource_id": 113,
      "text": "/ Ей, Мұхаммед! /  Сен қайдан білесің, мүмкін ол тазаратын шығар?"
    },
    {
      "resource_id": 113,
      "text": "Немесе еске алар, сөйтіп еске салу оған пайда берер."
    },
    { "resource_id": 113, "text": "Ал, қажетсінбейтін біреу болса," },
    { "resource_id": 113, "text": "сен оған көңіл бөлесің." },
    {
      "resource_id": 113,
      "text": "Ал, оның тазармауында тұрған саған еш нәрсе жоқ."
    },
    { "resource_id": 113, "text": "Ал біреу саған ынталанып келсе," },
    { "resource_id": 113, "text": "әрі  / Раббысынан /  қорыққан халде," },
    { "resource_id": 113, "text": "сен одан  / басқаға /  көңіл аударасың." },
    {
      "resource_id": 113,
      "text": "Жоқ, олай емес! Анығында, бұл - бір еске салу."
    },
    { "resource_id": 113, "text": "Кім қаласа, оны есіне алады,-" },
    { "resource_id": 113, "text": "құрметті жазбаларда  / сухуфтарда / ," },
    { "resource_id": 113, "text": "жоғары бағаланған тап-таза," },
    { "resource_id": 113, "text": "елшілердің  / періштелердің /  қолында," },
    { "resource_id": 113, "text": "қадірлі, игі." },
    {
      "resource_id": 113,
      "text": "/ Сенбеген /  адамның жаны шықсын! Күпірлік етуде  / игіліктерді мойындамауда /  ол\nқандай қатты."
    },
    { "resource_id": 113, "text": "Ол  / Аллаһ /  оны не нәрседен жаратты?" },
    { "resource_id": 113, "text": "Бір тамшыдан жаратты, әрі белгіледі." },
    { "resource_id": 113, "text": "Содан соң оған жолды оңайластырды." },
    {
      "resource_id": 113,
      "text": "Одан кейін оның жанын алып,  / мүрдесін /  қабірге қойдырды."
    },
    {
      "resource_id": 113,
      "text": "Кейін оны Өзі қалаған кезде қайта тірілтеді."
    },
    { "resource_id": 113, "text": "Жоқ! Ол болса, Оның бұйрығын орындамады." },
    {
      "resource_id": 113,
      "text": "Ал, енді адам өзінің тамағына, бір көз салып қарасын:"
    },
    { "resource_id": 113, "text": "Біз суды мол етіп құйдық  / жаудырдық / ," },
    { "resource_id": 113, "text": "содан кейін жерді тілімдеп айырдық." },
    { "resource_id": 113, "text": "Сөйтіп одан дәнді / дақылдар /  өсірдік," },
    { "resource_id": 113, "text": "әрі жүзім мен жоңышқаны," },
    { "resource_id": 113, "text": "және зәйтүн мен құрманы," },
    { "resource_id": 113, "text": "және қалың бау-бақтарды," },
    { "resource_id": 113, "text": "сондай-ақ, жемістер мен шөптерді," },
    {
      "resource_id": 113,
      "text": "сендердің пайдалануларыңа әрі малдарың үшін."
    },
    { "resource_id": 113, "text": "Ал енді Сохха келген кезде," },
    { "resource_id": 113, "text": "сол күні адам, туған бауырынан қашады" },
    { "resource_id": 113, "text": "және анасы мен әкесінен," },
    { "resource_id": 113, "text": "әрі әйелі мен балаларынан." },
    {
      "resource_id": 113,
      "text": "Ол күні әрбір адамды әлек ететін оз ісі бар."
    },
    { "resource_id": 113, "text": "Ол күні  / кейбір /  жүздер жарқын," },
    { "resource_id": 113, "text": "күлімсіреген, қуанышты." },
    {
      "resource_id": 113,
      "text": "Әрі ол күні / кейбір /  жүздерді шаң басып,"
    },
    { "resource_id": 113, "text": "олар қарайған болады." },
    { "resource_id": 113, "text": "Міне, солар - кәпірлер, бұзықтар." }
  ],
