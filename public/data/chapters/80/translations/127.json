{
  "translations": [
    { "resource_id": 127, "text": "Афтини буриштирди ва юз ўгирди." },
    { "resource_id": 127, "text": "Кўзи кўр киши ҳузурига келганда." },
    { "resource_id": 127, "text": "Қаердан биласан, эҳтимол, у покланар." },
    {
      "resource_id": 127,
      "text": "Ёки насиҳат олар ва мавъиза унга манфаат берар."
    },
    { "resource_id": 127, "text": "Аммо истиғно қилганга бўлса..." },
    { "resource_id": 127, "text": "Сен у томонга интиласан." },
    {
      "resource_id": 127,
      "text": "У покланмаса, сени ҳеч ким маломат қилмас."
    },
    { "resource_id": 127, "text": "Аммо ҳузурингга шошилиб келган." },
    {
      "resource_id": 127,
      "text": "Ва У зотдан қўрққан ҳолда бўлгандан эса..."
    },
    { "resource_id": 127, "text": "Бас, Сен ундан машғул бўласан." },
    {
      "resource_id": 127,
      "text": "Йўқ! Албатта улар (Қуръон оятлари) эслатмадир."
    },
    { "resource_id": 127, "text": "Ким хоҳласа, уни эслайдир." },
    { "resource_id": 127, "text": "Ҳурматланган саҳифалардадир." },
    { "resource_id": 127, "text": "Олий қадардирлар, поклангандирлар." },
    { "resource_id": 127, "text": "Элчи(фаришта)лар қўлларидадир." },
    { "resource_id": 127, "text": "Улар ҳурматланган ва ўта яхшидирлар." },
    {
      "resource_id": 127,
      "text": "Инсонга лаънат бўлсин, унинг кофирлиги қандоқ шиддатли бўлди."
    },
    { "resource_id": 127, "text": "Уни қайси нарсадан яратди?" },
    { "resource_id": 127, "text": "Нутфадан уни яратди ва уни ўлчовли қилди." },
    { "resource_id": 127, "text": "Сўнгра, чиқиш йўлини осон қилди." },
    { "resource_id": 127, "text": "Сўнгра ўлдирди ва қабрга киргазди." },
    {
      "resource_id": 127,
      "text": "Сўнгра (қачон) хоҳласа, уни қайта тирилтиради."
    },
    {
      "resource_id": 127,
      "text": "Йўқ, ундоқ эмас! У ўзига У зот қилган амрни бажармади."
    },
    { "resource_id": 127, "text": "Инсон ўз таомига назар солсин." },
    { "resource_id": 127, "text": "Албатта, Биз сувни роса қуйиб қўйибмиз." },
    { "resource_id": 127, "text": "Сўнгра ерни ўзига хос ёрдик." },
    { "resource_id": 127, "text": "Биз унда донни." },
    { "resource_id": 127, "text": "Ва узум ва кўкларни." },
    { "resource_id": 127, "text": "Ва зайтун ва хурмоларни." },
    { "resource_id": 127, "text": "Ва тиғиз боғларни." },
    { "resource_id": 127, "text": "Ва мевалару ўт-ўланларни ўстириб қўйдик." },
    {
      "resource_id": 127,
      "text": "Сизларга ва чорваларингизга манфаат ва лаззат бўлсин, деб."
    },
    { "resource_id": 127, "text": "Вақтики кар қилувчи овоз келса." },
    { "resource_id": 127, "text": "У кунда киши ўз ака-укасидан." },
    { "resource_id": 127, "text": "Ва онаси ва отасидан." },
    { "resource_id": 127, "text": "Ва хотини ва бола-чақасидан қочади." },
    {
      "resource_id": 127,
      "text": "У кунда улардан ҳар бир шахсни овора қилувчи ўз иши бор."
    },
    { "resource_id": 127, "text": "У кунда порлоқ чеҳралар бор." },
    {
      "resource_id": 127,
      "text": "Улар кулгу билан тўла, етган хушхабардан хурсанд."
    },
    {
      "resource_id": 127,
      "text": "У кунда бир чеҳралар борки, уларни ғубор босган."
    },
    { "resource_id": 127, "text": "Устини зулмат қоплаган." },
    { "resource_id": 127, "text": "Улар, ана ўшалар, кофирлар ва фожирлардир." }
  ],
  "meta": {
    "translation_name": "Muhammad Sodik Muhammad Yusuf",
