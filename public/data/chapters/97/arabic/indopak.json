    {
      "id": 6126,
      "verse_key": "97:1",
      "text_indopak": "اِنَّاۤ اَنۡزَلۡنٰهُ فِىۡ لَيۡلَةِ الۡقَدۡرِ ۖ ۚ‏"
    },
    {
      "id": 6127,
      "verse_key": "97:2",
      "text_indopak": "وَمَاۤ اَدۡرٰٮكَ مَا لَيۡلَةُ الۡقَدۡرِؕ‏"
    },
    {
      "id": 6128,
      "verse_key": "97:3",
      "text_indopak": "لَيۡلَةُ الۡقَدۡرِ  ۙ خَيۡرٌ مِّنۡ اَلۡفِ شَهۡرٍؕ‏"
    },
    {
      "id": 6129,
      "verse_key": "97:4",
      "text_indopak": "تَنَزَّلُ الۡمَلٰٓٮِٕكَةُ وَالرُّوۡحُ فِيۡهَا بِاِذۡنِ رَبِّهِمۡ​ۚ مِّنۡ كُلِّ اَمۡرٍ ۛۙ‏"
    },
    {
      "id": 6130,
      "verse_key": "97:5",
      "text_indopak": "سَلٰمٌ   ۛهِىَ حَتّٰى مَطۡلَعِ الۡفَجۡرِ‏"
    }
  ],
