{
  "verses": [
    {
      "id": 6126,
      "verse_key": "97:1",
      "text_imlaei_simple": "انا انزلناه في ليلة القدر"
    },
    {
      "id": 6127,
      "verse_key": "97:2",
      "text_imlaei_simple": "وما ادراك ما ليلة القدر"
    },
    {
      "id": 6128,
      "verse_key": "97:3",
      "text_imlaei_simple": "ليلة القدر خير من الف شهر"
    },
    {
      "id": 6129,
      "verse_key": "97:4",
      "text_imlaei_simple": "تنزل الملائكة والروح فيها باذن ربهم من كل امر"
    },
    {
      "id": 6130,
      "verse_key": "97:5",
      "text_imlaei_simple": "سلام هي حتى مطلع الفجر"
    }
  ],
  "meta": { "filters": { "chapter_number": "97" } }
}
