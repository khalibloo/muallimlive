{
  "verses": [
    { "id": 4736, "verse_key": "52:1", "text_imlaei_simple": "والطور" },
    { "id": 4737, "verse_key": "52:2", "text_imlaei_simple": "وكتاب مسطور" },
    { "id": 4738, "verse_key": "52:3", "text_imlaei_simple": "في رق منشور" },
    { "id": 4739, "verse_key": "52:4", "text_imlaei_simple": "والبيت المعمور" },
    { "id": 4740, "verse_key": "52:5", "text_imlaei_simple": "والسقف المرفوع" },
    { "id": 4741, "verse_key": "52:6", "text_imlaei_simple": "والبحر المسجور" },
    {
      "id": 4742,
      "verse_key": "52:7",
      "text_imlaei_simple": "ان عذاب ربك لواقع"
    },
    { "id": 4743, "verse_key": "52:8", "text_imlaei_simple": "ما له من دافع" },
    {
      "id": 4744,
      "verse_key": "52:9",
      "text_imlaei_simple": "يوم تمور السماء مورا"
    },
    {
      "id": 4745,
      "verse_key": "52:10",
      "text_imlaei_simple": "وتسير الجبال سيرا"
    },
    {
      "id": 4746,
      "verse_key": "52:11",
      "text_imlaei_simple": "فويل يومئذ للمكذبين"
    },
    {
      "id": 4747,
      "verse_key": "52:12",
      "text_imlaei_simple": "الذين هم في خوض يلعبون"
    },
    {
      "id": 4748,
      "verse_key": "52:13",
      "text_imlaei_simple": "يوم يدعون الى نار جهنم دعا"
    },
    {
      "id": 4749,
      "verse_key": "52:14",
      "text_imlaei_simple": "هذه النار التي كنتم بها تكذبون"
    },
    {
      "id": 4750,
      "verse_key": "52:15",
      "text_imlaei_simple": "افسحر هذا ام انتم لا تبصرون"
    },
    {
      "id": 4751,
      "verse_key": "52:16",
      "text_imlaei_simple": "اصلوها فاصبروا او لا تصبروا سواء عليكم انما تجزون ما كنتم تعملون"
    },
    {
      "id": 4752,
      "verse_key": "52:17",
      "text_imlaei_simple": "ان المتقين في جنات ونعيم"
    },
    {
      "id": 4753,
      "verse_key": "52:18",
      "text_imlaei_simple": "فاكهين بما اتاهم ربهم ووقاهم ربهم عذاب الجحيم"
    },
    {
      "id": 4754,
      "verse_key": "52:19",
      "text_imlaei_simple": "كلوا واشربوا هنيئا بما كنتم تعملون"
    },
    {
      "id": 4755,
      "verse_key": "52:20",
      "text_imlaei_simple": "متكئين على سرر مصفوفة وزوجناهم بحور عين"
    },
    {
      "id": 4756,
      "verse_key": "52:21",
      "text_imlaei_simple": "والذين امنوا واتبعتهم ذريتهم بايمان الحقنا بهم ذريتهم وما التناهم من عملهم من شيء كل امرئ بما كسب رهين"
    },
    {
      "id": 4757,
      "verse_key": "52:22",
      "text_imlaei_simple": "وامددناهم بفاكهة ولحم مما يشتهون"
    },
    {
      "id": 4758,
      "verse_key": "52:23",
      "text_imlaei_simple": "يتنازعون فيها كاسا لا لغو فيها ولا تاثيم"
    },
    {
      "id": 4759,
      "verse_key": "52:24",
      "text_imlaei_simple": "ويطوف عليهم غلمان لهم كانهم لؤلؤ مكنون"
    },
    {
      "id": 4760,
      "verse_key": "52:25",
      "text_imlaei_simple": "واقبل بعضهم على بعض يتساءلون"
    },
    {
      "id": 4761,
      "verse_key": "52:26",
      "text_imlaei_simple": "قالوا انا كنا قبل في اهلنا مشفقين"
    },
    {
      "id": 4762,
      "verse_key": "52:27",
      "text_imlaei_simple": "فمن الله علينا ووقانا عذاب السموم"
    },
    {
      "id": 4763,
      "verse_key": "52:28",
      "text_imlaei_simple": "انا كنا من قبل ندعوه انه هو البر الرحيم"
    },
    {
      "id": 4764,
      "verse_key": "52:29",
      "text_imlaei_simple": "فذكر فما انت بنعمت ربك بكاهن ولا مجنون"
    },
    {
      "id": 4765,
      "verse_key": "52:30",
      "text_imlaei_simple": "ام يقولون شاعر نتربص به ريب المنون"
    },
    {
      "id": 4766,
      "verse_key": "52:31",
      "text_imlaei_simple": "قل تربصوا فاني معكم من المتربصين"
    },
    {
      "id": 4767,
      "verse_key": "52:32",
      "text_imlaei_simple": "ام تامرهم احلامهم بهذا ام هم قوم طاغون"
    },
    {
      "id": 4768,
      "verse_key": "52:33",
      "text_imlaei_simple": "ام يقولون تقوله بل لا يؤمنون"
    },
    {
      "id": 4769,
      "verse_key": "52:34",
      "text_imlaei_simple": "فلياتوا بحديث مثله ان كانوا صادقين"
    },
    {
      "id": 4770,
      "verse_key": "52:35",
      "text_imlaei_simple": "ام خلقوا من غير شيء ام هم الخالقون"
    },
    {
      "id": 4771,
      "verse_key": "52:36",
      "text_imlaei_simple": "ام خلقوا السماوات والارض بل لا يوقنون"
    },
    {
      "id": 4772,
      "verse_key": "52:37",
      "text_imlaei_simple": "ام عندهم خزائن ربك ام هم المصيطرون"
    },
    {
      "id": 4773,
      "verse_key": "52:38",
      "text_imlaei_simple": "ام لهم سلم يستمعون فيه فليات مستمعهم بسلطان مبين"
    },
    {
      "id": 4774,
      "verse_key": "52:39",
      "text_imlaei_simple": "ام له البنات ولكم البنون"
    },
    {
      "id": 4775,
      "verse_key": "52:40",
      "text_imlaei_simple": "ام تسالهم اجرا فهم من مغرم مثقلون"
    },
    {
      "id": 4776,
      "verse_key": "52:41",
      "text_imlaei_simple": "ام عندهم الغيب فهم يكتبون"
    },
    {
      "id": 4777,
      "verse_key": "52:42",
      "text_imlaei_simple": "ام يريدون كيدا فالذين كفروا هم المكيدون"
    },
    {
      "id": 4778,
      "verse_key": "52:43",
      "text_imlaei_simple": "ام لهم اله غير الله سبحان الله عما يشركون"
    },
    {
      "id": 4779,
      "verse_key": "52:44",
      "text_imlaei_simple": "وان يروا كسفا من السماء ساقطا يقولوا سحاب مركوم"
    },
    {
      "id": 4780,
      "verse_key": "52:45",
      "text_imlaei_simple": "فذرهم حتى يلاقوا يومهم الذي فيه يصعقون"
    },
    {
      "id": 4781,
      "verse_key": "52:46",
      "text_imlaei_simple": "يوم لا يغني عنهم كيدهم شيئا ولا هم ينصرون"
    },
    {
      "id": 4782,
      "verse_key": "52:47",
      "text_imlaei_simple": "وان للذين ظلموا عذابا دون ذلك ولكن اكثرهم لا يعلمون"
    },
    {
      "id": 4783,
      "verse_key": "52:48",
      "text_imlaei_simple": "واصبر لحكم ربك فانك باعيننا وسبح بحمد ربك حين تقوم"
    },
    {
      "id": 4784,
      "verse_key": "52:49",
      "text_imlaei_simple": "ومن الليل فسبحه وادبار النجوم"
    }
  ],
  "meta": { "filters": { "chapter_number": "52" } }
}
