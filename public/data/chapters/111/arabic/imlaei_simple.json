{
  "verses": [
    {
      "id": 6217,
      "verse_key": "111:1",
      "text_imlaei_simple": "تبت يدا ابي لهب وتب"
    },
    {
      "id": 6218,
      "verse_key": "111:2",
      "text_imlaei_simple": "ما اغنى عنه ماله وما كسب"
    },
    {
      "id": 6219,
      "verse_key": "111:3",
      "text_imlaei_simple": "سيصلى نارا ذات لهب"
    },
    {
      "id": 6220,
      "verse_key": "111:4",
      "text_imlaei_simple": "وامراته حمالة الحطب"
    },
    {
      "id": 6221,
      "verse_key": "111:5",
      "text_imlaei_simple": "في جيدها حبل من مسد"
    }
  ],
