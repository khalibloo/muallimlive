{
  "translations": [
    { "resource_id": 111, "text": "Aduufi ifa isheetiinin (kakadha)." },
    { "resource_id": 111, "text": "Ji’aanis; yeroo inni ishee hordofe." },
    {
      "resource_id": 111,
      "text": "Guyyaanis yeroo inni ishee (aduu) mul’ise;"
    },
    { "resource_id": 111, "text": "Halkaniinis yeroo inni ishee haguuge;" },
    { "resource_id": 111, "text": "Samiifi kan ishee ijaareenis;" },
    { "resource_id": 111, "text": "Dachiifi kan ishee diriirseenis;" },
    { "resource_id": 111, "text": "Lubbuufi kan ishee uumeenis;" },
    {
      "resource_id": 111,
      "text": "Kan jal’ina isheefi qajeeluma ishee ishee barsiise."
    },
    {
      "resource_id": 111,
      "text": "Namni ishee qulqulleesse dhugumatti milkaa’e."
    },
    { "resource_id": 111, "text": "Namni ishee xureesses dhugaatti hoonga’e." },
    {
      "resource_id": 111,
      "text": "Samuud daangaa dabruu isheetiin kijibsiifte."
    },
    {
      "resource_id": 111,
      "text": "Yeroo irra jallaan ishee ka’u (kijibsiifte)."
    },
    {
      "resource_id": 111,
      "text": "Ergamaan Rabbiis isaaniin “gaala Rabbiitiifi dhugaatii ishee (eeggadhaa)” jedhe."
    },
    {
      "resource_id": 111,
      "text": "Isa kijibsiisanii ishee waraananSababa badii isaanii isaan balleessee wal qixxeesse."
    },
    { "resource_id": 111, "text": "Haaloo ishee hin sodaatu" }
  ],
