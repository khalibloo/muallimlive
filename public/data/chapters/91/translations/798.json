{
  "translations": [
    { "resource_id": 798, "text": "Ngulilumbila lyuŵa ni lilanguka lyakwe." },
    { "resource_id": 798, "text": "Ni mwesi ndema jaukulikuya (lyuŵalyo)." },
    { "resource_id": 798, "text": "Ni muusi ndema jaukulilosya palangulangu." },
    {
      "resource_id": 798,
      "text": "Ni chilo ndema jachikuliunichila (kuti likaoneka)."
    },
    {
      "resource_id": 798,
      "text": "Kwisa soni liwunde ni Juŵalitaŵile (mwakulitalichisya)."
    },
    {
      "resource_id": 798,
      "text": "Ni litaka ni Juŵalitandiche (mwakulijaasasya)."
    },
    { "resource_id": 798, "text": "Kwisa soni ntima ni Juŵaukolosisye." },
    {
      "resource_id": 798,
      "text": "Basi ni jwaumanyisye yakusakala yakwe ni yambone yakwe."
    },
    {
      "resource_id": 798,
      "text": "Pamasile paapundile jwauswejesye (ntima wakwe)."
    },
    {
      "resource_id": 798,
      "text": "Soni pamasile papelelwe jwautwinyichisye (ntima wakwe mu sambi)."
    },
    {
      "resource_id": 798,
      "text": "Ŵaakaaniile ŵanya Asamudu ligongo lyakusumba mpika kwao."
    },
    {
      "resource_id": 798,
      "text": "Ndema jajwalipeleche jwakusakalilwa jwao (kuulaga ngamiya)."
    },
    {
      "resource_id": 798,
      "text": "Basi Ntenga jwa Allah (Swaleh) ŵasalile kuti: “(Aji ni) ngamiya ja Allah, sano ngasin'jikanyaga kumwa mesi (palisiku lyakwe)."
    },
    {
      "resource_id": 798,
      "text": "Nambo ŵankaaniile nikujiulaga; basi Ambuje ŵao ŵajonasile ligongo lya chileŵi chao; niŵalandenye pachonasikocho."
    },
    { "resource_id": 798, "text": "Soni jwangajogopa yakuichisya yakwe." }
  ],
  "meta": {
    "translation_name": "Abdul Hamid Silika",
    "author_name": "Abdul Hamid Silika",
    "filters": { "resource_id": 798, "chapter_number": "91" }
  }
}
