    { "resource_id": 233, "text": "אלה אשר מתמהמהים לקיים תפילה," },
    { "resource_id": 233, "text": "אלה אשר עושים רק בשביל להראות," },
    { "resource_id": 233, "text": "ומונעים את החסד הפשוט." }
