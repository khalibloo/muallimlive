{
  "translations": [
    {
      "resource_id": 46,
      "text": "Ka warrama kan beenin Abaalmarinta (Qiyaamada)."
    },
    { "resource_id": 46, "text": "Kaas xooga Agoonta (xageeda)." },
    { "resource_id": 46, "text": "Aan kuna boorrinaynin quudinta miskiinka." },
    {
      "resource_id": 46,
      "text": "Waxaa halaag u sugnaaday kuwa tukan (waa Munaafiqiinta)."
    },
    { "resource_id": 46, "text": "Ee haddana halmaansan Salaaddooda." },
    { "resource_id": 46, "text": "Dadkana is tusiya." },
    { "resource_id": 46, "text": "Una diida alaabta (Laysugu kaalmeeyo)." }
  ],
