    {
      "resource_id": 42,
      "text": "Czy widziałeś tego, który za kłamstwo uważa religię?"
    },
    {
      "resource_id": 42,
      "text": "To jest ten, który z pogardą odpycha sierotę,"
    },
    { "resource_id": 42, "text": "Który nie zachęca do nakarmienia biednego." },
    { "resource_id": 42, "text": "Biada modlącym się," },
    { "resource_id": 42, "text": "Którzy są niedbali w swojej modlitwie," },
    { "resource_id": 42, "text": "Którzy tylko chcą być widziani," },
    { "resource_id": 42, "text": "A odmawiają wspomożenia." }
  ],
