    { "resource_id": 124, "text": "Dini yalanlayanı gördün mü?" },
    { "resource_id": 124, "text": "İşte o, yetimi itip kakar," },
    { "resource_id": 124, "text": "Yoksulu doyurmaya teşvik etmez;" },
    { "resource_id": 124, "text": "Yazıklar olsun o namaz kılanlara ki," },
    { "resource_id": 124, "text": "onlar namazlarını ciddiye almazlar." },
    { "resource_id": 124, "text": "Onlar gösteriş yapanlardır," },
    { "resource_id": 124, "text": "hayra da mâni olurlar." }
