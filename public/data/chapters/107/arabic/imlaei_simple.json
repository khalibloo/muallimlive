{
  "verses": [
    {
      "id": 6198,
      "verse_key": "107:1",
      "text_imlaei_simple": "ارايت الذي يكذب بالدين"
    },
    {
      "id": 6199,
      "verse_key": "107:2",
      "text_imlaei_simple": "فذلك الذي يدع اليتيم"
    },
    {
      "id": 6200,
      "verse_key": "107:3",
      "text_imlaei_simple": "ولا يحض على طعام المسكين"
    },
    { "id": 6201, "verse_key": "107:4", "text_imlaei_simple": "فويل للمصلين" },
    {
      "id": 6202,
      "verse_key": "107:5",
      "text_imlaei_simple": "الذين هم عن صلاتهم ساهون"
    },
    {
      "id": 6203,
      "verse_key": "107:6",
      "text_imlaei_simple": "الذين هم يراءون"
    },
    {
      "id": 6204,
      "verse_key": "107:7",
      "text_imlaei_simple": "ويمنعون الماعون"
    }
  ],
  "meta": { "filters": { "chapter_number": "107" } }
}
