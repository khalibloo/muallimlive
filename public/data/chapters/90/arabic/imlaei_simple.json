{
  "verses": [
    {
      "id": 6024,
      "verse_key": "90:1",
      "text_imlaei_simple": "لا اقسم بهذا البلد"
    },
    {
      "id": 6025,
      "verse_key": "90:2",
      "text_imlaei_simple": "وانت حل بهذا البلد"
    },
    { "id": 6026, "verse_key": "90:3", "text_imlaei_simple": "ووالد وما ولد" },
    {
      "id": 6027,
      "verse_key": "90:4",
      "text_imlaei_simple": "لقد خلقنا الانسان في كبد"
    },
    {
      "id": 6028,
      "verse_key": "90:5",
      "text_imlaei_simple": "ايحسب ان لن يقدر عليه احد"
    },
    {
      "id": 6029,
      "verse_key": "90:6",
      "text_imlaei_simple": "يقول اهلكت مالا لبدا"
    },
    {
      "id": 6030,
      "verse_key": "90:7",
      "text_imlaei_simple": "ايحسب ان لم يره احد"
    },
    {
      "id": 6031,
      "verse_key": "90:8",
      "text_imlaei_simple": "الم نجعل له عينين"
    },
    { "id": 6032, "verse_key": "90:9", "text_imlaei_simple": "ولسانا وشفتين" },
    {
      "id": 6033,
      "verse_key": "90:10",
      "text_imlaei_simple": "وهديناه النجدين"
    },
    {
      "id": 6034,
      "verse_key": "90:11",
      "text_imlaei_simple": "فلا اقتحم العقبة"
    },
    {
      "id": 6035,
      "verse_key": "90:12",
      "text_imlaei_simple": "وما ادراك ما العقبة"
    },
    { "id": 6036, "verse_key": "90:13", "text_imlaei_simple": "فك رقبة" },
    {
      "id": 6037,
      "verse_key": "90:14",
      "text_imlaei_simple": "او اطعام في يوم ذي مسغبة"
    },
    {
      "id": 6038,
      "verse_key": "90:15",
      "text_imlaei_simple": "يتيما ذا مقربة"
    },
    {
      "id": 6039,
      "verse_key": "90:16",
      "text_imlaei_simple": "او مسكينا ذا متربة"
    },
    {
      "id": 6040,
      "verse_key": "90:17",
      "text_imlaei_simple": "ثم كان من الذين امنوا وتواصوا بالصبر وتواصوا بالمرحمة"
    },
    {
      "id": 6041,
      "verse_key": "90:18",
      "text_imlaei_simple": "اولئك اصحاب الميمنة"
    },
    {
      "id": 6042,
      "verse_key": "90:19",
      "text_imlaei_simple": "والذين كفروا باياتنا هم اصحاب المشامة"
    },
    {
      "id": 6043,
      "verse_key": "90:20",
      "text_imlaei_simple": "عليهم نار مؤصدة"
    }
  ],
  "meta": { "filters": { "chapter_number": "90" } }
}
