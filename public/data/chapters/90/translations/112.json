{
  "translations": [
    { "resource_id": 112, "text": "Yemin olsun, bu şehre!" },
    { "resource_id": 112, "text": "Sen de bu beldede bulunuyorsun." },
    { "resource_id": 112, "text": "Babaya ve doğan çocuğa da." },
    {
      "resource_id": 112,
      "text": "Ki şüphesiz biz insanı (dünyada) bir zorluk içinde yarattık."
    },
    {
      "resource_id": 112,
      "text": "Kimsenin kendisine güç yetiremeyeceğini mi sanıyor?"
    },
    { "resource_id": 112, "text": "Pek çok mal harcadım, diyor." },
    {
      "resource_id": 112,
      "text": "Kendisini kimsenin görmediğini mi sanıyor?"
    },
    { "resource_id": 112, "text": "Ona iki göz vermedik mi?" },
    { "resource_id": 112, "text": "Bir dil, iki dudak," },
    { "resource_id": 112, "text": "Ona iki de yol gösterdik." },
    { "resource_id": 112, "text": "O zor geçidi aşamadı." },
    {
      "resource_id": 112,
      "text": "Zor geçidin ne olduğunu sana bildiren nedir?"
    },
    { "resource_id": 112, "text": "Köle azat etmektir." },
    { "resource_id": 112, "text": "Veya açlık gününde doyurmaktır." },
    { "resource_id": 112, "text": "Yakınlığı olan bir yetimi," },
    { "resource_id": 112, "text": "Veya sürünen bir düşkünü." },
    {
      "resource_id": 112,
      "text": "Bir de iman edip, sabrı ve merhameti birbirine tavsiye edenlerden olmaktır."
    },
    { "resource_id": 112, "text": "İşte bunlar sağdakilerdir." },
    {
      "resource_id": 112,
      "text": "Ayetlerimize küfredenler ise işte onlar soldakilerdir."
    },
    {
      "resource_id": 112,
      "text": "Üzerlerine apanmış/kilitlenmiş bir ateş vardır."
    }
  ],
