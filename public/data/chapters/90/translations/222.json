{
  "translations": [
    { "resource_id": 222, "text": "Жоқ,осы(Мекке) қаласына ант етемін;" },
    { "resource_id": 222, "text": "(Мұхаммед Ғ.С) сен осы қалада отырасың." },
    {
      "resource_id": 222,
      "text": "Әкеге,балаға (Адам Ғ.С мен балаларына) серт!"
    },
    { "resource_id": 222, "text": "Расында адамзатты әбігер жараттық." },
    {
      "resource_id": 222,
      "text": "Өзіне біреудің күші жетпейді деп ойлай ма?"
    },
    { "resource_id": 222, "text": "«Толып жатқан мал ысырап еттім» дейді." },
    { "resource_id": 222, "text": "Оны біреу көрмеді дей ойлай ма?" },
    { "resource_id": 222, "text": "Оған екі көз жаратпадық па?" },
    { "resource_id": 222, "text": "Әрі бір тіл,екі ерін ?" },
    { "resource_id": 222, "text": "Оған екі жол көрсетпедік пе?" },
    { "resource_id": 222, "text": "Сонда ол,кедергіні асуға тырыспады." },
    { "resource_id": 222, "text": "Кедергінің не екенін қайдан білесің?" },
    { "resource_id": 222, "text": "Ол;құл азат ету," },
    { "resource_id": 222, "text": "Немесе ашаршылық күнде тамақтандыру;" },
    { "resource_id": 222, "text": "Жақындағы болған бір жетімді," },
    { "resource_id": 222, "text": "Немесе шаң топырақтағы сорлыны." },
    {
      "resource_id": 222,
      "text": "Сосын иман келтіріп,бір-біріне сабыр меңзегендерден және өзара мейірімділік кеңесін бергендерден болу."
    },
    { "resource_id": 222, "text": "Міне осылар; оңшылдар." },
    {
      "resource_id": 222,
      "text": "Сондай аяттарымызға қарсы шыққандар,олар; солшылдар."
    },
    { "resource_id": 222, "text": "Олар тозаққа қамалады." }
  ],
