{
  "translations": [
    { "resource_id": 49, "text": "Naapa kwa Mji huu!" },
    { "resource_id": 49, "text": "Nawe unaukaa Mji huu." },
    { "resource_id": 49, "text": "Na naapa kwa mzazi na alicho kizaa." },
    { "resource_id": 49, "text": "Hakika tumemuumba mtu katika taabu." },
    { "resource_id": 49, "text": "Ati anadhani hapana yeyote ataye muweza?" },
    { "resource_id": 49, "text": "Anasema: Nimeteketeza chungu ya mali." },
    {
      "resource_id": 49,
      "text": "Ati anadhani ya kuwa hapana yeyote anaye mwona?"
    },
    { "resource_id": 49, "text": "Kwani hatukumpa macho mawili?" },
    { "resource_id": 49, "text": "Na ulimi, na midomo miwili?" },
    { "resource_id": 49, "text": "Na tukambainishia zote njia mbili?" },
    {
      "resource_id": 49,
      "text": "Lakini hakujitoma kwenye njia ya vikwazo vya milimani."
    },
    {
      "resource_id": 49,
      "text": "Na nini kitakujuvya ni nini njia ya vikwazo vya milimani?"
    },
    { "resource_id": 49, "text": "Kumkomboa mtumwa;" },
    { "resource_id": 49, "text": "Au kumlisha siku ya njaa" },
    { "resource_id": 49, "text": "Yatima aliye jamaa," },
    { "resource_id": 49, "text": "Au masikini aliye vumbini." },
    {
      "resource_id": 49,
      "text": "Tena awe miongoni mwa walio amini na wakausiana kusubiri na wakausiana kuhurumiana."
    },
    { "resource_id": 49, "text": "Hao ndio watu wa kheri wa kuliani." },
    {
      "resource_id": 49,
      "text": "Lakini walio zikataa Ishara zetu, hao ndio watu wa shari wa kushotoni."
    },
    {
      "resource_id": 49,
      "text": "Juu yao utakuwa Moto ulio fungiwa kila upande."
    }
  ],
