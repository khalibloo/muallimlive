{
  "verses": [
    { "id": 2933, "verse_key": "26:1", "text_imlaei_simple": "طسم" },
    {
      "id": 2934,
      "verse_key": "26:2",
      "text_imlaei_simple": "تلك ايات الكتاب المبين"
    },
    {
      "id": 2935,
      "verse_key": "26:3",
      "text_imlaei_simple": "لعلك باخع نفسك الا يكونوا مؤمنين"
    },
    {
      "id": 2936,
      "verse_key": "26:4",
      "text_imlaei_simple": "ان نشا ننزل عليهم من السماء اية فظلت اعناقهم لها خاضعين"
    },
    {
      "id": 2937,
      "verse_key": "26:5",
      "text_imlaei_simple": "وما ياتيهم من ذكر من الرحمن محدث الا كانوا عنه معرضين"
    },
    {
      "id": 2938,
      "verse_key": "26:6",
      "text_imlaei_simple": "فقد كذبوا فسياتيهم انباء ما كانوا به يستهزئون"
    },
    {
      "id": 2939,
      "verse_key": "26:7",
      "text_imlaei_simple": "اولم يروا الى الارض كم انبتنا فيها من كل زوج كريم"
    },
    {
      "id": 2940,
      "verse_key": "26:8",
      "text_imlaei_simple": "ان في ذلك لاية وما كان اكثرهم مؤمنين"
    },
    {
      "id": 2941,
      "verse_key": "26:9",
      "text_imlaei_simple": "وان ربك لهو العزيز الرحيم"
    },
    {
      "id": 2942,
      "verse_key": "26:10",
      "text_imlaei_simple": "واذ نادى ربك موسى ان ائت القوم الظالمين"
    },
    {
      "id": 2943,
      "verse_key": "26:11",
      "text_imlaei_simple": "قوم فرعون الا يتقون"
    },
    {
      "id": 2944,
      "verse_key": "26:12",
      "text_imlaei_simple": "قال رب اني اخاف ان يكذبون"
    },
    {
      "id": 2945,
      "verse_key": "26:13",
      "text_imlaei_simple": "ويضيق صدري ولا ينطلق لساني فارسل الى هارون"
    },
    {
      "id": 2946,
      "verse_key": "26:14",
      "text_imlaei_simple": "ولهم علي ذنب فاخاف ان يقتلون"
    },
    {
      "id": 2947,
      "verse_key": "26:15",
      "text_imlaei_simple": "قال كلا فاذهبا باياتنا انا معكم مستمعون"
    },
    {
      "id": 2948,
      "verse_key": "26:16",
      "text_imlaei_simple": "فاتيا فرعون فقولا انا رسول رب العالمين"
    },
    {
      "id": 2949,
      "verse_key": "26:17",
      "text_imlaei_simple": "ان ارسل معنا بني اسرائيل"
    },
    {
      "id": 2950,
      "verse_key": "26:18",
      "text_imlaei_simple": "قال الم نربك فينا وليدا ولبثت فينا من عمرك سنين"
    },
    {
      "id": 2951,
      "verse_key": "26:19",
      "text_imlaei_simple": "وفعلت فعلتك التي فعلت وانت من الكافرين"
    },
    {
      "id": 2952,
      "verse_key": "26:20",
      "text_imlaei_simple": "قال فعلتها اذا وانا من الضالين"
    },
    {
      "id": 2953,
      "verse_key": "26:21",
      "text_imlaei_simple": "ففررت منكم لما خفتكم فوهب لي ربي حكما وجعلني من المرسلين"
    },
    {
      "id": 2954,
      "verse_key": "26:22",
      "text_imlaei_simple": "وتلك نعمة تمنها علي ان عبدت بني اسرائيل"
    },
    {
      "id": 2955,
      "verse_key": "26:23",
      "text_imlaei_simple": "قال فرعون وما رب العالمين"
    },
    {
      "id": 2956,
      "verse_key": "26:24",
      "text_imlaei_simple": "قال رب السماوات والارض وما بينهما ان كنتم موقنين"
    },
    {
      "id": 2957,
      "verse_key": "26:25",
      "text_imlaei_simple": "قال لمن حوله الا تستمعون"
    },
    {
      "id": 2958,
      "verse_key": "26:26",
      "text_imlaei_simple": "قال ربكم ورب ابائكم الاولين"
    },
    {
      "id": 2959,
      "verse_key": "26:27",
      "text_imlaei_simple": "قال ان رسولكم الذي ارسل اليكم لمجنون"
    },
    {
      "id": 2960,
      "verse_key": "26:28",
      "text_imlaei_simple": "قال رب المشرق والمغرب وما بينهما ان كنتم تعقلون"
    },
    {
      "id": 2961,
      "verse_key": "26:29",
      "text_imlaei_simple": "قال لئن اتخذت الها غيري لاجعلنك من المسجونين"
    },
    {
      "id": 2962,
      "verse_key": "26:30",
      "text_imlaei_simple": "قال اولو جئتك بشيء مبين"
    },
    {
      "id": 2963,
      "verse_key": "26:31",
      "text_imlaei_simple": "قال فات به ان كنت من الصادقين"
    },
    {
      "id": 2964,
      "verse_key": "26:32",
      "text_imlaei_simple": "فالقى عصاه فاذا هي ثعبان مبين"
    },
    {
      "id": 2965,
      "verse_key": "26:33",
      "text_imlaei_simple": "ونزع يده فاذا هي بيضاء للناظرين"
    },
    {
      "id": 2966,
      "verse_key": "26:34",
      "text_imlaei_simple": "قال للملا حوله ان هذا لساحر عليم"
    },
    {
      "id": 2967,
      "verse_key": "26:35",
      "text_imlaei_simple": "يريد ان يخرجكم من ارضكم بسحره فماذا تامرون"
    },
    {
      "id": 2968,
      "verse_key": "26:36",
      "text_imlaei_simple": "قالوا ارجه واخاه وابعث في المدائن حاشرين"
    },
    {
      "id": 2969,
      "verse_key": "26:37",
      "text_imlaei_simple": "ياتوك بكل سحار عليم"
    },
    {
      "id": 2970,
      "verse_key": "26:38",
      "text_imlaei_simple": "فجمع السحرة لميقات يوم معلوم"
    },
    {
      "id": 2971,
      "verse_key": "26:39",
      "text_imlaei_simple": "وقيل للناس هل انتم مجتمعون"
    },
    {
      "id": 2972,
      "verse_key": "26:40",
      "text_imlaei_simple": "لعلنا نتبع السحرة ان كانوا هم الغالبين"
    },
    {
      "id": 2973,
      "verse_key": "26:41",
      "text_imlaei_simple": "فلما جاء السحرة قالوا لفرعون ائن لنا لاجرا ان كنا نحن الغالبين"
    },
    {
      "id": 2974,
      "verse_key": "26:42",
      "text_imlaei_simple": "قال نعم وانكم اذا لمن المقربين"
    },
    {
      "id": 2975,
      "verse_key": "26:43",
      "text_imlaei_simple": "قال لهم موسى القوا ما انتم ملقون"
    },
    {
      "id": 2976,
      "verse_key": "26:44",
      "text_imlaei_simple": "فالقوا حبالهم وعصيهم وقالوا بعزة فرعون انا لنحن الغالبون"
    },
    {
      "id": 2977,
      "verse_key": "26:45",
      "text_imlaei_simple": "فالقى موسى عصاه فاذا هي تلقف ما يافكون"
    },
    {
      "id": 2978,
      "verse_key": "26:46",
      "text_imlaei_simple": "فالقي السحرة ساجدين"
    },
    {
      "id": 2979,
      "verse_key": "26:47",
      "text_imlaei_simple": "قالوا امنا برب العالمين"
    },
    {
      "id": 2980,
      "verse_key": "26:48",
      "text_imlaei_simple": "رب موسى وهارون"
    },
    {
      "id": 2981,
      "verse_key": "26:49",
      "text_imlaei_simple": "قال امنتم له قبل ان اذن لكم انه لكبيركم الذي علمكم السحر فلسوف تعلمون لاقطعن ايديكم وارجلكم من خلاف ولاصلبنكم اجمعين"
    },
    {
      "id": 2982,
      "verse_key": "26:50",
      "text_imlaei_simple": "قالوا لا ضير انا الى ربنا منقلبون"
    },
    {
      "id": 2983,
      "verse_key": "26:51",
      "text_imlaei_simple": "انا نطمع ان يغفر لنا ربنا خطايانا ان كنا اول المؤمنين"
    },
    {
      "id": 2984,
      "verse_key": "26:52",
      "text_imlaei_simple": "واوحينا الى موسى ان اسر بعبادي انكم متبعون"
    },
    {
      "id": 2985,
      "verse_key": "26:53",
      "text_imlaei_simple": "فارسل فرعون في المدائن حاشرين"
    },
    {
      "id": 2986,
      "verse_key": "26:54",
      "text_imlaei_simple": "ان هؤلاء لشرذمة قليلون"
    },
    {
      "id": 2987,
      "verse_key": "26:55",
      "text_imlaei_simple": "وانهم لنا لغائظون"
    },
    {
      "id": 2988,
      "verse_key": "26:56",
      "text_imlaei_simple": "وانا لجميع حاذرون"
    },
    {
      "id": 2989,
      "verse_key": "26:57",
      "text_imlaei_simple": "فاخرجناهم من جنات وعيون"
    },
    {
      "id": 2990,
      "verse_key": "26:58",
      "text_imlaei_simple": "وكنوز ومقام كريم"
    },
    {
      "id": 2991,
      "verse_key": "26:59",
      "text_imlaei_simple": "كذلك واورثناها بني اسرائيل"
    },
    {
      "id": 2992,
      "verse_key": "26:60",
      "text_imlaei_simple": "فاتبعوهم مشرقين"
    },
    {
      "id": 2993,
      "verse_key": "26:61",
      "text_imlaei_simple": "فلما تراءى الجمعان قال اصحاب موسى انا لمدركون"
    },
    {
      "id": 2994,
      "verse_key": "26:62",
      "text_imlaei_simple": "قال كلا ان معي ربي سيهدين"
    },
    {
      "id": 2995,
      "verse_key": "26:63",
      "text_imlaei_simple": "فاوحينا الى موسى ان اضرب بعصاك البحر فانفلق فكان كل فرق كالطود العظيم"
    },
    {
      "id": 2996,
      "verse_key": "26:64",
      "text_imlaei_simple": "وازلفنا ثم الاخرين"
    },
    {
      "id": 2997,
      "verse_key": "26:65",
      "text_imlaei_simple": "وانجينا موسى ومن معه اجمعين"
    },
    {
      "id": 2998,
      "verse_key": "26:66",
      "text_imlaei_simple": "ثم اغرقنا الاخرين"
    },
    {
      "id": 2999,
      "verse_key": "26:67",
      "text_imlaei_simple": "ان في ذلك لاية وما كان اكثرهم مؤمنين"
    },
    {
      "id": 3000,
      "verse_key": "26:68",
      "text_imlaei_simple": "وان ربك لهو العزيز الرحيم"
    },
    {
      "id": 3001,
      "verse_key": "26:69",
      "text_imlaei_simple": "واتل عليهم نبا ابراهيم"
    },
    {
      "id": 3002,
      "verse_key": "26:70",
      "text_imlaei_simple": "اذ قال لابيه وقومه ما تعبدون"
    },
    {
      "id": 3003,
      "verse_key": "26:71",
      "text_imlaei_simple": "قالوا نعبد اصناما فنظل لها عاكفين"
    },
    {
      "id": 3004,
      "verse_key": "26:72",
      "text_imlaei_simple": "قال هل يسمعونكم اذ تدعون"
    },
    {
      "id": 3005,
      "verse_key": "26:73",
      "text_imlaei_simple": "او ينفعونكم او يضرون"
    },
    {
      "id": 3006,
      "verse_key": "26:74",
      "text_imlaei_simple": "قالوا بل وجدنا اباءنا كذلك يفعلون"
    },
    {
      "id": 3007,
      "verse_key": "26:75",
      "text_imlaei_simple": "قال افرايتم ما كنتم تعبدون"
    },
    {
      "id": 3008,
      "verse_key": "26:76",
      "text_imlaei_simple": "انتم واباؤكم الاقدمون"
    },
    {
      "id": 3009,
      "verse_key": "26:77",
      "text_imlaei_simple": "فانهم عدو لي الا رب العالمين"
    },
    {
      "id": 3010,
      "verse_key": "26:78",
      "text_imlaei_simple": "الذي خلقني فهو يهدين"
    },
    {
      "id": 3011,
      "verse_key": "26:79",
      "text_imlaei_simple": "والذي هو يطعمني ويسقين"
    },
    {
      "id": 3012,
      "verse_key": "26:80",
      "text_imlaei_simple": "واذا مرضت فهو يشفين"
    },
    {
      "id": 3013,
      "verse_key": "26:81",
      "text_imlaei_simple": "والذي يميتني ثم يحيين"
    },
    {
      "id": 3014,
      "verse_key": "26:82",
      "text_imlaei_simple": "والذي اطمع ان يغفر لي خطيئتي يوم الدين"
    },
    {
      "id": 3015,
      "verse_key": "26:83",
      "text_imlaei_simple": "رب هب لي حكما والحقني بالصالحين"
    },
    {
      "id": 3016,
      "verse_key": "26:84",
      "text_imlaei_simple": "واجعل لي لسان صدق في الاخرين"
    },
    {
      "id": 3017,
      "verse_key": "26:85",
      "text_imlaei_simple": "واجعلني من ورثة جنة النعيم"
    },
    {
      "id": 3018,
      "verse_key": "26:86",
      "text_imlaei_simple": "واغفر لابي انه كان من الضالين"
    },
    {
      "id": 3019,
      "verse_key": "26:87",
      "text_imlaei_simple": "ولا تخزني يوم يبعثون"
    },
    {
      "id": 3020,
      "verse_key": "26:88",
      "text_imlaei_simple": "يوم لا ينفع مال ولا بنون"
    },
    {
      "id": 3021,
      "verse_key": "26:89",
      "text_imlaei_simple": "الا من اتى الله بقلب سليم"
    },
    {
      "id": 3022,
      "verse_key": "26:90",
      "text_imlaei_simple": "وازلفت الجنة للمتقين"
    },
    {
      "id": 3023,
      "verse_key": "26:91",
      "text_imlaei_simple": "وبرزت الجحيم للغاوين"
    },
    {
      "id": 3024,
      "verse_key": "26:92",
      "text_imlaei_simple": "وقيل لهم اين ما كنتم تعبدون"
    },
    {
      "id": 3025,
      "verse_key": "26:93",
      "text_imlaei_simple": "من دون الله هل ينصرونكم او ينتصرون"
    },
    {
      "id": 3026,
      "verse_key": "26:94",
      "text_imlaei_simple": "فكبكبوا فيها هم والغاوون"
    },
    {
      "id": 3027,
      "verse_key": "26:95",
      "text_imlaei_simple": "وجنود ابليس اجمعون"
    },
    {
      "id": 3028,
      "verse_key": "26:96",
      "text_imlaei_simple": "قالوا وهم فيها يختصمون"
    },
    {
      "id": 3029,
      "verse_key": "26:97",
      "text_imlaei_simple": "تالله ان كنا لفي ضلال مبين"
    },
    {
      "id": 3030,
      "verse_key": "26:98",
      "text_imlaei_simple": "اذ نسويكم برب العالمين"
    },
    {
      "id": 3031,
      "verse_key": "26:99",
      "text_imlaei_simple": "وما اضلنا الا المجرمون"
    },
    {
      "id": 3032,
      "verse_key": "26:100",
      "text_imlaei_simple": "فما لنا من شافعين"
    },
    {
      "id": 3033,
      "verse_key": "26:101",
      "text_imlaei_simple": "ولا صديق حميم"
    },
    {
      "id": 3034,
      "verse_key": "26:102",
      "text_imlaei_simple": "فلو ان لنا كرة فنكون من المؤمنين"
    },
    {
      "id": 3035,
      "verse_key": "26:103",
      "text_imlaei_simple": "ان في ذلك لاية وما كان اكثرهم مؤمنين"
    },
    {
      "id": 3036,
      "verse_key": "26:104",
      "text_imlaei_simple": "وان ربك لهو العزيز الرحيم"
    },
    {
      "id": 3037,
      "verse_key": "26:105",
      "text_imlaei_simple": "كذبت قوم نوح المرسلين"
    },
    {
      "id": 3038,
      "verse_key": "26:106",
      "text_imlaei_simple": "اذ قال لهم اخوهم نوح الا تتقون"
    },
    {
      "id": 3039,
      "verse_key": "26:107",
      "text_imlaei_simple": "اني لكم رسول امين"
    },
    {
      "id": 3040,
      "verse_key": "26:108",
      "text_imlaei_simple": "فاتقوا الله واطيعون"
    },
    {
      "id": 3041,
      "verse_key": "26:109",
      "text_imlaei_simple": "وما اسالكم عليه من اجر ان اجري الا على رب العالمين"
    },
    {
      "id": 3042,
      "verse_key": "26:110",
      "text_imlaei_simple": "فاتقوا الله واطيعون"
    },
    {
      "id": 3043,
      "verse_key": "26:111",
      "text_imlaei_simple": "قالوا انؤمن لك واتبعك الارذلون"
    },
    {
      "id": 3044,
      "verse_key": "26:112",
      "text_imlaei_simple": "قال وما علمي بما كانوا يعملون"
    },
    {
      "id": 3045,
      "verse_key": "26:113",
      "text_imlaei_simple": "ان حسابهم الا على ربي لو تشعرون"
    },
    {
      "id": 3046,
      "verse_key": "26:114",
      "text_imlaei_simple": "وما انا بطارد المؤمنين"
    },
    {
      "id": 3047,
      "verse_key": "26:115",
      "text_imlaei_simple": "ان انا الا نذير مبين"
    },
    {
      "id": 3048,
      "verse_key": "26:116",
      "text_imlaei_simple": "قالوا لئن لم تنته يا نوح لتكونن من المرجومين"
    },
    {
      "id": 3049,
      "verse_key": "26:117",
      "text_imlaei_simple": "قال رب ان قومي كذبون"
    },
    {
      "id": 3050,
      "verse_key": "26:118",
      "text_imlaei_simple": "فافتح بيني وبينهم فتحا ونجني ومن معي من المؤمنين"
    },
    {
      "id": 3051,
      "verse_key": "26:119",
      "text_imlaei_simple": "فانجيناه ومن معه في الفلك المشحون"
    },
    {
      "id": 3052,
      "verse_key": "26:120",
      "text_imlaei_simple": "ثم اغرقنا بعد الباقين"
    },
    {
      "id": 3053,
      "verse_key": "26:121",
      "text_imlaei_simple": "ان في ذلك لاية وما كان اكثرهم مؤمنين"
    },
    {
      "id": 3054,
      "verse_key": "26:122",
      "text_imlaei_simple": "وان ربك لهو العزيز الرحيم"
    },
    {
      "id": 3055,
      "verse_key": "26:123",
      "text_imlaei_simple": "كذبت عاد المرسلين"
    },
    {
      "id": 3056,
      "verse_key": "26:124",
      "text_imlaei_simple": "اذ قال لهم اخوهم هود الا تتقون"
    },
    {
      "id": 3057,
      "verse_key": "26:125",
      "text_imlaei_simple": "اني لكم رسول امين"
    },
    {
      "id": 3058,
      "verse_key": "26:126",
      "text_imlaei_simple": "فاتقوا الله واطيعون"
    },
    {
      "id": 3059,
      "verse_key": "26:127",
      "text_imlaei_simple": "وما اسالكم عليه من اجر ان اجري الا على رب العالمين"
    },
    {
      "id": 3060,
      "verse_key": "26:128",
      "text_imlaei_simple": "اتبنون بكل ريع اية تعبثون"
    },
    {
      "id": 3061,
      "verse_key": "26:129",
      "text_imlaei_simple": "وتتخذون مصانع لعلكم تخلدون"
    },
    {
      "id": 3062,
      "verse_key": "26:130",
      "text_imlaei_simple": "واذا بطشتم بطشتم جبارين"
    },
    {
      "id": 3063,
      "verse_key": "26:131",
      "text_imlaei_simple": "فاتقوا الله واطيعون"
    },
    {
      "id": 3064,
      "verse_key": "26:132",
      "text_imlaei_simple": "واتقوا الذي امدكم بما تعلمون"
    },
    {
      "id": 3065,
      "verse_key": "26:133",
      "text_imlaei_simple": "امدكم بانعام وبنين"
    },
    { "id": 3066, "verse_key": "26:134", "text_imlaei_simple": "وجنات وعيون" },
    {
      "id": 3067,
      "verse_key": "26:135",
      "text_imlaei_simple": "اني اخاف عليكم عذاب يوم عظيم"
    },
    {
      "id": 3068,
      "verse_key": "26:136",
      "text_imlaei_simple": "قالوا سواء علينا اوعظت ام لم تكن من الواعظين"
    },
    {
      "id": 3069,
      "verse_key": "26:137",
      "text_imlaei_simple": "ان هذا الا خلق الاولين"
    },
    {
      "id": 3070,
      "verse_key": "26:138",
      "text_imlaei_simple": "وما نحن بمعذبين"
    },
    {
      "id": 3071,
      "verse_key": "26:139",
      "text_imlaei_simple": "فكذبوه فاهلكناهم ان في ذلك لاية وما كان اكثرهم مؤمنين"
    },
    {
      "id": 3072,
      "verse_key": "26:140",
      "text_imlaei_simple": "وان ربك لهو العزيز الرحيم"
    },
    {
      "id": 3073,
      "verse_key": "26:141",
      "text_imlaei_simple": "كذبت ثمود المرسلين"
    },
    {
      "id": 3074,
      "verse_key": "26:142",
      "text_imlaei_simple": "اذ قال لهم اخوهم صالح الا تتقون"
    },
    {
      "id": 3075,
      "verse_key": "26:143",
      "text_imlaei_simple": "اني لكم رسول امين"
    },
    {
      "id": 3076,
      "verse_key": "26:144",
      "text_imlaei_simple": "فاتقوا الله واطيعون"
    },
    {
      "id": 3077,
      "verse_key": "26:145",
      "text_imlaei_simple": "وما اسالكم عليه من اجر ان اجري الا على رب العالمين"
    },
    {
      "id": 3078,
      "verse_key": "26:146",
      "text_imlaei_simple": "اتتركون في ما هاهنا امنين"
    },
    {
      "id": 3079,
      "verse_key": "26:147",
      "text_imlaei_simple": "في جنات وعيون"
    },
    {
      "id": 3080,
      "verse_key": "26:148",
      "text_imlaei_simple": "وزروع ونخل طلعها هضيم"
    },
    {
      "id": 3081,
      "verse_key": "26:149",
      "text_imlaei_simple": "وتنحتون من الجبال بيوتا فارهين"
    },
    {
      "id": 3082,
      "verse_key": "26:150",
      "text_imlaei_simple": "فاتقوا الله واطيعون"
    },
    {
      "id": 3083,
      "verse_key": "26:151",
      "text_imlaei_simple": "ولا تطيعوا امر المسرفين"
    },
    {
      "id": 3084,
      "verse_key": "26:152",
      "text_imlaei_simple": "الذين يفسدون في الارض ولا يصلحون"
    },
    {
      "id": 3085,
      "verse_key": "26:153",
      "text_imlaei_simple": "قالوا انما انت من المسحرين"
    },
    {
      "id": 3086,
      "verse_key": "26:154",
      "text_imlaei_simple": "ما انت الا بشر مثلنا فات باية ان كنت من الصادقين"
    },
    {
      "id": 3087,
      "verse_key": "26:155",
      "text_imlaei_simple": "قال هذه ناقة لها شرب ولكم شرب يوم معلوم"
    },
    {
      "id": 3088,
      "verse_key": "26:156",
      "text_imlaei_simple": "ولا تمسوها بسوء فياخذكم عذاب يوم عظيم"
    },
    {
      "id": 3089,
      "verse_key": "26:157",
      "text_imlaei_simple": "فعقروها فاصبحوا نادمين"
    },
    {
      "id": 3090,
      "verse_key": "26:158",
      "text_imlaei_simple": "فاخذهم العذاب ان في ذلك لاية وما كان اكثرهم مؤمنين"
    },
    {
      "id": 3091,
      "verse_key": "26:159",
      "text_imlaei_simple": "وان ربك لهو العزيز الرحيم"
    },
    {
      "id": 3092,
      "verse_key": "26:160",
      "text_imlaei_simple": "كذبت قوم لوط المرسلين"
    },
    {
      "id": 3093,
      "verse_key": "26:161",
      "text_imlaei_simple": "اذ قال لهم اخوهم لوط الا تتقون"
    },
    {
      "id": 3094,
      "verse_key": "26:162",
      "text_imlaei_simple": "اني لكم رسول امين"
    },
    {
      "id": 3095,
      "verse_key": "26:163",
      "text_imlaei_simple": "فاتقوا الله واطيعون"
    },
    {
      "id": 3096,
      "verse_key": "26:164",
      "text_imlaei_simple": "وما اسالكم عليه من اجر ان اجري الا على رب العالمين"
    },
    {
      "id": 3097,
      "verse_key": "26:165",
      "text_imlaei_simple": "اتاتون الذكران من العالمين"
    },
    {
      "id": 3098,
      "verse_key": "26:166",
      "text_imlaei_simple": "وتذرون ما خلق لكم ربكم من ازواجكم بل انتم قوم عادون"
    },
    {
      "id": 3099,
      "verse_key": "26:167",
      "text_imlaei_simple": "قالوا لئن لم تنته يا لوط لتكونن من المخرجين"
    },
    {
      "id": 3100,
      "verse_key": "26:168",
      "text_imlaei_simple": "قال اني لعملكم من القالين"
    },
    {
      "id": 3101,
      "verse_key": "26:169",
      "text_imlaei_simple": "رب نجني واهلي مما يعملون"
    },
    {
      "id": 3102,
      "verse_key": "26:170",
      "text_imlaei_simple": "فنجيناه واهله اجمعين"
    },
    {
      "id": 3103,
      "verse_key": "26:171",
      "text_imlaei_simple": "الا عجوزا في الغابرين"
    },
    {
      "id": 3104,
      "verse_key": "26:172",
      "text_imlaei_simple": "ثم دمرنا الاخرين"
    },
    {
      "id": 3105,
      "verse_key": "26:173",
      "text_imlaei_simple": "وامطرنا عليهم مطرا فساء مطر المنذرين"
    },
    {
      "id": 3106,
      "verse_key": "26:174",
      "text_imlaei_simple": "ان في ذلك لاية وما كان اكثرهم مؤمنين"
    },
    {
      "id": 3107,
      "verse_key": "26:175",
      "text_imlaei_simple": "وان ربك لهو العزيز الرحيم"
    },
    {
      "id": 3108,
      "verse_key": "26:176",
      "text_imlaei_simple": "كذب اصحاب الايكة المرسلين"
    },
    {
      "id": 3109,
      "verse_key": "26:177",
      "text_imlaei_simple": "اذ قال لهم شعيب الا تتقون"
    },
    {
      "id": 3110,
      "verse_key": "26:178",
      "text_imlaei_simple": "اني لكم رسول امين"
    },
    {
      "id": 3111,
      "verse_key": "26:179",
      "text_imlaei_simple": "فاتقوا الله واطيعون"
    },
    {
      "id": 3112,
      "verse_key": "26:180",
      "text_imlaei_simple": "وما اسالكم عليه من اجر ان اجري الا على رب العالمين"
    },
    {
      "id": 3113,
      "verse_key": "26:181",
      "text_imlaei_simple": "اوفوا الكيل ولا تكونوا من المخسرين"
    },
    {
      "id": 3114,
      "verse_key": "26:182",
      "text_imlaei_simple": "وزنوا بالقسطاس المستقيم"
    },
    {
      "id": 3115,
      "verse_key": "26:183",
      "text_imlaei_simple": "ولا تبخسوا الناس اشياءهم ولا تعثوا في الارض مفسدين"
    },
    {
      "id": 3116,
      "verse_key": "26:184",
      "text_imlaei_simple": "واتقوا الذي خلقكم والجبلة الاولين"
    },
    {
      "id": 3117,
      "verse_key": "26:185",
      "text_imlaei_simple": "قالوا انما انت من المسحرين"
    },
    {
      "id": 3118,
      "verse_key": "26:186",
      "text_imlaei_simple": "وما انت الا بشر مثلنا وان نظنك لمن الكاذبين"
    },
    {
      "id": 3119,
      "verse_key": "26:187",
      "text_imlaei_simple": "فاسقط علينا كسفا من السماء ان كنت من الصادقين"
    },
    {
      "id": 3120,
      "verse_key": "26:188",
      "text_imlaei_simple": "قال ربي اعلم بما تعملون"
    },
    {
      "id": 3121,
      "verse_key": "26:189",
      "text_imlaei_simple": "فكذبوه فاخذهم عذاب يوم الظلة انه كان عذاب يوم عظيم"
    },
    {
      "id": 3122,
      "verse_key": "26:190",
      "text_imlaei_simple": "ان في ذلك لاية وما كان اكثرهم مؤمنين"
    },
    {
      "id": 3123,
      "verse_key": "26:191",
      "text_imlaei_simple": "وان ربك لهو العزيز الرحيم"
    },
    {
      "id": 3124,
      "verse_key": "26:192",
      "text_imlaei_simple": "وانه لتنزيل رب العالمين"
    },
    {
      "id": 3125,
      "verse_key": "26:193",
      "text_imlaei_simple": "نزل به الروح الامين"
    },
    {
      "id": 3126,
      "verse_key": "26:194",
      "text_imlaei_simple": "على قلبك لتكون من المنذرين"
    },
    {
      "id": 3127,
      "verse_key": "26:195",
      "text_imlaei_simple": "بلسان عربي مبين"
    },
    {
      "id": 3128,
      "verse_key": "26:196",
      "text_imlaei_simple": "وانه لفي زبر الاولين"
    },
    {
      "id": 3129,
      "verse_key": "26:197",
      "text_imlaei_simple": "اولم يكن لهم اية ان يعلمه علماء بني اسرائيل"
    },
    {
      "id": 3130,
      "verse_key": "26:198",
      "text_imlaei_simple": "ولو نزلناه على بعض الاعجمين"
    },
    {
      "id": 3131,
      "verse_key": "26:199",
      "text_imlaei_simple": "فقراه عليهم ما كانوا به مؤمنين"
    },
    {
      "id": 3132,
      "verse_key": "26:200",
      "text_imlaei_simple": "كذلك سلكناه في قلوب المجرمين"
    },
    {
      "id": 3133,
      "verse_key": "26:201",
      "text_imlaei_simple": "لا يؤمنون به حتى يروا العذاب الاليم"
    },
    {
      "id": 3134,
      "verse_key": "26:202",
      "text_imlaei_simple": "فياتيهم بغتة وهم لا يشعرون"
    },
    {
      "id": 3135,
      "verse_key": "26:203",
      "text_imlaei_simple": "فيقولوا هل نحن منظرون"
    },
    {
      "id": 3136,
      "verse_key": "26:204",
      "text_imlaei_simple": "افبعذابنا يستعجلون"
    },
    {
      "id": 3137,
      "verse_key": "26:205",
      "text_imlaei_simple": "افرايت ان متعناهم سنين"
    },
    {
      "id": 3138,
      "verse_key": "26:206",
      "text_imlaei_simple": "ثم جاءهم ما كانوا يوعدون"
    },
    {
      "id": 3139,
      "verse_key": "26:207",
      "text_imlaei_simple": "ما اغنى عنهم ما كانوا يمتعون"
    },
    {
      "id": 3140,
      "verse_key": "26:208",
      "text_imlaei_simple": "وما اهلكنا من قرية الا لها منذرون"
    },
    {
      "id": 3141,
      "verse_key": "26:209",
      "text_imlaei_simple": "ذكرى وما كنا ظالمين"
    },
    {
      "id": 3142,
      "verse_key": "26:210",
      "text_imlaei_simple": "وما تنزلت به الشياطين"
    },
    {
      "id": 3143,
      "verse_key": "26:211",
      "text_imlaei_simple": "وما ينبغي لهم وما يستطيعون"
    },
    {
      "id": 3144,
      "verse_key": "26:212",
      "text_imlaei_simple": "انهم عن السمع لمعزولون"
    },
    {
      "id": 3145,
      "verse_key": "26:213",
      "text_imlaei_simple": "فلا تدع مع الله الها اخر فتكون من المعذبين"
    },
    {
      "id": 3146,
      "verse_key": "26:214",
      "text_imlaei_simple": "وانذر عشيرتك الاقربين"
    },
    {
      "id": 3147,
      "verse_key": "26:215",
      "text_imlaei_simple": "واخفض جناحك لمن اتبعك من المؤمنين"
    },
    {
      "id": 3148,
      "verse_key": "26:216",
      "text_imlaei_simple": "فان عصوك فقل اني بريء مما تعملون"
    },
    {
      "id": 3149,
      "verse_key": "26:217",
      "text_imlaei_simple": "وتوكل على العزيز الرحيم"
    },
    {
      "id": 3150,
      "verse_key": "26:218",
      "text_imlaei_simple": "الذي يراك حين تقوم"
    },
    {
      "id": 3151,
      "verse_key": "26:219",
      "text_imlaei_simple": "وتقلبك في الساجدين"
    },
    {
      "id": 3152,
      "verse_key": "26:220",
      "text_imlaei_simple": "انه هو السميع العليم"
    },
    {
      "id": 3153,
      "verse_key": "26:221",
      "text_imlaei_simple": "هل انبئكم على من تنزل الشياطين"
    },
    {
      "id": 3154,
      "verse_key": "26:222",
      "text_imlaei_simple": "تنزل على كل افاك اثيم"
    },
    {
      "id": 3155,
      "verse_key": "26:223",
      "text_imlaei_simple": "يلقون السمع واكثرهم كاذبون"
    },
    {
      "id": 3156,
      "verse_key": "26:224",
      "text_imlaei_simple": "والشعراء يتبعهم الغاوون"
    },
    {
      "id": 3157,
      "verse_key": "26:225",
      "text_imlaei_simple": "الم تر انهم في كل واد يهيمون"
    },
    {
      "id": 3158,
      "verse_key": "26:226",
      "text_imlaei_simple": "وانهم يقولون ما لا يفعلون"
    },
    {
      "id": 3159,
      "verse_key": "26:227",
      "text_imlaei_simple": "الا الذين امنوا وعملوا الصالحات وذكروا الله كثيرا وانتصروا من بعد ما ظلموا وسيعلم الذين ظلموا اي منقلب ينقلبون"
    }
  ],
  "meta": { "filters": { "chapter_number": "26" } }
}
