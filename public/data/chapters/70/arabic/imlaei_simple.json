{
  "verses": [
    {
      "id": 5376,
      "verse_key": "70:1",
      "text_imlaei_simple": "سال سائل بعذاب واقع"
    },
    {
      "id": 5377,
      "verse_key": "70:2",
      "text_imlaei_simple": "للكافرين ليس له دافع"
    },
    {
      "id": 5378,
      "verse_key": "70:3",
      "text_imlaei_simple": "من الله ذي المعارج"
    },
    {
      "id": 5379,
      "verse_key": "70:4",
      "text_imlaei_simple": "تعرج الملائكة والروح اليه في يوم كان مقداره خمسين الف سنة"
    },
    {
      "id": 5380,
      "verse_key": "70:5",
      "text_imlaei_simple": "فاصبر صبرا جميلا"
    },
    {
      "id": 5381,
      "verse_key": "70:6",
      "text_imlaei_simple": "انهم يرونه بعيدا"
    },
    { "id": 5382, "verse_key": "70:7", "text_imlaei_simple": "ونراه قريبا" },
    {
      "id": 5383,
      "verse_key": "70:8",
      "text_imlaei_simple": "يوم تكون السماء كالمهل"
    },
    {
      "id": 5384,
      "verse_key": "70:9",
      "text_imlaei_simple": "وتكون الجبال كالعهن"
    },
    {
      "id": 5385,
      "verse_key": "70:10",
      "text_imlaei_simple": "ولا يسال حميم حميما"
    },
    {
      "id": 5386,
      "verse_key": "70:11",
      "text_imlaei_simple": "يبصرونهم يود المجرم لو يفتدي من عذاب يومئذ ببنيه"
    },
    { "id": 5387, "verse_key": "70:12", "text_imlaei_simple": "وصاحبته واخيه" },
    {
      "id": 5388,
      "verse_key": "70:13",
      "text_imlaei_simple": "وفصيلته التي تؤويه"
    },
    {
      "id": 5389,
      "verse_key": "70:14",
      "text_imlaei_simple": "ومن في الارض جميعا ثم ينجيه"
    },
    { "id": 5390, "verse_key": "70:15", "text_imlaei_simple": "كلا انها لظى" },
    { "id": 5391, "verse_key": "70:16", "text_imlaei_simple": "نزاعة للشوى" },
    {
      "id": 5392,
      "verse_key": "70:17",
      "text_imlaei_simple": "تدعو من ادبر وتولى"
    },
    { "id": 5393, "verse_key": "70:18", "text_imlaei_simple": "وجمع فاوعى" },
    {
      "id": 5394,
      "verse_key": "70:19",
      "text_imlaei_simple": "ان الانسان خلق هلوعا"
    },
    {
      "id": 5395,
      "verse_key": "70:20",
      "text_imlaei_simple": "اذا مسه الشر جزوعا"
    },
    {
      "id": 5396,
      "verse_key": "70:21",
      "text_imlaei_simple": "واذا مسه الخير منوعا"
    },
    { "id": 5397, "verse_key": "70:22", "text_imlaei_simple": "الا المصلين" },
    {
      "id": 5398,
      "verse_key": "70:23",
      "text_imlaei_simple": "الذين هم على صلاتهم دائمون"
    },
    {
      "id": 5399,
      "verse_key": "70:24",
      "text_imlaei_simple": "والذين في اموالهم حق معلوم"
    },
    {
      "id": 5400,
      "verse_key": "70:25",
      "text_imlaei_simple": "للسائل والمحروم"
    },
    {
      "id": 5401,
      "verse_key": "70:26",
      "text_imlaei_simple": "والذين يصدقون بيوم الدين"
    },
    {
      "id": 5402,
      "verse_key": "70:27",
      "text_imlaei_simple": "والذين هم من عذاب ربهم مشفقون"
    },
    {
      "id": 5403,
      "verse_key": "70:28",
      "text_imlaei_simple": "ان عذاب ربهم غير مامون"
    },
    {
      "id": 5404,
      "verse_key": "70:29",
      "text_imlaei_simple": "والذين هم لفروجهم حافظون"
    },
    {
      "id": 5405,
      "verse_key": "70:30",
      "text_imlaei_simple": "الا على ازواجهم او ما ملكت ايمانهم فانهم غير ملومين"
    },
    {
      "id": 5406,
      "verse_key": "70:31",
      "text_imlaei_simple": "فمن ابتغى وراء ذلك فاولئك هم العادون"
    },
    {
      "id": 5407,
      "verse_key": "70:32",
      "text_imlaei_simple": "والذين هم لاماناتهم وعهدهم راعون"
    },
    {
      "id": 5408,
      "verse_key": "70:33",
      "text_imlaei_simple": "والذين هم بشهاداتهم قائمون"
    },
    {
      "id": 5409,
      "verse_key": "70:34",
      "text_imlaei_simple": "والذين هم على صلاتهم يحافظون"
    },
    {
      "id": 5410,
      "verse_key": "70:35",
      "text_imlaei_simple": "اولئك في جنات مكرمون"
    },
    {
      "id": 5411,
      "verse_key": "70:36",
      "text_imlaei_simple": "فمال الذين كفروا قبلك مهطعين"
    },
    {
      "id": 5412,
      "verse_key": "70:37",
      "text_imlaei_simple": "عن اليمين وعن الشمال عزين"
    },
    {
      "id": 5413,
      "verse_key": "70:38",
      "text_imlaei_simple": "ايطمع كل امرئ منهم ان يدخل جنة نعيم"
    },
    {
      "id": 5414,
      "verse_key": "70:39",
      "text_imlaei_simple": "كلا انا خلقناهم مما يعلمون"
    },
    {
      "id": 5415,
      "verse_key": "70:40",
      "text_imlaei_simple": "فلا اقسم برب المشارق والمغارب انا لقادرون"
    },
    {
      "id": 5416,
      "verse_key": "70:41",
      "text_imlaei_simple": "على ان نبدل خيرا منهم وما نحن بمسبوقين"
    },
    {
      "id": 5417,
      "verse_key": "70:42",
      "text_imlaei_simple": "فذرهم يخوضوا ويلعبوا حتى يلاقوا يومهم الذي يوعدون"
    },
    {
      "id": 5418,
      "verse_key": "70:43",
      "text_imlaei_simple": "يوم يخرجون من الاجداث سراعا كانهم الى نصب يوفضون"
    },
    {
      "id": 5419,
      "verse_key": "70:44",
      "text_imlaei_simple": "خاشعة ابصارهم ترهقهم ذلة ذلك اليوم الذي كانوا يوعدون"
    }
  ],
  "meta": { "filters": { "chapter_number": "70" } }
}
