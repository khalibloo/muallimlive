{
  "verses": [
    {
      "id": 4980,
      "verse_key": "56:1",
      "text_imlaei_simple": "اذا وقعت الواقعة"
    },
    {
      "id": 4981,
      "verse_key": "56:2",
      "text_imlaei_simple": "ليس لوقعتها كاذبة"
    },
    { "id": 4982, "verse_key": "56:3", "text_imlaei_simple": "خافضة رافعة" },
    {
      "id": 4983,
      "verse_key": "56:4",
      "text_imlaei_simple": "اذا رجت الارض رجا"
    },
    {
      "id": 4984,
      "verse_key": "56:5",
      "text_imlaei_simple": "وبست الجبال بسا"
    },
    {
      "id": 4985,
      "verse_key": "56:6",
      "text_imlaei_simple": "فكانت هباء منبثا"
    },
    {
      "id": 4986,
      "verse_key": "56:7",
      "text_imlaei_simple": "وكنتم ازواجا ثلاثة"
    },
    {
      "id": 4987,
      "verse_key": "56:8",
      "text_imlaei_simple": "فاصحاب الميمنة ما اصحاب الميمنة"
    },
    {
      "id": 4988,
      "verse_key": "56:9",
      "text_imlaei_simple": "واصحاب المشامة ما اصحاب المشامة"
    },
    {
      "id": 4989,
      "verse_key": "56:10",
      "text_imlaei_simple": "والسابقون السابقون"
    },
    {
      "id": 4990,
      "verse_key": "56:11",
      "text_imlaei_simple": "اولئك المقربون"
    },
    {
      "id": 4991,
      "verse_key": "56:12",
      "text_imlaei_simple": "في جنات النعيم"
    },
    {
      "id": 4992,
      "verse_key": "56:13",
      "text_imlaei_simple": "ثلة من الاولين"
    },
    {
      "id": 4993,
      "verse_key": "56:14",
      "text_imlaei_simple": "وقليل من الاخرين"
    },
    {
      "id": 4994,
      "verse_key": "56:15",
      "text_imlaei_simple": "على سرر موضونة"
    },
    {
      "id": 4995,
      "verse_key": "56:16",
      "text_imlaei_simple": "متكئين عليها متقابلين"
    },
    {
      "id": 4996,
      "verse_key": "56:17",
      "text_imlaei_simple": "يطوف عليهم ولدان مخلدون"
    },
    {
      "id": 4997,
      "verse_key": "56:18",
      "text_imlaei_simple": "باكواب واباريق وكاس من معين"
    },
    {
      "id": 4998,
      "verse_key": "56:19",
      "text_imlaei_simple": "لا يصدعون عنها ولا ينزفون"
    },
    {
      "id": 4999,
      "verse_key": "56:20",
      "text_imlaei_simple": "وفاكهة مما يتخيرون"
    },
    {
      "id": 5000,
      "verse_key": "56:21",
      "text_imlaei_simple": "ولحم طير مما يشتهون"
    },
    { "id": 5001, "verse_key": "56:22", "text_imlaei_simple": "وحور عين" },
    {
      "id": 5002,
      "verse_key": "56:23",
      "text_imlaei_simple": "كامثال اللؤلؤ المكنون"
    },
    {
      "id": 5003,
      "verse_key": "56:24",
      "text_imlaei_simple": "جزاء بما كانوا يعملون"
    },
    {
      "id": 5004,
      "verse_key": "56:25",
      "text_imlaei_simple": "لا يسمعون فيها لغوا ولا تاثيما"
    },
    {
      "id": 5005,
      "verse_key": "56:26",
      "text_imlaei_simple": "الا قيلا سلاما سلاما"
    },
    {
      "id": 5006,
      "verse_key": "56:27",
      "text_imlaei_simple": "واصحاب اليمين ما اصحاب اليمين"
    },
    { "id": 5007, "verse_key": "56:28", "text_imlaei_simple": "في سدر مخضود" },
    { "id": 5008, "verse_key": "56:29", "text_imlaei_simple": "وطلح منضود" },
    { "id": 5009, "verse_key": "56:30", "text_imlaei_simple": "وظل ممدود" },
    { "id": 5010, "verse_key": "56:31", "text_imlaei_simple": "وماء مسكوب" },
    { "id": 5011, "verse_key": "56:32", "text_imlaei_simple": "وفاكهة كثيرة" },
    {
      "id": 5012,
      "verse_key": "56:33",
      "text_imlaei_simple": "لا مقطوعة ولا ممنوعة"
    },
    { "id": 5013, "verse_key": "56:34", "text_imlaei_simple": "وفرش مرفوعة" },
    {
      "id": 5014,
      "verse_key": "56:35",
      "text_imlaei_simple": "انا انشاناهن انشاء"
    },
    {
      "id": 5015,
      "verse_key": "56:36",
      "text_imlaei_simple": "فجعلناهن ابكارا"
    },
    { "id": 5016, "verse_key": "56:37", "text_imlaei_simple": "عربا اترابا" },
    { "id": 5017, "verse_key": "56:38", "text_imlaei_simple": "لاصحاب اليمين" },
    {
      "id": 5018,
      "verse_key": "56:39",
      "text_imlaei_simple": "ثلة من الاولين"
    },
    {
      "id": 5019,
      "verse_key": "56:40",
      "text_imlaei_simple": "وثلة من الاخرين"
    },
    {
      "id": 5020,
      "verse_key": "56:41",
      "text_imlaei_simple": "واصحاب الشمال ما اصحاب الشمال"
    },
    { "id": 5021, "verse_key": "56:42", "text_imlaei_simple": "في سموم وحميم" },
    { "id": 5022, "verse_key": "56:43", "text_imlaei_simple": "وظل من يحموم" },
    {
      "id": 5023,
      "verse_key": "56:44",
      "text_imlaei_simple": "لا بارد ولا كريم"
    },
    {
      "id": 5024,
      "verse_key": "56:45",
      "text_imlaei_simple": "انهم كانوا قبل ذلك مترفين"
    },
    {
      "id": 5025,
      "verse_key": "56:46",
      "text_imlaei_simple": "وكانوا يصرون على الحنث العظيم"
    },
    {
      "id": 5026,
      "verse_key": "56:47",
      "text_imlaei_simple": "وكانوا يقولون ائذا متنا وكنا ترابا وعظاما اانا لمبعوثون"
    },
    {
      "id": 5027,
      "verse_key": "56:48",
      "text_imlaei_simple": "اواباؤنا الاولون"
    },
    {
      "id": 5028,
      "verse_key": "56:49",
      "text_imlaei_simple": "قل ان الاولين والاخرين"
    },
    {
      "id": 5029,
      "verse_key": "56:50",
      "text_imlaei_simple": "لمجموعون الى ميقات يوم معلوم"
    },
    {
      "id": 5030,
      "verse_key": "56:51",
      "text_imlaei_simple": "ثم انكم ايها الضالون المكذبون"
    },
    {
      "id": 5031,
      "verse_key": "56:52",
      "text_imlaei_simple": "لاكلون من شجر من زقوم"
    },
    {
      "id": 5032,
      "verse_key": "56:53",
      "text_imlaei_simple": "فمالئون منها البطون"
    },
    {
      "id": 5033,
      "verse_key": "56:54",
      "text_imlaei_simple": "فشاربون عليه من الحميم"
    },
    {
      "id": 5034,
      "verse_key": "56:55",
      "text_imlaei_simple": "فشاربون شرب الهيم"
    },
    {
      "id": 5035,
      "verse_key": "56:56",
      "text_imlaei_simple": "هذا نزلهم يوم الدين"
    },
    {
      "id": 5036,
      "verse_key": "56:57",
      "text_imlaei_simple": "نحن خلقناكم فلولا تصدقون"
    },
    {
      "id": 5037,
      "verse_key": "56:58",
      "text_imlaei_simple": "افرايتم ما تمنون"
    },
    {
      "id": 5038,
      "verse_key": "56:59",
      "text_imlaei_simple": "اانتم تخلقونه ام نحن الخالقون"
    },
    {
      "id": 5039,
      "verse_key": "56:60",
      "text_imlaei_simple": "نحن قدرنا بينكم الموت وما نحن بمسبوقين"
    },
    {
      "id": 5040,
      "verse_key": "56:61",
      "text_imlaei_simple": "على ان نبدل امثالكم وننشئكم في ما لا تعلمون"
    },
    {
      "id": 5041,
      "verse_key": "56:62",
      "text_imlaei_simple": "ولقد علمتم النشاة الاولى فلولا تذكرون"
    },
    {
      "id": 5042,
      "verse_key": "56:63",
      "text_imlaei_simple": "افرايتم ما تحرثون"
    },
    {
      "id": 5043,
      "verse_key": "56:64",
      "text_imlaei_simple": "اانتم تزرعونه ام نحن الزارعون"
    },
    {
      "id": 5044,
      "verse_key": "56:65",
      "text_imlaei_simple": "لو نشاء لجعلناه حطاما فظلتم تفكهون"
    },
    { "id": 5045, "verse_key": "56:66", "text_imlaei_simple": "انا لمغرمون" },
    {
      "id": 5046,
      "verse_key": "56:67",
      "text_imlaei_simple": "بل نحن محرومون"
    },
    {
      "id": 5047,
      "verse_key": "56:68",
      "text_imlaei_simple": "افرايتم الماء الذي تشربون"
    },
    {
      "id": 5048,
      "verse_key": "56:69",
      "text_imlaei_simple": "اانتم انزلتموه من المزن ام نحن المنزلون"
    },
    {
      "id": 5049,
      "verse_key": "56:70",
      "text_imlaei_simple": "لو نشاء جعلناه اجاجا فلولا تشكرون"
    },
    {
      "id": 5050,
      "verse_key": "56:71",
      "text_imlaei_simple": "افرايتم النار التي تورون"
    },
    {
      "id": 5051,
      "verse_key": "56:72",
      "text_imlaei_simple": "اانتم انشاتم شجرتها ام نحن المنشئون"
    },
    {
      "id": 5052,
      "verse_key": "56:73",
      "text_imlaei_simple": "نحن جعلناها تذكرة ومتاعا للمقوين"
    },
    {
      "id": 5053,
      "verse_key": "56:74",
      "text_imlaei_simple": "فسبح باسم ربك العظيم"
    },
    {
      "id": 5054,
      "verse_key": "56:75",
      "text_imlaei_simple": "فلا اقسم بمواقع النجوم"
    },
    {
      "id": 5055,
      "verse_key": "56:76",
      "text_imlaei_simple": "وانه لقسم لو تعلمون عظيم"
    },
    {
      "id": 5056,
      "verse_key": "56:77",
      "text_imlaei_simple": "انه لقران كريم"
    },
    { "id": 5057, "verse_key": "56:78", "text_imlaei_simple": "في كتاب مكنون" },
    {
      "id": 5058,
      "verse_key": "56:79",
      "text_imlaei_simple": "لا يمسه الا المطهرون"
    },
    {
      "id": 5059,
      "verse_key": "56:80",
      "text_imlaei_simple": "تنزيل من رب العالمين"
    },
    {
      "id": 5060,
      "verse_key": "56:81",
      "text_imlaei_simple": "افبهذا الحديث انتم مدهنون"
    },
    {
      "id": 5061,
      "verse_key": "56:82",
      "text_imlaei_simple": "وتجعلون رزقكم انكم تكذبون"
    },
    {
      "id": 5062,
      "verse_key": "56:83",
      "text_imlaei_simple": "فلولا اذا بلغت الحلقوم"
    },
    {
      "id": 5063,
      "verse_key": "56:84",
      "text_imlaei_simple": "وانتم حينئذ تنظرون"
    },
    {
      "id": 5064,
      "verse_key": "56:85",
      "text_imlaei_simple": "ونحن اقرب اليه منكم ولكن لا تبصرون"
    },
    {
      "id": 5065,
      "verse_key": "56:86",
      "text_imlaei_simple": "فلولا ان كنتم غير مدينين"
    },
    {
      "id": 5066,
      "verse_key": "56:87",
      "text_imlaei_simple": "ترجعونها ان كنتم صادقين"
    },
    {
      "id": 5067,
      "verse_key": "56:88",
      "text_imlaei_simple": "فاما ان كان من المقربين"
    },
    {
      "id": 5068,
      "verse_key": "56:89",
      "text_imlaei_simple": "فروح وريحان وجنت نعيم"
    },
    {
      "id": 5069,
      "verse_key": "56:90",
      "text_imlaei_simple": "واما ان كان من اصحاب اليمين"
    },
    {
      "id": 5070,
      "verse_key": "56:91",
      "text_imlaei_simple": "فسلام لك من اصحاب اليمين"
    },
    {
      "id": 5071,
      "verse_key": "56:92",
      "text_imlaei_simple": "واما ان كان من المكذبين الضالين"
    },
    { "id": 5072, "verse_key": "56:93", "text_imlaei_simple": "فنزل من حميم" },
    { "id": 5073, "verse_key": "56:94", "text_imlaei_simple": "وتصلية جحيم" },
    {
      "id": 5074,
      "verse_key": "56:95",
      "text_imlaei_simple": "ان هذا لهو حق اليقين"
    },
    {
      "id": 5075,
      "verse_key": "56:96",
      "text_imlaei_simple": "فسبح باسم ربك العظيم"
    }
  ],
  "meta": { "filters": { "chapter_number": "56" } }
}
