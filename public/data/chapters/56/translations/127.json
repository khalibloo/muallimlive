{
  "translations": [
    { "resource_id": 127, "text": "Воқиъа воқеъ бўлганда." },
    {
      "resource_id": 127,
      "text": "Унинг воқеъ бўлишини ёлғонга чиқарувчи (жон) йўқдир."
    },
    { "resource_id": 127, "text": "У, ҳам пастлатувчи, ҳам кўтарувчидир." },
    { "resource_id": 127, "text": "Қачонки, ер қаттиқ ларзага келтирилганда." },
    { "resource_id": 127, "text": "Ва тоғлар майдаланиб титилганда." },
    { "resource_id": 127, "text": "Бас, тарқалган чанг-тўзонга айланганда." },
    { "resource_id": 127, "text": "Ва сиз уч тоифа бўлганингизда." },
    {
      "resource_id": 127,
      "text": "Ўнг тараф эгалари. Ўнг тараф эгалари не(саодатлилар)дир?!"
    },
    {
      "resource_id": 127,
      "text": "Ва чап тараф эгалари. Чап тараф эгалари не(бадбахтлар)дир?!"
    },
    { "resource_id": 127, "text": "Ва пешқадамлар, пешқадамлар." },
    { "resource_id": 127, "text": "Улар (Аллоҳга) яқин бўлганлар." },
    { "resource_id": 127, "text": "Сернеъмат жаннатлардадирлар." },
    { "resource_id": 127, "text": "Аввалгилардан кўпчилик." },
    { "resource_id": 127, "text": "Ва охиргилардан озчилик." },
    { "resource_id": 127, "text": "Тўқима сўрилардадирлар." },
    {
      "resource_id": 127,
      "text": "Бир-бирларига рўбарў бўлиб ёнбошлаган ҳолдадирлар.<sup foot_note=48748>1</sup>"
    },
    {
      "resource_id": 127,
      "text": "Атрофларида мангу ёш йигитлар айланиб юрурлар."
    },
    {
      "resource_id": 127,
      "text": "Оқар чашма майи тўлатилган қадаҳлар, кўзалар ва косалар билан."
    },
    { "resource_id": 127, "text": "Ундан бош оғриғи ҳам, маст ҳам бўлмаслар." },
    { "resource_id": 127, "text": "Ва хоҳлаган меваларини олиб." },
    {
      "resource_id": 127,
      "text": "Иштаҳалари тусаган қуш гўштини олиб (айланиб юрарлар)."
    },
    { "resource_id": 127, "text": "Яна, кўзи гўзал ҳурлар бор." },
    { "resource_id": 127, "text": "Мисоли яширилган дур каби." },
    {
      "resource_id": 127,
      "text": "Булар — қилиб юрган амалларнинг мукофотидур."
    },
    {
      "resource_id": 127,
      "text": "Улар у(Жаннат)да беҳуда, гуноҳ(сўз)ларни эшитмаслар."
    },
    {
      "resource_id": 127,
      "text": "Магар «Салом, салом» дейилганини (эшитарлар).<sup foot_note=48749>1</sup>"
    },
    {
      "resource_id": 127,
      "text": "Ўнг тараф эгалари. Ўнг тараф эгалари недир?!"
    },
    {
      "resource_id": 127,
      "text": "Улар тиконсиз сидрзорлардадирлар.<sup foot_note=48750>1</sup>"
    },
    { "resource_id": 127, "text": "Сермева бананзорлардадирлар." },
    { "resource_id": 127, "text": "Ва ёйилган соялардадирлар." },
    { "resource_id": 127, "text": "Оқар сувлардадирлар." },
    { "resource_id": 127, "text": "Сероб мевалардадирлар." },
    { "resource_id": 127, "text": "Тугамайдиган ва ман қилинмайдиган." },
    { "resource_id": 127, "text": "Кўтарилган кўрпачалардадирлар." },
    {
      "resource_id": 127,
      "text": "Албатта, Биз у(ҳур)ларни дафъатан, хос қилиб, яратдик."
    },
    { "resource_id": 127, "text": "Бас, уларни бокира қизлар қилиб қўйдик." },
    { "resource_id": 127, "text": "Бир хилдаги тенгдош маҳбубалар." },
    { "resource_id": 127, "text": "Ўнг тараф эгалари учун." },
    { "resource_id": 127, "text": "Улар аввалгилардан кўпгина жамоатлардир." },
    { "resource_id": 127, "text": "Кейингилардан ҳам кўпгина жамоатлардир." },
    {
      "resource_id": 127,
      "text": "Ва чап тараф эгалари. Чап тараф эгалари не(бадбахтлар)дир?!"
    },
    { "resource_id": 127, "text": "Улар ўт шамол ва қайноқ сувдадирлар." },
    { "resource_id": 127, "text": "Қоп-қора тутун соясидадирлар." },
    { "resource_id": 127, "text": "На салқин эмас ва на фойдали." },
    {
      "resource_id": 127,
      "text": "Албатта, улар бундан олдин ҳойи-ҳавасга берилганлардан эдилар."
    },
    {
      "resource_id": 127,
      "text": "Ва катта гуноҳ ишларни мудом қилиб келган эдилар."
    },
    {
      "resource_id": 127,
      "text": "Ва: «Биз ўлиб, тупроқ ва суяк бўлсак ҳам, қайта тирилтириламизми?"
    },
    {
      "resource_id": 127,
      "text": "Аввал ўтган оталаримиз ҳам-а?!» – дер эдилар."
    },
    {
      "resource_id": 127,
      "text": "Айтгил: «Албатта, аввалгилар ва охиргилар."
    },
    {
      "resource_id": 127,
      "text": "Маълум куни, белгиланган вақтда, албатта, тўпланувчидирлар»."
    },
    {
      "resource_id": 127,
      "text": "Сўнгра, сиз, эй адашганлар, ёлғонга чиқарувчилар."
    },
    { "resource_id": 127, "text": "Албатта, заққум дарахтидан еювчисизлар." },
    { "resource_id": 127, "text": "Бас, ундан қоринларни тўлдирувчисизлар." },
    {
      "resource_id": 127,
      "text": "Бас, устидан ўта қайноқ сувдан ичувчисизлар."
    },
    { "resource_id": 127, "text": "Бас, чанқоқ туядек ичувчисизлар." },
    {
      "resource_id": 127,
      "text": "Уларнинг қиёмат кунидаги зиёфатлари мана шу!!!"
    },
    { "resource_id": 127, "text": "Биз сизларни яратганмиз, ишонсаларинг-чи!" },
    {
      "resource_id": 127,
      "text": "Ўзингиз чиқарадиган манийни ўйлаб кўринг-а!"
    },
    {
      "resource_id": 127,
      "text": "Уни сизлар яратасизми ёки Биз яратувчимизми?!"
    },
    {
      "resource_id": 127,
      "text": "Орангиздаги ўлимни Биз белгилаганмиз ва Биз ожиз эмасмиз."
    },
    {
      "resource_id": 127,
      "text": "Сизнинг ўрнингизга сизга ўхшашларни алмаштиришдан ва сизларни ўзингиз билмайдиган ҳолда яратишдан."
    },
    {
      "resource_id": 127,
      "text": "Ва дарҳақиқат, аввалги яратилишни билдингиз-ку, эсласаларингиз-чи!"
    },
    { "resource_id": 127, "text": "Ўзингиз экаётган нарсани ўйлаб кўринг-а!" },
    {
      "resource_id": 127,
      "text": "Уни сиз ундирасизми ёки Биз ундирувчимизми?"
    },
    {
      "resource_id": 127,
      "text": "Агар хоҳласак, Биз уни қуруқ чўпга айлантириб қўямиз. Сизлар эса, надоматда ажабланиб:"
    },
    { "resource_id": 127, "text": "«Албатта, биз зиён кўрганлармиз." },
    { "resource_id": 127, "text": "Балки, маҳрум бўлганлармиз, (дейсизлар)." },
    { "resource_id": 127, "text": "Ўзингиз ичадиган сувни ўйлаб кўринг-а!" },
    {
      "resource_id": 127,
      "text": "Уни булутлардан сиз туширасизми ёки Биз туширувчимизми?"
    },
    {
      "resource_id": 127,
      "text": "Агар хоҳласак, Биз уни шўр қилиб қўямиз. Шукр қилсангиз-чи!"
    },
    { "resource_id": 127, "text": "Ўзингиз ёқадиган оловни ўйлаб кўринг-а!" },
    {
      "resource_id": 127,
      "text": "Унинг дарахтини сиз яратгансизми ёки Биз яратувчимизми?"
    },
    {
      "resource_id": 127,
      "text": "Биз у(олов)ни эслатма ва йўловчилар учун манфаат қилиб қўйдик."
    },
    {
      "resource_id": 127,
      "text": "Бас, улуғ Роббинг исмини поклаб ёд эт!<sup foot_note=48751>1</sup>"
    },
    {
      "resource_id": 127,
      "text": "Юлдузларнинг манзиллари билан қасам ичаман.<sup foot_note=48752>1</sup>"
    },
    {
      "resource_id": 127,
      "text": "Албатта, агар билсангиз, бу улуғ қасамдир.<sup foot_note=48753>1</sup>"
    },
    { "resource_id": 127, "text": "Албатта, у Қуръони Каримдир." },
    { "resource_id": 127, "text": "У Сақланган китобдадир." },
    { "resource_id": 127, "text": "Уни фақат покланганларгина ушлайдир." },
    { "resource_id": 127, "text": "У, оламларнинг Роббисидан туширилгандир." },
    {
      "resource_id": 127,
      "text": "Бас, сизлар мана шу сўзга бепарволик қиласизларми?"
    },
    {
      "resource_id": 127,
      "text": "Ва ёлғонга чиқаришни ризқ қилиб оласизларми?<sup foot_note=48754>1</sup>"
    },
    { "resource_id": 127, "text": "Жонингиз ҳалқумга келиб қолса-чи?" },
    {
      "resource_id": 127,
      "text": "Ҳолбуки, ўша вақтда ўзларингиз қараб турган бўласизлар."
    },
    {
      "resource_id": 127,
      "text": "Биз эса, сиздан кўра унга яқинмиз, аммо сизлар кўрмайсизлар.<sup foot_note=48755>1</sup>"
    },
    { "resource_id": 127, "text": "Агар сиз ҳисоб бермайдиган бўлсангиз." },
    {
      "resource_id": 127,
      "text": "У(жон)ни ўрнига қайтаринг-чи, агар гапларингиз рост бўлса."
    },
    { "resource_id": 127, "text": "Агар яқинлаштирилганлардан бўлса." },
    {
      "resource_id": 127,
      "text": "Роҳатда, фароғатда ва сернеъмат жаннатдадир."
    },
    { "resource_id": 127, "text": "Агар ўнг тараф эгаларидан бўлса." },
    {
      "resource_id": 127,
      "text": "Бас, сенга ўнг тараф эгаларидан Салом бўлсин, (дейилур)."
    },
    {
      "resource_id": 127,
      "text": "Ва агар ёлғонга чиқарувчи гумроҳлардан бўлса."
    },
    { "resource_id": 127, "text": "Бас, ўта қайноқ сувдан зиёфат." },
    { "resource_id": 127, "text": "Ва дўзахга киришдир." },
    {
      "resource_id": 127,
      "text": "Албатта, ушбу, таъкидки, очиқ-ойдин ҳақиқатдир."
    },
    { "resource_id": 127, "text": "Бас, улуғ Роббинг исмини поклаб ёд эт!" }
  ],
