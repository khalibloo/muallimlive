{
  "verses": [
    {
      "id": 5272,
      "verse_key": "68:1",
      "text_imlaei_simple": "ن والقلم وما يسطرون"
    },
    {
      "id": 5273,
      "verse_key": "68:2",
      "text_imlaei_simple": "ما انت بنعمة ربك بمجنون"
    },
    {
      "id": 5274,
      "verse_key": "68:3",
      "text_imlaei_simple": "وان لك لاجرا غير ممنون"
    },
    {
      "id": 5275,
      "verse_key": "68:4",
      "text_imlaei_simple": "وانك لعلى خلق عظيم"
    },
    { "id": 5276, "verse_key": "68:5", "text_imlaei_simple": "فستبصر ويبصرون" },
    { "id": 5277, "verse_key": "68:6", "text_imlaei_simple": "باييكم المفتون" },
    {
      "id": 5278,
      "verse_key": "68:7",
      "text_imlaei_simple": "ان ربك هو اعلم بمن ضل عن سبيله وهو اعلم بالمهتدين"
    },
    {
      "id": 5279,
      "verse_key": "68:8",
      "text_imlaei_simple": "فلا تطع المكذبين"
    },
    {
      "id": 5280,
      "verse_key": "68:9",
      "text_imlaei_simple": "ودوا لو تدهن فيدهنون"
    },
    {
      "id": 5281,
      "verse_key": "68:10",
      "text_imlaei_simple": "ولا تطع كل حلاف مهين"
    },
    {
      "id": 5282,
      "verse_key": "68:11",
      "text_imlaei_simple": "هماز مشاء بنميم"
    },
    {
      "id": 5283,
      "verse_key": "68:12",
      "text_imlaei_simple": "مناع للخير معتد اثيم"
    },
    {
      "id": 5284,
      "verse_key": "68:13",
      "text_imlaei_simple": "عتل بعد ذلك زنيم"
    },
    {
      "id": 5285,
      "verse_key": "68:14",
      "text_imlaei_simple": "ان كان ذا مال وبنين"
    },
    {
      "id": 5286,
      "verse_key": "68:15",
      "text_imlaei_simple": "اذا تتلى عليه اياتنا قال اساطير الاولين"
    },
    {
      "id": 5287,
      "verse_key": "68:16",
      "text_imlaei_simple": "سنسمه على الخرطوم"
    },
    {
      "id": 5288,
      "verse_key": "68:17",
      "text_imlaei_simple": "انا بلوناهم كما بلونا اصحاب الجنة اذ اقسموا ليصرمنها مصبحين"
    },
    { "id": 5289, "verse_key": "68:18", "text_imlaei_simple": "ولا يستثنون" },
    {
      "id": 5290,
      "verse_key": "68:19",
      "text_imlaei_simple": "فطاف عليها طائف من ربك وهم نائمون"
    },
    {
      "id": 5291,
      "verse_key": "68:20",
      "text_imlaei_simple": "فاصبحت كالصريم"
    },
    {
      "id": 5292,
      "verse_key": "68:21",
      "text_imlaei_simple": "فتنادوا مصبحين"
    },
    {
      "id": 5293,
      "verse_key": "68:22",
      "text_imlaei_simple": "ان اغدوا على حرثكم ان كنتم صارمين"
    },
    {
      "id": 5294,
      "verse_key": "68:23",
      "text_imlaei_simple": "فانطلقوا وهم يتخافتون"
    },
    {
      "id": 5295,
      "verse_key": "68:24",
      "text_imlaei_simple": "ان لا يدخلنها اليوم عليكم مسكين"
    },
    {
      "id": 5296,
      "verse_key": "68:25",
      "text_imlaei_simple": "وغدوا على حرد قادرين"
    },
    {
      "id": 5297,
      "verse_key": "68:26",
      "text_imlaei_simple": "فلما راوها قالوا انا لضالون"
    },
    {
      "id": 5298,
      "verse_key": "68:27",
      "text_imlaei_simple": "بل نحن محرومون"
    },
    {
      "id": 5299,
      "verse_key": "68:28",
      "text_imlaei_simple": "قال اوسطهم الم اقل لكم لولا تسبحون"
    },
    {
      "id": 5300,
      "verse_key": "68:29",
      "text_imlaei_simple": "قالوا سبحان ربنا انا كنا ظالمين"
    },
    {
      "id": 5301,
      "verse_key": "68:30",
      "text_imlaei_simple": "فاقبل بعضهم على بعض يتلاومون"
    },
    {
      "id": 5302,
      "verse_key": "68:31",
      "text_imlaei_simple": "قالوا يا ويلنا انا كنا طاغين"
    },
    {
      "id": 5303,
      "verse_key": "68:32",
      "text_imlaei_simple": "عسى ربنا ان يبدلنا خيرا منها انا الى ربنا راغبون"
    },
    {
      "id": 5304,
      "verse_key": "68:33",
      "text_imlaei_simple": "كذلك العذاب ولعذاب الاخرة اكبر لو كانوا يعلمون"
    },
    {
      "id": 5305,
      "verse_key": "68:34",
      "text_imlaei_simple": "ان للمتقين عند ربهم جنات النعيم"
    },
    {
      "id": 5306,
      "verse_key": "68:35",
      "text_imlaei_simple": "افنجعل المسلمين كالمجرمين"
    },
    {
      "id": 5307,
      "verse_key": "68:36",
      "text_imlaei_simple": "ما لكم كيف تحكمون"
    },
    {
      "id": 5308,
      "verse_key": "68:37",
      "text_imlaei_simple": "ام لكم كتاب فيه تدرسون"
    },
    {
      "id": 5309,
      "verse_key": "68:38",
      "text_imlaei_simple": "ان لكم فيه لما تخيرون"
    },
    {
      "id": 5310,
      "verse_key": "68:39",
      "text_imlaei_simple": "ام لكم ايمان علينا بالغة الى يوم القيامة ان لكم لما تحكمون"
    },
    {
      "id": 5311,
      "verse_key": "68:40",
      "text_imlaei_simple": "سلهم ايهم بذلك زعيم"
    },
    {
      "id": 5312,
      "verse_key": "68:41",
      "text_imlaei_simple": "ام لهم شركاء فلياتوا بشركائهم ان كانوا صادقين"
    },
    {
      "id": 5313,
      "verse_key": "68:42",
      "text_imlaei_simple": "يوم يكشف عن ساق ويدعون الى السجود فلا يستطيعون"
    },
    {
      "id": 5314,
      "verse_key": "68:43",
      "text_imlaei_simple": "خاشعة ابصارهم ترهقهم ذلة وقد كانوا يدعون الى السجود وهم سالمون"
    },
    {
      "id": 5315,
      "verse_key": "68:44",
      "text_imlaei_simple": "فذرني ومن يكذب بهذا الحديث سنستدرجهم من حيث لا يعلمون"
    },
    {
      "id": 5316,
      "verse_key": "68:45",
      "text_imlaei_simple": "واملي لهم ان كيدي متين"
    },
    {
      "id": 5317,
      "verse_key": "68:46",
      "text_imlaei_simple": "ام تسالهم اجرا فهم من مغرم مثقلون"
    },
    {
      "id": 5318,
      "verse_key": "68:47",
      "text_imlaei_simple": "ام عندهم الغيب فهم يكتبون"
    },
    {
      "id": 5319,
      "verse_key": "68:48",
      "text_imlaei_simple": "فاصبر لحكم ربك ولا تكن كصاحب الحوت اذ نادى وهو مكظوم"
    },
    {
      "id": 5320,
      "verse_key": "68:49",
      "text_imlaei_simple": "لولا ان تداركه نعمة من ربه لنبذ بالعراء وهو مذموم"
    },
    {
      "id": 5321,
      "verse_key": "68:50",
      "text_imlaei_simple": "فاجتباه ربه فجعله من الصالحين"
    },
    {
      "id": 5322,
      "verse_key": "68:51",
      "text_imlaei_simple": "وان يكاد الذين كفروا ليزلقونك بابصارهم لما سمعوا الذكر ويقولون انه لمجنون"
    },
    {
      "id": 5323,
      "verse_key": "68:52",
      "text_imlaei_simple": "وما هو الا ذكر للعالمين"
    }
  ],
  "meta": { "filters": { "chapter_number": "68" } }
}
