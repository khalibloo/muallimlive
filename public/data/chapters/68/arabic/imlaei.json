{
  "verses": [
    {
      "id": 5272,
      "verse_key": "68:1",
      "text_imlaei": "ن ۚ وَالْقَلَمِ وَمَا يَسْطُرُونَ"
    },
    {
      "id": 5273,
      "verse_key": "68:2",
      "text_imlaei": "مَا أَنتَ بِنِعْمَةِ رَبِّكَ بِمَجْنُونٍ"
    },
    {
      "id": 5274,
      "verse_key": "68:3",
      "text_imlaei": "وَإِنَّ لَكَ لَأَجْرًا غَيْرَ مَمْنُونٍ"
    },
    {
      "id": 5275,
      "verse_key": "68:4",
      "text_imlaei": "وَإِنَّكَ لَعَلَىٰ خُلُقٍ عَظِيمٍ"
    },
    {
      "id": 5276,
      "verse_key": "68:5",
      "text_imlaei": "فَسَتُبْصِرُ وَيُبْصِرُونَ"
    },
    {
      "id": 5277,
      "verse_key": "68:6",
      "text_imlaei": "بِأَييِّكُمُ الْمَفْتُونُ"
    },
    {
      "id": 5278,
      "verse_key": "68:7",
      "text_imlaei": "إِنَّ رَبَّكَ هُوَ أَعْلَمُ بِمَن ضَلَّ عَن سَبِيلِهِ وَهُوَ أَعْلَمُ بِالْمُهْتَدِينَ"
    },
    {
      "id": 5279,
      "verse_key": "68:8",
      "text_imlaei": "فَلَا تُطِعِ الْمُكَذِّبِينَ"
    },
    {
      "id": 5280,
      "verse_key": "68:9",
      "text_imlaei": "وَدُّوا لَوْ تُدْهِنُ فَيُدْهِنُونَ"
    },
    {
      "id": 5281,
      "verse_key": "68:10",
      "text_imlaei": "وَلَا تُطِعْ كُلَّ حَلَّافٍ مَّهِينٍ"
    },
    {
      "id": 5282,
      "verse_key": "68:11",
      "text_imlaei": "هَمَّازٍ مَّشَّاءٍ بِنَمِيمٍ"
    },
    {
      "id": 5283,
      "verse_key": "68:12",
      "text_imlaei": "مَّنَّاعٍ لِّلْخَيْرِ مُعْتَدٍ أَثِيمٍ"
    },
    {
      "id": 5284,
      "verse_key": "68:13",
      "text_imlaei": "عُتُلٍّ بَعْدَ ذَٰلِكَ زَنِيمٍ"
    },
    {
      "id": 5285,
      "verse_key": "68:14",
      "text_imlaei": "أَن كَانَ ذَا مَالٍ وَبَنِينَ"
    },
    {
      "id": 5286,
      "verse_key": "68:15",
      "text_imlaei": "إِذَا تُتْلَىٰ عَلَيْهِ آيَاتُنَا قَالَ أَسَاطِيرُ الْأَوَّلِينَ"
    },
    {
      "id": 5287,
      "verse_key": "68:16",
      "text_imlaei": "سَنَسِمُهُ عَلَى الْخُرْطُومِ"
    },
    {
      "id": 5288,
      "verse_key": "68:17",
      "text_imlaei": "إِنَّا بَلَوْنَاهُمْ كَمَا بَلَوْنَا أَصْحَابَ الْجَنَّةِ إِذْ أَقْسَمُوا لَيَصْرِمُنَّهَا مُصْبِحِينَ"
    },
    { "id": 5289, "verse_key": "68:18", "text_imlaei": "وَلَا يَسْتَثْنُونَ" },
    {
      "id": 5290,
      "verse_key": "68:19",
      "text_imlaei": "فَطَافَ عَلَيْهَا طَائِفٌ مِّن رَّبِّكَ وَهُمْ نَائِمُونَ"
    },
    {
      "id": 5291,
      "verse_key": "68:20",
      "text_imlaei": "فَأَصْبَحَتْ كَالصَّرِيمِ"
    },
    {
      "id": 5292,
      "verse_key": "68:21",
      "text_imlaei": "فَتَنَادَوْا مُصْبِحِينَ"
    },
    {
      "id": 5293,
      "verse_key": "68:22",
      "text_imlaei": "أَنِ اغْدُوا عَلَىٰ حَرْثِكُمْ إِن كُنتُمْ صَارِمِينَ"
    },
    {
      "id": 5294,
      "verse_key": "68:23",
      "text_imlaei": "فَانطَلَقُوا وَهُمْ يَتَخَافَتُونَ"
    },
    {
      "id": 5295,
      "verse_key": "68:24",
      "text_imlaei": "أَن لَّا يَدْخُلَنَّهَا الْيَوْمَ عَلَيْكُم مِّسْكِينٌ"
    },
    {
      "id": 5296,
      "verse_key": "68:25",
      "text_imlaei": "وَغَدَوْا عَلَىٰ حَرْدٍ قَادِرِينَ"
    },
    {
      "id": 5297,
      "verse_key": "68:26",
      "text_imlaei": "فَلَمَّا رَأَوْهَا قَالُوا إِنَّا لَضَالُّونَ"
    },
    {
      "id": 5298,
      "verse_key": "68:27",
      "text_imlaei": "بَلْ نَحْنُ مَحْرُومُونَ"
    },
    {
      "id": 5299,
      "verse_key": "68:28",
      "text_imlaei": "قَالَ أَوْسَطُهُمْ أَلَمْ أَقُل لَّكُمْ لَوْلَا تُسَبِّحُونَ"
    },
    {
      "id": 5300,
      "verse_key": "68:29",
      "text_imlaei": "قَالُوا سُبْحَانَ رَبِّنَا إِنَّا كُنَّا ظَالِمِينَ"
    },
    {
      "id": 5301,
      "verse_key": "68:30",
      "text_imlaei": "فَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَلَاوَمُونَ"
    },
    {
      "id": 5302,
      "verse_key": "68:31",
      "text_imlaei": "قَالُوا يَا وَيْلَنَا إِنَّا كُنَّا طَاغِينَ"
    },
    {
      "id": 5303,
      "verse_key": "68:32",
      "text_imlaei": "عَسَىٰ رَبُّنَا أَن يُبْدِلَنَا خَيْرًا مِّنْهَا إِنَّا إِلَىٰ رَبِّنَا رَاغِبُونَ"
    },
    {
      "id": 5304,
      "verse_key": "68:33",
      "text_imlaei": "كَذَٰلِكَ الْعَذَابُ ۖ وَلَعَذَابُ الْآخِرَةِ أَكْبَرُ ۚ لَوْ كَانُوا يَعْلَمُونَ"
    },
    {
      "id": 5305,
      "verse_key": "68:34",
      "text_imlaei": "إِنَّ لِلْمُتَّقِينَ عِندَ رَبِّهِمْ جَنَّاتِ النَّعِيمِ"
    },
    {
      "id": 5306,
      "verse_key": "68:35",
      "text_imlaei": "أَفَنَجْعَلُ الْمُسْلِمِينَ كَالْمُجْرِمِينَ"
    },
    {
      "id": 5307,
      "verse_key": "68:36",
      "text_imlaei": "مَا لَكُمْ كَيْفَ تَحْكُمُونَ"
    },
    {
      "id": 5308,
      "verse_key": "68:37",
      "text_imlaei": "أَمْ لَكُمْ كِتَابٌ فِيهِ تَدْرُسُونَ"
    },
    {
      "id": 5309,
      "verse_key": "68:38",
      "text_imlaei": "إِنَّ لَكُمْ فِيهِ لَمَا تَخَيَّرُونَ"
    },
    {
      "id": 5310,
      "verse_key": "68:39",
      "text_imlaei": "أَمْ لَكُمْ أَيْمَانٌ عَلَيْنَا بَالِغَةٌ إِلَىٰ يَوْمِ الْقِيَامَةِ ۙ إِنَّ لَكُمْ لَمَا تَحْكُمُونَ"
    },
    {
      "id": 5311,
      "verse_key": "68:40",
      "text_imlaei": "سَلْهُمْ أَيُّهُم بِذَٰلِكَ زَعِيمٌ"
    },
    {
      "id": 5312,
      "verse_key": "68:41",
      "text_imlaei": "أَمْ لَهُمْ شُرَكَاءُ فَلْيَأْتُوا بِشُرَكَائِهِمْ إِن كَانُوا صَادِقِينَ"
    },
    {
      "id": 5313,
      "verse_key": "68:42",
      "text_imlaei": "يَوْمَ يُكْشَفُ عَن سَاقٍ وَيُدْعَوْنَ إِلَى السُّجُودِ فَلَا يَسْتَطِيعُونَ"
    },
    {
      "id": 5314,
      "verse_key": "68:43",
      "text_imlaei": "خَاشِعَةً أَبْصَارُهُمْ تَرْهَقُهُمْ ذِلَّةٌ ۖ وَقَدْ كَانُوا يُدْعَوْنَ إِلَى السُّجُودِ وَهُمْ سَالِمُونَ"
    },
    {
      "id": 5315,
      "verse_key": "68:44",
      "text_imlaei": "فَذَرْنِي وَمَن يُكَذِّبُ بِهَٰذَا الْحَدِيثِ ۖ سَنَسْتَدْرِجُهُم مِّنْ حَيْثُ لَا يَعْلَمُونَ"
    },
    {
      "id": 5316,
      "verse_key": "68:45",
      "text_imlaei": "وَأُمْلِي لَهُمْ ۚ إِنَّ كَيْدِي مَتِينٌ"
    },
    {
      "id": 5317,
      "verse_key": "68:46",
      "text_imlaei": "أَمْ تَسْأَلُهُمْ أَجْرًا فَهُم مِّن مَّغْرَمٍ مُّثْقَلُونَ"
    },
    {
      "id": 5318,
      "verse_key": "68:47",
      "text_imlaei": "أَمْ عِندَهُمُ الْغَيْبُ فَهُمْ يَكْتُبُونَ"
    },
    {
      "id": 5319,
      "verse_key": "68:48",
      "text_imlaei": "فَاصْبِرْ لِحُكْمِ رَبِّكَ وَلَا تَكُن كَصَاحِبِ الْحُوتِ إِذْ نَادَىٰ وَهُوَ مَكْظُومٌ"
    },
    {
      "id": 5320,
      "verse_key": "68:49",
      "text_imlaei": "لَّوْلَا أَن تَدَارَكَهُ نِعْمَةٌ مِّن رَّبِّهِ لَنُبِذَ بِالْعَرَاءِ وَهُوَ مَذْمُومٌ"
    },
    {
      "id": 5321,
      "verse_key": "68:50",
      "text_imlaei": "فَاجْتَبَاهُ رَبُّهُ فَجَعَلَهُ مِنَ الصَّالِحِينَ"
    },
    {
      "id": 5322,
      "verse_key": "68:51",
      "text_imlaei": "وَإِن يَكَادُ الَّذِينَ كَفَرُوا لَيُزْلِقُونَكَ بِأَبْصَارِهِمْ لَمَّا سَمِعُوا الذِّكْرَ وَيَقُولُونَ إِنَّهُ لَمَجْنُونٌ"
    },
    {
      "id": 5323,
      "verse_key": "68:52",
      "text_imlaei": "وَمَا هُوَ إِلَّا ذِكْرٌ لِّلْعَالَمِينَ"
    }
  ],
  "meta": { "filters": { "chapter_number": "68" } }
}
