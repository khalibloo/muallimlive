    { "resource_id": 57, "text": "Bi-ayyikumu almaftoon" },
    {
      "resource_id": 57,
      "text": "Inna rabbaka huwa aAAlamu biman dallaAAan sabeelihi wahuwa aAAlamu bilmuhtadeen"
    },
    { "resource_id": 57, "text": "Fala tutiAAi almukaththibeen" },
    { "resource_id": 57, "text": "Waddoo law tudhinu fayudhinoon" },
    { "resource_id": 57, "text": "Wala tutiAA kulla hallafinmaheen" },
    { "resource_id": 57, "text": "Hammazin mashsha-in binameem" },
    { "resource_id": 57, "text": "MannaAAin lilkhayri muAAtadin atheem" },
    { "resource_id": 57, "text": "AAutullin baAAda thalika zaneem" },
    { "resource_id": 57, "text": "An kana tha malinwabaneen" },
    {
      "resource_id": 57,
      "text": "Itha tutla AAalayhi ayatunaqala asateeru al-awwaleen"
    },
    { "resource_id": 57, "text": "Sanasimuhu AAala alkhurtoom" },
    {
      "resource_id": 57,
      "text": "Inna balawnahum kamabalawna as-haba aljannati ith aqsamoolayasrimunnaha musbiheen"
    },
    { "resource_id": 57, "text": "Wala yastathnoon" },
    {
      "resource_id": 57,
      "text": "Fatafa AAalayha ta-ifunmin rabbika wahum na-imoon"
    },
    { "resource_id": 57, "text": "Faasbahat kassareem" },
    { "resource_id": 57, "text": "Fatanadaw musbiheen" },
    {
      "resource_id": 57,
      "text": "Ani ighdoo AAala harthikum inkuntum sarimeen"
    },
    { "resource_id": 57, "text": "Fantalaqoo wahum yatakhafatoon" },
    {
      "resource_id": 57,
      "text": "An la yadkhulannaha alyawmaAAalaykum miskeen"
    },
    { "resource_id": 57, "text": "Waghadaw AAala hardin qadireen" },
    { "resource_id": 57, "text": "Falamma raawha qalooinna ladalloon" },
    { "resource_id": 57, "text": "Bal nahnu mahroomoon" },
    {
      "resource_id": 57,
      "text": "Qala awsatuhum alam aqul lakumlawla tusabbihoon"
    },
    { "resource_id": 57, "text": "Qaloo subhana rabbinainna kunna thalimeen" },
    {
      "resource_id": 57,
      "text": "Faaqbala baAAduhum AAala baAAdinyatalawamoon"
    },
    { "resource_id": 57, "text": "Qaloo ya waylana innakunna tagheen" },
    {
      "resource_id": 57,
      "text": "AAasa rabbuna an yubdilanakhayran minha inna ila rabbina raghiboon"
    },
    {
      "resource_id": 57,
      "text": "Kathalika alAAathabu walaAAathabual-akhirati akbaru law kanoo yaAAlamoon"
    },
    {
      "resource_id": 57,
      "text": "Inna lilmuttaqeena AAinda rabbihim jannatiannaAAeem"
    },
    { "resource_id": 57, "text": "AfanajAAalu almuslimeena kalmujrimeen" },
    { "resource_id": 57, "text": "Ma lakum kayfa tahkumoon" },
    { "resource_id": 57, "text": "Am lakum kitabun feehi tadrusoon" },
    { "resource_id": 57, "text": "Inna lakum feehi lama takhayyaroon" },
    {
      "resource_id": 57,
      "text": "Am lakum aymanun AAalayna balighatunila yawmi alqiyamati inna lakum lama tahkumoon"
    },
    { "resource_id": 57, "text": "Salhum ayyuhum bithalika zaAAeem" },
    {
      "resource_id": 57,
      "text": "Am lahum shurakao falya/too bishuraka-ihimin kanoo sadiqeen"
    },
    {
      "resource_id": 57,
      "text": "Yawma yukshafu AAan saqin wayudAAawnaila assujoodi fala yastateeAAoon"
    },
    {
      "resource_id": 57,
      "text": "KhashiAAatan absaruhumtarhaquhum thillatun waqad kanoo yudAAawna ilaassujoodi wahum salimoon"
    },
    {
      "resource_id": 57,
      "text": "Fatharnee waman yukaththibubihatha alhadeethi sanastadrijuhum min haythula yaAAlamoon"
    },
    { "resource_id": 57, "text": "Waomlee lahum inna kaydee mateen" },
    {
      "resource_id": 57,
      "text": "Am tas-aluhum ajran fahum min maghraminmuthqaloon"
    },
    { "resource_id": 57, "text": "Am AAindahumu alghaybu fahum yaktuboon" },
    {
      "resource_id": 57,
      "text": "Fasbir lihukmi rabbikawala takun kasahibi alhooti ith nadawahuwa makthoom"
    },
    {
      "resource_id": 57,
      "text": "Lawla an tadarakahu niAAmatunmin rabbihi lanubitha bilAAara-i wahuwa mathmoom"
    },
    {
      "resource_id": 57,
      "text": "Fajtabahu rabbuhu fajaAAalahumina assaliheen"
    },
    {
      "resource_id": 57,
      "text": "Wa-in yakadu allatheenakafaroo layuzliqoonaka bi-absarihim lamma samiAAooaththikra wayaqooloona innahu lamajnoon"
    },
    { "resource_id": 57, "text": "Wama huwa illa thikrunlilAAalameen" }
