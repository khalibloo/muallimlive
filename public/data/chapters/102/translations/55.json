{
  "translations": [
    { "resource_id": 55, "text": "«Kimniki ko`p» sizni mashg`ul qildi." },
    { "resource_id": 55, "text": "To qabrlarni ziyorat qilguningizcha." },
    { "resource_id": 55, "text": "Yo`q! Sizlar tezda bilasiz." },
    { "resource_id": 55, "text": "Yana yo`q! Sizlar tezda bilasiz." },
    {
      "resource_id": 55,
      "text": "Yo`q! Agar Siz aniq ilm ila bilganingizda edi."
    },
    { "resource_id": 55, "text": "Albatta, jahannamni ko`rasiz." },
    {
      "resource_id": 55,
      "text": "Va yana, albatta, uni ishonch ko`zi bilan ko`rasiz."
    },
    {
      "resource_id": 55,
      "text": "So`ngra, o`sha kunda, albatta, berilgan ne'matlardan so`ralasiz.<sup foot_note=\"170143\">1</sup>"
    }
  ],
