{
  "verses": [
    {
      "id": 6169,
      "verse_key": "102:1",
      "text_imlaei_simple": "الهاكم التكاثر"
    },
    {
      "id": 6170,
      "verse_key": "102:2",
      "text_imlaei_simple": "حتى زرتم المقابر"
    },
    {
      "id": 6171,
      "verse_key": "102:3",
      "text_imlaei_simple": "كلا سوف تعلمون"
    },
    {
      "id": 6172,
      "verse_key": "102:4",
      "text_imlaei_simple": "ثم كلا سوف تعلمون"
    },
    {
      "id": 6173,
      "verse_key": "102:5",
      "text_imlaei_simple": "كلا لو تعلمون علم اليقين"
    },
    { "id": 6174, "verse_key": "102:6", "text_imlaei_simple": "لترون الجحيم" },
    {
      "id": 6175,
      "verse_key": "102:7",
      "text_imlaei_simple": "ثم لترونها عين اليقين"
    },
    {
      "id": 6176,
      "verse_key": "102:8",
      "text_imlaei_simple": "ثم لتسالن يومئذ عن النعيم"
    }
  ],
