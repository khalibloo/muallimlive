{
  "verses": [
    {
      "id": 6180,
      "verse_key": "104:1",
      "text_indopak": "وَيۡلٌ لِّـكُلِّ هُمَزَةٍ لُّمَزَةِ ۙ‏"
    },
    {
      "id": 6181,
      "verse_key": "104:2",
      "text_indopak": "اۨلَّذِىۡ جَمَعَ مَالًا وَّعَدَّدَهٗ ۙ‏"
    },
    {
      "id": 6182,
      "verse_key": "104:3",
      "text_indopak": "يَحۡسَبُ اَنَّ مَالَهٗۤ اَخۡلَدَهٗ​ ۚ‏"
    },
    {
      "id": 6183,
      "verse_key": "104:4",
      "text_indopak": "كَلَّا​ لَيُنۡۢبَذَنَّ فِى الۡحُطَمَةِ  ۖ‏"
    },
    {
      "id": 6184,
      "verse_key": "104:5",
      "text_indopak": "وَمَاۤ اَدۡرٰٮكَ مَا الۡحُطَمَةُ ؕ‏"
    },
    {
      "id": 6185,
      "verse_key": "104:6",
      "text_indopak": "نَارُ اللّٰهِ الۡمُوۡقَدَةُ ۙ‏"
    },
    {
      "id": 6186,
      "verse_key": "104:7",
      "text_indopak": "الَّتِىۡ تَطَّلِعُ عَلَى الۡاَفۡـــِٕدَةِ ؕ‏"
    },
    {
      "id": 6187,
      "verse_key": "104:8",
      "text_indopak": "اِنَّهَا عَلَيۡهِمۡ مُّؤۡصَدَةٌ ۙ‏"
    },
    {
      "id": 6188,
      "verse_key": "104:9",
      "text_indopak": "فِىۡ عَمَدٍ مُّمَدَّدَةٍ‏"
    }
  ],
  "meta": { "filters": { "chapter_number": "104" } }
}
