{
  "translations": [
    {
      "resource_id": 111,
      "text": "Hamattuufi arrabsituu hundaaf badiin mirkanaa’e."
    },
    { "resource_id": 111, "text": "Isa qabeenya walitti qabee lakkaa'e." },
    { "resource_id": 111, "text": "Qabeenyi isaa isa hambisuu kan yaadu." },
    {
      "resource_id": 111,
      "text": "Haa dhorgamu! [Ibidda] caccabsituu keessatti darbamuuf jira."
    },
    {
      "resource_id": 111,
      "text": "Caccabsituun maal akka taate maaltu si beeksise?"
    },
    {
      "resource_id": 111,
      "text": "(Isheen) ibidda Rabbii kan qabsiifamtuudha."
    },
    {
      "resource_id": 111,
      "text": "Ishee onnee irratti (laalaan ishee) mul’attuudha."
    },
    {
      "resource_id": 111,
      "text": "Dhugumatti, isheen isaan irratti cufamtuudha."
    },
    {
      "resource_id": 111,
      "text": "Utubaalee diriirfamaa keessatti (itti cufama)."
    }
  ],
