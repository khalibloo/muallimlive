{
  "translations": [
    { "resource_id": 214, "text": "Teško svakom klevetniku podrugljivcu," },
    { "resource_id": 214, "text": "koji blago gomila i prebrojava ga," },
    {
      "resource_id": 214,
      "text": "i misli da će ga blago njegovo besmrtnim učiniti."
    },
    {
      "resource_id": 214,
      "text": "A ne treba tako! On će, sigurno, biti bačen u Hutamu!"
    },
    { "resource_id": 214, "text": "A znaš li ti šta je Hutama?" },
    { "resource_id": 214, "text": "Vatra Allahova razbuktala," },
    { "resource_id": 214, "text": "koja će do srca dopirati." },
    { "resource_id": 214, "text": "Ona će nad njima biti zatvorena" },
    { "resource_id": 214, "text": "plamenim izduženim stubovima." }
