{
  "translations": [
    { "resource_id": 113, "text": "Күн оратылып бүктелген кезде," },
    { "resource_id": 113, "text": "әрі жұлдыздар төгілген кезде" },
    {
      "resource_id": 113,
      "text": "және таулар  / орнынан /  қозғалтылған кезде,"
    },
    {
      "resource_id": 113,
      "text": "және он айлық буаз түйелер қараусыз қалған кезде,"
    },
    { "resource_id": 113, "text": "әрі аңдар жиылған кезде," },
    { "resource_id": 113, "text": "және теңіздер отқа айналған кезде," },
    { "resource_id": 113, "text": "және жандар топтастырылған кезде," },
    { "resource_id": 113, "text": "әрі тірідей көмілген қыз сұралған кезде," },
    { "resource_id": 113, "text": "«Қай күнәсі үшін өлтірілді?» -  / деп / ." },
    { "resource_id": 113, "text": "/ Аман /  жазбалары таратылған кезде," },
    { "resource_id": 113, "text": "және аспан сыдырылып алынған кезде," },
    { "resource_id": 113, "text": "және Жахим (Тозақ) жандырылған кезде," },
    { "resource_id": 113, "text": "әрі Жәннат жақындатылған кезде" },
    {
      "resource_id": 113,
      "text": "әрбір жан нені дайындап, алып келгенін біледі."
    },
    { "resource_id": 113, "text": "Жоқ!  Қайтушылармен ант етемін," },
    { "resource_id": 113, "text": "қозғалушы, жасырынушы." },
    { "resource_id": 113, "text": "әрі қараңғылығы түсе бастаған түнмен," },
    { "resource_id": 113, "text": "және тыныс алып, ағарабастаған таңмен." },
    { "resource_id": 113, "text": "Анығында, бұл - ардақты Елшінің сөзі," },
    {
      "resource_id": 113,
      "text": "күшті және Аршы Иесінің алдында мәртебелі,"
    },
    {
      "resource_id": 113,
      "text": "оған ол жақта  / періштелер /  бағынатын, сенімді."
    },
    { "resource_id": 113, "text": "Сендердің жолдастарың жынды емес," },
    { "resource_id": 113, "text": "Ол, оны анық көкжиекте көрді." },
    {
      "resource_id": 113,
      "text": "Әрі ол ғайыпты  / уахиды /  жеткізуде сараңдық  / кемітушілік /  істемеді,"
    },
    { "resource_id": 113, "text": "және бұл - қуылған шайтанның сөзі емес." },
    { "resource_id": 113, "text": "Сонда, қайда барасыңдар?" },
    {
      "resource_id": 113,
      "text": "Ол  / Құран /  - әлемдер үшін еске салу ғана,"
    },
    {
      "resource_id": 113,
      "text": "сендерден туралықты ұстануды қалағандар үшін."
    },
    {
      "resource_id": 113,
      "text": "Әлемдердің Раббысы Аллаһ қаламайынша, сендер қалай алмайсыңдар."
    }
  ],
  "meta": {
