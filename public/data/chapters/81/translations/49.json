{
  "translations": [
    { "resource_id": 49, "text": "Jua litakapo kunjwa," },
    { "resource_id": 49, "text": "Na nyota zikazimwa," },
    { "resource_id": 49, "text": "Na milima ikaondolewa," },
    {
      "resource_id": 49,
      "text": "Na ngamia wenye mimba pevu watakapo achwa wasishughulikiwe,"
    },
    { "resource_id": 49, "text": "Na wanyama wa mwituni wakakusanywa," },
    { "resource_id": 49, "text": "Na bahari zikawaka moto," },
    { "resource_id": 49, "text": "Na nafsi zikaunganishwa," },
    { "resource_id": 49, "text": "Na msichana aliye zikwa hai atapo ulizwa," },
    { "resource_id": 49, "text": "Kwa kosa gani aliuliwa?" },
    { "resource_id": 49, "text": "Na madaftari yatakapo enezwa," },
    { "resource_id": 49, "text": "Na mbingu itapo tanduliwa," },
    { "resource_id": 49, "text": "Na Jahannamu itapo chochewa," },
    { "resource_id": 49, "text": "Na Pepo ikasogezwa," },
    { "resource_id": 49, "text": "Kila nafsi itajua ilicho kihudhurisha." },
    { "resource_id": 49, "text": "Naapa kwa nyota zinapo rejea nyuma," },
    { "resource_id": 49, "text": "Zinazo kwenda, kisha zikajificha," },
    { "resource_id": 49, "text": "Na kwa usiku unapo pungua," },
    { "resource_id": 49, "text": "Na kwa asubuhi inapo pambazuka," },
    {
      "resource_id": 49,
      "text": "Kwamba hakika bila ya shaka hii ni kauli ya Mjumbe mtukufu,"
    },
    {
      "resource_id": 49,
      "text": "Mwenye nguvu na mwenye cheo kwa huyo Mwenye Kiti cha Enzi,"
    },
    { "resource_id": 49, "text": "Anaye t'iiwa, tena muaminifu." },
    { "resource_id": 49, "text": "Na wala huyu mwenzenu hana wazimu." },
    {
      "resource_id": 49,
      "text": "Na hakika yeye alimwona kwenye upeo wa macho ulio safi."
    },
    { "resource_id": 49, "text": "Wala yeye si bakhili kwa mambo ya ghaibu." },
    { "resource_id": 49, "text": "Wala hii si kauli ya Shetani maluuni." },
    { "resource_id": 49, "text": "Basi mnakwenda wapi?" },
    {
      "resource_id": 49,
      "text": "Haikuwa hii ila ni ukumbusho kwa walimwengu wote."
    },
    {
      "resource_id": 49,
      "text": "Kwa yule miongoni mwenu anaye taka kwenda sawa."
    },
    {
      "resource_id": 49,
      "text": "Wala nyinyi hamtataka isipo kuwa atake Mwenyezi Mungu Mola Mlezi wa walimwengu wote."
    }
  ],
  "meta": {
