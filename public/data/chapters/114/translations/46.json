{
  "translations": [
    {
      "resource_id": 46,
      "text": "Waxaad dhahdaa «Nabiyow» waxaan ka magangeley Eebaha dadka barbaariya."
    },
    { "resource_id": 46, "text": "Ee dadka xukuma." },
    { "resource_id": 46, "text": "Ee ah Ilaaha dadka." },
    {
      "resource_id": 46,
      "text": "Sharka «iyo xumaanta» waswaasiyaha qarsoon."
    },
    { "resource_id": 46, "text": "Ee waswaasiya laabta «Qalbiga» dadka." },
    {
      "resource_id": 46,
      "text": "«Kuwaas wax waswaasinayna» oo ah jinni iyo insiba."
    }
  ],
