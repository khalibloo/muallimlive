      "resource_id": 27,
      "text": "Sag: Ich nehme Zuflucht beim Herrn der Menschen,"
    },
    { "resource_id": 27, "text": "dem König der Menschen," },
    { "resource_id": 27, "text": "dem Gott der Menschen," },
    {
      "resource_id": 27,
      "text": "vor dem Übel des Einflüsterers, des Davonschleichers,"
    },
    {
      "resource_id": 27,
      "text": "der in die Brüste der Menschen einflüstert,"
    },
    { "resource_id": 27, "text": "von den Ginn und den Menschen." }
