{
  "translations": [
    {
      "resource_id": 774,
      "text": "Vuga (yewe Muhamadi) uti “Niragije (Allah), Nyagasani w’abantu,”"
    },
    { "resource_id": 774, "text": "“Umwami w’abantu,”" },
    { "resource_id": 774, "text": "“Imana y’abantu,”" },
    {
      "resource_id": 774,
      "text": "“Ngo andinde ububi bwa (shitani) yoshya gukora ibibi, nyuma ikabireka (igihe umuntu yibutse Allah),”"
    },
    { "resource_id": 774, "text": "“Yoshya imitima y’abantu gukora ibibi; ”" },
    {
      "resource_id": 774,
      "text": "“Yaba (shitani) yo mu majini cyangwa mu bantu.”"
    }
  ],
