{
  "translations": [
    {
      "resource_id": 823,
      "text": "Say, ‘I seek refuge in the Lord of people,"
    },
    { "resource_id": 823, "text": "the King of people," },
    { "resource_id": 823, "text": "the God of people," },
    {
      "resource_id": 823,
      "text": "from the mischief of every sneaking whisperer,"
    },
    { "resource_id": 823, "text": "who whispers into the hearts of people," },
    {
      "resource_id": 823,
      "text": "from jinn and men.’<sup foot_note=213015>1</sup>"
    }
  ],
