    { "resource_id": 74, "text": "Албатта аз паи душворӣ осонист." },
    { "resource_id": 74, "text": "Чун аз кор фориғ шавӣ, ба ибодат кӯш" },
    { "resource_id": 74, "text": "ва ба Парвардигорат рағбат намо!" }
