{
  "translations": [
    {
      "resource_id": 774,
      "text": "Ese (yewe Muhamadi) ntitwakwaguriye igituza?"
    },
    {
      "resource_id": 774,
      "text": "Tukagutura umutwaro (tukorohereza gusohoza ubutumwa),"
    },
    { "resource_id": 774, "text": "Wari ukuremereye umugongo?" },
    { "resource_id": 774, "text": "Tukanamenyekanisha izina ryawe?" },
    {
      "resource_id": 774,
      "text": "Mu by’ukuri nyuma y’ingorane haza ibihe byiza."
    },
    { "resource_id": 774, "text": "Rwose, nyuma y’ingorane haza ibihe byiza." },
    {
      "resource_id": 774,
      "text": "Bityo nuhuguka, ujye ushishikara (mu kugaragira Allah)."
    },
    {
      "resource_id": 774,
      "text": "Kandi (imigambi n’ibyiringiro byawe) ujye ubitura Nyagasani wawe (wenyine)."
    }
  ],
