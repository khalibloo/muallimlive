{
  "verses": [
    {
      "id": 6091,
      "verse_key": "94:1",
      "text_imlaei_simple": "الم نشرح لك صدرك"
    },
    {
      "id": 6092,
      "verse_key": "94:2",
      "text_imlaei_simple": "ووضعنا عنك وزرك"
    },
    { "id": 6093, "verse_key": "94:3", "text_imlaei_simple": "الذي انقض ظهرك" },
    { "id": 6094, "verse_key": "94:4", "text_imlaei_simple": "ورفعنا لك ذكرك" },
    {
      "id": 6095,
      "verse_key": "94:5",
      "text_imlaei_simple": "فان مع العسر يسرا"
    },
    {
      "id": 6096,
      "verse_key": "94:6",
      "text_imlaei_simple": "ان مع العسر يسرا"
    },
    {
      "id": 6097,
      "verse_key": "94:7",
      "text_imlaei_simple": "فاذا فرغت فانصب"
    },
    { "id": 6098, "verse_key": "94:8", "text_imlaei_simple": "والى ربك فارغب" }
  ],
