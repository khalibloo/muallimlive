    {
      "id": 6091,
      "verse_key": "94:1",
      "text_indopak": "اَلَمۡ نَشۡرَحۡ لَـكَ صَدۡرَكَۙ‏"
    },
    {
      "id": 6092,
      "verse_key": "94:2",
      "text_indopak": "وَوَضَعۡنَا عَنۡكَ وِزۡرَكَۙ‏"
    },
    {
      "id": 6093,
      "verse_key": "94:3",
      "text_indopak": "الَّذِىۡۤ اَنۡقَضَ ظَهۡرَكَۙ‏"
    },
    {
      "id": 6094,
      "verse_key": "94:4",
      "text_indopak": "وَرَفَعۡنَا لَـكَ ذِكۡرَكَؕ‏"
    },
    {
      "id": 6095,
      "verse_key": "94:5",
      "text_indopak": "فَاِنَّ مَعَ الۡعُسۡرِ يُسۡرًا ۙ‏"
    },
    {
      "id": 6096,
      "verse_key": "94:6",
      "text_indopak": "اِنَّ مَعَ الۡعُسۡرِ يُسۡرًا ؕ‏"
    },
    {
      "id": 6097,
      "verse_key": "94:7",
      "text_indopak": "فَاِذَا فَرَغۡتَ فَانۡصَبۡۙ‏"
    },
    {
      "id": 6098,
      "verse_key": "94:8",
      "text_indopak": "وَاِلٰى رَبِّكَ فَارۡغَب‏"
    }
  ],
