{
  "verses": [
    { "id": 4902, "verse_key": "55:1", "text_imlaei": "الرَّحْمَٰنُ" },
    { "id": 4903, "verse_key": "55:2", "text_imlaei": "عَلَّمَ الْقُرْآنَ" },
    { "id": 4904, "verse_key": "55:3", "text_imlaei": "خَلَقَ الْإِنسَانَ" },
    { "id": 4905, "verse_key": "55:4", "text_imlaei": "عَلَّمَهُ الْبَيَانَ" },
    {
      "id": 4906,
      "verse_key": "55:5",
      "text_imlaei": "الشَّمْسُ وَالْقَمَرُ بِحُسْبَانٍ"
    },
    {
      "id": 4907,
      "verse_key": "55:6",
      "text_imlaei": "وَالنَّجْمُ وَالشَّجَرُ يَسْجُدَانِ"
    },
    {
      "id": 4908,
      "verse_key": "55:7",
      "text_imlaei": "وَالسَّمَاءَ رَفَعَهَا وَوَضَعَ الْمِيزَانَ"
    },
    {
      "id": 4909,
      "verse_key": "55:8",
      "text_imlaei": "أَلَّا تَطْغَوْا فِي الْمِيزَانِ"
    },
    {
      "id": 4910,
      "verse_key": "55:9",
      "text_imlaei": "وَأَقِيمُوا الْوَزْنَ بِالْقِسْطِ وَلَا تُخْسِرُوا الْمِيزَانَ"
    },
    {
      "id": 4911,
      "verse_key": "55:10",
      "text_imlaei": "وَالْأَرْضَ وَضَعَهَا لِلْأَنَامِ"
    },
    {
      "id": 4912,
      "verse_key": "55:11",
      "text_imlaei": "فِيهَا فَاكِهَةٌ وَالنَّخْلُ ذَاتُ الْأَكْمَامِ"
    },
    {
      "id": 4913,
      "verse_key": "55:12",
      "text_imlaei": "وَالْحَبُّ ذُو الْعَصْفِ وَالرَّيْحَانُ"
    },
    {
      "id": 4914,
      "verse_key": "55:13",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4915,
      "verse_key": "55:14",
      "text_imlaei": "خَلَقَ الْإِنسَانَ مِن صَلْصَالٍ كَالْفَخَّارِ"
    },
    {
      "id": 4916,
      "verse_key": "55:15",
      "text_imlaei": "وَخَلَقَ الْجَانَّ مِن مَّارِجٍ مِّن نَّارٍ"
    },
    {
      "id": 4917,
      "verse_key": "55:16",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4918,
      "verse_key": "55:17",
      "text_imlaei": "رَبُّ الْمَشْرِقَيْنِ وَرَبُّ الْمَغْرِبَيْنِ"
    },
    {
      "id": 4919,
      "verse_key": "55:18",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4920,
      "verse_key": "55:19",
      "text_imlaei": "مَرَجَ الْبَحْرَيْنِ يَلْتَقِيَانِ"
    },
    {
      "id": 4921,
      "verse_key": "55:20",
      "text_imlaei": "بَيْنَهُمَا بَرْزَخٌ لَّا يَبْغِيَانِ"
    },
    {
      "id": 4922,
      "verse_key": "55:21",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4923,
      "verse_key": "55:22",
      "text_imlaei": "يَخْرُجُ مِنْهُمَا اللُّؤْلُؤُ وَالْمَرْجَانُ"
    },
    {
      "id": 4924,
      "verse_key": "55:23",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4925,
      "verse_key": "55:24",
      "text_imlaei": "وَلَهُ الْجَوَارِ الْمُنشَآتُ فِي الْبَحْرِ كَالْأَعْلَامِ"
    },
    {
      "id": 4926,
      "verse_key": "55:25",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4927,
      "verse_key": "55:26",
      "text_imlaei": "كُلُّ مَنْ عَلَيْهَا فَانٍ"
    },
    {
      "id": 4928,
      "verse_key": "55:27",
      "text_imlaei": "وَيَبْقَىٰ وَجْهُ رَبِّكَ ذُو الْجَلَالِ وَالْإِكْرَامِ"
    },
    {
      "id": 4929,
      "verse_key": "55:28",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4930,
      "verse_key": "55:29",
      "text_imlaei": "يَسْأَلُهُ مَن فِي السَّمَاوَاتِ وَالْأَرْضِ ۚ كُلَّ يَوْمٍ هُوَ فِي شَأْنٍ"
    },
    {
      "id": 4931,
      "verse_key": "55:30",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4932,
      "verse_key": "55:31",
      "text_imlaei": "سَنَفْرُغُ لَكُمْ أَيُّهَ الثَّقَلَانِ"
    },
    {
      "id": 4933,
      "verse_key": "55:32",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4934,
      "verse_key": "55:33",
      "text_imlaei": "يَا مَعْشَرَ الْجِنِّ وَالْإِنسِ إِنِ اسْتَطَعْتُمْ أَن تَنفُذُوا مِنْ أَقْطَارِ السَّمَاوَاتِ وَالْأَرْضِ فَانفُذُوا ۚ لَا تَنفُذُونَ إِلَّا بِسُلْطَانٍ"
    },
    {
      "id": 4935,
      "verse_key": "55:34",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4936,
      "verse_key": "55:35",
      "text_imlaei": "يُرْسَلُ عَلَيْكُمَا شُوَاظٌ مِّن نَّارٍ وَنُحَاسٌ فَلَا تَنتَصِرَانِ"
    },
    {
      "id": 4937,
      "verse_key": "55:36",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4938,
      "verse_key": "55:37",
      "text_imlaei": "فَإِذَا انشَقَّتِ السَّمَاءُ فَكَانَتْ وَرْدَةً كَالدِّهَانِ"
    },
    {
      "id": 4939,
      "verse_key": "55:38",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4940,
      "verse_key": "55:39",
      "text_imlaei": "فَيَوْمَئِذٍ لَّا يُسْأَلُ عَن ذَنبِهِ إِنسٌ وَلَا جَانٌّ"
    },
    {
      "id": 4941,
      "verse_key": "55:40",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4942,
      "verse_key": "55:41",
      "text_imlaei": "يُعْرَفُ الْمُجْرِمُونَ بِسِيمَاهُمْ فَيُؤْخَذُ بِالنَّوَاصِي وَالْأَقْدَامِ"
    },
    {
      "id": 4943,
      "verse_key": "55:42",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4944,
      "verse_key": "55:43",
      "text_imlaei": "هَٰذِهِ جَهَنَّمُ الَّتِي يُكَذِّبُ بِهَا الْمُجْرِمُونَ"
    },
    {
      "id": 4945,
      "verse_key": "55:44",
      "text_imlaei": "يَطُوفُونَ بَيْنَهَا وَبَيْنَ حَمِيمٍ آنٍ"
    },
    {
      "id": 4946,
      "verse_key": "55:45",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4947,
      "verse_key": "55:46",
      "text_imlaei": "وَلِمَنْ خَافَ مَقَامَ رَبِّهِ جَنَّتَانِ"
    },
    {
      "id": 4948,
      "verse_key": "55:47",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    { "id": 4949, "verse_key": "55:48", "text_imlaei": "ذَوَاتَا أَفْنَانٍ" },
    {
      "id": 4950,
      "verse_key": "55:49",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4951,
      "verse_key": "55:50",
      "text_imlaei": "فِيهِمَا عَيْنَانِ تَجْرِيَانِ"
    },
    {
      "id": 4952,
      "verse_key": "55:51",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4953,
      "verse_key": "55:52",
      "text_imlaei": "فِيهِمَا مِن كُلِّ فَاكِهَةٍ زَوْجَانِ"
    },
    {
      "id": 4954,
      "verse_key": "55:53",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4955,
      "verse_key": "55:54",
      "text_imlaei": "مُتَّكِئِينَ عَلَىٰ فُرُشٍ بَطَائِنُهَا مِنْ إِسْتَبْرَقٍ ۚ وَجَنَى الْجَنَّتَيْنِ دَانٍ"
    },
    {
      "id": 4956,
      "verse_key": "55:55",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4957,
      "verse_key": "55:56",
      "text_imlaei": "فِيهِنَّ قَاصِرَاتُ الطَّرْفِ لَمْ يَطْمِثْهُنَّ إِنسٌ قَبْلَهُمْ وَلَا جَانٌّ"
    },
    {
      "id": 4958,
      "verse_key": "55:57",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4959,
      "verse_key": "55:58",
      "text_imlaei": "كَأَنَّهُنَّ الْيَاقُوتُ وَالْمَرْجَانُ"
    },
    {
      "id": 4960,
      "verse_key": "55:59",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4961,
      "verse_key": "55:60",
      "text_imlaei": "هَلْ جَزَاءُ الْإِحْسَانِ إِلَّا الْإِحْسَانُ"
    },
    {
      "id": 4962,
      "verse_key": "55:61",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4963,
      "verse_key": "55:62",
      "text_imlaei": "وَمِن دُونِهِمَا جَنَّتَانِ"
    },
    {
      "id": 4964,
      "verse_key": "55:63",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    { "id": 4965, "verse_key": "55:64", "text_imlaei": "مُدْهَامَّتَانِ" },
    {
      "id": 4966,
      "verse_key": "55:65",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4967,
      "verse_key": "55:66",
      "text_imlaei": "فِيهِمَا عَيْنَانِ نَضَّاخَتَانِ"
    },
    {
      "id": 4968,
      "verse_key": "55:67",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4969,
      "verse_key": "55:68",
      "text_imlaei": "فِيهِمَا فَاكِهَةٌ وَنَخْلٌ وَرُمَّانٌ"
    },
    {
      "id": 4970,
      "verse_key": "55:69",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4971,
      "verse_key": "55:70",
      "text_imlaei": "فِيهِنَّ خَيْرَاتٌ حِسَانٌ"
    },
    {
      "id": 4972,
      "verse_key": "55:71",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4973,
      "verse_key": "55:72",
      "text_imlaei": "حُورٌ مَّقْصُورَاتٌ فِي الْخِيَامِ"
    },
    {
      "id": 4974,
      "verse_key": "55:73",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4975,
      "verse_key": "55:74",
      "text_imlaei": "لَمْ يَطْمِثْهُنَّ إِنسٌ قَبْلَهُمْ وَلَا جَانٌّ"
    },
    {
      "id": 4976,
      "verse_key": "55:75",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4977,
      "verse_key": "55:76",
      "text_imlaei": "مُتَّكِئِينَ عَلَىٰ رَفْرَفٍ خُضْرٍ وَعَبْقَرِيٍّ حِسَانٍ"
    },
    {
      "id": 4978,
      "verse_key": "55:77",
      "text_imlaei": "فَبِأَيِّ آلَاءِ رَبِّكُمَا تُكَذِّبَانِ"
    },
    {
      "id": 4979,
      "verse_key": "55:78",
      "text_imlaei": "تَبَارَكَ اسْمُ رَبِّكَ ذِي الْجَلَالِ وَالْإِكْرَامِ"
    }
  ],
