{
  "verses": [
    {
      "id": 5623,
      "verse_key": "77:1",
      "text_indopak": "وَالۡمُرۡسَلٰتِ عُرۡفًا ۙ‏"
    },
    {
      "id": 5624,
      "verse_key": "77:2",
      "text_indopak": "فَالۡعٰصِفٰتِ عَصۡفًا ۙ‏"
    },
    {
      "id": 5625,
      "verse_key": "77:3",
      "text_indopak": "وَّالنّٰشِرٰتِ نَشۡرًا ۙ‏"
    },
    {
      "id": 5626,
      "verse_key": "77:4",
      "text_indopak": "فَالۡفٰرِقٰتِ فَرۡقًا ۙ‏"
    },
    {
      "id": 5627,
      "verse_key": "77:5",
      "text_indopak": "فَالۡمُلۡقِيٰتِ ذِكۡرًا ۙ‏"
    },
    {
      "id": 5628,
      "verse_key": "77:6",
      "text_indopak": "عُذۡرًا اَوۡ نُذۡرًا ۙ‏"
    },
    {
      "id": 5629,
      "verse_key": "77:7",
      "text_indopak": "اِنَّمَا تُوۡعَدُوۡنَ لَوَاقِعٌ ؕ‏"
    },
    {
      "id": 5630,
      "verse_key": "77:8",
      "text_indopak": "فَاِذَا النُّجُوۡمُ طُمِسَتۡۙ‏"
    },
    {
      "id": 5631,
      "verse_key": "77:9",
      "text_indopak": "وَ اِذَا السَّمَآءُ فُرِجَتۡۙ‏"
    },
    {
      "id": 5632,
      "verse_key": "77:10",
      "text_indopak": "وَاِذَا الۡجِبَالُ نُسِفَتۡۙ‏"
    },
    {
      "id": 5633,
      "verse_key": "77:11",
      "text_indopak": "وَاِذَا الرُّسُلُ اُقِّتَتۡؕ‏"
    },
    {
      "id": 5634,
      "verse_key": "77:12",
      "text_indopak": "لِاَىِّ يَوۡمٍ اُجِّلَتۡؕ‏"
    },
    {
      "id": 5635,
      "verse_key": "77:13",
      "text_indopak": "لِيَوۡمِ الۡفَصۡلِ​ۚ‏"
    },
    {
      "id": 5636,
      "verse_key": "77:14",
      "text_indopak": "وَمَاۤ اَدۡرٰٮكَ مَا يَوۡمُ الۡفَصۡلِؕ‏"
    },
    {
      "id": 5637,
      "verse_key": "77:15",
      "text_indopak": "وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ‏"
    },
    {
      "id": 5638,
      "verse_key": "77:16",
      "text_indopak": "اَلَمۡ نُهۡلِكِ الۡاَوَّلِيۡنَؕ‏"
    },
    {
      "id": 5639,
      "verse_key": "77:17",
      "text_indopak": "ثُمَّ نُتۡبِعُهُمُ الۡاٰخِرِيۡنَ‏"
    },
    {
      "id": 5640,
      "verse_key": "77:18",
      "text_indopak": "كَذٰلِكَ نَفۡعَلُ بِالۡمُجۡرِمِيۡنَ‏"
    },
    {
      "id": 5641,
      "verse_key": "77:19",
      "text_indopak": "وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ‏"
    },
    {
      "id": 5642,
      "verse_key": "77:20",
      "text_indopak": "اَلَمۡ نَخۡلُقۡكُّمۡ مِّنۡ مَّآءٍ مَّهِيۡنٍۙ‏"
    },
    {
      "id": 5643,
      "verse_key": "77:21",
      "text_indopak": "فَجَعَلۡنٰهُ فِىۡ قَرَارٍ مَّكِيۡنٍۙ‏"
    },
    {
      "id": 5644,
      "verse_key": "77:22",
      "text_indopak": "اِلٰى قَدَرٍ مَّعۡلُوۡمٍۙ‏"
    },
    {
      "id": 5645,
      "verse_key": "77:23",
      "text_indopak": "فَقَدَرۡنَا ۖ فَنِعۡمَ الۡقٰدِرُوۡنَ‏"
    },
    {
      "id": 5646,
      "verse_key": "77:24",
      "text_indopak": "وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ‏"
    },
    {
      "id": 5647,
      "verse_key": "77:25",
      "text_indopak": "اَلَمۡ نَجۡعَلِ الۡاَرۡضَ كِفَاتًا ۙ‏"
    },
    {
      "id": 5648,
      "verse_key": "77:26",
      "text_indopak": "اَحۡيَآءً وَّاَمۡوَاتًا ۙ‏"
    },
    {
      "id": 5649,
      "verse_key": "77:27",
      "text_indopak": "وَّجَعَلۡنَا فِيۡهَا رَوَاسِىَ شٰمِخٰتٍ وَّ اَسۡقَيۡنٰكُمۡ مَّآءً فُرَاتًا ؕ‏"
    },
    {
      "id": 5650,
      "verse_key": "77:28",
      "text_indopak": "وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ‏"
    },
    {
      "id": 5651,
      "verse_key": "77:29",
      "text_indopak": "اِنْطَلِقُوۡۤا اِلٰى مَا كُنۡتُمۡ بِهٖ تُكَذِّبُوۡنَ​ۚ‏"
    },
    {
      "id": 5652,
      "verse_key": "77:30",
      "text_indopak": "اِنْطَلِقُوۡۤا اِلٰى ظِلٍّ ذِىۡ ثَلٰثِ شُعَبٍۙ‏"
    },
    {
      "id": 5653,
      "verse_key": "77:31",
      "text_indopak": "لَّا ظَلِيۡلٍ وَّلَا يُغۡنِىۡ مِنَ اللَّهَبِؕ‏"
    },
    {
      "id": 5654,
      "verse_key": "77:32",
      "text_indopak": "اِنَّهَا تَرۡمِىۡ بِشَرَرٍ كَالۡقَصۡرِ​ۚ‏"
    },
    {
      "id": 5655,
      "verse_key": "77:33",
      "text_indopak": "كَاَنَّهٗ جِمٰلَتٌ صُفۡرٌ ؕ‏"
    },
    {
      "id": 5656,
      "verse_key": "77:34",
      "text_indopak": "وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ‏"
    },
    {
      "id": 5657,
      "verse_key": "77:35",
      "text_indopak": "هٰذَا يَوۡمُ لَا يَنۡطِقُوۡنَۙ‏"
    },
    {
      "id": 5658,
      "verse_key": "77:36",
      "text_indopak": "وَلَا يُؤۡذَنُ لَهُمۡ فَيَـعۡتَذِرُوۡنَ‏"
    },
    {
      "id": 5659,
      "verse_key": "77:37",
      "text_indopak": "وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ‏"
    },
    {
      "id": 5660,
      "verse_key": "77:38",
      "text_indopak": "هٰذَا يَوۡمُ الۡفَصۡلِ​ۚ جَمَعۡنٰكُمۡ وَالۡاَوَّلِيۡنَ‏"
    },
    {
      "id": 5661,
      "verse_key": "77:39",
      "text_indopak": "فَاِنۡ كَانَ لَـكُمۡ كَيۡدٌ فَكِيۡدُوۡنِ‏"
    },
    {
      "id": 5662,
      "verse_key": "77:40",
      "text_indopak": "وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ‏"
    },
    {
      "id": 5663,
      "verse_key": "77:41",
      "text_indopak": "اِنَّ الۡمُتَّقِيۡنَ فِىۡ ظِلٰلٍ وَّعُيُوۡنٍۙ‏"
    },
    {
      "id": 5664,
      "verse_key": "77:42",
      "text_indopak": "وَّفَوَاكِهَ مِمَّا يَشۡتَهُوۡنَؕ‏"
    },
    {
      "id": 5665,
      "verse_key": "77:43",
      "text_indopak": "كُلُوۡا وَاشۡرَبُوۡا هَنِيْٓئًا ۢ بِمَا كُنۡتُمۡ تَعۡمَلُوۡنَ‏"
    },
    {
      "id": 5666,
      "verse_key": "77:44",
      "text_indopak": "اِنَّا كَذٰلِكَ نَجۡزِى الۡمُحۡسِنِيۡنَ‏"
    },
    {
      "id": 5667,
      "verse_key": "77:45",
      "text_indopak": "وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ‏"
    },
    {
      "id": 5668,
      "verse_key": "77:46",
      "text_indopak": "كُلُوۡا وَتَمَتَّعُوۡا قَلِيۡلًا اِنَّكُمۡ مُّجۡرِمُوۡنَ‏"
    },
    {
      "id": 5669,
      "verse_key": "77:47",
      "text_indopak": "وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ‏"
    },
    {
      "id": 5670,
      "verse_key": "77:48",
      "text_indopak": "وَاِذَا قِيۡلَ لَهُمُ ارۡكَعُوۡا لَا يَرۡكَعُوۡنَ‏"
    },
    {
      "id": 5671,
      "verse_key": "77:49",
      "text_indopak": "وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ‏"
    },
    {
      "id": 5672,
      "verse_key": "77:50",
      "text_indopak": "فَبِاَىِّ حَدِيۡثٍۢ بَعۡدَهٗ يُؤۡمِنُوۡنَ‏"
    }
  ],
  "meta": { "filters": { "chapter_number": "77" } }
}
