{
  "translations": [
    {
      "resource_id": 25,
      "text": "Čitaj s imenom Gospodara tvoga Koji stvara,"
    },
    { "resource_id": 25, "text": "stvara čovjeka od zakvačka." },
    { "resource_id": 25, "text": "Čitaj, plemenit je Gospodar tvoj," },
    { "resource_id": 25, "text": "Koji poučava peru," },
    { "resource_id": 25, "text": "Koji čovjeka poučava onome što ne zna." },
    { "resource_id": 25, "text": "Uistinu čovjek se uzobijesti" },
    { "resource_id": 25, "text": "čim se neovisnim osjeti" },
    {
      "resource_id": 25,
      "text": "a Gospodaru tvome će se, doista, svi vratiti!"
    },
    { "resource_id": 25, "text": "Vidjeli ti onoga koji sprečava" },
    { "resource_id": 25, "text": "roba da klanja?" },
    { "resource_id": 25, "text": "Šta misliš, ako on je on na Pravom putu," },
    { "resource_id": 25, "text": "ili ako bogobojaznost naređuje?!" },
    { "resource_id": 25, "text": "Šta misliš, ako on poriče i okreće se," },
    { "resource_id": 25, "text": "zar on ne zna da Allah vidi?!" },
    {
      "resource_id": 25,
      "text": "Naprotiv! Ako se ne okani, ocrnit ćemo mu čelo,"
    },
    { "resource_id": 25, "text": "čelo lažno i grešno," },
    { "resource_id": 25, "text": "pa neka on pozove društvo svoje." },
    { "resource_id": 25, "text": "Mi ćemo pozvati zebanije." },
    {
      "resource_id": 25,
      "text": "Naprotiv! Ti se njemu ne pokoravaj, već čini sedždu i približi se Gospodaru svome!"
    }
  ],
  "meta": {
