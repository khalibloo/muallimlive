{
  "translations": [
    {
      "resource_id": 111,
      "text": "Maqaa Gooftaa keetii Isa (waan hunda) uumeetiin dubbisi."
    },
    { "resource_id": 111, "text": "(Isa) nama dhiiga ititaa irraa uume." },
    {
      "resource_id": 111,
      "text": "Haala Gooftaan kee hunda irra arjaa taâ€™een qara'i."
    },
    { "resource_id": 111, "text": "Isa qalamaan (barreessuu) barsiise sani." },
    { "resource_id": 111, "text": "Nama, waan inni hin beekne barsiise." },
    { "resource_id": 111, "text": "Dhugumatti, namni daangaa dabra" },
    { "resource_id": 111, "text": "Sababa duroomuu of arguutiin." },
    { "resource_id": 111, "text": "Deebiin garuma Gooftaa keetiiti." },
    { "resource_id": 111, "text": "Sila isa dhorgu san argitee?" },
    {
      "resource_id": 111,
      "text": "Gabricha yeroo inni salaatu (kan dhorgu argitee)?"
    },
    {
      "resource_id": 111,
      "text": "Yoo inni qajeeluma irra jiraate hoo mee naaf himi!"
    },
    {
      "resource_id": 111,
      "text": "Yookiin immoo sodaa Rabbiitti yoo ajaje hoo?"
    },
    {
      "resource_id": 111,
      "text": "Yoo inni (Abuu Jahal) kijibsiisee irraa gara gale hoo mee naaf himi?"
    },
    { "resource_id": 111, "text": "Sila inni Rabbiin arguu hin beekuu?" },
    {
      "resource_id": 111,
      "text": "Haa dhorgamu; yoo inni (Abuu Jahal) dhorgamuu baate, sammuu qabna."
    },
    { "resource_id": 111, "text": "Sammuu sobduu, dogoggortuu taate (qabna)." },
    { "resource_id": 111, "text": "Garee isaa haa waammatu." },
    {
      "resource_id": 111,
      "text": "Nutis waardiyyaa keenya waammachuuf jirra."
    },
    {
      "resource_id": 111,
      "text": "Dhorgami! Isaan hin ajajaminSujuudis gara Rabbiittis dhihaadhu."
    }
  ],
