{
  "translations": [
    {
      "resource_id": 214,
      "text": "Čitaj s imenom Gospodara tvoga Koji stvara,"
    },
    { "resource_id": 214, "text": "stvara čovjeka od zakvačka." },
    { "resource_id": 214, "text": "Čitaj, plemenit je Gospodar tvoj," },
    { "resource_id": 214, "text": "Koji poučava peru," },
    { "resource_id": 214, "text": "Koji čovjeka poučava onome što ne zna." },
    { "resource_id": 214, "text": "Uistinu, čovjek se uzobijesti" },
    { "resource_id": 214, "text": "čim se neovisnim osjeti," },
    {
      "resource_id": 214,
      "text": "a Gospodaru tvome će se, doista, svi vratiti!"
    },
    { "resource_id": 214, "text": "Vidje li ti onoga koji sprečava" },
    { "resource_id": 214, "text": "roba da klanja?" },
    { "resource_id": 214, "text": "Šta misliš, ako je on na Pravom putu," },
    { "resource_id": 214, "text": "ili ako bogobojaznost naređuje?!" },
    { "resource_id": 214, "text": "Šta misliš, ako on poriče i okreće se," },
    { "resource_id": 214, "text": "zar on ne zna da Allah vidi?!" },
    {
      "resource_id": 214,
      "text": "Ne valja to! Ako se ne okani, dohvatićemo ga za kiku,"
    },
    { "resource_id": 214, "text": "kiku lažnu i grješnu," },
    { "resource_id": 214, "text": "pa neka on pozove društvo svoje." },
    { "resource_id": 214, "text": "Mi ćemo pozvati zebanije." },
    {
      "resource_id": 214,
      "text": "Naprotiv! Ti se njemu ne pokoravaj, već čini sedždu i približi se\nGospodaru svome"
    }
  ],
