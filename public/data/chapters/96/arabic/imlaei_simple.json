{
  "verses": [
    {
      "id": 6107,
      "verse_key": "96:1",
      "text_imlaei_simple": "اقرا باسم ربك الذي خلق"
    },
    {
      "id": 6108,
      "verse_key": "96:2",
      "text_imlaei_simple": "خلق الانسان من علق"
    },
    {
      "id": 6109,
      "verse_key": "96:3",
      "text_imlaei_simple": "اقرا وربك الاكرم"
    },
    {
      "id": 6110,
      "verse_key": "96:4",
      "text_imlaei_simple": "الذي علم بالقلم"
    },
    {
      "id": 6111,
      "verse_key": "96:5",
      "text_imlaei_simple": "علم الانسان ما لم يعلم"
    },
    {
      "id": 6112,
      "verse_key": "96:6",
      "text_imlaei_simple": "كلا ان الانسان ليطغى"
    },
    { "id": 6113, "verse_key": "96:7", "text_imlaei_simple": "ان راه استغنى" },
    {
      "id": 6114,
      "verse_key": "96:8",
      "text_imlaei_simple": "ان الى ربك الرجعى"
    },
    {
      "id": 6115,
      "verse_key": "96:9",
      "text_imlaei_simple": "ارايت الذي ينهى"
    },
    { "id": 6116, "verse_key": "96:10", "text_imlaei_simple": "عبدا اذا صلى" },
    {
      "id": 6117,
      "verse_key": "96:11",
      "text_imlaei_simple": "ارايت ان كان على الهدى"
    },
    {
      "id": 6118,
      "verse_key": "96:12",
      "text_imlaei_simple": "او امر بالتقوى"
    },
    {
      "id": 6119,
      "verse_key": "96:13",
      "text_imlaei_simple": "ارايت ان كذب وتولى"
    },
    {
      "id": 6120,
      "verse_key": "96:14",
      "text_imlaei_simple": "الم يعلم بان الله يرى"
    },
    {
      "id": 6121,
      "verse_key": "96:15",
      "text_imlaei_simple": "كلا لئن لم ينته لنسفعا بالناصية"
    },
    {
      "id": 6122,
      "verse_key": "96:16",
      "text_imlaei_simple": "ناصية كاذبة خاطئة"
    },
    { "id": 6123, "verse_key": "96:17", "text_imlaei_simple": "فليدع ناديه" },
    { "id": 6124, "verse_key": "96:18", "text_imlaei_simple": "سندع الزبانية" },
    {
      "id": 6125,
      "verse_key": "96:19",
      "text_imlaei_simple": "كلا لا تطعه واسجد واقترب"
    }
  ],
  "meta": { "filters": { "chapter_number": "96" } }
}
