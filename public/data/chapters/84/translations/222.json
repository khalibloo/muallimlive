{
  "translations": [
    { "resource_id": 222, "text": "Аспан жарылған сәтте," },
    { "resource_id": 222, "text": "Лайықты болып,Раббына бой ұсынған кезде," },
    { "resource_id": 222, "text": "Жер созылған сәтте," },
    { "resource_id": 222, "text": "Ол,ішіндегіні шығарып босатқан сәтте," },
    { "resource_id": 222, "text": "Раббына бой ұсынып,лайықты болған кезде;" },
    {
      "resource_id": 222,
      "text": "Әй адамзат! Негізінде сен Раббыңа қарай өте тырыссаң; сонда Оған жолығасың."
    },
    { "resource_id": 222, "text": "Ал енді кімнің дәптері оңынан берілсе;" },
    { "resource_id": 222, "text": "Сонда Ол,дереу жеңіл есеппен есептеледі." },
    { "resource_id": 222, "text": "Ол,үй ішіне-қуанып кайтады." },
    {
      "resource_id": 222,
      "text": "Ал және кімнің дәптері арт жағынан берілсе;"
    },
    { "resource_id": 222, "text": "Сонда ол,дереу өлім шақырады." },
    { "resource_id": 222, "text": "Қайнаған тозаққа түседі." },
    { "resource_id": 222, "text": "Өйткені ол.өз үйінде мәз-мейрам еді." },
    {
      "resource_id": 222,
      "text": "Негізінде ол,әсте (Аллаға) қайтпаймын деп,ойлады."
    },
    { "resource_id": 222, "text": "Олай емес.Расында Раббы оны көздеуде еді." },
    { "resource_id": 222, "text": "Жоқ,шапақ уақытына," },
    { "resource_id": 222, "text": "Түнге әрі ішіндегі нәрсеге," },
    { "resource_id": 222, "text": "Толған айға серт." },
    {
      "resource_id": 222,
      "text": "Әлбетте сендер,бір жағдайдан бір жағдайға өтесіңдер."
    },
    { "resource_id": 222, "text": "Сонда оларға иман келтірмейтін не болды?" },
    {
      "resource_id": 222,
      "text": "Оларға Құран оқылған кезде,сәжде қылмайды."
    },
    { "resource_id": 222, "text": "Әрине кәпірлер «Өтірік» дейді." },
    {
      "resource_id": 222,
      "text": "Алла олардың бүкпелеген нәрселерін жақсы біледі."
    },
    {
      "resource_id": 222,
      "text": "(Мұхаммед Ғ.С) оларды күйзелтетін азаппен шүйіншіле."
    },
    {
      "resource_id": 222,
      "text": "Бірақ иман келтіріп,түзу ғамал істеушілерге таусылмайтын сыйлық бар."
    }
  ],
