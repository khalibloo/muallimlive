{
  "translations": [
    { "resource_id": 112, "text": "Rahman ve Rahim Allah’ın adıyla..." },
    { "resource_id": 112, "text": "Hamd, alemlerin Rab'bi Allah'a mahsustur." },
    { "resource_id": 112, "text": "O Rahman'dır, O Rahim'dir." },
    { "resource_id": 112, "text": "Hesap gününün hakimi Allah’a mahsustur." },
    {
      "resource_id": 112,
      "text": "Yalnız sana ibadet ederiz ve yalnız senden yardım dileriz."
    },
    { "resource_id": 112, "text": "Bizi doğru yola ilet." },
    {
      "resource_id": 112,
      "text": "Nimet verdiğin kimselerin yoluna. Gazaba uğrayanların ve sapanların değil."
    }
  ],
