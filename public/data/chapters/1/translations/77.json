{
  "translations": [
    { "resource_id": 77, "text": "Rahman ve Rahim olan Allah'ın adıyla:" },
    { "resource_id": 77, "text": "Hamd, Alemlerin Rabbi Allah'a mahsustur." },
    { "resource_id": 77, "text": "O Rahman ve Rahim'dir," },
    { "resource_id": 77, "text": "Din Gününün sahibidir." },
    {
      "resource_id": 77,
      "text": "Ancak Sana kulluk eder ve yalnız Senden yardım dileriz."
    },
    { "resource_id": 77, "text": "Bizi doğru yola eriştir." },
    {
      "resource_id": 77,
      "text": "Nimete erdirdiğin kimselerin yoluna; gazaba uğrayanların, ya da sapıtanların yoluna değil."
    }
  ],
