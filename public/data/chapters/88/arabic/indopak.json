{
  "verses": [
    {
      "id": 5968,
      "verse_key": "88:1",
      "text_indopak": "هَلۡ اَتٰٮكَ حَدِيۡثُ الۡغَاشِيَةِؕ‏"
    },
    {
      "id": 5969,
      "verse_key": "88:2",
      "text_indopak": "وُجُوۡهٌ يَّوۡمَٮِٕذٍ خَاشِعَةٌ ۙ‏"
    },
    {
      "id": 5970,
      "verse_key": "88:3",
      "text_indopak": "عَامِلَةٌ نَّاصِبَةٌ ۙ‏"
    },
    {
      "id": 5971,
      "verse_key": "88:4",
      "text_indopak": "تَصۡلٰى نَارًا حَامِيَةً ۙ‏"
    },
    {
      "id": 5972,
      "verse_key": "88:5",
      "text_indopak": "تُسۡقٰى مِنۡ عَيۡنٍ اٰنِيَةٍؕ‏"
    },
    {
      "id": 5973,
      "verse_key": "88:6",
      "text_indopak": "لَـيۡسَ لَهُمۡ طَعَامٌ اِلَّا مِنۡ ضَرِيۡعٍۙ‏"
    },
    {
      "id": 5974,
      "verse_key": "88:7",
      "text_indopak": "لَّا يُسۡمِنُ وَلَا يُغۡنِىۡ مِنۡ جُوۡعٍؕ‏"
    },
    {
      "id": 5975,
      "verse_key": "88:8",
      "text_indopak": "وُجُوۡهٌ يَّوۡمَٮِٕذٍ نَّاعِمَةٌ ۙ‏"
    },
    {
      "id": 5976,
      "verse_key": "88:9",
      "text_indopak": "لِّسَعۡيِهَا رَاضِيَةٌ ۙ‏"
    },
    {
      "id": 5977,
      "verse_key": "88:10",
      "text_indopak": "فِىۡ جَنَّةٍ عَالِيَةٍۙ‏"
    },
    {
      "id": 5978,
      "verse_key": "88:11",
      "text_indopak": "لَّا تَسۡمَعُ فِيۡهَا لَاغِيَةً ؕ‏"
    },
    {
      "id": 5979,
      "verse_key": "88:12",
      "text_indopak": "فِيۡهَا عَيۡنٌ جَارِيَةٌ​ ۘ‏"
    },
    {
      "id": 5980,
      "verse_key": "88:13",
      "text_indopak": "فِيۡهَا سُرُرٌ مَّرۡفُوۡعَةٌ ۙ‏"
    },
    {
      "id": 5981,
      "verse_key": "88:14",
      "text_indopak": "وَّاَكۡوَابٌ مَّوۡضُوۡعَةٌ ۙ‏"
    },
    {
      "id": 5982,
      "verse_key": "88:15",
      "text_indopak": "وَّنَمَارِقُ مَصۡفُوۡفَةٌ ۙ‏"
    },
    {
      "id": 5983,
      "verse_key": "88:16",
      "text_indopak": "وَّزَرَابِىُّ مَبۡثُوۡثَةٌ ؕ‏"
    },
    {
      "id": 5984,
      "verse_key": "88:17",
      "text_indopak": "اَفَلَا يَنۡظُرُوۡنَ اِلَى الۡاِ بِلِ كَيۡفَ خُلِقَتۡ‏"
    },
    {
      "id": 5985,
      "verse_key": "88:18",
      "text_indopak": "وَاِلَى السَّمَآءِ كَيۡفَ رُفِعَتۡ‏"
    },
    {
      "id": 5986,
      "verse_key": "88:19",
      "text_indopak": "وَاِلَى الۡجِبَالِ كَيۡفَ نُصِبَتۡ‏"
    },
    {
      "id": 5987,
      "verse_key": "88:20",
      "text_indopak": "وَاِلَى الۡاَرۡضِ كَيۡفَ سُطِحَتۡ‏"
    },
    {
      "id": 5988,
      "verse_key": "88:21",
      "text_indopak": "فَذَكِّرۡ ؕ اِنَّمَاۤ اَنۡتَ مُذَكِّرٌ ؕ‏"
    },
    {
      "id": 5989,
      "verse_key": "88:22",
      "text_indopak": "لَـسۡتَ عَلَيۡهِمۡ بِمُصَۜيۡطِرٍۙ‏"
    },
    {
      "id": 5990,
      "verse_key": "88:23",
      "text_indopak": "اِلَّا مَنۡ تَوَلّٰى وَكَفَرَۙ‏"
    },
    {
      "id": 5991,
      "verse_key": "88:24",
      "text_indopak": "فَيُعَذِّبُهُ اللّٰهُ الۡعَذَابَ الۡاَكۡبَرَؕ‏"
    },
    {
      "id": 5992,
      "verse_key": "88:25",
      "text_indopak": "اِنَّ اِلَيۡنَاۤ اِيَابَهُمۡۙ‏"
    },
    {
      "id": 5993,
      "verse_key": "88:26",
      "text_indopak": "ثُمَّ اِنَّ عَلَيۡنَا حِسَابَهُمْ‏"
    }
  ],
  "meta": { "filters": { "chapter_number": "88" } }
}
