{
  "translations": [
    {
      "resource_id": 113,
      "text": "/ Ей, Мұхаммед! /  Саған Fашияның хабары келді ме?"
    },
    {
      "resource_id": 113,
      "text": "Ол күні  / кейбір /  жүздер қорланған болады,"
    },
    { "resource_id": 113, "text": "амал етуші, шаршаған," },
    { "resource_id": 113, "text": "/ олар /  ыстығы қатты отқа жанады," },
    { "resource_id": 113, "text": "қатты қайнап тұрған бастаудан ішкізіледі." },
    {
      "resource_id": 113,
      "text": "Оларға «дариъден»  / ұлы тікеннен /  басқа ешбір тамақ жоқ,"
    },
    { "resource_id": 113, "text": "ол оңалтпайды да, аштықты да баспайды." },
    { "resource_id": 113, "text": "Сол күні  / кейбір /  жүздер шаттықта," },
    { "resource_id": 113, "text": "амал, әрекеттеріне разы," },
    { "resource_id": 113, "text": "биік бақтарда болады." },
    { "resource_id": 113, "text": "Олар ол жерде бос сөз естімейді." },
    { "resource_id": 113, "text": "Ол жерде ағып жатқан бұлақ бар." },
    { "resource_id": 113, "text": "Онда жоғары көтерілген жайлы орындар," },
    { "resource_id": 113, "text": "әрі қойылған кеселер," },
    { "resource_id": 113, "text": "және тізілген жастықтар," },
    { "resource_id": 113, "text": "жайылған кілемдер бар." },
    {
      "resource_id": 113,
      "text": "Олар түйеге, оның қалай жаратылғанына назар салмай ма,"
    },
    { "resource_id": 113, "text": "әрі аспанға, оның қалай көтерілгеніне," },
    { "resource_id": 113, "text": "және тауларға, оның қалай тұрғызылғанына," },
    { "resource_id": 113, "text": "әрі жерге, оның қалай төселгеніне?" },
    {
      "resource_id": 113,
      "text": "/ Ей, Мұхаммед! /  Еске сал, анығында сен тек еске салушысың."
    },
    { "resource_id": 113, "text": "Сен оларға күшпен үстемдік етуші емессің." },
    {
      "resource_id": 113,
      "text": "Алайда кім теріс бұрылса және күпірлік етсе  / қарсы келсе / ,"
    },
    { "resource_id": 113, "text": "онда Аллаһ оны ең үлкен азаппен қинайды." },
    { "resource_id": 113, "text": "Ақиқатында, олардың қайтуы - Бізге." },
    {
      "resource_id": 113,
      "text": "Содан кейін, күмәнсіз, олардың есебі - Бізде."
    }
  ],
