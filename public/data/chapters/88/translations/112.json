{
  "translations": [
    { "resource_id": 112, "text": "Bürüyen günün haberi sana geldi mi?" },
    { "resource_id": 112, "text": "O gün, yüzler vardır yere yıkılmış," },
    { "resource_id": 112, "text": "Çalışmış, boşa yorulmuş," },
    { "resource_id": 112, "text": "Kızgın ateşe yaslanan," },
    { "resource_id": 112, "text": "Kızgın bir kaynaktan içen," },
    {
      "resource_id": 112,
      "text": "Onlara kuru dikenden başka yiyecek yoktur."
    },
    { "resource_id": 112, "text": "Ne semirtir ne açlığı giderir." },
    { "resource_id": 112, "text": "O günde yüzler vardır pırıl pırıl." },
    { "resource_id": 112, "text": "Çalışmasından hoşnuttur." },
    { "resource_id": 112, "text": "Yüksek bir Cennet'tedir." },
    { "resource_id": 112, "text": "Orada boş söz işitmezler." },
    { "resource_id": 112, "text": "Orada akan bir pınar." },
    { "resource_id": 112, "text": "Orada yükseltilmiş sedirler/yataklar ." },
    { "resource_id": 112, "text": "Konulmuş kadehler." },
    { "resource_id": 112, "text": "Sıra sıra yastıklar." },
    { "resource_id": 112, "text": "Serilmiş halılar vardır." },
    { "resource_id": 112, "text": "Bakmazlar mı deveye? Nasıl yaratıldı?" },
    { "resource_id": 112, "text": "Ve göğe, nasıl yükseltildi?" },
    { "resource_id": 112, "text": "Ve dağlara, nasıl dikildi?" },
    { "resource_id": 112, "text": "Ve yere, nasıl yayıldı?" },
    { "resource_id": 112, "text": "Hatırlat! Sen ancak uyarıcısın." },
    { "resource_id": 112, "text": "Onlar üzerinde bir zorba değilsin." },
    { "resource_id": 112, "text": "Ancak, kim yüz çevirir ve kâfir olursa," },
    { "resource_id": 112, "text": "Allah, onu en büyük azap ile azap eder." },
    { "resource_id": 112, "text": "Şüphesiz onların dönüşü bizedir." },
    { "resource_id": 112, "text": "Sonra da onların hesabını görmek bizedir." }
  ],
  "meta": {
