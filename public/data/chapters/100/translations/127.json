{
  "translations": [
    { "resource_id": 127, "text": "Ҳарсиллаб чопувчи отлар билан қасам." },
    { "resource_id": 127, "text": "Ўт чақнатиб чопувчи отлар билан қасам." },
    {
      "resource_id": 127,
      "text": "Тонг чоғида ғорат қилувчи отлар билан қасам."
    },
    { "resource_id": 127, "text": "Ўша ила чанг қўзғатдилар." },
    { "resource_id": 127, "text": "Ва у ила жамоа ўртасига (ёриб) кирдилар." },
    { "resource_id": 127, "text": "Албатта, инсон ўз Роббига ношукрдир." },
    { "resource_id": 127, "text": "Ва албатта, у ана шунга ўзи гувоҳдир." },
    {
      "resource_id": 127,
      "text": "Албатта, у мол-дунёга ҳаддан ташқари ўчдир."
    },
    { "resource_id": 127, "text": "У билмасми?! Қабрдагилар қўзғатилганда." },
    { "resource_id": 127, "text": "Ва кўкракларидаги ошкор бўлганда." },
    {
      "resource_id": 127,
      "text": "Ўша кунда Роббилари улардан хабардор эканини?!"
    }
  ],
