{
  "translations": [
    { "resource_id": 52, "text": "O harıl harıl (savaşa) koşanlara," },
    { "resource_id": 52, "text": "(Tırnaklarıyla yerden) ateş çıkaranlara," },
    { "resource_id": 52, "text": "Sabahleyin akın edenlere," },
    { "resource_id": 52, "text": "Tozu dumana karıştıranlara," },
    {
      "resource_id": 52,
      "text": "Derken bir topluluğun ortasına dalanlara yemin ederim ki,"
    },
    {
      "resource_id": 52,
      "text": "Şüphesiz insan, Rabbine karşı çok nankördür."
    },
    { "resource_id": 52, "text": "Ve kendisi de buna şahittir." },
    {
      "resource_id": 52,
      "text": "Gerçekten o dünya malını çok sevdiği için katıdır."
    },
    {
      "resource_id": 52,
      "text": "Bilmiyor mu ki, kabirlerin içindekiler fırlatılacak."
    },
    { "resource_id": 52, "text": "Ve sinelerin içindekiler derlenecek." },
    {
      "resource_id": 52,
      "text": "O gün Rableri onların bütün yaptıklarından haberdardır"
    }
  ],
