{
  "translations": [
    {
      "resource_id": 774,
      "text": "Ndahiye amafarasi yiruka cyane avuza imirindi,"
    },
    {
      "resource_id": 774,
      "text": "Akubita (ibinono) bigatarutsa ibishashi by’umuriro,"
    },
    { "resource_id": 774, "text": "N’amafarasi agaba igitero mu rukerera," },
    { "resource_id": 774, "text": "Akanatumura umukungugu," },
    { "resource_id": 774, "text": "Akinjira hagati mu ngabo z’umwanzi." },
    {
      "resource_id": 774,
      "text": "Mu by’ukuri umuntu ni indashima kuri Nyagasani we,"
    },
    { "resource_id": 774, "text": "Kandi rwose ibyo yabibera umuhamya," },
    { "resource_id": 774, "text": "Ndetse akunda imitungo bikabije." },
    {
      "resource_id": 774,
      "text": "Ese ntazi ko hari igihe abari mu mva bazazurwa?"
    },
    {
      "resource_id": 774,
      "text": "N’ibiri mu bituza (by’abantu) bigashyirwa ahagaragara?"
    },
    {
      "resource_id": 774,
      "text": "Mu by’ukuri uwo munsi Nyagasani wabo azaba azi neza ibyabo."
    }
  ],
