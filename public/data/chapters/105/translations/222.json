    { "resource_id": 222, "text": "Оларға топ-топ құс жіберді." },
    {
      "resource_id": 222,
      "text": "Құстар,оларға балшықтан жасалған тастар атып;"
    },
    { "resource_id": 222, "text": "Сонда оларды желінген топан тәрізді қылды." }
