    { "resource_id": 25, "text": "i Sinajske gore," },
    { "resource_id": 25, "text": "i grada ovog, sigurnog," },
    { "resource_id": 25, "text": "Mi čovjeka stvaramo u najljepšem obliku," },
    { "resource_id": 25, "text": "zatim ćemo ga u najniže nizine vratiti." },
    {
      "resource_id": 25,
      "text": "samo ne one koji budu vjerovali i dobra djela činili, njima pripada nagrada neprekidna."
    },
    { "resource_id": 25, "text": "Pa, šta te onda navodi da poričeš obračun?" },
    { "resource_id": 25, "text": "Zar Allah nije sudija najpravedniji?!" }
