{
  "verses": [
    {
      "id": 6099,
      "verse_key": "95:1",
      "text_imlaei_simple": "والتين والزيتون"
    },
    { "id": 6100, "verse_key": "95:2", "text_imlaei_simple": "وطور سينين" },
    {
      "id": 6101,
      "verse_key": "95:3",
      "text_imlaei_simple": "وهذا البلد الامين"
    },
    {
      "id": 6102,
      "verse_key": "95:4",
      "text_imlaei_simple": "لقد خلقنا الانسان في احسن تقويم"
    },
    {
      "id": 6103,
      "verse_key": "95:5",
      "text_imlaei_simple": "ثم رددناه اسفل سافلين"
    },
    {
      "id": 6104,
      "verse_key": "95:6",
      "text_imlaei_simple": "الا الذين امنوا وعملوا الصالحات فلهم اجر غير ممنون"
    },
    {
      "id": 6105,
      "verse_key": "95:7",
      "text_imlaei_simple": "فما يكذبك بعد بالدين"
    },
    {
      "id": 6106,
      "verse_key": "95:8",
      "text_imlaei_simple": "اليس الله باحكم الحاكمين"
    }
  ],
  "meta": { "filters": { "chapter_number": "95" } }
}
