{
  "translations": [
    { "resource_id": 222, "text": "Сүңгіп шығарушы," },
    { "resource_id": 222, "text": "Баяу тартушы," },
    { "resource_id": 222, "text": "Малтап кетуші," },
    { "resource_id": 222, "text": "Жарысып озушы," },
    {
      "resource_id": 222,
      "text": "Сондай-ақ істі меңгеруші періштелерге серт."
    },
    { "resource_id": 222, "text": "Ол күні қатты сілкініс болады." },
    { "resource_id": 222, "text": "Оның артынан тағы бір сілкінеді." },
    { "resource_id": 222, "text": "Ол күні жүректер тітірейді." },
    { "resource_id": 222, "text": "Көздері төмен қарайды." },
    {
      "resource_id": 222,
      "text": "(Олар): \"Шын, қайта тірілеміз бе?\",- дейді."
    },
    { "resource_id": 222, "text": "\"Қоқым, сүйек болған сәтте де?\"" },
    {
      "resource_id": 222,
      "text": "(Олар): \"Ендеше ол қайғылы бір қайту екен\" дейді."
    },
    { "resource_id": 222, "text": "Негізінен ол, бір айғай ғана." },
    { "resource_id": 222, "text": "Дереу сол уақытта олар майданда." },
    { "resource_id": 222, "text": "Саған Мұсаның хабары келді ме?" },
    {
      "resource_id": 222,
      "text": "Сол уақытта, Раббы, оған қасиетгі \"Тұуа\" ойпатында дабыстады:"
    },
    { "resource_id": 222, "text": "\"Перғауынға бар. Өйткені ол, азды.\"" },
    { "resource_id": 222, "text": "Сонда оган: \"Тазарғың келе ме?\",- де." },
    {
      "resource_id": 222,
      "text": "\"Саған Раббыңа баратын жолды көрсетейін. Одан қорқарсың\""
    },
    { "resource_id": 222, "text": "Мұса, Перғауынға ірі мұғжизаны көрсетті." },
    { "resource_id": 222, "text": "Сонда ол, жасынға шығарып, қарсы шықты." },
    { "resource_id": 222, "text": "Кейін теріс айналып, жүгірді." },
    { "resource_id": 222, "text": "Адамдарды жинап, айғай салып:" },
    { "resource_id": 222, "text": "\"Мен сендердің ең ұлы раббыларың\" деді." },
    {
      "resource_id": 222,
      "text": "Сондықтан Алла, оны ахирет, дүние азабымен қолға алды."
    },
    {
      "resource_id": 222,
      "text": "Расында бұнда қорыққан кісі үшін әлбетте бір ғибрат бар."
    },
    {
      "resource_id": 222,
      "text": "Сендерді жарату қиын ба? Немесе аспанды? Алла, оны да жасады."
    },
    { "resource_id": 222, "text": "Оның биіктігін көтеріп, оны теңгерді." },
    {
      "resource_id": 222,
      "text": "Оның түнін қараңғылатып, күндізін жарыққа шығарды."
    },
    { "resource_id": 222, "text": "Сосын жерді төседі." },
    { "resource_id": 222, "text": "Одан су және жайылыс майданға келтірді." },
    { "resource_id": 222, "text": "Оған таулар орналастырды." },
    { "resource_id": 222, "text": "Сендер және малдарың пайдалану үшін." },
    {
      "resource_id": 222,
      "text": "Бірақ ол ірі уақиға (қиямет) келген сәтте;"
    },
    {
      "resource_id": 222,
      "text": "Ол күні адамзат не нәрсеге тырысқанын есіне алады."
    },
    { "resource_id": 222, "text": "Тозақ, көретіндерге көрсетіледі." },
    { "resource_id": 222, "text": "Ал енді кім шектен шыққан болса," },
    { "resource_id": 222, "text": "Оған дүние әсер еткен болса," },
    { "resource_id": 222, "text": "Сонда рас оның орны тозақ." },
    {
      "resource_id": 222,
      "text": "Ал енді кім, Раббыны; алдына түрудан қорқып, нәпсісі соққан нәрседен тежеген болса,"
    },
    { "resource_id": 222, "text": "Негізінде оның барар жері ұжмақ" },
    {
      "resource_id": 222,
      "text": "(Мұхаммед Ғ.С.) олар саған \"Қиямет қашан болады?\",- деп сұрақ қояды."
    },
    { "resource_id": 222, "text": "Ол жөнде түсіні беру сенің ісің бе?" },
    { "resource_id": 222, "text": "Оны қортындысы, Раббыңа байланысты." },
    {
      "resource_id": 222,
      "text": "Негізінен сен қияметте қорыққан кісілерге бір ескертуші ғанасың."
    },
    {
      "resource_id": 222,
      "text": "Олар қияметті көрге күні, дүниеде бір кеш немесе бір сәске тұрғандай ойлайды."
    }
  ],
