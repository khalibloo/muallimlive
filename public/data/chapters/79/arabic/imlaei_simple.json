{
  "verses": [
    { "id": 5713, "verse_key": "79:1", "text_imlaei_simple": "والنازعات غرقا" },
    { "id": 5714, "verse_key": "79:2", "text_imlaei_simple": "والناشطات نشطا" },
    { "id": 5715, "verse_key": "79:3", "text_imlaei_simple": "والسابحات سبحا" },
    { "id": 5716, "verse_key": "79:4", "text_imlaei_simple": "فالسابقات سبقا" },
    { "id": 5717, "verse_key": "79:5", "text_imlaei_simple": "فالمدبرات امرا" },
    {
      "id": 5718,
      "verse_key": "79:6",
      "text_imlaei_simple": "يوم ترجف الراجفة"
    },
    { "id": 5719, "verse_key": "79:7", "text_imlaei_simple": "تتبعها الرادفة" },
    {
      "id": 5720,
      "verse_key": "79:8",
      "text_imlaei_simple": "قلوب يومئذ واجفة"
    },
    { "id": 5721, "verse_key": "79:9", "text_imlaei_simple": "ابصارها خاشعة" },
    {
      "id": 5722,
      "verse_key": "79:10",
      "text_imlaei_simple": "يقولون اانا لمردودون في الحافرة"
    },
    {
      "id": 5723,
      "verse_key": "79:11",
      "text_imlaei_simple": "ااذا كنا عظاما نخرة"
    },
    {
      "id": 5724,
      "verse_key": "79:12",
      "text_imlaei_simple": "قالوا تلك اذا كرة خاسرة"
    },
    {
      "id": 5725,
      "verse_key": "79:13",
      "text_imlaei_simple": "فانما هي زجرة واحدة"
    },
    {
      "id": 5726,
      "verse_key": "79:14",
      "text_imlaei_simple": "فاذا هم بالساهرة"
    },
    {
      "id": 5727,
      "verse_key": "79:15",
      "text_imlaei_simple": "هل اتاك حديث موسى"
    },
    {
      "id": 5728,
      "verse_key": "79:16",
      "text_imlaei_simple": "اذ ناداه ربه بالواد المقدس طوى"
    },
    {
      "id": 5729,
      "verse_key": "79:17",
      "text_imlaei_simple": "اذهب الى فرعون انه طغى"
    },
    {
      "id": 5730,
      "verse_key": "79:18",
      "text_imlaei_simple": "فقل هل لك الى ان تزكى"
    },
    {
      "id": 5731,
      "verse_key": "79:19",
      "text_imlaei_simple": "واهديك الى ربك فتخشى"
    },
    {
      "id": 5732,
      "verse_key": "79:20",
      "text_imlaei_simple": "فاراه الاية الكبرى"
    },
    { "id": 5733, "verse_key": "79:21", "text_imlaei_simple": "فكذب وعصى" },
    { "id": 5734, "verse_key": "79:22", "text_imlaei_simple": "ثم ادبر يسعى" },
    { "id": 5735, "verse_key": "79:23", "text_imlaei_simple": "فحشر فنادى" },
    {
      "id": 5736,
      "verse_key": "79:24",
      "text_imlaei_simple": "فقال انا ربكم الاعلى"
    },
    {
      "id": 5737,
      "verse_key": "79:25",
      "text_imlaei_simple": "فاخذه الله نكال الاخرة والاولى"
    },
    {
      "id": 5738,
      "verse_key": "79:26",
      "text_imlaei_simple": "ان في ذلك لعبرة لمن يخشى"
    },
    {
      "id": 5739,
      "verse_key": "79:27",
      "text_imlaei_simple": "اانتم اشد خلقا ام السماء بناها"
    },
    {
      "id": 5740,
      "verse_key": "79:28",
      "text_imlaei_simple": "رفع سمكها فسواها"
    },
    {
      "id": 5741,
      "verse_key": "79:29",
      "text_imlaei_simple": "واغطش ليلها واخرج ضحاها"
    },
    {
      "id": 5742,
      "verse_key": "79:30",
      "text_imlaei_simple": "والارض بعد ذلك دحاها"
    },
    {
      "id": 5743,
      "verse_key": "79:31",
      "text_imlaei_simple": "اخرج منها ماءها ومرعاها"
    },
    {
      "id": 5744,
      "verse_key": "79:32",
      "text_imlaei_simple": "والجبال ارساها"
    },
    {
      "id": 5745,
      "verse_key": "79:33",
      "text_imlaei_simple": "متاعا لكم ولانعامكم"
    },
    {
      "id": 5746,
      "verse_key": "79:34",
      "text_imlaei_simple": "فاذا جاءت الطامة الكبرى"
    },
    {
      "id": 5747,
      "verse_key": "79:35",
      "text_imlaei_simple": "يوم يتذكر الانسان ما سعى"
    },
    {
      "id": 5748,
      "verse_key": "79:36",
      "text_imlaei_simple": "وبرزت الجحيم لمن يرى"
    },
    { "id": 5749, "verse_key": "79:37", "text_imlaei_simple": "فاما من طغى" },
    {
      "id": 5750,
      "verse_key": "79:38",
      "text_imlaei_simple": "واثر الحياة الدنيا"
    },
    {
      "id": 5751,
      "verse_key": "79:39",
      "text_imlaei_simple": "فان الجحيم هي الماوى"
    },
    {
      "id": 5752,
      "verse_key": "79:40",
      "text_imlaei_simple": "واما من خاف مقام ربه ونهى النفس عن الهوى"
    },
    {
      "id": 5753,
      "verse_key": "79:41",
      "text_imlaei_simple": "فان الجنة هي الماوى"
    },
    {
      "id": 5754,
      "verse_key": "79:42",
      "text_imlaei_simple": "يسالونك عن الساعة ايان مرساها"
    },
    {
      "id": 5755,
      "verse_key": "79:43",
      "text_imlaei_simple": "فيم انت من ذكراها"
    },
    {
      "id": 5756,
      "verse_key": "79:44",
      "text_imlaei_simple": "الى ربك منتهاها"
    },
    {
      "id": 5757,
      "verse_key": "79:45",
      "text_imlaei_simple": "انما انت منذر من يخشاها"
    },
    {
      "id": 5758,
      "verse_key": "79:46",
      "text_imlaei_simple": "كانهم يوم يرونها لم يلبثوا الا عشية او ضحاها"
    }
  ],
  "meta": { "filters": { "chapter_number": "79" } }
}
