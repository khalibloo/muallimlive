{
  "verses": [
    {
      "id": 5713,
      "verse_key": "79:1",
      "text_imlaei": "وَالنَّازِعَاتِ غَرْقًا"
    },
    {
      "id": 5714,
      "verse_key": "79:2",
      "text_imlaei": "وَالنَّاشِطَاتِ نَشْطًا"
    },
    {
      "id": 5715,
      "verse_key": "79:3",
      "text_imlaei": "وَالسَّابِحَاتِ سَبْحًا"
    },
    {
      "id": 5716,
      "verse_key": "79:4",
      "text_imlaei": "فَالسَّابِقَاتِ سَبْقًا"
    },
    {
      "id": 5717,
      "verse_key": "79:5",
      "text_imlaei": "فَالْمُدَبِّرَاتِ أَمْرًا"
    },
    {
      "id": 5718,
      "verse_key": "79:6",
      "text_imlaei": "يَوْمَ تَرْجُفُ الرَّاجِفَةُ"
    },
    {
      "id": 5719,
      "verse_key": "79:7",
      "text_imlaei": "تَتْبَعُهَا الرَّادِفَةُ"
    },
    {
      "id": 5720,
      "verse_key": "79:8",
      "text_imlaei": "قُلُوبٌ يَوْمَئِذٍ وَاجِفَةٌ"
    },
    {
      "id": 5721,
      "verse_key": "79:9",
      "text_imlaei": "أَبْصَارُهَا خَاشِعَةٌ"
    },
    {
      "id": 5722,
      "verse_key": "79:10",
      "text_imlaei": "يَقُولُونَ أَإِنَّا لَمَرْدُودُونَ فِي الْحَافِرَةِ"
    },
    {
      "id": 5723,
      "verse_key": "79:11",
      "text_imlaei": "أَإِذَا كُنَّا عِظَامًا نَّخِرَةً"
    },
    {
      "id": 5724,
      "verse_key": "79:12",
      "text_imlaei": "قَالُوا تِلْكَ إِذًا كَرَّةٌ خَاسِرَةٌ"
    },
    {
      "id": 5725,
      "verse_key": "79:13",
      "text_imlaei": "فَإِنَّمَا هِيَ زَجْرَةٌ وَاحِدَةٌ"
    },
    {
      "id": 5726,
      "verse_key": "79:14",
      "text_imlaei": "فَإِذَا هُم بِالسَّاهِرَةِ"
    },
    {
      "id": 5727,
      "verse_key": "79:15",
      "text_imlaei": "هَلْ أَتَاكَ حَدِيثُ مُوسَىٰ"
    },
    {
      "id": 5728,
      "verse_key": "79:16",
      "text_imlaei": "إِذْ نَادَاهُ رَبُّهُ بِالْوَادِ الْمُقَدَّسِ طُوًى"
    },
    {
      "id": 5729,
      "verse_key": "79:17",
      "text_imlaei": "اذْهَبْ إِلَىٰ فِرْعَوْنَ إِنَّهُ طَغَىٰ"
    },
    {
      "id": 5730,
      "verse_key": "79:18",
      "text_imlaei": "فَقُلْ هَل لَّكَ إِلَىٰ أَن تَزَكَّىٰ"
    },
    {
      "id": 5731,
      "verse_key": "79:19",
      "text_imlaei": "وَأَهْدِيَكَ إِلَىٰ رَبِّكَ فَتَخْشَىٰ"
    },
    {
      "id": 5732,
      "verse_key": "79:20",
      "text_imlaei": "فَأَرَاهُ الْآيَةَ الْكُبْرَىٰ"
    },
    { "id": 5733, "verse_key": "79:21", "text_imlaei": "فَكَذَّبَ وَعَصَىٰ" },
    {
      "id": 5734,
      "verse_key": "79:22",
      "text_imlaei": "ثُمَّ أَدْبَرَ يَسْعَىٰ"
    },
    { "id": 5735, "verse_key": "79:23", "text_imlaei": "فَحَشَرَ فَنَادَىٰ" },
    {
      "id": 5736,
      "verse_key": "79:24",
      "text_imlaei": "فَقَالَ أَنَا رَبُّكُمُ الْأَعْلَىٰ"
    },
    {
      "id": 5737,
      "verse_key": "79:25",
      "text_imlaei": "فَأَخَذَهُ اللَّهُ نَكَالَ الْآخِرَةِ وَالْأُولَىٰ"
    },
    {
      "id": 5738,
      "verse_key": "79:26",
      "text_imlaei": "إِنَّ فِي ذَٰلِكَ لَعِبْرَةً لِّمَن يَخْشَىٰ"
    },
    {
      "id": 5739,
      "verse_key": "79:27",
      "text_imlaei": "أَأَنتُمْ أَشَدُّ خَلْقًا أَمِ السَّمَاءُ ۚ بَنَاهَا"
    },
    {
      "id": 5740,
      "verse_key": "79:28",
      "text_imlaei": "رَفَعَ سَمْكَهَا فَسَوَّاهَا"
    },
    {
      "id": 5741,
      "verse_key": "79:29",
      "text_imlaei": "وَأَغْطَشَ لَيْلَهَا وَأَخْرَجَ ضُحَاهَا"
    },
    {
      "id": 5742,
      "verse_key": "79:30",
      "text_imlaei": "وَالْأَرْضَ بَعْدَ ذَٰلِكَ دَحَاهَا"
    },
    {
      "id": 5743,
      "verse_key": "79:31",
      "text_imlaei": "أَخْرَجَ مِنْهَا مَاءَهَا وَمَرْعَاهَا"
    },
    {
      "id": 5744,
      "verse_key": "79:32",
      "text_imlaei": "وَالْجِبَالَ أَرْسَاهَا"
    },
    {
      "id": 5745,
      "verse_key": "79:33",
      "text_imlaei": "مَتَاعًا لَّكُمْ وَلِأَنْعَامِكُمْ"
    },
    {
      "id": 5746,
      "verse_key": "79:34",
      "text_imlaei": "فَإِذَا جَاءَتِ الطَّامَّةُ الْكُبْرَىٰ"
    },
    {
      "id": 5747,
      "verse_key": "79:35",
      "text_imlaei": "يَوْمَ يَتَذَكَّرُ الْإِنسَانُ مَا سَعَىٰ"
    },
    {
      "id": 5748,
      "verse_key": "79:36",
      "text_imlaei": "وَبُرِّزَتِ الْجَحِيمُ لِمَن يَرَىٰ"
    },
    { "id": 5749, "verse_key": "79:37", "text_imlaei": "فَأَمَّا مَن طَغَىٰ" },
    {
      "id": 5750,
      "verse_key": "79:38",
      "text_imlaei": "وَآثَرَ الْحَيَاةَ الدُّنْيَا"
    },
    {
      "id": 5751,
      "verse_key": "79:39",
      "text_imlaei": "فَإِنَّ الْجَحِيمَ هِيَ الْمَأْوَىٰ"
    },
    {
      "id": 5752,
      "verse_key": "79:40",
      "text_imlaei": "وَأَمَّا مَنْ خَافَ مَقَامَ رَبِّهِ وَنَهَى النَّفْسَ عَنِ الْهَوَىٰ"
    },
    {
      "id": 5753,
      "verse_key": "79:41",
      "text_imlaei": "فَإِنَّ الْجَنَّةَ هِيَ الْمَأْوَىٰ"
    },
    {
      "id": 5754,
      "verse_key": "79:42",
      "text_imlaei": "يَسْأَلُونَكَ عَنِ السَّاعَةِ أَيَّانَ مُرْسَاهَا"
    },
    {
      "id": 5755,
      "verse_key": "79:43",
      "text_imlaei": "فِيمَ أَنتَ مِن ذِكْرَاهَا"
    },
    {
      "id": 5756,
      "verse_key": "79:44",
      "text_imlaei": "إِلَىٰ رَبِّكَ مُنتَهَاهَا"
    },
    {
      "id": 5757,
      "verse_key": "79:45",
      "text_imlaei": "إِنَّمَا أَنتَ مُنذِرُ مَن يَخْشَاهَا"
    },
    {
      "id": 5758,
      "verse_key": "79:46",
      "text_imlaei": "كَأَنَّهُمْ يَوْمَ يَرَوْنَهَا لَمْ يَلْبَثُوا إِلَّا عَشِيَّةً أَوْ ضُحَاهَا"
    }
  ],
  "meta": { "filters": { "chapter_number": "79" } }
}
