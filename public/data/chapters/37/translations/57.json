{
  "translations": [
    { "resource_id": 57, "text": "Wassaffati saffa" },
    { "resource_id": 57, "text": "Fazzajirati zajra" },
    { "resource_id": 57, "text": "Fattaliyati thikra" },
    { "resource_id": 57, "text": "Inna ilahakum lawahid" },
    {
      "resource_id": 57,
      "text": "Rabbu assamawati wal-ardiwama baynahuma warabbu almashariq"
    },
    {
      "resource_id": 57,
      "text": "Inna zayyanna assamaaaddunya bizeenatin alkawakib"
    },
    { "resource_id": 57, "text": "Wahifthan min kulli shaytaninmarid" },
    {
      "resource_id": 57,
      "text": "La yassammaAAoona ila almala-ial-aAAla wayuqthafoona min kulli janib"
    },
    { "resource_id": 57, "text": "Duhooran walahum AAathabun wasib" },
    {
      "resource_id": 57,
      "text": "Illa man khatifa alkhatfatafaatbaAAahu shihabun thaqib"
    },
    {
      "resource_id": 57,
      "text": "Fastaftihim ahum ashaddu khalqan amman khalaqna inna khalaqnahum min teeninlazib"
    },
    { "resource_id": 57, "text": "Bal AAajibta wayaskharoon" },
    { "resource_id": 57, "text": "Wa-itha thukkiroo la yathkuroon" },
    { "resource_id": 57, "text": "Wa-itha raaw ayatanyastaskhiroon" },
    { "resource_id": 57, "text": "Waqaloo in hatha illasihrun mubeen" },
    {
      "resource_id": 57,
      "text": "A-itha mitna wakunnaturaban waAAithaman a-innalamabAAoothoon"
    },
    { "resource_id": 57, "text": "Awa abaona al-awwaloon" },
    { "resource_id": 57, "text": "Qul naAAam waantum dakhiroon" },
    {
      "resource_id": 57,
      "text": "Fa-innama hiya zajratun wahidatunfa-itha hum yanthuroon"
    },
    { "resource_id": 57, "text": "Waqaloo ya waylana hathayawmu addeen" },
    {
      "resource_id": 57,
      "text": "Hatha yawmu alfasli allatheekuntum bihi tukaththiboon"
    },
    {
      "resource_id": 57,
      "text": "Ohshuroo allatheena thalamoowaazwajahum wama kanoo yaAAbudoon"
    },
    {
      "resource_id": 57,
      "text": "Min dooni Allahi fahdoohum ilasirati aljaheem"
    },
    { "resource_id": 57, "text": "Waqifoohum innahum masooloon" },
    { "resource_id": 57, "text": "Ma lakum la tanasaroon" },
    { "resource_id": 57, "text": "Bal humu alyawma mustaslimoon" },
    {
      "resource_id": 57,
      "text": "Waaqbala baAAduhum AAala baAAdinyatasaaloon"
    },
    {
      "resource_id": 57,
      "text": "Qaloo innakum kuntum ta/toonanaAAani alyameen"
    },
    { "resource_id": 57, "text": "Qaloo bal lam takoonoo mu/mineen" },
    {
      "resource_id": 57,
      "text": "Wama kana lanaAAalaykum min sultanin bal kuntum qawman tagheen"
    },
    {
      "resource_id": 57,
      "text": "Fahaqqa AAalayna qawlu rabbinainna latha-iqoon"
    },
    { "resource_id": 57, "text": "Faaghwaynakum inna kunnaghaween" },
    {
      "resource_id": 57,
      "text": "Fa-innahum yawma-ithin fee alAAathabimushtarikoon"
    },
    { "resource_id": 57, "text": "Inna kathalika nafAAalu bilmujrimeen" },
    {
      "resource_id": 57,
      "text": "Innahum kanoo itha qeela lahumla ilaha illa Allahu yastakbiroon"
    },
    {
      "resource_id": 57,
      "text": "Wayaqooloona a-inna latarikoo alihatinalishaAAirin majnoon"
    },
    { "resource_id": 57, "text": "Bal jaa bilhaqqi wasaddaqaalmursaleen" },
    { "resource_id": 57, "text": "Innakum latha-iqoo alAAathabial-aleem" },
    { "resource_id": 57, "text": "Wama tujzawna illa makuntum taAAmaloon" },
    { "resource_id": 57, "text": "Illa AAibada Allahialmukhlaseen" },
    { "resource_id": 57, "text": "Ola-ika lahum rizqun maAAloom" },
    { "resource_id": 57, "text": "Fawakihu wahum mukramoon" },
    { "resource_id": 57, "text": "Fee jannati annaAAeem" },
    { "resource_id": 57, "text": "AAala sururin mutaqabileen" },
    { "resource_id": 57, "text": "Yutafu AAalayhim bika/sin min maAAeen" },
    { "resource_id": 57, "text": "Baydaa laththatin lishsharibeen" },
    { "resource_id": 57, "text": "La feeha ghawlun walahum AAanha yunzafoon" },
    { "resource_id": 57, "text": "WaAAindahum qasiratu attarfiAAeen" },
    { "resource_id": 57, "text": "Kaannahunna baydun maknoon" },
    {
      "resource_id": 57,
      "text": "Faaqbala baAAduhum AAala baAAdinyatasaaloon"
    },
    { "resource_id": 57, "text": "Qala qa-ilun minhum innee kanalee qareen" },
    { "resource_id": 57, "text": "Yaqoolu a-innaka lamina almusaddiqeen" },
    {
      "resource_id": 57,
      "text": "A-itha mitna wakunnaturaban waAAithaman a-innalamadeenoon"
    },
    { "resource_id": 57, "text": "Qala hal antum muttaliAAoon" },
    { "resource_id": 57, "text": "FattalaAAa faraahu feesawa-i aljaheem" },
    { "resource_id": 57, "text": "Qala tallahi in kidtalaturdeen" },
    {
      "resource_id": 57,
      "text": "Walawla niAAmatu rabbee lakuntu minaalmuhdareen"
    },
    { "resource_id": 57, "text": "Afama nahnu bimayyiteen" },
    {
      "resource_id": 57,
      "text": "Illa mawtatana al-oolawama nahnu bimuAAaththabeen"
    },
    { "resource_id": 57, "text": "Inna hatha lahuwa alfawzu alAAatheem" },
    { "resource_id": 57, "text": "Limithli hatha falyaAAmali alAAamiloon" },
    {
      "resource_id": 57,
      "text": "Athalika khayrun nuzulan am shajaratuazzaqqoom"
    },
    { "resource_id": 57, "text": "Inna jaAAalnahafitnatan liththalimeen" },
    {
      "resource_id": 57,
      "text": "Innaha shajaratun takhruju fee aslialjaheem"
    },
    { "resource_id": 57, "text": "TalAAuha kaannahu ruoosu ashshayateen" },
    {
      "resource_id": 57,
      "text": "Fa-innahum laakiloona minhafamali-oona minha albutoon"
    },
    {
      "resource_id": 57,
      "text": "Thumma inna lahum AAalayha lashawbanmin hameem"
    },
    { "resource_id": 57, "text": "Thumma inna marjiAAahum la-ila aljaheem" },
    { "resource_id": 57, "text": "Innahum alfaw abaahum dalleen" },
    { "resource_id": 57, "text": "Fahum AAala atharihimyuhraAAoon" },
    { "resource_id": 57, "text": "Walaqad dalla qablahum aktharual-awwaleen" },
    { "resource_id": 57, "text": "Walaqad arsalna feehim munthireen" },
    { "resource_id": 57, "text": "Fanthur kayfa kanaAAaqibatu almunthareen" },
    { "resource_id": 57, "text": "Illa AAibada Allahialmukhlaseen" },
    {
      "resource_id": 57,
      "text": "Walaqad nadana noohunfalaniAAma almujeeboon"
    },
    {
      "resource_id": 57,
      "text": "Wanajjaynahu waahlahu mina alkarbialAAatheem"
    },
    { "resource_id": 57, "text": "WajaAAalna thurriyyatahu humualbaqeen" },
    { "resource_id": 57, "text": "Watarakna AAalayhi fee al-akhireen" },
    { "resource_id": 57, "text": "Salamun AAala noohinfee alAAalameen" },
    { "resource_id": 57, "text": "Inna kathalika najzee almuhsineen" },
    { "resource_id": 57, "text": "Innahu min AAibadinaalmu/mineen" },
    { "resource_id": 57, "text": "Thumma aghraqna al-akhareen" },
    { "resource_id": 57, "text": "Wa-inna min sheeAAatihi la-ibraheem" },
    { "resource_id": 57, "text": "Ith jaa rabbahu biqalbinsaleem" },
    {
      "resource_id": 57,
      "text": "Ith qala li-abeehi waqawmihi mathataAAbudoon"
    },
    { "resource_id": 57, "text": "A-ifkan alihatan doona Allahitureedoon" },
    { "resource_id": 57, "text": "Fama thannukum birabbialAAalameen" },
    { "resource_id": 57, "text": "Fanathara nathratanfee annujoom" },
    { "resource_id": 57, "text": "Faqala innee saqeem" },
    { "resource_id": 57, "text": "Fatawallaw AAanhu mudbireen" },
    { "resource_id": 57, "text": "Faragha ila alihatihimfaqala ala ta/kuloon" },
    { "resource_id": 57, "text": "Ma lakum la tantiqoon" },
    { "resource_id": 57, "text": "Faragha AAalayhim darban bilyameen" },
    { "resource_id": 57, "text": "Faaqbaloo ilayhi yaziffoon" },
    { "resource_id": 57, "text": "Qala ataAAbudoona ma tanhitoon" },
    { "resource_id": 57, "text": "Wallahu khalaqakum wamataAAmaloon" },
    {
      "resource_id": 57,
      "text": "Qaloo ibnoo lahu bunyananfaalqoohu fee aljaheem"
    },
    {
      "resource_id": 57,
      "text": "Faaradoo bihi kaydan fajaAAalnahumual-asfaleen"
    },
    { "resource_id": 57, "text": "Waqala innee thahibun ilarabbee sayahdeen" },
    { "resource_id": 57, "text": "Rabbi hablee mina assaliheen" },
    { "resource_id": 57, "text": "Fabashsharnahu bighulamin haleem" },
    {
      "resource_id": 57,
      "text": "Falamma balagha maAAahu assaAAyaqala ya bunayya innee ara fee almanamiannee athbahuka fanthur mathatara qala ya abati ifAAal ma tu/marusatajidunee in shaa Allahu mina assabireen"
    },
    { "resource_id": 57, "text": "Falamma aslama watallahuliljabeen" },
    { "resource_id": 57, "text": "Wanadaynahu an ya ibraheem" },
    {
      "resource_id": 57,
      "text": "Qad saddaqta arru/yainna kathalika najzee almuhsineen"
    },
    { "resource_id": 57, "text": "Inna hatha lahuwa albalaoalmubeen" },
    { "resource_id": 57, "text": "Wafadaynahu bithibhinAAatheem" },
    { "resource_id": 57, "text": "Watarakna AAalayhi fee al-akhireen" },
    { "resource_id": 57, "text": "Salamun AAala ibraheem" },
    { "resource_id": 57, "text": "Kathalika najzee almuhsineen" },
    { "resource_id": 57, "text": "Innahu min AAibadinaalmu/mineen" },
    {
      "resource_id": 57,
      "text": "Wabashsharnahu bi-ishaqanabiyyan mina assaliheen"
    },
    {
      "resource_id": 57,
      "text": "Wabarakna AAalayhi waAAalaishaqa wamin thurriyyatihima muhsinunwathalimun linafsihi mubeen"
    },
    { "resource_id": 57, "text": "Walaqad mananna AAala moosawaharoon" },
    {
      "resource_id": 57,
      "text": "Wanajjaynahuma waqawmahumamina alkarbi alAAatheem"
    },
    { "resource_id": 57, "text": "Wanasarnahum fakanoohumu alghalibeen" },
    { "resource_id": 57, "text": "Waataynahuma alkitabaalmustabeen" },
    { "resource_id": 57, "text": "Wahadaynahuma assirataalmustaqeem" },
    { "resource_id": 57, "text": "Watarakna AAalayhima fee al-akhireen" },
    { "resource_id": 57, "text": "Salamun AAala moosawaharoon" },
    { "resource_id": 57, "text": "Inna kathalika najzee almuhsineen" },
    { "resource_id": 57, "text": "Innahuma min AAibadinaalmu/mineen" },
    { "resource_id": 57, "text": "Wa-inna ilyasa lamina almursaleen" },
    { "resource_id": 57, "text": "Ith qala liqawmihi alatattaqoon" },
    {
      "resource_id": 57,
      "text": "AtadAAoona baAAlan watatharoona ahsanaalkhaliqeen"
    },
    {
      "resource_id": 57,
      "text": "Allaha rabbakum warabba aba-ikumual-awwaleen"
    },
    { "resource_id": 57, "text": "Fakaththaboohu fa-innahum lamuhdaroon" },
    { "resource_id": 57, "text": "Illa AAibada Allahialmukhlaseen" },
    { "resource_id": 57, "text": "Watarakna AAalayhi fee al-akhireen" },
    { "resource_id": 57, "text": "Salamun AAala il yaseen" },
    { "resource_id": 57, "text": "Inna kathalika najzee almuhsineen" },
    { "resource_id": 57, "text": "Innahu min AAibadinaalmu/mineen" },
    { "resource_id": 57, "text": "Wa-inna lootan lamina almursaleen" },
    { "resource_id": 57, "text": "Ith najjaynahu waahlahuajmaAAeen" },
    { "resource_id": 57, "text": "Illa AAajoozan fee alghabireen" },
    { "resource_id": 57, "text": "Thumma dammarna al-akhareen" },
    {
      "resource_id": 57,
      "text": "Wa-innakum latamurroona AAalayhim musbiheen"
    },
    { "resource_id": 57, "text": "Wabillayli afala taAAqiloon" },
    { "resource_id": 57, "text": "Wa-inna yoonusa lamina almursaleen" },
    { "resource_id": 57, "text": "Ith abaqa ila alfulki almashhoon" },
    { "resource_id": 57, "text": "Fasahama fakana mina almudhadeen" },
    { "resource_id": 57, "text": "Faltaqamahu alhootu wahuwamuleem" },
    { "resource_id": 57, "text": "Falawla annahu kana minaalmusabbiheen" },
    { "resource_id": 57, "text": "Lalabitha fee batnihi ilayawmi yubAAathoon" },
    { "resource_id": 57, "text": "Fanabathnahu bilAAara-iwahuwa saqeem" },
    { "resource_id": 57, "text": "Waanbatna AAalayhi shajaratan minyaqteen" },
    { "resource_id": 57, "text": "Waarsalnahu ila mi-ati alfinaw yazeedoon" },
    { "resource_id": 57, "text": "Faamanoo famattaAAnahum ilaheen" },
    {
      "resource_id": 57,
      "text": "Fastaftihim alirabbika albanatuwalahumu albanoon"
    },
    {
      "resource_id": 57,
      "text": "Am khalaqna almala-ikata inathanwahum shahidoon"
    },
    { "resource_id": 57, "text": "Ala innahum min ifkihim layaqooloon" },
    { "resource_id": 57, "text": "Walada Allahu wa-innahum lakathiboon" },
    { "resource_id": 57, "text": "Astafa albanati AAalaalbaneen" },
    { "resource_id": 57, "text": "Ma lakum kayfa tahkumoon" },
    { "resource_id": 57, "text": "Afala tathakkaroon" },
    { "resource_id": 57, "text": "Am lakum sultanun mubeen" },
    { "resource_id": 57, "text": "Fa/too bikitabikum in kuntum sadiqeen" },
    {
      "resource_id": 57,
      "text": "WajaAAaloo baynahu wabayna aljinnatinasaban walaqad AAalimati aljinnatu innahum lamuhdaroon"
    },
    { "resource_id": 57, "text": "Subhana Allahi AAammayasifoon" },
    { "resource_id": 57, "text": "Illa AAibada Allahialmukhlaseen" },
    { "resource_id": 57, "text": "Fa-innakum wama taAAbudoon" },
    { "resource_id": 57, "text": "Ma antum AAalayhi bifatineen" },
    { "resource_id": 57, "text": "Illa man huwa sali aljaheem" },
    { "resource_id": 57, "text": "Wama minna illa lahumaqamun maAAloom" },
    { "resource_id": 57, "text": "Wa-inna lanahnu assaffoon" },
    { "resource_id": 57, "text": "Wa-inna lanahnu almusabbihoon" },
    { "resource_id": 57, "text": "Wa-in kanoo layaqooloon" },
    { "resource_id": 57, "text": "Law anna AAindana thikranmina al-awwaleen" },
    { "resource_id": 57, "text": "Lakunna AAibada Allahialmukhlaseen" },
    { "resource_id": 57, "text": "Fakafaroo bihi fasawfa yaAAlamoon" },
    {
      "resource_id": 57,
      "text": "Walaqad sabaqat kalimatuna liAAibadinaalmursaleen"
    },
    { "resource_id": 57, "text": "Innahum lahumu almansooroon" },
    { "resource_id": 57, "text": "Wa-inna jundana lahumu alghaliboon" },
    { "resource_id": 57, "text": "Fatawalla AAanhum hatta heen" },
    { "resource_id": 57, "text": "Waabsirhum fasawfa yubsiroon" },
    { "resource_id": 57, "text": "AfabiAAathabina yastaAAjiloon" },
    {
      "resource_id": 57,
      "text": "Fa-itha nazala bisahatihimfasaa sabahu almunthareen"
    },
    { "resource_id": 57, "text": "Watawalla AAanhum hatta heen" },
    { "resource_id": 57, "text": "Waabsir fasawfa yubsiroon" },
    {
      "resource_id": 57,
      "text": "Subhana rabbika rabbi alAAizzatiAAamma yasifoon"
    },
    { "resource_id": 57, "text": "Wasalamun AAala almursaleen" },
    { "resource_id": 57, "text": "Walhamdu lillahirabbi alAAalameen" }
