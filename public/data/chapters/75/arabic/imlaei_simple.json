{
  "verses": [
    {
      "id": 5552,
      "verse_key": "75:1",
      "text_imlaei_simple": "لا اقسم بيوم القيامة"
    },
    {
      "id": 5553,
      "verse_key": "75:2",
      "text_imlaei_simple": "ولا اقسم بالنفس اللوامة"
    },
    {
      "id": 5554,
      "verse_key": "75:3",
      "text_imlaei_simple": "ايحسب الانسان الن نجمع عظامه"
    },
    {
      "id": 5555,
      "verse_key": "75:4",
      "text_imlaei_simple": "بلى قادرين على ان نسوي بنانه"
    },
    {
      "id": 5556,
      "verse_key": "75:5",
      "text_imlaei_simple": "بل يريد الانسان ليفجر امامه"
    },
    {
      "id": 5557,
      "verse_key": "75:6",
      "text_imlaei_simple": "يسال ايان يوم القيامة"
    },
    { "id": 5558, "verse_key": "75:7", "text_imlaei_simple": "فاذا برق البصر" },
    { "id": 5559, "verse_key": "75:8", "text_imlaei_simple": "وخسف القمر" },
    {
      "id": 5560,
      "verse_key": "75:9",
      "text_imlaei_simple": "وجمع الشمس والقمر"
    },
    {
      "id": 5561,
      "verse_key": "75:10",
      "text_imlaei_simple": "يقول الانسان يومئذ اين المفر"
    },
    { "id": 5562, "verse_key": "75:11", "text_imlaei_simple": "كلا لا وزر" },
    {
      "id": 5563,
      "verse_key": "75:12",
      "text_imlaei_simple": "الى ربك يومئذ المستقر"
    },
    {
      "id": 5564,
      "verse_key": "75:13",
      "text_imlaei_simple": "ينبا الانسان يومئذ بما قدم واخر"
    },
    {
      "id": 5565,
      "verse_key": "75:14",
      "text_imlaei_simple": "بل الانسان على نفسه بصيرة"
    },
    {
      "id": 5566,
      "verse_key": "75:15",
      "text_imlaei_simple": "ولو القى معاذيره"
    },
    {
      "id": 5567,
      "verse_key": "75:16",
      "text_imlaei_simple": "لا تحرك به لسانك لتعجل به"
    },
    {
      "id": 5568,
      "verse_key": "75:17",
      "text_imlaei_simple": "ان علينا جمعه وقرانه"
    },
    {
      "id": 5569,
      "verse_key": "75:18",
      "text_imlaei_simple": "فاذا قراناه فاتبع قرانه"
    },
    {
      "id": 5570,
      "verse_key": "75:19",
      "text_imlaei_simple": "ثم ان علينا بيانه"
    },
    {
      "id": 5571,
      "verse_key": "75:20",
      "text_imlaei_simple": "كلا بل تحبون العاجلة"
    },
    { "id": 5572, "verse_key": "75:21", "text_imlaei_simple": "وتذرون الاخرة" },
    {
      "id": 5573,
      "verse_key": "75:22",
      "text_imlaei_simple": "وجوه يومئذ ناضرة"
    },
    {
      "id": 5574,
      "verse_key": "75:23",
      "text_imlaei_simple": "الى ربها ناظرة"
    },
    {
      "id": 5575,
      "verse_key": "75:24",
      "text_imlaei_simple": "ووجوه يومئذ باسرة"
    },
    {
      "id": 5576,
      "verse_key": "75:25",
      "text_imlaei_simple": "تظن ان يفعل بها فاقرة"
    },
    {
      "id": 5577,
      "verse_key": "75:26",
      "text_imlaei_simple": "كلا اذا بلغت التراقي"
    },
    { "id": 5578, "verse_key": "75:27", "text_imlaei_simple": "وقيل من راق" },
    {
      "id": 5579,
      "verse_key": "75:28",
      "text_imlaei_simple": "وظن انه الفراق"
    },
    {
      "id": 5580,
      "verse_key": "75:29",
      "text_imlaei_simple": "والتفت الساق بالساق"
    },
    {
      "id": 5581,
      "verse_key": "75:30",
      "text_imlaei_simple": "الى ربك يومئذ المساق"
    },
    {
      "id": 5582,
      "verse_key": "75:31",
      "text_imlaei_simple": "فلا صدق ولا صلى"
    },
    {
      "id": 5583,
      "verse_key": "75:32",
      "text_imlaei_simple": "ولكن كذب وتولى"
    },
    {
      "id": 5584,
      "verse_key": "75:33",
      "text_imlaei_simple": "ثم ذهب الى اهله يتمطى"
    },
    { "id": 5585, "verse_key": "75:34", "text_imlaei_simple": "اولى لك فاولى" },
    {
      "id": 5586,
      "verse_key": "75:35",
      "text_imlaei_simple": "ثم اولى لك فاولى"
    },
    {
      "id": 5587,
      "verse_key": "75:36",
      "text_imlaei_simple": "ايحسب الانسان ان يترك سدى"
    },
    {
      "id": 5588,
      "verse_key": "75:37",
      "text_imlaei_simple": "الم يك نطفة من مني يمنى"
    },
    {
      "id": 5589,
      "verse_key": "75:38",
      "text_imlaei_simple": "ثم كان علقة فخلق فسوى"
    },
    {
      "id": 5590,
      "verse_key": "75:39",
      "text_imlaei_simple": "فجعل منه الزوجين الذكر والانثى"
    },
    {
      "id": 5591,
      "verse_key": "75:40",
      "text_imlaei_simple": "اليس ذلك بقادر على ان يحيي الموتى"
    }
  ],
  "meta": { "filters": { "chapter_number": "75" } }
}
