{
  "verses": [
    { "id": 5324, "verse_key": "69:1", "text_imlaei_simple": "الحاقة" },
    { "id": 5325, "verse_key": "69:2", "text_imlaei_simple": "ما الحاقة" },
    {
      "id": 5326,
      "verse_key": "69:3",
      "text_imlaei_simple": "وما ادراك ما الحاقة"
    },
    {
      "id": 5327,
      "verse_key": "69:4",
      "text_imlaei_simple": "كذبت ثمود وعاد بالقارعة"
    },
    {
      "id": 5328,
      "verse_key": "69:5",
      "text_imlaei_simple": "فاما ثمود فاهلكوا بالطاغية"
    },
    {
      "id": 5329,
      "verse_key": "69:6",
      "text_imlaei_simple": "واما عاد فاهلكوا بريح صرصر عاتية"
    },
    {
      "id": 5330,
      "verse_key": "69:7",
      "text_imlaei_simple": "سخرها عليهم سبع ليال وثمانية ايام حسوما فترى القوم فيها صرعى كانهم اعجاز نخل خاوية"
    },
    {
      "id": 5331,
      "verse_key": "69:8",
      "text_imlaei_simple": "فهل ترى لهم من باقية"
    },
    {
      "id": 5332,
      "verse_key": "69:9",
      "text_imlaei_simple": "وجاء فرعون ومن قبله والمؤتفكات بالخاطئة"
    },
    {
      "id": 5333,
      "verse_key": "69:10",
      "text_imlaei_simple": "فعصوا رسول ربهم فاخذهم اخذة رابية"
    },
    {
      "id": 5334,
      "verse_key": "69:11",
      "text_imlaei_simple": "انا لما طغى الماء حملناكم في الجارية"
    },
    {
      "id": 5335,
      "verse_key": "69:12",
      "text_imlaei_simple": "لنجعلها لكم تذكرة وتعيها اذن واعية"
    },
    {
      "id": 5336,
      "verse_key": "69:13",
      "text_imlaei_simple": "فاذا نفخ في الصور نفخة واحدة"
    },
    {
      "id": 5337,
      "verse_key": "69:14",
      "text_imlaei_simple": "وحملت الارض والجبال فدكتا دكة واحدة"
    },
    {
      "id": 5338,
      "verse_key": "69:15",
      "text_imlaei_simple": "فيومئذ وقعت الواقعة"
    },
    {
      "id": 5339,
      "verse_key": "69:16",
      "text_imlaei_simple": "وانشقت السماء فهي يومئذ واهية"
    },
    {
      "id": 5340,
      "verse_key": "69:17",
      "text_imlaei_simple": "والملك على ارجائها ويحمل عرش ربك فوقهم يومئذ ثمانية"
    },
    {
      "id": 5341,
      "verse_key": "69:18",
      "text_imlaei_simple": "يومئذ تعرضون لا تخفى منكم خافية"
    },
    {
      "id": 5342,
      "verse_key": "69:19",
      "text_imlaei_simple": "فاما من اوتي كتابه بيمينه فيقول هاؤم اقرءوا كتابيه"
    },
    {
      "id": 5343,
      "verse_key": "69:20",
      "text_imlaei_simple": "اني ظننت اني ملاق حسابيه"
    },
    {
      "id": 5344,
      "verse_key": "69:21",
      "text_imlaei_simple": "فهو في عيشة راضية"
    },
    { "id": 5345, "verse_key": "69:22", "text_imlaei_simple": "في جنة عالية" },
    { "id": 5346, "verse_key": "69:23", "text_imlaei_simple": "قطوفها دانية" },
    {
      "id": 5347,
      "verse_key": "69:24",
      "text_imlaei_simple": "كلوا واشربوا هنيئا بما اسلفتم في الايام الخالية"
    },
    {
      "id": 5348,
      "verse_key": "69:25",
      "text_imlaei_simple": "واما من اوتي كتابه بشماله فيقول يا ليتني لم اوت كتابيه"
    },
    {
      "id": 5349,
      "verse_key": "69:26",
      "text_imlaei_simple": "ولم ادر ما حسابيه"
    },
    {
      "id": 5350,
      "verse_key": "69:27",
      "text_imlaei_simple": "يا ليتها كانت القاضية"
    },
    {
      "id": 5351,
      "verse_key": "69:28",
      "text_imlaei_simple": "ما اغنى عني ماليه"
    },
    {
      "id": 5352,
      "verse_key": "69:29",
      "text_imlaei_simple": "هلك عني سلطانيه"
    },
    { "id": 5353, "verse_key": "69:30", "text_imlaei_simple": "خذوه فغلوه" },
    {
      "id": 5354,
      "verse_key": "69:31",
      "text_imlaei_simple": "ثم الجحيم صلوه"
    },
    {
      "id": 5355,
      "verse_key": "69:32",
      "text_imlaei_simple": "ثم في سلسلة ذرعها سبعون ذراعا فاسلكوه"
    },
    {
      "id": 5356,
      "verse_key": "69:33",
      "text_imlaei_simple": "انه كان لا يؤمن بالله العظيم"
    },
    {
      "id": 5357,
      "verse_key": "69:34",
      "text_imlaei_simple": "ولا يحض على طعام المسكين"
    },
    {
      "id": 5358,
      "verse_key": "69:35",
      "text_imlaei_simple": "فليس له اليوم هاهنا حميم"
    },
    {
      "id": 5359,
      "verse_key": "69:36",
      "text_imlaei_simple": "ولا طعام الا من غسلين"
    },
    {
      "id": 5360,
      "verse_key": "69:37",
      "text_imlaei_simple": "لا ياكله الا الخاطئون"
    },
    {
      "id": 5361,
      "verse_key": "69:38",
      "text_imlaei_simple": "فلا اقسم بما تبصرون"
    },
    { "id": 5362, "verse_key": "69:39", "text_imlaei_simple": "وما لا تبصرون" },
    {
      "id": 5363,
      "verse_key": "69:40",
      "text_imlaei_simple": "انه لقول رسول كريم"
    },
    {
      "id": 5364,
      "verse_key": "69:41",
      "text_imlaei_simple": "وما هو بقول شاعر قليلا ما تؤمنون"
    },
    {
      "id": 5365,
      "verse_key": "69:42",
      "text_imlaei_simple": "ولا بقول كاهن قليلا ما تذكرون"
    },
    {
      "id": 5366,
      "verse_key": "69:43",
      "text_imlaei_simple": "تنزيل من رب العالمين"
    },
    {
      "id": 5367,
      "verse_key": "69:44",
      "text_imlaei_simple": "ولو تقول علينا بعض الاقاويل"
    },
    {
      "id": 5368,
      "verse_key": "69:45",
      "text_imlaei_simple": "لاخذنا منه باليمين"
    },
    {
      "id": 5369,
      "verse_key": "69:46",
      "text_imlaei_simple": "ثم لقطعنا منه الوتين"
    },
    {
      "id": 5370,
      "verse_key": "69:47",
      "text_imlaei_simple": "فما منكم من احد عنه حاجزين"
    },
    {
      "id": 5371,
      "verse_key": "69:48",
      "text_imlaei_simple": "وانه لتذكرة للمتقين"
    },
    {
      "id": 5372,
      "verse_key": "69:49",
      "text_imlaei_simple": "وانا لنعلم ان منكم مكذبين"
    },
    {
      "id": 5373,
      "verse_key": "69:50",
      "text_imlaei_simple": "وانه لحسرة على الكافرين"
    },
    {
      "id": 5374,
      "verse_key": "69:51",
      "text_imlaei_simple": "وانه لحق اليقين"
    },
    {
      "id": 5375,
      "verse_key": "69:52",
      "text_imlaei_simple": "فسبح باسم ربك العظيم"
    }
  ],
  "meta": { "filters": { "chapter_number": "69" } }
}
