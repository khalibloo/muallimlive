{
  "translations": [
    { "resource_id": 222, "text": "Қараңғылығы басқан сәтте,түнге," },
    { "resource_id": 222, "text": "Жарқыраған сәтте күндізге," },
    { "resource_id": 222, "text": "Және еркек,ұрғашыны жаратқанға серт." },
    { "resource_id": 222, "text": "Негізінен кәсіптерің түрлі-түрлі." },
    {
      "resource_id": 222,
      "text": "Ал енді әлде кім (Алла жолында) берсе,тақуалық қылса,"
    },
    { "resource_id": 222, "text": "Жақсылықты дұрыс деп білсе;" },
    { "resource_id": 222, "text": "Сонда оның жолын оңайластырамыз." },
    { "resource_id": 222, "text": "Ал енді әлде кім сараңдық істесе,елемесе," },
    { "resource_id": 222, "text": "Жақсылықты жасынға шығарса;" },
    {
      "resource_id": 222,
      "text": "Сонда оның ауырлыққа ұшырауын оңайластырамыз."
    },
    {
      "resource_id": 222,
      "text": "(Тозаққа) жығылған сәтте,оның малы оған пайда бермейді."
    },
    { "resource_id": 222, "text": "Негізінде тура жол көрсету бізге тән." },
    { "resource_id": 222, "text": "Сөз жоқ,ақирет те дүние де біздікі." },
    {
      "resource_id": 222,
      "text": "Сондықтан сендерге жалындаған отты ескерттім."
    },
    { "resource_id": 222, "text": "Оған жауыздар ғана кіреді." },
    { "resource_id": 222, "text": "Сондай өтірік деп жалтарғандар." },
    { "resource_id": 222, "text": "Тақуарақ болған одан ұзақтастырылады." },
    { "resource_id": 222, "text": "Сондай малын сарып қылып тазарғандар." },
    {
      "resource_id": 222,
      "text": "Оның онда ешкімге берер еселі сыйлығы жоқ. (Біреудің өзіне істегенінің бодауы үшін емес)"
    },
    { "resource_id": 222, "text": "Бірақ ұлы Раббының дидарына ғана іздейді." },
    { "resource_id": 222, "text": "Сондай ақ ол,таяуда разы болады." }
  ],
