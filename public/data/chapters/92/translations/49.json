{
  "translations": [
    { "resource_id": 49, "text": "Naapa kwa usiku unapo funika!" },
    { "resource_id": 49, "text": "Na mchana unapo dhihiri!" },
    { "resource_id": 49, "text": "Na kwa Aliye umba dume na jike!" },
    {
      "resource_id": 49,
      "text": "Hakika juhudi zenu bila ya shaka ni mbali mbali."
    },
    { "resource_id": 49, "text": "Ama mwenye kutoa na akamchamngu," },
    { "resource_id": 49, "text": "Na akaliwafiki lilio jema," },
    { "resource_id": 49, "text": "Tutamsahilishia yawe mepesi." },
    {
      "resource_id": 49,
      "text": "Na ama mwenye kufanya ubakhili, na asiwe na haja ya wenzake,"
    },
    { "resource_id": 49, "text": "Na akakanusha lilio jema," },
    { "resource_id": 49, "text": "Tutamsahilishia yawe mazito!" },
    {
      "resource_id": 49,
      "text": "Na mali yake yatamfaa nini atapo kuwa anadidimia?"
    },
    { "resource_id": 49, "text": "Hakika ni juu yetu kuonyesha uwongofu." },
    { "resource_id": 49, "text": "Na hakika ni yetu Sisi Akhera na dunia." },
    { "resource_id": 49, "text": "Basi nakuonyeni na Moto unao waka!" },
    { "resource_id": 49, "text": "Hatauingia ila mwovu kabisa!" },
    { "resource_id": 49, "text": "Anaye kadhibisha na kupa mgongo." },
    { "resource_id": 49, "text": "Na mchamngu ataepushwa nao," },
    {
      "resource_id": 49,
      "text": "Ambaye hutoa mali yake kwa ajili ya kujitakasa."
    },
    {
      "resource_id": 49,
      "text": "Na wala si kwa kuwa yupo yeyote aliye mfanyia hisani ndio anamlipa."
    },
    {
      "resource_id": 49,
      "text": "Ila ni kutaka radhi ya Mola wake Mlezi aliye juu kabisa."
    },
    { "resource_id": 49, "text": "Naye atakuja ridhika!" }
  ],
