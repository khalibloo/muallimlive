{
  "verses": [
    {
      "id": 6059,
      "verse_key": "92:1",
      "text_indopak": "وَالَّيۡلِ اِذَا يَغۡشٰىۙ‏"
    },
    {
      "id": 6060,
      "verse_key": "92:2",
      "text_indopak": "وَالنَّهَارِ اِذَا تَجَلّٰىۙ‏"
    },
    {
      "id": 6061,
      "verse_key": "92:3",
      "text_indopak": "وَمَا خَلَقَ الذَّكَرَ وَالۡاُنۡثٰٓىۙ‏"
    },
    {
      "id": 6062,
      "verse_key": "92:4",
      "text_indopak": "اِنَّ سَعۡيَكُمۡ لَشَتّٰىؕ‏"
    },
    {
      "id": 6063,
      "verse_key": "92:5",
      "text_indopak": "فَاَمَّا مَنۡ اَعۡطٰى وَاتَّقٰىۙ‏"
    },
    {
      "id": 6064,
      "verse_key": "92:6",
      "text_indopak": "وَصَدَّقَ بِالۡحُسۡنٰىۙ‏"
    },
    {
      "id": 6065,
      "verse_key": "92:7",
      "text_indopak": "فَسَنُيَسِّرُهٗ لِلۡيُسۡرٰىؕ‏"
    },
    {
      "id": 6066,
      "verse_key": "92:8",
      "text_indopak": "وَاَمَّا مَنۡۢ بَخِلَ وَاسۡتَغۡنٰىۙ‏"
    },
    {
      "id": 6067,
      "verse_key": "92:9",
      "text_indopak": "وَكَذَّبَ بِالۡحُسۡنٰىۙ‏"
    },
    {
      "id": 6068,
      "verse_key": "92:10",
      "text_indopak": "فَسَنُيَسِّرُهٗ لِلۡعُسۡرٰىؕ‏"
    },
    {
      "id": 6069,
      "verse_key": "92:11",
      "text_indopak": "وَمَا يُغۡنِىۡ عَنۡهُ مَالُهٗۤ اِذَا تَرَدّٰىؕ‏"
    },
    {
      "id": 6070,
      "verse_key": "92:12",
      "text_indopak": "اِنَّ عَلَيۡنَا لَـلۡهُدٰىۖ‏"
    },
    {
      "id": 6071,
      "verse_key": "92:13",
      "text_indopak": "وَاِنَّ لَـنَا لَـلۡاٰخِرَةَ وَالۡاُوۡلٰى‏"
    },
    {
      "id": 6072,
      "verse_key": "92:14",
      "text_indopak": "فَاَنۡذَرۡتُكُمۡ نَارًا تَلَظّٰى​ۚ‏"
    },
    {
      "id": 6073,
      "verse_key": "92:15",
      "text_indopak": "لَا يَصۡلٰٮهَاۤ اِلَّا الۡاَشۡقَىۙ‏"
    },
    {
      "id": 6074,
      "verse_key": "92:16",
      "text_indopak": "الَّذِىۡ كَذَّبَ وَتَوَلّٰىؕ‏"
    },
    {
      "id": 6075,
      "verse_key": "92:17",
      "text_indopak": "وَسَيُجَنَّبُهَا الۡاَتۡقَىۙ‏"
    },
    {
      "id": 6076,
      "verse_key": "92:18",
      "text_indopak": "الَّذِىۡ يُؤۡتِىۡ مَالَهٗ يَتَزَكّٰى​ۚ‏"
    },
    {
      "id": 6077,
      "verse_key": "92:19",
      "text_indopak": "وَمَا لِاَحَدٍ عِنۡدَهٗ مِنۡ نِّعۡمَةٍ تُجۡزٰٓىۙ‏"
    },
    {
      "id": 6078,
      "verse_key": "92:20",
      "text_indopak": "اِلَّا ابۡتِغَآءَ وَجۡهِ رَبِّهِ الۡاَعۡلٰى​ۚ‏"
    },
    { "id": 6079, "verse_key": "92:21", "text_indopak": "وَلَسَوۡفَ يَرۡضٰى‏" }
  ],
  "meta": { "filters": { "chapter_number": "92" } }
}
