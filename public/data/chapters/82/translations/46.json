{
  "translations": [
    { "resource_id": 46, "text": "Markay Samadu Dildillaacdo." },
    { "resource_id": 46, "text": "Xiddiguhuna daataan." },
    { "resource_id": 46, "text": "Badahana laysku furo." },
    { "resource_id": 46, "text": "Xabaalahana la soo bixiyo (Dadkii)." },
    {
      "resource_id": 46,
      "text": "Waxay ogaan naftu waxay hor marsatay iyo waxay dibmarisay."
    },
    {
      "resource_id": 46,
      "text": "Dadow maxaa kugu kadiyay Eebahaaga sharafta leh."
    },
    {
      "resource_id": 46,
      "text": "Ee kii ku abuuray ah kuna ekeeyay kuna simay."
    },
    { "resource_id": 46, "text": "Siduu doonana kuu sameeyey." },
    {
      "resource_id": 46,
      "text": "Saas ma aha ee waxaad beeninaysaan abaal marinta."
    },
    { "resource_id": 46, "text": "Malaa'ig baana idin ilaalinaysa." },
    { "resource_id": 46, "text": "Oo sharaf leh waxna qori." },
    { "resource_id": 46, "text": "Ogna waxaad falaysaan." },
    { "resource_id": 46, "text": "Kuwa wanaagsani waxay gali Naciima." },
    { "resource_id": 46, "text": "Kuwa xun Xunna waxay gali Jaxiimo." },
    { "resource_id": 46, "text": "Wayna ku guban Maalinta abaalmarinta." },
    { "resource_id": 46, "text": "Kamana maqnaadaan Naarta." },
    { "resource_id": 46, "text": "Maalinta abaalmarinta ma taqaannaa." },
    {
      "resource_id": 46,
      "text": "Haddana ma taqaannaa Maalinta abaalmarinta."
    },
    {
      "resource_id": 46,
      "text": "Waa Maalinta ayna u hanannin Nafna Naf kale waxba, Amarkana Maalintaas waxaa iska leh Eebe."
    }
  ],
  "meta": {
