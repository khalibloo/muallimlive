{
  "translations": [
    { "resource_id": 112, "text": "Andolsun kuşluk vaktine," },
    { "resource_id": 112, "text": "Sükuna erdiği vakit geceye," },
    {
      "resource_id": 112,
      "text": "Rabbin seni terk etmedi ve (vahyi geciktirerek) seni öfkelendirmedi."
    },
    {
      "resource_id": 112,
      "text": "Gerçekten ahiret senin için dünyadan daha hayırlıdır."
    },
    {
      "resource_id": 112,
      "text": "Rabbin sana verecek ve sen de hoşnut olacaksın."
    },
    { "resource_id": 112, "text": "Seni yetim bulup barındırmadı mı?" },
    {
      "resource_id": 112,
      "text": "Seni bilmez bulup doğru yolu göstermedi mi?"
    },
    { "resource_id": 112, "text": "Seni fakir bulup zengin etmedi mi?" },
    { "resource_id": 112, "text": "Şimdi yetimi hor görme!" },
    { "resource_id": 112, "text": "İsteyeni azarlama!" },
    { "resource_id": 112, "text": "Rabbinin nimetini ise haydi anlat." }
  ],
