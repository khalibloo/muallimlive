{
  "translations": [
    { "resource_id": 111, "text": "Waareenin (kakadha)." },
    { "resource_id": 111, "text": "Halkaniinis yeroo inni callise." },
    {
      "resource_id": 111,
      "text": "Gooftaan kee si hin dhiifne; si hin jibbines"
    },
    {
      "resource_id": 111,
      "text": "Dhugumatti, Aakhiraatu addunyaa irra siif caala."
    },
    {
      "resource_id": 111,
      "text": "Dhugumatti, Rabbiin kee fuula dura (kenna guddaa) siif kenna; ni jaallattas"
    },
    {
      "resource_id": 111,
      "text": "Sila yatiima ta’uu si argee si hin hirkifnee?"
    },
    { "resource_id": 111, "text": "Dogoggoraas ta’uu si argee si qajeelche." },
    { "resource_id": 111, "text": "Hiyyeessas taatee si argee si duroomse." },
    { "resource_id": 111, "text": "Yatiima hin hacuucin" },
    { "resource_id": 111, "text": "Kadhataas hin ifatin" },
    { "resource_id": 111, "text": "Ni'imaa (qananii) Gooftaa keetiis odeessi." }
  ],
