{
  "translations": [
    { "resource_id": 232, "text": "Ndayira ekiseera ky’a kalasa mayanzi." },
    { "resource_id": 232, "text": "Era ndayira e kiro bwekiba nga kikutte." },
    {
      "resource_id": 232,
      "text": "Omuleziwo Katonda takwabuliranga (gwe Nabbi Muhammad) era takukyawanga."
    },
    {
      "resource_id": 232,
      "text": "Era ddala e nkomerero y’ennungi gy’oli okusinga entandikwa."
    },
    {
      "resource_id": 232,
      "text": "Era ddala Omuleziwo ajja kukuwa naawe osiime."
    },
    { "resource_id": 232, "text": "Teyakusanga ng’oli mulekwa n'akulabirira?" },
    {
      "resource_id": 232,
      "text": "Era teyakusanga nga tomanyi      n’akulungamya?"
    },
    {
      "resource_id": 232,
      "text": "Era teyakusanga nga oli mwavu      n’akugaggawaza?"
    },
    { "resource_id": 232, "text": "Kale nno mulekwa tomukaayuukiranga." },
    { "resource_id": 232, "text": "N’omuntu asaba tomuboggoleranga." },
    {
      "resource_id": 232,
      "text": "Ate era e kyengera kyonna Omuleziwo Katonda ky’abanga akuwadde kinyumyengako."
    }
  ],
