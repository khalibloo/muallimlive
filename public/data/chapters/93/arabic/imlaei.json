{
  "verses": [
    { "id": 6080, "verse_key": "93:1", "text_imlaei": "وَالضُّحَىٰ" },
    {
      "id": 6081,
      "verse_key": "93:2",
      "text_imlaei": "وَاللَّيْلِ إِذَا سَجَىٰ"
    },
    {
      "id": 6082,
      "verse_key": "93:3",
      "text_imlaei": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلَىٰ"
    },
    {
      "id": 6083,
      "verse_key": "93:4",
      "text_imlaei": "وَلَلْآخِرَةُ خَيْرٌ لَّكَ مِنَ الْأُولَىٰ"
    },
    {
      "id": 6084,
      "verse_key": "93:5",
      "text_imlaei": "وَلَسَوْفَ يُعْطِيكَ رَبُّكَ فَتَرْضَىٰ"
    },
    {
      "id": 6085,
      "verse_key": "93:6",
      "text_imlaei": "أَلَمْ يَجِدْكَ يَتِيمًا فَآوَىٰ"
    },
    {
      "id": 6086,
      "verse_key": "93:7",
      "text_imlaei": "وَوَجَدَكَ ضَالًّا فَهَدَىٰ"
    },
    {
      "id": 6087,
      "verse_key": "93:8",
      "text_imlaei": "وَوَجَدَكَ عَائِلًا فَأَغْنَىٰ"
    },
    {
      "id": 6088,
      "verse_key": "93:9",
      "text_imlaei": "فَأَمَّا الْيَتِيمَ فَلَا تَقْهَرْ"
    },
    {
      "id": 6089,
      "verse_key": "93:10",
      "text_imlaei": "وَأَمَّا السَّائِلَ فَلَا تَنْهَرْ"
    },
    {
      "id": 6090,
      "verse_key": "93:11",
      "text_imlaei": "وَأَمَّا بِنِعْمَةِ رَبِّكَ فَحَدِّثْ"
    }
  ],
