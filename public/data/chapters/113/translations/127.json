{
  "translations": [
    { "resource_id": 127, "text": "«Тонг Роббисидан паноҳ сўрайман." },
    { "resource_id": 127, "text": "У яратган нарсалар ёмонлигидан." },
    {
      "resource_id": 127,
      "text": "Ва кириб келган қоронғу кечанинг ёмонлигидан."
    },
    {
      "resource_id": 127,
      "text": "Ва тугунларга дам солувчилар ёмонлигидан.<sup foot_note=48901>1</sup>"
    },
    {
      "resource_id": 127,
      "text": "Ва ҳасад қилган ҳасадчининг ёмонлигидан», деб айт."
    }
  ],
