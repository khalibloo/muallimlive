{
  "translations": [
    { "resource_id": 126, "text": "Smak svijeta!" },
    { "resource_id": 126, "text": "Šta je Smak svijeta?" },
    { "resource_id": 126, "text": "i šta ti znaš o Smaku svijeta?" },
    {
      "resource_id": 126,
      "text": "Toga dana će ljudi biti kao leptiri raštrkani,"
    },
    { "resource_id": 126, "text": "a planine kao šarena vuna iščupanā." },
    { "resource_id": 126, "text": "Onaj u koga njegova djela budu teška –" },
    { "resource_id": 126, "text": "u ugodnu životu će živjeti," },
    { "resource_id": 126, "text": "a onaj u koga njegova djela budu lahka –" },
    { "resource_id": 126, "text": "boravište će mu bezdan biti." },
    { "resource_id": 126, "text": "A znaš li ti šta će to biti? –" },
    { "resource_id": 126, "text": "Vatra užarena!" }
  ],
