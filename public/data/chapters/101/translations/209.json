    { "resource_id": 209, "text": "Lo Scuotimento!" },
    { "resource_id": 209, "text": "Cos'è lo Scuotimento?" },
    { "resource_id": 209, "text": "E che ne sai dello Scuotimento?" },
    {
      "resource_id": 209,
      "text": "Il giorno in cui gli uomini saranno come falene sparpagliate"
    },
    { "resource_id": 209, "text": "e i monti come ciuffi di lana colorata:" },
    { "resource_id": 209, "text": "chi ha avuto un peso maggiore" },
    { "resource_id": 209, "text": "ha un'esistenza felice," },
    { "resource_id": 209, "text": "ma chi ha avuto un peso leggero," },
    { "resource_id": 209, "text": "lo accoglierà l'Abisso." },
    { "resource_id": 209, "text": "Cosa sai tu dell'Abisso?" },
    { "resource_id": 209, "text": "È un fuoco ardente." }
  ],
