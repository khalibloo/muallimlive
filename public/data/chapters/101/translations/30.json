{
  "translations": [
    { "resource_id": 30, "text": "Vyöryvä tuho!" },
    { "resource_id": 30, "text": "Miten hirveä on vyöryvä tuho!" },
    { "resource_id": 30, "text": "Ymmärrätkö, kuinka kauhea on vyöryvä tuho?" },
    {
      "resource_id": 30,
      "text": "Päivä, jolloin ihmiset ovat kuin hajoitettu sääksien parvi,"
    },
    { "resource_id": 30, "text": "ja vuoret kuin keritty villa." },
    {
      "resource_id": 30,
      "text": "Sen osana, jonka hyvien tekojen mitta on suuri,"
    },
    { "resource_id": 30, "text": "on elämä täynnä tyydytystä," },
    {
      "resource_id": 30,
      "text": "mutta toisin käy sen, jonka hyvät teot vaa'assa keveiksi havaitaan,"
    },
    { "resource_id": 30, "text": "hänen asumuksensa on oleva horna." },
    { "resource_id": 30, "text": "Tiedätkö, mikä horna on?" },
    { "resource_id": 30, "text": "Se on polttava tuli." }
  ],
