{
  "translations": [
    { "resource_id": 26, "text": "Roztříštění!" },
    { "resource_id": 26, "text": "Co je to roztříštění?" },
    { "resource_id": 26, "text": "Víš ty vůbec, co je to roztříštěni?" },
    { "resource_id": 26, "text": "V ten den budou lidé jak můry rozptýlené" },
    { "resource_id": 26, "text": "a hory podobat se budou rozcupované vlně." },
    { "resource_id": 26, "text": "Tehdy ten, jehož skutků váha bude těžká," },
    { "resource_id": 26, "text": "se octne v blaženém žití," },
    {
      "resource_id": 26,
      "text": "zatímco ten, jehož skutků váha bude lehká -"
    },
    { "resource_id": 26, "text": "Propast bude jeho mátí." },
    { "resource_id": 26, "text": "Víš ty vůbec, co je to Propast?" },
    { "resource_id": 26, "text": "Oheň planoucí!" }
  ],
