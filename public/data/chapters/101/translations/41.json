{
  "translations": [
    { "resource_id": 41, "text": "Det knusende slag!" },
    { "resource_id": 41, "text": "Hva er det knusende slag?" },
    {
      "resource_id": 41,
      "text": "Hvordan kan du vite hva det knusende slag er?"
    },
    {
      "resource_id": 41,
      "text": "Den dag blir menneskene som flagrende mygg,"
    },
    { "resource_id": 41, "text": "og fjellene som kardet ull!" },
    { "resource_id": 41, "text": "Den, hvis vektskåler er tunge," },
    { "resource_id": 41, "text": "vil gå til et godt liv!" },
    { "resource_id": 41, "text": "Men den, hvis vektskåler er lette," },
    { "resource_id": 41, "text": "avgrunnen har omsorg for ham." },
    { "resource_id": 41, "text": "Og hvordan kan du vite hva det er?" },
    { "resource_id": 41, "text": "Det er het ild!" }
  ],
