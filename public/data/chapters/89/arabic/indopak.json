{
  "verses": [
    { "id": 5994, "verse_key": "89:1", "text_indopak": "وَالۡفَجۡرِۙ‏" },
    { "id": 5995, "verse_key": "89:2", "text_indopak": "وَلَيَالٍ عَشۡرٍۙ‏" },
    {
      "id": 5996,
      "verse_key": "89:3",
      "text_indopak": "وَّالشَّفۡعِ وَالۡوَتۡرِۙ‏"
    },
    {
      "id": 5997,
      "verse_key": "89:4",
      "text_indopak": "وَالَّيۡلِ اِذَا يَسۡرِ​ۚ‏"
    },
    {
      "id": 5998,
      "verse_key": "89:5",
      "text_indopak": "هَلۡ فِىۡ ذٰلِكَ قَسَمٌ لِّذِىۡ حِجۡرٍؕ‏"
    },
    {
      "id": 5999,
      "verse_key": "89:6",
      "text_indopak": "اَلَمۡ تَرَ كَيۡفَ فَعَلَ رَبُّكَ بِعَادٍۙ‏"
    },
    {
      "id": 6000,
      "verse_key": "89:7",
      "text_indopak": "اِرَمَ ذَاتِ الۡعِمَادِۙ‏"
    },
    {
      "id": 6001,
      "verse_key": "89:8",
      "text_indopak": "الَّتِىۡ لَمۡ يُخۡلَقۡ مِثۡلُهَا فِى الۡبِلَادِۙ‏"
    },
    {
      "id": 6002,
      "verse_key": "89:9",
      "text_indopak": "وَثَمُوۡدَ الَّذِيۡنَ جَابُوا الصَّخۡرَ بِالۡوَادِۙ‏"
    },
    {
      "id": 6003,
      "verse_key": "89:10",
      "text_indopak": "وَفِرۡعَوۡنَ ذِى الۡاَوۡتَادِۙ‏"
    },
    {
      "id": 6004,
      "verse_key": "89:11",
      "text_indopak": "الَّذِيۡنَ طَغَوۡا فِى الۡبِلَادِۙ‏"
    },
    {
      "id": 6005,
      "verse_key": "89:12",
      "text_indopak": "فَاَكۡثَرُوۡا فِيۡهَا الۡفَسَادَۙ‏"
    },
    {
      "id": 6006,
      "verse_key": "89:13",
      "text_indopak": "فَصَبَّ عَلَيۡهِمۡ رَبُّكَ سَوۡطَ عَذَابٍ ۙۚ‏"
    },
    {
      "id": 6007,
      "verse_key": "89:14",
      "text_indopak": "اِنَّ رَبَّكَ لَبِالۡمِرۡصَادِؕ‏"
    },
    {
      "id": 6008,
      "verse_key": "89:15",
      "text_indopak": "فَاَمَّا الۡاِنۡسَانُ اِذَا مَا ابۡتَلٰٮهُ رَبُّهٗ فَاَكۡرَمَهٗ وَنَعَّمَهٗ  ۙ فَيَقُوۡلُ رَبِّىۡۤ اَكۡرَمَنِؕ‏"
    },
    {
      "id": 6009,
      "verse_key": "89:16",
      "text_indopak": "وَاَمَّاۤ اِذَا مَا ابۡتَلٰٮهُ فَقَدَرَ عَلَيۡهِ رِزۡقَهٗ ۙ فَيَقُوۡلُ رَبِّىۡۤ اَهَانَنِ​ۚ‏"
    },
    {
      "id": 6010,
      "verse_key": "89:17",
      "text_indopak": "كَلَّا​ بَلۡ لَّا تُكۡرِمُوۡنَ الۡيَتِيۡمَۙ‏"
    },
    {
      "id": 6011,
      "verse_key": "89:18",
      "text_indopak": "وَلَا تَحٰٓضُّوۡنَ عَلٰى طَعَامِ الۡمِسۡكِيۡنِۙ‏"
    },
    {
      "id": 6012,
      "verse_key": "89:19",
      "text_indopak": "وَتَاۡكُلُوۡنَ التُّرَاثَ اَكۡلًا لَّـمًّا ۙ‏"
    },
    {
      "id": 6013,
      "verse_key": "89:20",
      "text_indopak": "وَّتُحِبُّوۡنَ الۡمَالَ حُبًّا جَمًّا ؕ‏"
    },
    {
      "id": 6014,
      "verse_key": "89:21",
      "text_indopak": "كَلَّاۤ اِذَا دُكَّتِ الۡاَرۡضُ دَكًّا دَكًّا ۙ‏"
    },
    {
      "id": 6015,
      "verse_key": "89:22",
      "text_indopak": "وَّجَآءَ رَبُّكَ وَالۡمَلَكُ صَفًّا صَفًّا ۚ‏"
    },
    {
      "id": 6016,
      "verse_key": "89:23",
      "text_indopak": "وَجِاىْٓءَ يَوۡمَٮِٕذٍۢ بِجَهَنَّمَ  ۙ​ يَوۡمَٮِٕذٍ يَّتَذَكَّرُ الۡاِنۡسَانُ وَاَنّٰى لَـهُ الذِّكۡرٰىؕ‏"
    },
    {
      "id": 6017,
      "verse_key": "89:24",
      "text_indopak": "يَقُوۡلُ يٰلَيۡتَنِىۡ قَدَّمۡتُ لِحَـيَاتِى​ۚ‏"
    },
    {
      "id": 6018,
      "verse_key": "89:25",
      "text_indopak": "فَيَوۡمَٮِٕذٍ لَّا يُعَذِّبُ عَذَابَهٗۤ اَحَدٌ ۙ‏"
    },
    {
      "id": 6019,
      "verse_key": "89:26",
      "text_indopak": "وَّلَا يُوۡثِقُ وَثَاقَهٗۤ اَحَدٌ ؕ‏"
    },
    {
      "id": 6020,
      "verse_key": "89:27",
      "text_indopak": "يٰۤاَيَّتُهَا النَّفۡسُ الۡمُطۡمَٮِٕنَّةُ  ۖ‏"
    },
    {
      "id": 6021,
      "verse_key": "89:28",
      "text_indopak": "ارۡجِعِىۡۤ اِلٰى رَبِّكِ رَاضِيَةً مَّرۡضِيَّةً​ ۚ‏"
    },
    {
      "id": 6022,
      "verse_key": "89:29",
      "text_indopak": "فَادۡخُلِىۡ فِىۡ عِبٰدِىۙ‏"
    },
    {
      "id": 6023,
      "verse_key": "89:30",
      "text_indopak": "وَادۡخُلِىۡ جَنَّتِى‏"
    }
  ],
  "meta": { "filters": { "chapter_number": "89" } }
}
