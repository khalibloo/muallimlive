{
  "translations": [
    { "resource_id": 112, "text": "Andolsun fecre," },
    { "resource_id": 112, "text": "On geceye," },
    { "resource_id": 112, "text": "Çifte ve teke," },
    { "resource_id": 112, "text": "Geçip giden geceye," },
    {
      "resource_id": 112,
      "text": "Bunlarda akıl sahibi için elbette birer yemin vardır."
    },
    {
      "resource_id": 112,
      "text": "Ad kavmine Rabbin neler yaptı, görmedin mi?"
    },
    { "resource_id": 112, "text": "Sütunlar sahibi İrem’e…" },
    {
      "resource_id": 112,
      "text": "Ve şehirler arasında bir benzeri yaratılmamıştı."
    },
    { "resource_id": 112, "text": "Ki vadide kayalar oyan Semud’a," },
    { "resource_id": 112, "text": "Kazıklar/ordular sahibi Firavun’a," },
    { "resource_id": 112, "text": "Onlar, ülkelerde azmışlardı." },
    { "resource_id": 112, "text": "Oralarda fesadı çoğaltmışlardı." },
    { "resource_id": 112, "text": "Rabbin de onlara azap kırbacı yağdırdı." },
    { "resource_id": 112, "text": "Çünkü Rabbin gözetlemektedir." },
    {
      "resource_id": 112,
      "text": "İşte insan, onu Rabbi imtihan ettiği zaman, ona ikramda bulunup, nimetlendirince: \"Rabbim bana ikramda bulundu\" der."
    },
    {
      "resource_id": 112,
      "text": "Ama onu, imtihan edip rızkını daralttığında: \"Rabbim beni zelil etti\" der."
    },
    { "resource_id": 112, "text": "Hayır! Yetime ikramda bulunmuyorsunuz." },
    { "resource_id": 112, "text": "Düşkünü doyurmaya teşvik etmiyorsunuz" },
    {
      "resource_id": 112,
      "text": "Mirası da şiddetle (hak gözetmeden) yiyorsunuz."
    },
    { "resource_id": 112, "text": "Malı da pek çok seviyorsunuz." },
    { "resource_id": 112, "text": "Hayır! Yer paramparça olduğu zaman," },
    { "resource_id": 112, "text": "Rabbin ve saf saf melekler geldiği zaman," },
    {
      "resource_id": 112,
      "text": "İşte cehennem o gün getirilir. Ve insan o gün (yaptıklarını) hatırlar. Fakat bu hatırlamanın ne faydası var!"
    },
    {
      "resource_id": 112,
      "text": "Keşke hayatım için (ameller) yapıp sunsaydım."
    },
    {
      "resource_id": 112,
      "text": "İşte, hiç kimse o gün Allah’ın cezalandırdığı gibi cezalandıramaz."
    },
    { "resource_id": 112, "text": "Hiç kimse onun bağladığı gibi bağlayamaz." },
    { "resource_id": 112, "text": "Ey huzura ermiş nefis!" },
    {
      "resource_id": 112,
      "text": "Sen O'ndan hoşnut, O da senden hoşnut olarak Rabbine dön!"
    },
    { "resource_id": 112, "text": "Kullarımın arasına katıl!" },
    { "resource_id": 112, "text": "Ve gir Cennet'ime!" }
  ],
  "meta": {
    "translation_name": "Shaban Britch",
    "author_name": "Shaban Britch",
    "filters": { "resource_id": 112, "chapter_number": "89" }
  }
}
